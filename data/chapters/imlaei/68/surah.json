{
  "id": 68,
  "name": "The Pen",
  "verses": [
    {
      "id": 5272,
      "verse_number": 1,
      "verse_key": "68:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ن ۚ وَالْقَلَمِ وَمَا يَسْطُرُونَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5273,
      "verse_number": 2,
      "verse_key": "68:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5274,
      "verse_number": 3,
      "verse_key": "68:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5275,
      "verse_number": 4,
      "verse_key": "68:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5276,
      "verse_number": 5,
      "verse_key": "68:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَسَتُبْصِرُ وَيُبْصِرُونَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5277,
      "verse_number": 6,
      "verse_key": "68:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بِأَييِّكُمُ الْمَفْتُونُ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5278,
      "verse_number": 7,
      "verse_key": "68:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِ وَهُوَ أَعْلَمُ بِالْمُهْتَدِينَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5279,
      "verse_number": 8,
      "verse_key": "68:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا تُطِعِ الْمُكَذِّبِينَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5280,
      "verse_number": 9,
      "verse_key": "68:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَدُّوا لَوْ تُدْهِنُ فَيُدْهِنُونَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5281,
      "verse_number": 10,
      "verse_key": "68:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5282,
      "verse_number": 11,
      "verse_key": "68:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَمَّازٍ مَّشَّاءٍ بِنَمِيمٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5283,
      "verse_number": 12,
      "verse_key": "68:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5284,
      "verse_number": 13,
      "verse_key": "68:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عُتُلٍّ بَعْدَ ذَٰلِكَ زَنِيمٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5285,
      "verse_number": 14,
      "verse_key": "68:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَن كَانَ ذَا مَالٍ وَبَنِينَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5286,
      "verse_number": 15,
      "verse_key": "68:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5287,
      "verse_number": 16,
      "verse_key": "68:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "سَنَسِمُهُ عَلَى الْخُرْطُومِ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5288,
      "verse_number": 17,
      "verse_key": "68:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا بَلَوْنَاهُمْ كَمَا بَلَوْنَا أَصْحَابَ الْجَنَّةِ إِذْ أَقْسَمُوا لَيَصْرِمُنَّهَا مُصْبِحِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5289,
      "verse_number": 18,
      "verse_key": "68:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَسْتَثْنُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5290,
      "verse_number": 19,
      "verse_key": "68:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَطَافَ عَلَيْهَا طَائِفٌ مِّن رَّبِّكَ وَهُمْ نَائِمُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5291,
      "verse_number": 20,
      "verse_key": "68:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَصْبَحَتْ كَالصَّرِيمِ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5292,
      "verse_number": 21,
      "verse_key": "68:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَتَنَادَوْا مُصْبِحِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5293,
      "verse_number": 22,
      "verse_key": "68:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَنِ اغْدُوا عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَارِمِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5294,
      "verse_number": 23,
      "verse_key": "68:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَانطَلَقُوا وَهُمْ يَتَخَافَتُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5295,
      "verse_number": 24,
      "verse_key": "68:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَن لَّا يَدْخُلَنَّهَا الْيَوْمَ عَلَيْكُم مِّسْكِينٌ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5296,
      "verse_number": 25,
      "verse_key": "68:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَغَدَوْا عَلَىٰ حَرْدٍ قَادِرِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5297,
      "verse_number": 26,
      "verse_key": "68:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا رَأَوْهَا قَالُوا إِنَّا لَضَالُّونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5298,
      "verse_number": 27,
      "verse_key": "68:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلْ نَحْنُ مَحْرُومُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5299,
      "verse_number": 28,
      "verse_key": "68:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5300,
      "verse_number": 29,
      "verse_key": "68:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا سُبْحَانَ رَبِّنَا إِنَّا كُنَّا ظَالِمِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5301,
      "verse_number": 30,
      "verse_key": "68:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَاوَمُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5302,
      "verse_number": 31,
      "verse_key": "68:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا طَاغِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5303,
      "verse_number": 32,
      "verse_key": "68:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَسَىٰ رَبُّنَا أَن يُبْدِلَنَا خَيْرًا مِّنْهَا إِنَّا إِلَىٰ رَبِّنَا رَاغِبُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5304,
      "verse_number": 33,
      "verse_key": "68:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 500,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ الْعَذَابُ ۖ وَلَعَذَابُ الْآخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا يَعْلَمُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5305,
      "verse_number": 34,
      "verse_key": "68:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّاتِ النَّعِيمِ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5306,
      "verse_number": 35,
      "verse_key": "68:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَنَجْعَلُ الْمُسْلِمِينَ كَالْمُجْرِمِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5307,
      "verse_number": 36,
      "verse_key": "68:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا لَكُمْ كَيْفَ تَحْكُمُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5308,
      "verse_number": 37,
      "verse_key": "68:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَكُمْ كِتَابٌ فِيهِ تَدْرُسُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5309,
      "verse_number": 38,
      "verse_key": "68:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5310,
      "verse_number": 39,
      "verse_key": "68:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَكُمْ أَيْمَانٌ عَلَيْنَا بَالِغَةٌ إِلَىٰ يَوْمِ الْقِيَامَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5311,
      "verse_number": 40,
      "verse_key": "68:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5312,
      "verse_number": 41,
      "verse_key": "68:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَهُمْ شُرَكَاءُ فَلْيَأْتُوا بِشُرَكَائِهِمْ إِن كَانُوا صَادِقِينَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5313,
      "verse_number": 42,
      "verse_key": "68:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى السُّجُودِ فَلَا يَسْتَطِيعُونَ",
      "page_number": 565,
      "juz_number": 29
    },
    {
      "id": 5314,
      "verse_number": 43,
      "verse_key": "68:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "خَاشِعَةً أَبْصَارُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا يُدْعَوْنَ إِلَى السُّجُودِ وَهُمْ سَالِمُونَ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5315,
      "verse_number": 44,
