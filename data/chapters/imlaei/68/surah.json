      "verse_number": 50,
      "verse_key": "68:50",
      "hizb_number": 57,
      "rub_el_hizb_number": 226,
      "ruku_number": 501,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَاجْتَبَاهُ رَبُّهُ فَجَعَلَهُ مِنَ الصَّالِحِينَ",
      "page_number": 566,
      "juz_number": 29
    },
    {
