      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَا أُقْسِمُ بِهَٰذَا الْبَلَدِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6025,
      "verse_number": 2,
      "verse_key": "90:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنتَ حِلٌّ بِهَٰذَا الْبَلَدِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6026,
      "verse_number": 3,
      "verse_key": "90:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَوَالِدٍ وَمَا وَلَدَ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6027,
      "verse_number": 4,
      "verse_key": "90:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَقَدْ خَلَقْنَا الْإِنسَانَ فِي كَبَدٍ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6028,
      "verse_number": 5,
      "verse_key": "90:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6029,
      "verse_number": 6,
      "verse_key": "90:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6030,
      "verse_number": 7,
      "verse_key": "90:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَيَحْسَبُ أَن لَّمْ يَرَهُ أَحَدٌ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6031,
      "verse_number": 8,
      "verse_key": "90:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ نَجْعَل لَّهُ عَيْنَيْنِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6032,
      "verse_number": 9,
      "verse_key": "90:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلِسَانًا وَشَفَتَيْنِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6033,
      "verse_number": 10,
      "verse_key": "90:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَهَدَيْنَاهُ النَّجْدَيْنِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6034,
      "verse_number": 11,
      "verse_key": "90:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا اقْتَحَمَ الْعَقَبَةَ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6035,
      "verse_number": 12,
      "verse_key": "90:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَدْرَاكَ مَا الْعَقَبَةُ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6036,
      "verse_number": 13,
      "verse_key": "90:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَكُّ رَقَبَةٍ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6037,
      "verse_number": 14,
      "verse_key": "90:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَوْ إِطْعَامٌ فِي يَوْمٍ ذِي مَسْغَبَةٍ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6038,
      "verse_number": 15,
      "verse_key": "90:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَتِيمًا ذَا مَقْرَبَةٍ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6039,
      "verse_number": 16,
      "verse_key": "90:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6040,
      "verse_number": 17,
      "verse_key": "90:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ كَانَ مِنَ الَّذِينَ آمَنُوا وَتَوَاصَوْا بِالصَّبْرِ وَتَوَاصَوْا بِالْمَرْحَمَةِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6041,
      "verse_number": 18,
      "verse_key": "90:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أُولَٰئِكَ أَصْحَابُ الْمَيْمَنَةِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6042,
      "verse_number": 19,
      "verse_key": "90:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ كَفَرُوا بِآيَاتِنَا هُمْ أَصْحَابُ الْمَشْأَمَةِ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6043,
      "verse_number": 20,
      "verse_key": "90:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 534,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌ",
      "page_number": 594,
      "juz_number": 30
    }
  ]
}