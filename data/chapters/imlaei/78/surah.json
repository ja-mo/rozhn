      "verse_key": "78:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا النَّهَارَ مَعَاشًا",
      "page_number": 582,
