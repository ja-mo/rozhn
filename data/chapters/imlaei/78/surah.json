    },
    {
      "id": 5684,
      "verse_number": 12,
      "verse_key": "78:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5685,
      "verse_number": 13,
      "verse_key": "78:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5686,
      "verse_number": 14,
      "verse_key": "78:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنزَلْنَا مِنَ الْمُعْصِرَاتِ مَاءً ثَجَّاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5687,
      "verse_number": 15,
      "verse_key": "78:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّنُخْرِجَ بِهِ حَبًّا وَنَبَاتًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5688,
      "verse_number": 16,
      "verse_key": "78:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَنَّاتٍ أَلْفَافًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5689,
      "verse_number": 17,
      "verse_key": "78:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ يَوْمَ الْفَصْلِ كَانَ مِيقَاتًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5690,
      "verse_number": 18,
      "verse_key": "78:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يُنفَخُ فِي الصُّورِ فَتَأْتُونَ أَفْوَاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5691,
      "verse_number": 19,
      "verse_key": "78:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفُتِحَتِ السَّمَاءُ فَكَانَتْ أَبْوَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5692,
      "verse_number": 20,
      "verse_key": "78:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَسُيِّرَتِ الْجِبَالُ فَكَانَتْ سَرَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5693,
      "verse_number": 21,
      "verse_key": "78:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5694,
      "verse_number": 22,
      "verse_key": "78:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّلطَّاغِينَ مَآبًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5695,
      "verse_number": 23,
      "verse_key": "78:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَّابِثِينَ فِيهَا أَحْقَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5696,
      "verse_number": 24,
      "verse_key": "78:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5697,
      "verse_number": 25,
      "verse_key": "78:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا حَمِيمًا وَغَسَّاقًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5698,
      "verse_number": 26,
      "verse_key": "78:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "جَزَاءً وِفَاقًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5699,
      "verse_number": 27,
      "verse_key": "78:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُمْ كَانُوا لَا يَرْجُونَ حِسَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5700,
      "verse_number": 28,
      "verse_key": "78:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَذَّبُوا بِآيَاتِنَا كِذَّابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5701,
      "verse_number": 29,
      "verse_key": "78:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ كِتَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5702,
      "verse_number": 30,
      "verse_key": "78:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَذُوقُوا فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5703,
      "verse_number": 31,
      "verse_key": "78:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5704,
      "verse_number": 32,
      "verse_key": "78:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "حَدَائِقَ وَأَعْنَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5705,
      "verse_number": 33,
      "verse_key": "78:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَوَاعِبَ أَتْرَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5706,
      "verse_number": 34,
      "verse_key": "78:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَأْسًا دِهَاقًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5707,
      "verse_number": 35,
      "verse_key": "78:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5708,
      "verse_number": 36,
      "verse_key": "78:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "جَزَاءً مِّن رَّبِّكَ عَطَاءً حِسَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5709,
      "verse_number": 37,
      "verse_key": "78:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "رَّبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا الرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5710,
      "verse_number": 38,
      "verse_key": "78:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَقُومُ الرُّوحُ وَالْمَلَائِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ الرَّحْمَٰنُ وَقَالَ صَوَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5711,
      "verse_number": 39,
      "verse_key": "78:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذَٰلِكَ الْيَوْمُ الْحَقُّ ۖ فَمَن شَاءَ اتَّخَذَ إِلَىٰ رَبِّهِ مَآبًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5712,
      "verse_number": 40,
      "verse_key": "78:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا أَنذَرْنَاكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ الْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ الْكَافِرُ يَا لَيْتَنِي كُنتُ تُرَابًا",
      "page_number": 583,
      "juz_number": 30
    }
  ]
}