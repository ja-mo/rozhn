{
  "id": 71,
  "name": "Noah",
  "verses": [
    {
      "id": 5420,
      "verse_number": 1,
      "verse_key": "71:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِ أَنْ أَنذِرْ قَوْمَكَ مِن قَبْلِ أَن يَأْتِيَهُمْ عَذَابٌ أَلِيمٌ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5421,
      "verse_number": 2,
      "verse_key": "71:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ يَا قَوْمِ إِنِّي لَكُمْ نَذِيرٌ مُّبِينٌ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5422,
      "verse_number": 3,
      "verse_key": "71:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَنِ اعْبُدُوا اللَّهَ وَاتَّقُوهُ وَأَطِيعُونِ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5423,
      "verse_number": 4,
      "verse_key": "71:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَغْفِرْ لَكُم مِّن ذُنُوبِكُمْ وَيُؤَخِّرْكُمْ إِلَىٰ أَجَلٍ مُّسَمًّى ۚ إِنَّ أَجَلَ اللَّهِ إِذَا جَاءَ لَا يُؤَخَّرُ ۖ لَوْ كُنتُمْ تَعْلَمُونَ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5424,
      "verse_number": 5,
      "verse_key": "71:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ إِنِّي دَعَوْتُ قَوْمِي لَيْلًا وَنَهَارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5425,
      "verse_number": 6,
      "verse_key": "71:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَمْ يَزِدْهُمْ دُعَائِي إِلَّا فِرَارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5426,
      "verse_number": 7,
      "verse_key": "71:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِنِّي كُلَّمَا دَعَوْتُهُمْ لِتَغْفِرَ لَهُمْ جَعَلُوا أَصَابِعَهُمْ فِي آذَانِهِمْ وَاسْتَغْشَوْا ثِيَابَهُمْ وَأَصَرُّوا وَاسْتَكْبَرُوا اسْتِكْبَارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5427,
      "verse_number": 8,
      "verse_key": "71:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ إِنِّي دَعَوْتُهُمْ جِهَارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5428,
      "verse_number": 9,
      "verse_key": "71:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ إِنِّي أَعْلَنتُ لَهُمْ وَأَسْرَرْتُ لَهُمْ إِسْرَارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5429,
      "verse_number": 10,
      "verse_key": "71:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَقُلْتُ اسْتَغْفِرُوا رَبَّكُمْ إِنَّهُ كَانَ غَفَّارًا",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5430,
      "verse_number": 11,
      "verse_key": "71:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يُرْسِلِ السَّمَاءَ عَلَيْكُم مِّدْرَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5431,
      "verse_number": 12,
      "verse_key": "71:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيُمْدِدْكُم بِأَمْوَالٍ وَبَنِينَ وَيَجْعَل لَّكُمْ جَنَّاتٍ وَيَجْعَل لَّكُمْ أَنْهَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5432,
      "verse_number": 13,
      "verse_key": "71:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّا لَكُمْ لَا تَرْجُونَ لِلَّهِ وَقَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5433,
      "verse_number": 14,
      "verse_key": "71:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَدْ خَلَقَكُمْ أَطْوَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5434,
      "verse_number": 15,
      "verse_key": "71:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ تَرَوْا كَيْفَ خَلَقَ اللَّهُ سَبْعَ سَمَاوَاتٍ طِبَاقًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5435,
      "verse_number": 16,
      "verse_key": "71:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلَ الْقَمَرَ فِيهِنَّ نُورًا وَجَعَلَ الشَّمْسَ سِرَاجًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5436,
      "verse_number": 17,
      "verse_key": "71:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّهُ أَنبَتَكُم مِّنَ الْأَرْضِ نَبَاتًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5437,
      "verse_number": 18,
      "verse_key": "71:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ يُعِيدُكُمْ فِيهَا وَيُخْرِجُكُمْ إِخْرَاجًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5438,
      "verse_number": 19,
      "verse_key": "71:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّهُ جَعَلَ لَكُمُ الْأَرْضَ بِسَاطًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5439,
      "verse_number": 20,
      "verse_key": "71:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 506,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّتَسْلُكُوا مِنْهَا سُبُلًا فِجَاجًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5440,
      "verse_number": 21,
      "verse_key": "71:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ نُوحٌ رَّبِّ إِنَّهُمْ عَصَوْنِي وَاتَّبَعُوا مَن لَّمْ يَزِدْهُ مَالُهُ وَوَلَدُهُ إِلَّا خَسَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5441,
      "verse_number": 22,
      "verse_key": "71:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَكَرُوا مَكْرًا كُبَّارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5442,
      "verse_number": 23,
      "verse_key": "71:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا لَا تَذَرُنَّ آلِهَتَكُمْ وَلَا تَذَرُنَّ وَدًّا وَلَا سُوَاعًا وَلَا يَغُوثَ وَيَعُوقَ وَنَسْرًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5443,
      "verse_number": 24,
      "verse_key": "71:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَدْ أَضَلُّوا كَثِيرًا ۖ وَلَا تَزِدِ الظَّالِمِينَ إِلَّا ضَلَالًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5444,
      "verse_number": 25,
      "verse_key": "71:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مِّمَّا خَطِيئَاتِهِمْ أُغْرِقُوا فَأُدْخِلُوا نَارًا فَلَمْ يَجِدُوا لَهُم مِّن دُونِ اللَّهِ أَنصَارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5445,
      "verse_number": 26,
      "verse_key": "71:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ نُوحٌ رَّبِّ لَا تَذَرْ عَلَى الْأَرْضِ مِنَ الْكَافِرِينَ دَيَّارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5446,
      "verse_number": 27,
      "verse_key": "71:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكَ إِن تَذَرْهُمْ يُضِلُّوا عِبَادَكَ وَلَا يَلِدُوا إِلَّا فَاجِرًا كَفَّارًا",
      "page_number": 571,
      "juz_number": 29
    },
    {
      "id": 5447,
      "verse_number": 28,
      "verse_key": "71:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 507,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "رَّبِّ اغْفِرْ لِي وَلِوَالِدَيَّ وَلِمَن دَخَلَ بَيْتِيَ مُؤْمِنًا وَلِلْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ وَلَا تَزِدِ الظَّالِمِينَ إِلَّا تَبَارًا",
      "page_number": 571,
      "juz_number": 29
    }
  ]
}