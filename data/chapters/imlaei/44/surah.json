{
  "id": 44,
  "name": "The Smoke",
  "verses": [
    {
      "id": 4415,
      "verse_number": 1,
      "verse_key": "44:1",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "حم",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4416,
      "verse_number": 2,
      "verse_key": "44:2",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَالْكِتَابِ الْمُبِينِ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4417,
      "verse_number": 3,
      "verse_key": "44:3",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا أَنزَلْنَاهُ فِي لَيْلَةٍ مُّبَارَكَةٍ ۚ إِنَّا كُنَّا مُنذِرِينَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4418,
      "verse_number": 4,
      "verse_key": "44:4",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فِيهَا يُفْرَقُ كُلُّ أَمْرٍ حَكِيمٍ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4419,
      "verse_number": 5,
      "verse_key": "44:5",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمْرًا مِّنْ عِندِنَا ۚ إِنَّا كُنَّا مُرْسِلِينَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4420,
      "verse_number": 6,
      "verse_key": "44:6",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "رَحْمَةً مِّن رَّبِّكَ ۚ إِنَّهُ هُوَ السَّمِيعُ الْعَلِيمُ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4421,
      "verse_number": 7,
      "verse_key": "44:7",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "رَبِّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ إِن كُنتُم مُّوقِنِينَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4422,
      "verse_number": 8,
      "verse_key": "44:8",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لَا إِلَٰهَ إِلَّا هُوَ يُحْيِي وَيُمِيتُ ۖ رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4423,
      "verse_number": 9,
      "verse_key": "44:9",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ هُمْ فِي شَكٍّ يَلْعَبُونَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4424,
      "verse_number": 10,
      "verse_key": "44:10",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَارْتَقِبْ يَوْمَ تَأْتِي السَّمَاءُ بِدُخَانٍ مُّبِينٍ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4425,
      "verse_number": 11,
      "verse_key": "44:11",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "يَغْشَى النَّاسَ ۖ هَٰذَا عَذَابٌ أَلِيمٌ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4426,
      "verse_number": 12,
      "verse_key": "44:12",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "رَّبَّنَا اكْشِفْ عَنَّا الْعَذَابَ إِنَّا مُؤْمِنُونَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4427,
      "verse_number": 13,
      "verse_key": "44:13",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَنَّىٰ لَهُمُ الذِّكْرَىٰ وَقَدْ جَاءَهُمْ رَسُولٌ مُّبِينٌ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4428,
      "verse_number": 14,
      "verse_key": "44:14",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ تَوَلَّوْا عَنْهُ وَقَالُوا مُعَلَّمٌ مَّجْنُونٌ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4429,
      "verse_number": 15,
      "verse_key": "44:15",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا كَاشِفُو الْعَذَابِ قَلِيلًا ۚ إِنَّكُمْ عَائِدُونَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4430,
      "verse_number": 16,
      "verse_key": "44:16",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ نَبْطِشُ الْبَطْشَةَ الْكُبْرَىٰ إِنَّا مُنتَقِمُونَ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4431,
      "verse_number": 17,
      "verse_key": "44:17",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ فَتَنَّا قَبْلَهُمْ قَوْمَ فِرْعَوْنَ وَجَاءَهُمْ رَسُولٌ كَرِيمٌ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4432,
      "verse_number": 18,
      "verse_key": "44:18",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَنْ أَدُّوا إِلَيَّ عِبَادَ اللَّهِ ۖ إِنِّي لَكُمْ رَسُولٌ أَمِينٌ",
      "page_number": 496,
      "juz_number": 25
    },
    {
      "id": 4433,
      "verse_number": 19,
      "verse_key": "44:19",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَأَن لَّا تَعْلُوا عَلَى اللَّهِ ۖ إِنِّي آتِيكُم بِسُلْطَانٍ مُّبِينٍ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4434,
      "verse_number": 20,
      "verse_key": "44:20",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنِّي عُذْتُ بِرَبِّي وَرَبِّكُمْ أَن تَرْجُمُونِ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4435,
      "verse_number": 21,
      "verse_key": "44:21",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِن لَّمْ تُؤْمِنُوا لِي فَاعْتَزِلُونِ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4436,
      "verse_number": 22,
      "verse_key": "44:22",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَدَعَا رَبَّهُ أَنَّ هَٰؤُلَاءِ قَوْمٌ مُّجْرِمُونَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4437,
      "verse_number": 23,
      "verse_key": "44:23",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَأَسْرِ بِعِبَادِي لَيْلًا إِنَّكُم مُّتَّبَعُونَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4438,
      "verse_number": 24,
      "verse_key": "44:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَاتْرُكِ الْبَحْرَ رَهْوًا ۖ إِنَّهُمْ جُندٌ مُّغْرَقُونَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4439,
      "verse_number": 25,
      "verse_key": "44:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "كَمْ تَرَكُوا مِن جَنَّاتٍ وَعُيُونٍ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4440,
      "verse_number": 26,
      "verse_key": "44:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَزُرُوعٍ وَمَقَامٍ كَرِيمٍ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4441,
      "verse_number": 27,
      "verse_key": "44:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَنَعْمَةٍ كَانُوا فِيهَا فَاكِهِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4442,
      "verse_number": 28,
      "verse_key": "44:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ ۖ وَأَوْرَثْنَاهَا قَوْمًا آخَرِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4443,
      "verse_number": 29,
      "verse_key": "44:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 433,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَمَا بَكَتْ عَلَيْهِمُ السَّمَاءُ وَالْأَرْضُ وَمَا كَانُوا مُنظَرِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4444,
      "verse_number": 30,
      "verse_key": "44:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ نَجَّيْنَا بَنِي إِسْرَائِيلَ مِنَ الْعَذَابِ الْمُهِينِ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4445,
      "verse_number": 31,
      "verse_key": "44:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "مِن فِرْعَوْنَ ۚ إِنَّهُ كَانَ عَالِيًا مِّنَ الْمُسْرِفِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4446,
      "verse_number": 32,
      "verse_key": "44:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدِ اخْتَرْنَاهُمْ عَلَىٰ عِلْمٍ عَلَى الْعَالَمِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4447,
      "verse_number": 33,
      "verse_key": "44:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَآتَيْنَاهُم مِّنَ الْآيَاتِ مَا فِيهِ بَلَاءٌ مُّبِينٌ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4448,
      "verse_number": 34,
      "verse_key": "44:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ هَٰؤُلَاءِ لَيَقُولُونَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4449,
      "verse_number": 35,
      "verse_key": "44:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنْ هِيَ إِلَّا مَوْتَتُنَا الْأُولَىٰ وَمَا نَحْنُ بِمُنشَرِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4450,
      "verse_number": 36,
      "verse_key": "44:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَأْتُوا بِآبَائِنَا إِن كُنتُمْ صَادِقِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4451,
      "verse_number": 37,
      "verse_key": "44:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَهُمْ خَيْرٌ أَمْ قَوْمُ تُبَّعٍ وَالَّذِينَ مِن قَبْلِهِمْ ۚ أَهْلَكْنَاهُمْ ۖ إِنَّهُمْ كَانُوا مُجْرِمِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4452,
      "verse_number": 38,
      "verse_key": "44:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا خَلَقْنَا السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا لَاعِبِينَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4453,
      "verse_number": 39,
      "verse_key": "44:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "مَا خَلَقْنَاهُمَا إِلَّا بِالْحَقِّ وَلَٰكِنَّ أَكْثَرَهُمْ لَا يَعْلَمُونَ",
      "page_number": 497,
      "juz_number": 25
    },
    {
      "id": 4454,
      "verse_number": 40,
      "verse_key": "44:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ يَوْمَ الْفَصْلِ مِيقَاتُهُمْ أَجْمَعِينَ",
      "page_number": 498,
      "juz_number": 25
    },
    {
      "id": 4455,
      "verse_number": 41,
      "verse_key": "44:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 434,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ لَا يُغْنِي مَوْلًى عَن مَّوْلًى شَيْئًا وَلَا هُمْ يُنصَرُونَ",
      "page_number": 498,
      "juz_number": 25
    },
    {
      "id": 4456,
      "verse_number": 42,
      "verse_key": "44:42",
      "hizb_number": 50,
