      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكِتَابٍ مَّسْطُورٍ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4738,
      "verse_number": 3,
      "verse_key": "52:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِي رَقٍّ مَّنشُورٍ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4739,
      "verse_number": 4,
      "verse_key": "52:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْبَيْتِ الْمَعْمُورِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4740,
      "verse_number": 5,
      "verse_key": "52:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّقْفِ الْمَرْفُوعِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4741,
      "verse_number": 6,
      "verse_key": "52:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْبَحْرِ الْمَسْجُورِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4742,
      "verse_number": 7,
      "verse_key": "52:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ عَذَابَ رَبِّكَ لَوَاقِعٌ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4743,
      "verse_number": 8,
      "verse_key": "52:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّا لَهُ مِن دَافِعٍ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4744,
      "verse_number": 9,
      "verse_key": "52:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ تَمُورُ السَّمَاءُ مَوْرًا",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4745,
      "verse_number": 10,
      "verse_key": "52:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتَسِيرُ الْجِبَالُ سَيْرًا",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4746,
      "verse_number": 11,
      "verse_key": "52:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَوَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4747,
      "verse_number": 12,
      "verse_key": "52:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ هُمْ فِي خَوْضٍ يَلْعَبُونَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4748,
      "verse_number": 13,
      "verse_key": "52:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يُدَعُّونَ إِلَىٰ نَارِ جَهَنَّمَ دَعًّا",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4749,
      "verse_number": 14,
      "verse_key": "52:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَٰذِهِ النَّارُ الَّتِي كُنتُم بِهَا تُكَذِّبُونَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4750,
      "verse_number": 15,
      "verse_key": "52:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَسِحْرٌ هَٰذَا أَمْ أَنتُمْ لَا تُبْصِرُونَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4751,
      "verse_number": 16,
      "verse_key": "52:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "اصْلَوْهَا فَاصْبِرُوا أَوْ لَا تَصْبِرُوا سَوَاءٌ عَلَيْكُمْ ۖ إِنَّمَا تُجْزَوْنَ مَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4752,
      "verse_number": 17,
      "verse_key": "52:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَنَعِيمٍ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4753,
      "verse_number": 18,
      "verse_key": "52:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَاكِهِينَ بِمَا آتَاهُمْ رَبُّهُمْ وَوَقَاهُمْ رَبُّهُمْ عَذَابَ الْجَحِيمِ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4754,
      "verse_number": 19,
      "verse_key": "52:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كُلُوا وَاشْرَبُوا هَنِيئًا بِمَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4755,
      "verse_number": 20,
      "verse_key": "52:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مُتَّكِئِينَ عَلَىٰ سُرُرٍ مَّصْفُوفَةٍ ۖ وَزَوَّجْنَاهُم بِحُورٍ عِينٍ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4756,
      "verse_number": 21,
      "verse_key": "52:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ آمَنُوا وَاتَّبَعَتْهُمْ ذُرِّيَّتُهُم بِإِيمَانٍ أَلْحَقْنَا بِهِمْ ذُرِّيَّتَهُمْ وَمَا أَلَتْنَاهُم مِّنْ عَمَلِهِم مِّن شَيْءٍ ۚ كُلُّ امْرِئٍ بِمَا كَسَبَ رَهِينٌ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4757,
      "verse_number": 22,
      "verse_key": "52:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمْدَدْنَاهُم بِفَاكِهَةٍ وَلَحْمٍ مِّمَّا يَشْتَهُونَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4758,
      "verse_number": 23,
      "verse_key": "52:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَتَنَازَعُونَ فِيهَا كَأْسًا لَّا لَغْوٌ فِيهَا وَلَا تَأْثِيمٌ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4759,
      "verse_number": 24,
      "verse_key": "52:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيَطُوفُ عَلَيْهِمْ غِلْمَانٌ لَّهُمْ كَأَنَّهُمْ لُؤْلُؤٌ مَّكْنُونٌ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4760,
      "verse_number": 25,
      "verse_key": "52:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4761,
      "verse_number": 26,
      "verse_key": "52:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنَّا كُنَّا قَبْلُ فِي أَهْلِنَا مُشْفِقِينَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4762,
      "verse_number": 27,
      "verse_key": "52:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَنَّ اللَّهُ عَلَيْنَا وَوَقَانَا عَذَابَ السَّمُومِ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4763,
      "verse_number": 28,
      "verse_key": "52:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا كُنَّا مِن قَبْلُ نَدْعُوهُ ۖ إِنَّهُ هُوَ الْبَرُّ الرَّحِيمُ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4764,
      "verse_number": 29,
      "verse_key": "52:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَذَكِّرْ فَمَا أَنتَ بِنِعْمَتِ رَبِّكَ بِكَاهِنٍ وَلَا مَجْنُونٍ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4765,
      "verse_number": 30,
      "verse_key": "52:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ يَقُولُونَ شَاعِرٌ نَّتَرَبَّصُ بِهِ رَيْبَ الْمَنُونِ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4766,
      "verse_number": 31,
      "verse_key": "52:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ تَرَبَّصُوا فَإِنِّي مَعَكُم مِّنَ الْمُتَرَبِّصِينَ",
      "page_number": 524,
      "juz_number": 27
    },
    {
      "id": 4767,
      "verse_number": 32,
      "verse_key": "52:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ تَأْمُرُهُمْ أَحْلَامُهُم بِهَٰذَا ۚ أَمْ هُمْ قَوْمٌ طَاغُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4768,
      "verse_number": 33,
      "verse_key": "52:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ يَقُولُونَ تَقَوَّلَهُ ۚ بَل لَّا يُؤْمِنُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4769,
      "verse_number": 34,
      "verse_key": "52:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلْيَأْتُوا بِحَدِيثٍ مِّثْلِهِ إِن كَانُوا صَادِقِينَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4770,
      "verse_number": 35,
      "verse_key": "52:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ خُلِقُوا مِنْ غَيْرِ شَيْءٍ أَمْ هُمُ الْخَالِقُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4771,
      "verse_number": 36,
      "verse_key": "52:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ خَلَقُوا السَّمَاوَاتِ وَالْأَرْضَ ۚ بَل لَّا يُوقِنُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4772,
      "verse_number": 37,
      "verse_key": "52:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ عِندَهُمْ خَزَائِنُ رَبِّكَ أَمْ هُمُ الْمُصَيْطِرُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4773,
      "verse_number": 38,
      "verse_key": "52:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَهُمْ سُلَّمٌ يَسْتَمِعُونَ فِيهِ ۖ فَلْيَأْتِ مُسْتَمِعُهُم بِسُلْطَانٍ مُّبِينٍ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4774,
      "verse_number": 39,
      "verse_key": "52:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَهُ الْبَنَاتُ وَلَكُمُ الْبَنُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4775,
      "verse_number": 40,
      "verse_key": "52:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ تَسْأَلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4776,
      "verse_number": 41,
      "verse_key": "52:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ عِندَهُمُ الْغَيْبُ فَهُمْ يَكْتُبُونَ",
      "page_number": 525,
      "juz_number": 27
    },
    {
      "id": 4777,
      "verse_number": 42,
      "verse_key": "52:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
