{
  "id": 21,
  "name": "The Prophets",
  "verses": [
    {
      "id": 2484,
      "verse_number": 1,
      "verse_key": "21:1",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اقْتَرَبَ لِلنَّاسِ حِسَابُهُمْ وَهُمْ فِي غَفْلَةٍ مُّعْرِضُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2485,
      "verse_number": 2,
      "verse_key": "21:2",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مَا يَأْتِيهِم مِّن ذِكْرٍ مِّن رَّبِّهِم مُّحْدَثٍ إِلَّا اسْتَمَعُوهُ وَهُمْ يَلْعَبُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2486,
      "verse_number": 3,
      "verse_key": "21:3",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَاهِيَةً قُلُوبُهُمْ ۗ وَأَسَرُّوا النَّجْوَى الَّذِينَ ظَلَمُوا هَلْ هَٰذَا إِلَّا بَشَرٌ مِّثْلُكُمْ ۖ أَفَتَأْتُونَ السِّحْرَ وَأَنتُمْ تُبْصِرُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2487,
      "verse_number": 4,
      "verse_key": "21:4",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّي يَعْلَمُ الْقَوْلَ فِي السَّمَاءِ وَالْأَرْضِ ۖ وَهُوَ السَّمِيعُ الْعَلِيمُ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2488,
      "verse_number": 5,
      "verse_key": "21:5",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "بَلْ قَالُوا أَضْغَاثُ أَحْلَامٍ بَلِ افْتَرَاهُ بَلْ هُوَ شَاعِرٌ فَلْيَأْتِنَا بِآيَةٍ كَمَا أُرْسِلَ الْأَوَّلُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2489,
      "verse_number": 6,
      "verse_key": "21:6",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مَا آمَنَتْ قَبْلَهُم مِّن قَرْيَةٍ أَهْلَكْنَاهَا ۖ أَفَهُمْ يُؤْمِنُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2490,
      "verse_number": 7,
      "verse_key": "21:7",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَرْسَلْنَا قَبْلَكَ إِلَّا رِجَالًا نُّوحِي إِلَيْهِمْ ۖ فَاسْأَلُوا أَهْلَ الذِّكْرِ إِن كُنتُمْ لَا تَعْلَمُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2491,
      "verse_number": 8,
      "verse_key": "21:8",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا جَعَلْنَاهُمْ جَسَدًا لَّا يَأْكُلُونَ الطَّعَامَ وَمَا كَانُوا خَالِدِينَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2492,
      "verse_number": 9,
      "verse_key": "21:9",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ صَدَقْنَاهُمُ الْوَعْدَ فَأَنجَيْنَاهُمْ وَمَن نَّشَاءُ وَأَهْلَكْنَا الْمُسْرِفِينَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2493,
      "verse_number": 10,
      "verse_key": "21:10",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَقَدْ أَنزَلْنَا إِلَيْكُمْ كِتَابًا فِيهِ ذِكْرُكُمْ ۖ أَفَلَا تَعْقِلُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2494,
      "verse_number": 11,
      "verse_key": "21:11",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكَمْ قَصَمْنَا مِن قَرْيَةٍ كَانَتْ ظَالِمَةً وَأَنشَأْنَا بَعْدَهَا قَوْمًا آخَرِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2495,
      "verse_number": 12,
      "verse_key": "21:12",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا أَحَسُّوا بَأْسَنَا إِذَا هُم مِّنْهَا يَرْكُضُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2496,
      "verse_number": 13,
      "verse_key": "21:13",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَا تَرْكُضُوا وَارْجِعُوا إِلَىٰ مَا أُتْرِفْتُمْ فِيهِ وَمَسَاكِنِكُمْ لَعَلَّكُمْ تُسْأَلُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2497,
      "verse_number": 14,
      "verse_key": "21:14",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا يَا وَيْلَنَا إِنَّا كُنَّا ظَالِمِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2498,
      "verse_number": 15,
      "verse_key": "21:15",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَمَا زَالَت تِّلْكَ دَعْوَاهُمْ حَتَّىٰ جَعَلْنَاهُمْ حَصِيدًا خَامِدِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2499,
      "verse_number": 16,
      "verse_key": "21:16",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا خَلَقْنَا السَّمَاءَ وَالْأَرْضَ وَمَا بَيْنَهُمَا لَاعِبِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2500,
      "verse_number": 17,
      "verse_key": "21:17",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَوْ أَرَدْنَا أَن نَّتَّخِذَ لَهْوًا لَّاتَّخَذْنَاهُ مِن لَّدُنَّا إِن كُنَّا فَاعِلِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2501,
      "verse_number": 18,
      "verse_key": "21:18",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "بَلْ نَقْذِفُ بِالْحَقِّ عَلَى الْبَاطِلِ فَيَدْمَغُهُ فَإِذَا هُوَ زَاهِقٌ ۚ وَلَكُمُ الْوَيْلُ مِمَّا تَصِفُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2502,
      "verse_number": 19,
      "verse_key": "21:19",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَهُ مَن فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ وَمَنْ عِندَهُ لَا يَسْتَكْبِرُونَ عَنْ عِبَادَتِهِ وَلَا يَسْتَحْسِرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2503,
      "verse_number": 20,
      "verse_key": "21:20",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يُسَبِّحُونَ اللَّيْلَ وَالنَّهَارَ لَا يَفْتُرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2504,
      "verse_number": 21,
      "verse_key": "21:21",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَمِ اتَّخَذُوا آلِهَةً مِّنَ الْأَرْضِ هُمْ يُنشِرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2505,
      "verse_number": 22,
      "verse_key": "21:22",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَوْ كَانَ فِيهِمَا آلِهَةٌ إِلَّا اللَّهُ لَفَسَدَتَا ۚ فَسُبْحَانَ اللَّهِ رَبِّ الْعَرْشِ عَمَّا يَصِفُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2506,
      "verse_number": 23,
      "verse_key": "21:23",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَا يُسْأَلُ عَمَّا يَفْعَلُ وَهُمْ يُسْأَلُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2507,
      "verse_number": 24,
      "verse_key": "21:24",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَمِ اتَّخَذُوا مِن دُونِهِ آلِهَةً ۖ قُلْ هَاتُوا بُرْهَانَكُمْ ۖ هَٰذَا ذِكْرُ مَن مَّعِيَ وَذِكْرُ مَن قَبْلِي ۗ بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ الْحَقَّ ۖ فَهُم مُّعْرِضُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2508,
      "verse_number": 25,
      "verse_key": "21:25",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَرْسَلْنَا مِن قَبْلِكَ مِن رَّسُولٍ إِلَّا نُوحِي إِلَيْهِ أَنَّهُ لَا إِلَٰهَ إِلَّا أَنَا فَاعْبُدُونِ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2509,
      "verse_number": 26,
      "verse_key": "21:26",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا اتَّخَذَ الرَّحْمَٰنُ وَلَدًا ۗ سُبْحَانَهُ ۚ بَلْ عِبَادٌ مُّكْرَمُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2510,
      "verse_number": 27,
      "verse_key": "21:27",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَا يَسْبِقُونَهُ بِالْقَوْلِ وَهُم بِأَمْرِهِ يَعْمَلُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2511,
      "verse_number": 28,
      "verse_key": "21:28",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَعْلَمُ مَا بَيْنَ أَيْدِيهِمْ وَمَا خَلْفَهُمْ وَلَا يَشْفَعُونَ إِلَّا لِمَنِ ارْتَضَىٰ وَهُم مِّنْ خَشْيَتِهِ مُشْفِقُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2512,
      "verse_number": 29,
      "verse_key": "21:29",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَن يَقُلْ مِنْهُمْ إِنِّي إِلَٰهٌ مِّن دُونِهِ فَذَٰلِكَ نَجْزِيهِ جَهَنَّمَ ۚ كَذَٰلِكَ نَجْزِي الظَّالِمِينَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2513,
      "verse_number": 30,
      "verse_key": "21:30",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَمْ يَرَ الَّذِينَ كَفَرُوا أَنَّ السَّمَاوَاتِ وَالْأَرْضَ كَانَتَا رَتْقًا فَفَتَقْنَاهُمَا ۖ وَجَعَلْنَا مِنَ الْمَاءِ كُلَّ شَيْءٍ حَيٍّ ۖ أَفَلَا يُؤْمِنُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2514,
      "verse_number": 31,
      "verse_key": "21:31",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا فِي الْأَرْضِ رَوَاسِيَ أَن تَمِيدَ بِهِمْ وَجَعَلْنَا فِيهَا فِجَاجًا سُبُلًا لَّعَلَّهُمْ يَهْتَدُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2515,
      "verse_number": 32,
      "verse_key": "21:32",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا السَّمَاءَ سَقْفًا مَّحْفُوظًا ۖ وَهُمْ عَنْ آيَاتِهَا مُعْرِضُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2516,
      "verse_number": 33,
      "verse_key": "21:33",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي خَلَقَ اللَّيْلَ وَالنَّهَارَ وَالشَّمْسَ وَالْقَمَرَ ۖ كُلٌّ فِي فَلَكٍ يَسْبَحُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2517,
      "verse_number": 34,
      "verse_key": "21:34",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا جَعَلْنَا لِبَشَرٍ مِّن قَبْلِكَ الْخُلْدَ ۖ أَفَإِن مِّتَّ فَهُمُ الْخَالِدُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2518,
      "verse_number": 35,
      "verse_key": "21:35",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كُلُّ نَفْسٍ ذَائِقَةُ الْمَوْتِ ۗ وَنَبْلُوكُم بِالشَّرِّ وَالْخَيْرِ فِتْنَةً ۖ وَإِلَيْنَا تُرْجَعُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2519,
      "verse_number": 36,
      "verse_key": "21:36",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا رَآكَ الَّذِينَ كَفَرُوا إِن يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَٰذَا الَّذِي يَذْكُرُ آلِهَتَكُمْ وَهُم بِذِكْرِ الرَّحْمَٰنِ هُمْ كَافِرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2520,
      "verse_number": 37,
      "verse_key": "21:37",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "خُلِقَ الْإِنسَانُ مِنْ عَجَلٍ ۚ سَأُرِيكُمْ آيَاتِي فَلَا تَسْتَعْجِلُونِ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2521,
      "verse_number": 38,
      "verse_key": "21:38",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَقُولُونَ مَتَىٰ هَٰذَا الْوَعْدُ إِن كُنتُمْ صَادِقِينَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2522,
      "verse_number": 39,
      "verse_key": "21:39",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَوْ يَعْلَمُ الَّذِينَ كَفَرُوا حِينَ لَا يَكُفُّونَ عَن وُجُوهِهِمُ النَّارَ وَلَا عَن ظُهُورِهِمْ وَلَا هُمْ يُنصَرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2523,
      "verse_number": 40,
      "verse_key": "21:40",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "بَلْ تَأْتِيهِم بَغْتَةً فَتَبْهَتُهُمْ فَلَا يَسْتَطِيعُونَ رَدَّهَا وَلَا هُمْ يُنظَرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2524,
      "verse_number": 41,
      "verse_key": "21:41",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدِ اسْتُهْزِئَ بِرُسُلٍ مِّن قَبْلِكَ فَحَاقَ بِالَّذِينَ سَخِرُوا مِنْهُم مَّا كَانُوا بِهِ يَسْتَهْزِئُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2525,
      "verse_number": 42,
      "verse_key": "21:42",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ مَن يَكْلَؤُكُم بِاللَّيْلِ وَالنَّهَارِ مِنَ الرَّحْمَٰنِ ۗ بَلْ هُمْ عَن ذِكْرِ رَبِّهِم مُّعْرِضُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2526,
      "verse_number": 43,
      "verse_key": "21:43",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَهُمْ آلِهَةٌ تَمْنَعُهُم مِّن دُونِنَا ۚ لَا يَسْتَطِيعُونَ نَصْرَ أَنفُسِهِمْ وَلَا هُم مِّنَّا يُصْحَبُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2527,
      "verse_number": 44,
      "verse_key": "21:44",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "بَلْ مَتَّعْنَا هَٰؤُلَاءِ وَآبَاءَهُمْ حَتَّىٰ طَالَ عَلَيْهِمُ الْعُمُرُ ۗ أَفَلَا يَرَوْنَ أَنَّا نَأْتِي الْأَرْضَ نَنقُصُهَا مِنْ أَطْرَافِهَا ۚ أَفَهُمُ الْغَالِبُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2528,
      "verse_number": 45,
      "verse_key": "21:45",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنَّمَا أُنذِرُكُم بِالْوَحْيِ ۚ وَلَا يَسْمَعُ الصُّمُّ الدُّعَاءَ إِذَا مَا يُنذَرُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2529,
      "verse_number": 46,
      "verse_key": "21:46",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَئِن مَّسَّتْهُمْ نَفْحَةٌ مِّنْ عَذَابِ رَبِّكَ لَيَقُولُنَّ يَا وَيْلَنَا إِنَّا كُنَّا ظَالِمِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2530,
      "verse_number": 47,
      "verse_key": "21:47",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَضَعُ الْمَوَازِينَ الْقِسْطَ لِيَوْمِ الْقِيَامَةِ فَلَا تُظْلَمُ نَفْسٌ شَيْئًا ۖ وَإِن كَانَ مِثْقَالَ حَبَّةٍ مِّنْ خَرْدَلٍ أَتَيْنَا بِهَا ۗ وَكَفَىٰ بِنَا حَاسِبِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2531,
      "verse_number": 48,
      "verse_key": "21:48",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ آتَيْنَا مُوسَىٰ وَهَارُونَ الْفُرْقَانَ وَضِيَاءً وَذِكْرًا لِّلْمُتَّقِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2532,
      "verse_number": 49,
      "verse_key": "21:49",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ يَخْشَوْنَ رَبَّهُم بِالْغَيْبِ وَهُم مِّنَ السَّاعَةِ مُشْفِقُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2533,
      "verse_number": 50,
      "verse_key": "21:50",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهَٰذَا ذِكْرٌ مُّبَارَكٌ أَنزَلْنَاهُ ۚ أَفَأَنتُمْ لَهُ مُنكِرُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2534,
      "verse_number": 51,
      "verse_key": "21:51",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ آتَيْنَا إِبْرَاهِيمَ رُشْدَهُ مِن قَبْلُ وَكُنَّا بِهِ عَالِمِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2535,
      "verse_number": 52,
      "verse_key": "21:52",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا هَٰذِهِ التَّمَاثِيلُ الَّتِي أَنتُمْ لَهَا عَاكِفُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2536,
      "verse_number": 53,
      "verse_key": "21:53",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا وَجَدْنَا آبَاءَنَا لَهَا عَابِدِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2537,
      "verse_number": 54,
      "verse_key": "21:54",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَقَدْ كُنتُمْ أَنتُمْ وَآبَاؤُكُمْ فِي ضَلَالٍ مُّبِينٍ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2538,
      "verse_number": 55,
      "verse_key": "21:55",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا أَجِئْتَنَا بِالْحَقِّ أَمْ أَنتَ مِنَ اللَّاعِبِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2539,
      "verse_number": 56,
      "verse_key": "21:56",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ بَل رَّبُّكُمْ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ الَّذِي فَطَرَهُنَّ وَأَنَا عَلَىٰ ذَٰلِكُم مِّنَ الشَّاهِدِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2540,
      "verse_number": 57,
      "verse_key": "21:57",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَتَاللَّهِ لَأَكِيدَنَّ أَصْنَامَكُم بَعْدَ أَن تُوَلُّوا مُدْبِرِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2541,
      "verse_number": 58,
      "verse_key": "21:58",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَجَعَلَهُمْ جُذَاذًا إِلَّا كَبِيرًا لَّهُمْ لَعَلَّهُمْ إِلَيْهِ يَرْجِعُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2542,
      "verse_number": 59,
      "verse_key": "21:59",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا مَن فَعَلَ هَٰذَا بِآلِهَتِنَا إِنَّهُ لَمِنَ الظَّالِمِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2543,
      "verse_number": 60,
      "verse_key": "21:60",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا سَمِعْنَا فَتًى يَذْكُرُهُمْ يُقَالُ لَهُ إِبْرَاهِيمُ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2544,
      "verse_number": 61,
      "verse_key": "21:61",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا فَأْتُوا بِهِ عَلَىٰ أَعْيُنِ النَّاسِ لَعَلَّهُمْ يَشْهَدُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2545,
      "verse_number": 62,
      "verse_key": "21:62",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا أَأَنتَ فَعَلْتَ هَٰذَا بِآلِهَتِنَا يَا إِبْرَاهِيمُ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2546,
      "verse_number": 63,
      "verse_key": "21:63",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ بَلْ فَعَلَهُ كَبِيرُهُمْ هَٰذَا فَاسْأَلُوهُمْ إِن كَانُوا يَنطِقُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2547,
      "verse_number": 64,
      "verse_key": "21:64",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَرَجَعُوا إِلَىٰ أَنفُسِهِمْ فَقَالُوا إِنَّكُمْ أَنتُمُ الظَّالِمُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2548,
      "verse_number": 65,
      "verse_key": "21:65",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ نُكِسُوا عَلَىٰ رُءُوسِهِمْ لَقَدْ عَلِمْتَ مَا هَٰؤُلَاءِ يَنطِقُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2549,
      "verse_number": 66,
      "verse_key": "21:66",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَفَتَعْبُدُونَ مِن دُونِ اللَّهِ مَا لَا يَنفَعُكُمْ شَيْئًا وَلَا يَضُرُّكُمْ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2550,
      "verse_number": 67,
      "verse_key": "21:67",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أُفٍّ لَّكُمْ وَلِمَا تَعْبُدُونَ مِن دُونِ اللَّهِ ۖ أَفَلَا تَعْقِلُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2551,
      "verse_number": 68,
      "verse_key": "21:68",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا حَرِّقُوهُ وَانصُرُوا آلِهَتَكُمْ إِن كُنتُمْ فَاعِلِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2552,
      "verse_number": 69,
      "verse_key": "21:69",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْنَا يَا نَارُ كُونِي بَرْدًا وَسَلَامًا عَلَىٰ إِبْرَاهِيمَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2553,
      "verse_number": 70,
      "verse_key": "21:70",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَرَادُوا بِهِ كَيْدًا فَجَعَلْنَاهُمُ الْأَخْسَرِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2554,
      "verse_number": 71,
      "verse_key": "21:71",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَجَّيْنَاهُ وَلُوطًا إِلَى الْأَرْضِ الَّتِي بَارَكْنَا فِيهَا لِلْعَالَمِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2555,
      "verse_number": 72,
      "verse_key": "21:72",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَوَهَبْنَا لَهُ إِسْحَاقَ وَيَعْقُوبَ نَافِلَةً ۖ وَكُلًّا جَعَلْنَا صَالِحِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2556,
      "verse_number": 73,
      "verse_key": "21:73",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَاهُمْ أَئِمَّةً يَهْدُونَ بِأَمْرِنَا وَأَوْحَيْنَا إِلَيْهِمْ فِعْلَ الْخَيْرَاتِ وَإِقَامَ الصَّلَاةِ وَإِيتَاءَ الزَّكَاةِ ۖ وَكَانُوا لَنَا عَابِدِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2557,
      "verse_number": 74,
      "verse_key": "21:74",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلُوطًا آتَيْنَاهُ حُكْمًا وَعِلْمًا وَنَجَّيْنَاهُ مِنَ الْقَرْيَةِ الَّتِي كَانَت تَّعْمَلُ الْخَبَائِثَ ۗ إِنَّهُمْ كَانُوا قَوْمَ سَوْءٍ فَاسِقِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2558,
      "verse_number": 75,
      "verse_key": "21:75",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَدْخَلْنَاهُ فِي رَحْمَتِنَا ۖ إِنَّهُ مِنَ الصَّالِحِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2559,
      "verse_number": 76,
      "verse_key": "21:76",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنُوحًا إِذْ نَادَىٰ مِن قَبْلُ فَاسْتَجَبْنَا لَهُ فَنَجَّيْنَاهُ وَأَهْلَهُ مِنَ الْكَرْبِ الْعَظِيمِ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2560,
      "verse_number": 77,
      "verse_key": "21:77",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَصَرْنَاهُ مِنَ الْقَوْمِ الَّذِينَ كَذَّبُوا بِآيَاتِنَا ۚ إِنَّهُمْ كَانُوا قَوْمَ سَوْءٍ فَأَغْرَقْنَاهُمْ أَجْمَعِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2561,
      "verse_number": 78,
      "verse_key": "21:78",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَدَاوُودَ وَسُلَيْمَانَ إِذْ يَحْكُمَانِ فِي الْحَرْثِ إِذْ نَفَشَتْ فِيهِ غَنَمُ الْقَوْمِ وَكُنَّا لِحُكْمِهِمْ شَاهِدِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2562,
      "verse_number": 79,
      "verse_key": "21:79",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَفَهَّمْنَاهَا سُلَيْمَانَ ۚ وَكُلًّا آتَيْنَا حُكْمًا وَعِلْمًا ۚ وَسَخَّرْنَا مَعَ دَاوُودَ الْجِبَالَ يُسَبِّحْنَ وَالطَّيْرَ ۚ وَكُنَّا فَاعِلِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2563,
      "verse_number": 80,
      "verse_key": "21:80",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَعَلَّمْنَاهُ صَنْعَةَ لَبُوسٍ لَّكُمْ لِتُحْصِنَكُم مِّن بَأْسِكُمْ ۖ فَهَلْ أَنتُمْ شَاكِرُونَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2564,
      "verse_number": 81,
      "verse_key": "21:81",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلِسُلَيْمَانَ الرِّيحَ عَاصِفَةً تَجْرِي بِأَمْرِهِ إِلَى الْأَرْضِ الَّتِي بَارَكْنَا فِيهَا ۚ وَكُنَّا بِكُلِّ شَيْءٍ عَالِمِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2565,
      "verse_number": 82,
      "verse_key": "21:82",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمِنَ الشَّيَاطِينِ مَن يَغُوصُونَ لَهُ وَيَعْمَلُونَ عَمَلًا دُونَ ذَٰلِكَ ۖ وَكُنَّا لَهُمْ حَافِظِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2566,
      "verse_number": 83,
      "verse_key": "21:83",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَيُّوبَ إِذْ نَادَىٰ رَبَّهُ أَنِّي مَسَّنِيَ الضُّرُّ وَأَنتَ أَرْحَمُ الرَّاحِمِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2567,
      "verse_number": 84,
      "verse_key": "21:84",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَجَبْنَا لَهُ فَكَشَفْنَا مَا بِهِ مِن ضُرٍّ ۖ وَآتَيْنَاهُ أَهْلَهُ وَمِثْلَهُم مَّعَهُمْ رَحْمَةً مِّنْ عِندِنَا وَذِكْرَىٰ لِلْعَابِدِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2568,
      "verse_number": 85,
      "verse_key": "21:85",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِسْمَاعِيلَ وَإِدْرِيسَ وَذَا الْكِفْلِ ۖ كُلٌّ مِّنَ الصَّابِرِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2569,
      "verse_number": 86,
      "verse_key": "21:86",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَدْخَلْنَاهُمْ فِي رَحْمَتِنَا ۖ إِنَّهُم مِّنَ الصَّالِحِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2570,
      "verse_number": 87,
      "verse_key": "21:87",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَذَا النُّونِ إِذ ذَّهَبَ مُغَاضِبًا فَظَنَّ أَن لَّن نَّقْدِرَ عَلَيْهِ فَنَادَىٰ فِي الظُّلُمَاتِ أَن لَّا إِلَٰهَ إِلَّا أَنتَ سُبْحَانَكَ إِنِّي كُنتُ مِنَ الظَّالِمِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2571,
      "verse_number": 88,
      "verse_key": "21:88",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَجَبْنَا لَهُ وَنَجَّيْنَاهُ مِنَ الْغَمِّ ۚ وَكَذَٰلِكَ نُنجِي الْمُؤْمِنِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2572,
      "verse_number": 89,
      "verse_key": "21:89",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَزَكَرِيَّا إِذْ نَادَىٰ رَبَّهُ رَبِّ لَا تَذَرْنِي فَرْدًا وَأَنتَ خَيْرُ الْوَارِثِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2573,
      "verse_number": 90,
      "verse_key": "21:90",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَجَبْنَا لَهُ وَوَهَبْنَا لَهُ يَحْيَىٰ وَأَصْلَحْنَا لَهُ زَوْجَهُ ۚ إِنَّهُمْ كَانُوا يُسَارِعُونَ فِي الْخَيْرَاتِ وَيَدْعُونَنَا رَغَبًا وَرَهَبًا ۖ وَكَانُوا لَنَا خَاشِعِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2574,
      "verse_number": 91,
      "verse_key": "21:91",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّتِي أَحْصَنَتْ فَرْجَهَا فَنَفَخْنَا فِيهَا مِن رُّوحِنَا وَجَعَلْنَاهَا وَابْنَهَا آيَةً لِّلْعَالَمِينَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2575,
      "verse_number": 92,
      "verse_key": "21:92",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ هَٰذِهِ أُمَّتُكُمْ أُمَّةً وَاحِدَةً وَأَنَا رَبُّكُمْ فَاعْبُدُونِ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2576,
      "verse_number": 93,
      "verse_key": "21:93",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَتَقَطَّعُوا أَمْرَهُم بَيْنَهُمْ ۖ كُلٌّ إِلَيْنَا رَاجِعُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2577,
      "verse_number": 94,
      "verse_key": "21:94",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَمَن يَعْمَلْ مِنَ الصَّالِحَاتِ وَهُوَ مُؤْمِنٌ فَلَا كُفْرَانَ لِسَعْيِهِ وَإِنَّا لَهُ كَاتِبُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2578,
      "verse_number": 95,
      "verse_key": "21:95",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَحَرَامٌ عَلَىٰ قَرْيَةٍ أَهْلَكْنَاهَا أَنَّهُمْ لَا يَرْجِعُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2579,
      "verse_number": 96,
      "verse_key": "21:96",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "حَتَّىٰ إِذَا فُتِحَتْ يَأْجُوجُ وَمَأْجُوجُ وَهُم مِّن كُلِّ حَدَبٍ يَنسِلُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2580,
      "verse_number": 97,
      "verse_key": "21:97",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاقْتَرَبَ الْوَعْدُ الْحَقُّ فَإِذَا هِيَ شَاخِصَةٌ أَبْصَارُ الَّذِينَ كَفَرُوا يَا وَيْلَنَا قَدْ كُنَّا فِي غَفْلَةٍ مِّنْ هَٰذَا بَلْ كُنَّا ظَالِمِينَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2581,
      "verse_number": 98,
      "verse_key": "21:98",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكُمْ وَمَا تَعْبُدُونَ مِن دُونِ اللَّهِ حَصَبُ جَهَنَّمَ أَنتُمْ لَهَا وَارِدُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2582,
      "verse_number": 99,
      "verse_key": "21:99",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَوْ كَانَ هَٰؤُلَاءِ آلِهَةً مَّا وَرَدُوهَا ۖ وَكُلٌّ فِيهَا خَالِدُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2583,
      "verse_number": 100,
      "verse_key": "21:100",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَهُمْ فِيهَا زَفِيرٌ وَهُمْ فِيهَا لَا يَسْمَعُونَ",
      "page_number": 330,
      "juz_number": 17
    }
  ]
}