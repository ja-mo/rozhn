      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4788,
      "verse_number": 4,
      "verse_key": "53:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنْ هُوَ إِلَّا وَحْيٌ يُوحَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4789,
      "verse_number": 5,
      "verse_key": "53:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلَّمَهُ شَدِيدُ الْقُوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4790,
      "verse_number": 6,
      "verse_key": "53:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذُو مِرَّةٍ فَاسْتَوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4791,
      "verse_number": 7,
      "verse_key": "53:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ بِالْأُفُقِ الْأَعْلَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4792,
      "verse_number": 8,
      "verse_key": "53:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4793,
      "verse_number": 9,
      "verse_key": "53:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4794,
      "verse_number": 10,
      "verse_key": "53:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَوْحَىٰ إِلَىٰ عَبْدِهِ مَا أَوْحَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4795,
      "verse_number": 11,
      "verse_key": "53:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا كَذَبَ الْفُؤَادُ مَا رَأَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4796,
      "verse_number": 12,
      "verse_key": "53:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَتُمَارُونَهُ عَلَىٰ مَا يَرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4797,
      "verse_number": 13,
      "verse_key": "53:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ رَآهُ نَزْلَةً أُخْرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4798,
      "verse_number": 14,
      "verse_key": "53:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عِندَ سِدْرَةِ الْمُنتَهَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4799,
      "verse_number": 15,
      "verse_key": "53:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عِندَهَا جَنَّةُ الْمَأْوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4800,
      "verse_number": 16,
      "verse_key": "53:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذْ يَغْشَى السِّدْرَةَ مَا يَغْشَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4801,
      "verse_number": 17,
      "verse_key": "53:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا زَاغَ الْبَصَرُ وَمَا طَغَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4802,
      "verse_number": 18,
      "verse_key": "53:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَقَدْ رَأَىٰ مِنْ آيَاتِ رَبِّهِ الْكُبْرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4803,
      "verse_number": 19,
      "verse_key": "53:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَرَأَيْتُمُ اللَّاتَ وَالْعُزَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4804,
      "verse_number": 20,
      "verse_key": "53:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَنَاةَ الثَّالِثَةَ الْأُخْرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4805,
      "verse_number": 21,
      "verse_key": "53:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَكُمُ الذَّكَرُ وَلَهُ الْأُنثَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4806,
      "verse_number": 22,
      "verse_key": "53:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تِلْكَ إِذًا قِسْمَةٌ ضِيزَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4807,
      "verse_number": 23,
      "verse_key": "53:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنْ هِيَ إِلَّا أَسْمَاءٌ سَمَّيْتُمُوهَا أَنتُمْ وَآبَاؤُكُم مَّا أَنزَلَ اللَّهُ بِهَا مِن سُلْطَانٍ ۚ إِن يَتَّبِعُونَ إِلَّا الظَّنَّ وَمَا تَهْوَى الْأَنفُسُ ۖ وَلَقَدْ جَاءَهُم مِّن رَّبِّهِمُ الْهُدَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4808,
      "verse_number": 24,
      "verse_key": "53:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لِلْإِنسَانِ مَا تَمَنَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4809,
      "verse_number": 25,
      "verse_key": "53:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلِلَّهِ الْآخِرَةُ وَالْأُولَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4810,
      "verse_number": 26,
      "verse_key": "53:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَم مِّن مَّلَكٍ فِي السَّمَاوَاتِ لَا تُغْنِي شَفَاعَتُهُمْ شَيْئًا إِلَّا مِن بَعْدِ أَن يَأْذَنَ اللَّهُ لِمَن يَشَاءُ وَيَرْضَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4811,
      "verse_number": 27,
      "verse_key": "53:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ لَا يُؤْمِنُونَ بِالْآخِرَةِ لَيُسَمُّونَ الْمَلَائِكَةَ تَسْمِيَةَ الْأُنثَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4812,
      "verse_number": 28,
      "verse_key": "53:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا لَهُم بِهِ مِنْ عِلْمٍ ۖ إِن يَتَّبِعُونَ إِلَّا الظَّنَّ ۖ وَإِنَّ الظَّنَّ لَا يُغْنِي مِنَ الْحَقِّ شَيْئًا",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4813,
      "verse_number": 29,
      "verse_key": "53:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَعْرِضْ عَن مَّن تَوَلَّىٰ عَن ذِكْرِنَا وَلَمْ يُرِدْ إِلَّا الْحَيَاةَ الدُّنْيَا",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4814,
      "verse_number": 30,
      "verse_key": "53:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذَٰلِكَ مَبْلَغُهُم مِّنَ الْعِلْمِ ۚ إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِ وَهُوَ أَعْلَمُ بِمَنِ اهْتَدَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4815,
      "verse_number": 31,
      "verse_key": "53:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلِلَّهِ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ لِيَجْزِيَ الَّذِينَ أَسَاءُوا بِمَا عَمِلُوا وَيَجْزِيَ الَّذِينَ أَحْسَنُوا بِالْحُسْنَى",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4816,
      "verse_number": 32,
      "verse_key": "53:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ يَجْتَنِبُونَ كَبَائِرَ الْإِثْمِ وَالْفَوَاحِشَ إِلَّا اللَّمَمَ ۚ إِنَّ رَبَّكَ وَاسِعُ الْمَغْفِرَةِ ۚ هُوَ أَعْلَمُ بِكُمْ إِذْ أَنشَأَكُم مِّنَ الْأَرْضِ وَإِذْ أَنتُمْ أَجِنَّةٌ فِي بُطُونِ أُمَّهَاتِكُمْ ۖ فَلَا تُزَكُّوا أَنفُسَكُمْ ۖ هُوَ أَعْلَمُ بِمَنِ اتَّقَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4817,
      "verse_number": 33,
      "verse_key": "53:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَرَأَيْتَ الَّذِي تَوَلَّىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4818,
      "verse_number": 34,
      "verse_key": "53:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَعْطَىٰ قَلِيلًا وَأَكْدَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4819,
      "verse_number": 35,
      "verse_key": "53:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَعِندَهُ عِلْمُ الْغَيْبِ فَهُوَ يَرَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4820,
      "verse_number": 36,
      "verse_key": "53:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ لَمْ يُنَبَّأْ بِمَا فِي صُحُفِ مُوسَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4821,
      "verse_number": 37,
      "verse_key": "53:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِبْرَاهِيمَ الَّذِي وَفَّىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4822,
      "verse_number": 38,
      "verse_key": "53:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَّا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4823,
      "verse_number": 39,
      "verse_key": "53:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَن لَّيْسَ لِلْإِنسَانِ إِلَّا مَا سَعَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4824,
      "verse_number": 40,
      "verse_key": "53:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّ سَعْيَهُ سَوْفَ يُرَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4825,
      "verse_number": 41,
      "verse_key": "53:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ يُجْزَاهُ الْجَزَاءَ الْأَوْفَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4826,
