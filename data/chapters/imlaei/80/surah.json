    },
    {
      "id": 5760,
      "verse_number": 2,
      "verse_key": "80:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَن جَاءَهُ الْأَعْمَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5761,
      "verse_number": 3,
      "verse_key": "80:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يُدْرِيكَ لَعَلَّهُ يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5762,
      "verse_number": 4,
      "verse_key": "80:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ الذِّكْرَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5763,
      "verse_number": 5,
      "verse_key": "80:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمَّا مَنِ اسْتَغْنَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5764,
      "verse_number": 6,
      "verse_key": "80:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَنتَ لَهُ تَصَدَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5765,
      "verse_number": 7,
      "verse_key": "80:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5766,
      "verse_number": 8,
      "verse_key": "80:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا مَن جَاءَكَ يَسْعَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5767,
      "verse_number": 9,
      "verse_key": "80:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ يَخْشَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5768,
      "verse_number": 10,
      "verse_key": "80:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5769,
      "verse_number": 11,
      "verse_key": "80:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِنَّهَا تَذْكِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5770,
      "verse_number": 12,
      "verse_key": "80:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَن شَاءَ ذَكَرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5771,
      "verse_number": 13,
      "verse_key": "80:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِي صُحُفٍ مُّكَرَّمَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5772,
      "verse_number": 14,
      "verse_key": "80:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّرْفُوعَةٍ مُّطَهَّرَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5773,
      "verse_number": 15,
      "verse_key": "80:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بِأَيْدِي سَفَرَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5774,
      "verse_number": 16,
      "verse_key": "80:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كِرَامٍ بَرَرَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5775,
      "verse_number": 17,
      "verse_key": "80:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُتِلَ الْإِنسَانُ مَا أَكْفَرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5776,
      "verse_number": 18,
      "verse_key": "80:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مِنْ أَيِّ شَيْءٍ خَلَقَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5777,
      "verse_number": 19,
      "verse_key": "80:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مِن نُّطْفَةٍ خَلَقَهُ فَقَدَّرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5778,
      "verse_number": 20,
      "verse_key": "80:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ السَّبِيلَ يَسَّرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5779,
      "verse_number": 21,
      "verse_key": "80:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ أَمَاتَهُ فَأَقْبَرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5780,
      "verse_number": 22,
      "verse_key": "80:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ إِذَا شَاءَ أَنشَرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5781,
      "verse_number": 23,
      "verse_key": "80:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا لَمَّا يَقْضِ مَا أَمَرَهُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5782,
      "verse_number": 24,
      "verse_key": "80:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلْيَنظُرِ الْإِنسَانُ إِلَىٰ طَعَامِهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5783,
      "verse_number": 25,
      "verse_key": "80:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَنَّا صَبَبْنَا الْمَاءَ صَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5784,
      "verse_number": 26,
      "verse_key": "80:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ شَقَقْنَا الْأَرْضَ شَقًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5785,
      "verse_number": 27,
      "verse_key": "80:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَنبَتْنَا فِيهَا حَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5786,
      "verse_number": 28,
      "verse_key": "80:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَعِنَبًا وَقَضْبًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5787,
      "verse_number": 29,
      "verse_key": "80:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَزَيْتُونًا وَنَخْلًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5788,
      "verse_number": 30,
      "verse_key": "80:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَحَدَائِقَ غُلْبًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5789,
      "verse_number": 31,
      "verse_key": "80:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفَاكِهَةً وَأَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5790,
      "verse_number": 32,
      "verse_key": "80:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5791,
      "verse_number": 33,
      "verse_key": "80:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا جَاءَتِ الصَّاخَّةُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5792,
      "verse_number": 34,
      "verse_key": "80:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَفِرُّ الْمَرْءُ مِنْ أَخِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5793,
      "verse_number": 35,
      "verse_key": "80:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأُمِّهِ وَأَبِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5794,
      "verse_number": 36,
      "verse_key": "80:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَصَاحِبَتِهِ وَبَنِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5795,
      "verse_number": 37,
      "verse_key": "80:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِكُلِّ امْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5796,
      "verse_number": 38,
      "verse_key": "80:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5797,
      "verse_number": 39,
      "verse_key": "80:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5798,
      "verse_number": 40,
      "verse_key": "80:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5799,
      "verse_number": 41,
      "verse_key": "80:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَرْهَقُهَا قَتَرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5800,
      "verse_number": 42,
      "verse_key": "80:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أُولَٰئِكَ هُمُ الْكَفَرَةُ الْفَجَرَةُ",
