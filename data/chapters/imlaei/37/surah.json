{
  "id": 37,
  "name": "Those who set the Ranks",
  "verses": [
    {
      "id": 3789,
      "verse_number": 1,
      "verse_key": "37:1",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَالصَّافَّاتِ صَفًّا",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3790,
      "verse_number": 2,
      "verse_key": "37:2",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَالزَّاجِرَاتِ زَجْرًا",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3791,
      "verse_number": 3,
      "verse_key": "37:3",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَالتَّالِيَاتِ ذِكْرًا",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3792,
      "verse_number": 4,
      "verse_key": "37:4",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ إِلَٰهَكُمْ لَوَاحِدٌ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3793,
      "verse_number": 5,
      "verse_key": "37:5",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "رَّبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَرَبُّ الْمَشَارِقِ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3794,
      "verse_number": 6,
      "verse_key": "37:6",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا زَيَّنَّا السَّمَاءَ الدُّنْيَا بِزِينَةٍ الْكَوَاكِبِ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3795,
      "verse_number": 7,
      "verse_key": "37:7",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَحِفْظًا مِّن كُلِّ شَيْطَانٍ مَّارِدٍ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3796,
      "verse_number": 8,
      "verse_key": "37:8",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لَّا يَسَّمَّعُونَ إِلَى الْمَلَإِ الْأَعْلَىٰ وَيُقْذَفُونَ مِن كُلِّ جَانِبٍ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3797,
      "verse_number": 9,
      "verse_key": "37:9",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "دُحُورًا ۖ وَلَهُمْ عَذَابٌ وَاصِبٌ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3798,
      "verse_number": 10,
      "verse_key": "37:10",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا مَنْ خَطِفَ الْخَطْفَةَ فَأَتْبَعَهُ شِهَابٌ ثَاقِبٌ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3799,
      "verse_number": 11,
      "verse_key": "37:11",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَفْتِهِمْ أَهُمْ أَشَدُّ خَلْقًا أَم مَّنْ خَلَقْنَا ۚ إِنَّا خَلَقْنَاهُم مِّن طِينٍ لَّازِبٍ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3800,
      "verse_number": 12,
      "verse_key": "37:12",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ عَجِبْتَ وَيَسْخَرُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3801,
      "verse_number": 13,
      "verse_key": "37:13",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا ذُكِّرُوا لَا يَذْكُرُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3802,
      "verse_number": 14,
      "verse_key": "37:14",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا رَأَوْا آيَةً يَسْتَسْخِرُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3803,
      "verse_number": 15,
      "verse_key": "37:15",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا إِنْ هَٰذَا إِلَّا سِحْرٌ مُّبِينٌ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3804,
      "verse_number": 16,
      "verse_key": "37:16",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَامًا أَإِنَّا لَمَبْعُوثُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3805,
      "verse_number": 17,
      "verse_key": "37:17",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَوَآبَاؤُنَا الْأَوَّلُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3806,
      "verse_number": 18,
      "verse_key": "37:18",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "قُلْ نَعَمْ وَأَنتُمْ دَاخِرُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3807,
      "verse_number": 19,
      "verse_key": "37:19",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّمَا هِيَ زَجْرَةٌ وَاحِدَةٌ فَإِذَا هُمْ يَنظُرُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3808,
      "verse_number": 20,
      "verse_key": "37:20",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا يَا وَيْلَنَا هَٰذَا يَوْمُ الدِّينِ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3809,
      "verse_number": 21,
      "verse_key": "37:21",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 388,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "هَٰذَا يَوْمُ الْفَصْلِ الَّذِي كُنتُم بِهِ تُكَذِّبُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3810,
      "verse_number": 22,
      "verse_key": "37:22",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "احْشُرُوا الَّذِينَ ظَلَمُوا وَأَزْوَاجَهُمْ وَمَا كَانُوا يَعْبُدُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3811,
      "verse_number": 23,
      "verse_key": "37:23",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "مِن دُونِ اللَّهِ فَاهْدُوهُمْ إِلَىٰ صِرَاطِ الْجَحِيمِ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3812,
      "verse_number": 24,
      "verse_key": "37:24",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقِفُوهُمْ ۖ إِنَّهُم مَّسْئُولُونَ",
      "page_number": 446,
      "juz_number": 23
    },
    {
      "id": 3813,
      "verse_number": 25,
      "verse_key": "37:25",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "مَا لَكُمْ لَا تَنَاصَرُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3814,
      "verse_number": 26,
      "verse_key": "37:26",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ هُمُ الْيَوْمَ مُسْتَسْلِمُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3815,
      "verse_number": 27,
      "verse_key": "37:27",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَسَاءَلُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3816,
      "verse_number": 28,
      "verse_key": "37:28",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنَّكُمْ كُنتُمْ تَأْتُونَنَا عَنِ الْيَمِينِ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3817,
      "verse_number": 29,
      "verse_key": "37:29",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا بَل لَّمْ تَكُونُوا مُؤْمِنِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3818,
      "verse_number": 30,
      "verse_key": "37:30",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا كَانَ لَنَا عَلَيْكُم مِّن سُلْطَانٍ ۖ بَلْ كُنتُمْ قَوْمًا طَاغِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3819,
      "verse_number": 31,
      "verse_key": "37:31",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَحَقَّ عَلَيْنَا قَوْلُ رَبِّنَا ۖ إِنَّا لَذَائِقُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3820,
      "verse_number": 32,
      "verse_key": "37:32",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَأَغْوَيْنَاكُمْ إِنَّا كُنَّا غَاوِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3821,
      "verse_number": 33,
      "verse_key": "37:33",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّهُمْ يَوْمَئِذٍ فِي الْعَذَابِ مُشْتَرِكُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3822,
      "verse_number": 34,
      "verse_key": "37:34",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا كَذَٰلِكَ نَفْعَلُ بِالْمُجْرِمِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3823,
      "verse_number": 35,
      "verse_key": "37:35",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُمْ كَانُوا إِذَا قِيلَ لَهُمْ لَا إِلَٰهَ إِلَّا اللَّهُ يَسْتَكْبِرُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3824,
      "verse_number": 36,
      "verse_key": "37:36",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَيَقُولُونَ أَئِنَّا لَتَارِكُو آلِهَتِنَا لِشَاعِرٍ مَّجْنُونٍ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3825,
      "verse_number": 37,
      "verse_key": "37:37",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ جَاءَ بِالْحَقِّ وَصَدَّقَ الْمُرْسَلِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3826,
      "verse_number": 38,
      "verse_key": "37:38",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكُمْ لَذَائِقُو الْعَذَابِ الْأَلِيمِ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3827,
      "verse_number": 39,
      "verse_key": "37:39",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3828,
      "verse_number": 40,
      "verse_key": "37:40",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا عِبَادَ اللَّهِ الْمُخْلَصِينَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3829,
      "verse_number": 41,
      "verse_key": "37:41",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أُولَٰئِكَ لَهُمْ رِزْقٌ مَّعْلُومٌ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3830,
      "verse_number": 42,
      "verse_key": "37:42",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَوَاكِهُ ۖ وَهُم مُّكْرَمُونَ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3831,
      "verse_number": 43,
      "verse_key": "37:43",
      "hizb_number": 45,
      "rub_el_hizb_number": 179,
      "ruku_number": 389,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فِي جَنَّاتِ النَّعِيمِ",
      "page_number": 447,
      "juz_number": 23
    },
    {
      "id": 3832,
      "verse_number": 44,
      "verse_key": "37:44",
      "hizb_number": 45,
