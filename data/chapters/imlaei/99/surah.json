      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا زُلْزِلَتِ الْأَرْضُ زِلْزَالَهَا",
      "page_number": 599,
      "juz_number": 30
    },
    {
      "id": 6140,
      "verse_number": 2,
      "verse_key": "99:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَخْرَجَتِ الْأَرْضُ أَثْقَالَهَا",
      "page_number": 599,
      "juz_number": 30
    },
    {
