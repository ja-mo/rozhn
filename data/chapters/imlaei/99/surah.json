    },
    {
      "id": 6144,
      "verse_number": 6,
      "verse_key": "99:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَئِذٍ يَصْدُرُ النَّاسُ أَشْتَاتًا لِّيُرَوْا أَعْمَالَهُمْ",
      "page_number": 599,
      "juz_number": 30
    },
    {
      "id": 6145,
      "verse_number": 7,
      "verse_key": "99:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَن يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُ",
      "page_number": 599,
      "juz_number": 30
    },
    {
      "id": 6146,
      "verse_number": 8,
      "verse_key": "99:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَن يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُ",
      "page_number": 599,
      "juz_number": 30
