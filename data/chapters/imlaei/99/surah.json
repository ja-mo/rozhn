      "verse_number": 5,
      "verse_key": "99:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا",
      "page_number": 599,
