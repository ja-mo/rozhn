      "verse_key": "99:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ الْإِنسَانُ مَا لَهَا",
      "page_number": 599,
      "juz_number": 30
    },
    {
      "id": 6142,
      "verse_number": 4,
      "verse_key": "99:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 543,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا",
      "page_number": 599,
      "juz_number": 30
    },
    {
