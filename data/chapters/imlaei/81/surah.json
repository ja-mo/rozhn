{
  "id": 81,
  "name": "The Overthrowing",
  "verses": [
    {
      "id": 5801,
      "verse_number": 1,
      "verse_key": "81:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا الشَّمْسُ كُوِّرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5802,
      "verse_number": 2,
      "verse_key": "81:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا النُّجُومُ انكَدَرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5803,
      "verse_number": 3,
      "verse_key": "81:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْجِبَالُ سُيِّرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5804,
      "verse_number": 4,
      "verse_key": "81:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْعِشَارُ عُطِّلَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5805,
      "verse_number": 5,
      "verse_key": "81:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْوُحُوشُ حُشِرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5806,
      "verse_number": 6,
      "verse_key": "81:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْبِحَارُ سُجِّرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5807,
      "verse_number": 7,
      "verse_key": "81:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا النُّفُوسُ زُوِّجَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5808,
      "verse_number": 8,
      "verse_key": "81:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْمَوْءُودَةُ سُئِلَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5809,
      "verse_number": 9,
      "verse_key": "81:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بِأَيِّ ذَنبٍ قُتِلَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5810,
      "verse_number": 10,
      "verse_key": "81:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الصُّحُفُ نُشِرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5811,
      "verse_number": 11,
      "verse_key": "81:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا السَّمَاءُ كُشِطَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5812,
      "verse_number": 12,
      "verse_key": "81:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْجَحِيمُ سُعِّرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5813,
      "verse_number": 13,
      "verse_key": "81:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْجَنَّةُ أُزْلِفَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5814,
      "verse_number": 14,
      "verse_key": "81:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلِمَتْ نَفْسٌ مَّا أَحْضَرَتْ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5815,
      "verse_number": 15,
      "verse_key": "81:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا أُقْسِمُ بِالْخُنَّسِ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5816,
      "verse_number": 16,
      "verse_key": "81:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الْجَوَارِ الْكُنَّسِ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5817,
      "verse_number": 17,
      "verse_key": "81:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّيْلِ إِذَا عَسْعَسَ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5818,
      "verse_number": 18,
      "verse_key": "81:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالصُّبْحِ إِذَا تَنَفَّسَ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5819,
      "verse_number": 19,
      "verse_key": "81:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5820,
      "verse_number": 20,
      "verse_key": "81:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذِي قُوَّةٍ عِندَ ذِي الْعَرْشِ مَكِينٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5821,
      "verse_number": 21,
      "verse_key": "81:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مُّطَاعٍ ثَمَّ أَمِينٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5822,
      "verse_number": 22,
      "verse_key": "81:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5823,
      "verse_number": 23,
      "verse_key": "81:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ رَآهُ بِالْأُفُقِ الْمُبِينِ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5824,
      "verse_number": 24,
      "verse_key": "81:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا هُوَ عَلَى الْغَيْبِ بِضَنِينٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5825,
      "verse_number": 25,
      "verse_key": "81:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا هُوَ بِقَوْلِ شَيْطَانٍ رَّجِيمٍ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5826,
      "verse_number": 26,
      "verse_key": "81:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَيْنَ تَذْهَبُونَ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5827,
      "verse_number": 27,
      "verse_key": "81:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَالَمِينَ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5828,
      "verse_number": 28,
      "verse_key": "81:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِمَن شَاءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "page_number": 586,
      "juz_number": 30
    },
    {
      "id": 5829,
      "verse_number": 29,
      "verse_key": "81:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا تَشَاءُونَ إِلَّا أَن يَشَاءَ اللَّهُ رَبُّ الْعَالَمِينَ",
      "page_number": 586,
      "juz_number": 30
    }
  ]
}