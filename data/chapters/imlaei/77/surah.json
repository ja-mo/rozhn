      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ",
      "page_number": 581,
      "juz_number": 29
    },
    {
