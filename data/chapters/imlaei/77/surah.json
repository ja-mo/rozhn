      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَحْيَاءً وَأَمْوَاتًا",
      "page_number": 581,
      "juz_number": 29
    },
    {
      "id": 5649,
      "verse_number": 27,
      "verse_key": "77:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
