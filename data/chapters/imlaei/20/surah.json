{
  "id": 20,
  "name": "Ta-Ha",
  "verses": [
    {
      "id": 2349,
      "verse_number": 1,
      "verse_key": "20:1",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "طه",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2350,
      "verse_number": 2,
      "verse_key": "20:2",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مَا أَنزَلْنَا عَلَيْكَ الْقُرْآنَ لِتَشْقَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2351,
      "verse_number": 3,
      "verse_key": "20:3",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا تَذْكِرَةً لِّمَن يَخْشَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2352,
      "verse_number": 4,
      "verse_key": "20:4",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تَنزِيلًا مِّمَّنْ خَلَقَ الْأَرْضَ وَالسَّمَاوَاتِ الْعُلَى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2353,
      "verse_number": 5,
      "verse_key": "20:5",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الرَّحْمَٰنُ عَلَى الْعَرْشِ اسْتَوَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2354,
      "verse_number": 6,
      "verse_key": "20:6",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَهُ مَا فِي السَّمَاوَاتِ وَمَا فِي الْأَرْضِ وَمَا بَيْنَهُمَا وَمَا تَحْتَ الثَّرَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2355,
      "verse_number": 7,
      "verse_key": "20:7",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِن تَجْهَرْ بِالْقَوْلِ فَإِنَّهُ يَعْلَمُ السِّرَّ وَأَخْفَى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2356,
      "verse_number": 8,
      "verse_key": "20:8",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اللَّهُ لَا إِلَٰهَ إِلَّا هُوَ ۖ لَهُ الْأَسْمَاءُ الْحُسْنَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2357,
      "verse_number": 9,
      "verse_key": "20:9",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2358,
      "verse_number": 10,
      "verse_key": "20:10",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ رَأَىٰ نَارًا فَقَالَ لِأَهْلِهِ امْكُثُوا إِنِّي آنَسْتُ نَارًا لَّعَلِّي آتِيكُم مِّنْهَا بِقَبَسٍ أَوْ أَجِدُ عَلَى النَّارِ هُدًى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2359,
      "verse_number": 11,
      "verse_key": "20:11",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا أَتَاهَا نُودِيَ يَا مُوسَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2360,
      "verse_number": 12,
      "verse_key": "20:12",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنِّي أَنَا رَبُّكَ فَاخْلَعْ نَعْلَيْكَ ۖ إِنَّكَ بِالْوَادِ الْمُقَدَّسِ طُوًى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2361,
      "verse_number": 13,
      "verse_key": "20:13",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَا اخْتَرْتُكَ فَاسْتَمِعْ لِمَا يُوحَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2362,
      "verse_number": 14,
      "verse_key": "20:14",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّنِي أَنَا اللَّهُ لَا إِلَٰهَ إِلَّا أَنَا فَاعْبُدْنِي وَأَقِمِ الصَّلَاةَ لِذِكْرِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2363,
      "verse_number": 15,
      "verse_key": "20:15",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ السَّاعَةَ آتِيَةٌ أَكَادُ أُخْفِيهَا لِتُجْزَىٰ كُلُّ نَفْسٍ بِمَا تَسْعَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2364,
      "verse_number": 16,
      "verse_key": "20:16",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَا يَصُدَّنَّكَ عَنْهَا مَن لَّا يُؤْمِنُ بِهَا وَاتَّبَعَ هَوَاهُ فَتَرْدَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2365,
      "verse_number": 17,
      "verse_key": "20:17",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا تِلْكَ بِيَمِينِكَ يَا مُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2366,
      "verse_number": 18,
      "verse_key": "20:18",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ هِيَ عَصَايَ أَتَوَكَّأُ عَلَيْهَا وَأَهُشُّ بِهَا عَلَىٰ غَنَمِي وَلِيَ فِيهَا مَآرِبُ أُخْرَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2367,
      "verse_number": 19,
      "verse_key": "20:19",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَلْقِهَا يَا مُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2368,
      "verse_number": 20,
      "verse_key": "20:20",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَلْقَاهَا فَإِذَا هِيَ حَيَّةٌ تَسْعَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2369,
      "verse_number": 21,
      "verse_key": "20:21",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ خُذْهَا وَلَا تَخَفْ ۖ سَنُعِيدُهَا سِيرَتَهَا الْأُولَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2370,
      "verse_number": 22,
      "verse_key": "20:22",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاضْمُمْ يَدَكَ إِلَىٰ جَنَاحِكَ تَخْرُجْ بَيْضَاءَ مِنْ غَيْرِ سُوءٍ آيَةً أُخْرَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2371,
      "verse_number": 23,
      "verse_key": "20:23",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لِنُرِيَكَ مِنْ آيَاتِنَا الْكُبْرَى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2372,
      "verse_number": 24,
      "verse_key": "20:24",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2373,
      "verse_number": 25,
      "verse_key": "20:25",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ اشْرَحْ لِي صَدْرِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2374,
      "verse_number": 26,
      "verse_key": "20:26",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَسِّرْ لِي أَمْرِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2375,
      "verse_number": 27,
      "verse_key": "20:27",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاحْلُلْ عُقْدَةً مِّن لِّسَانِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2376,
      "verse_number": 28,
      "verse_key": "20:28",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَفْقَهُوا قَوْلِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2377,
      "verse_number": 29,
      "verse_key": "20:29",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاجْعَل لِّي وَزِيرًا مِّنْ أَهْلِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2378,
      "verse_number": 30,
      "verse_key": "20:30",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "هَارُونَ أَخِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2379,
      "verse_number": 31,
      "verse_key": "20:31",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اشْدُدْ بِهِ أَزْرِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2380,
      "verse_number": 32,
      "verse_key": "20:32",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَشْرِكْهُ فِي أَمْرِي",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2381,
      "verse_number": 33,
      "verse_key": "20:33",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كَيْ نُسَبِّحَكَ كَثِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2382,
      "verse_number": 34,
      "verse_key": "20:34",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَذْكُرَكَ كَثِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2383,
      "verse_number": 35,
      "verse_key": "20:35",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكَ كُنتَ بِنَا بَصِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2384,
      "verse_number": 36,
      "verse_key": "20:36",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ قَدْ أُوتِيتَ سُؤْلَكَ يَا مُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2385,
      "verse_number": 37,
      "verse_key": "20:37",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ مَنَنَّا عَلَيْكَ مَرَّةً أُخْرَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2386,
      "verse_number": 38,
      "verse_key": "20:38",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ أَوْحَيْنَا إِلَىٰ أُمِّكَ مَا يُوحَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2387,
      "verse_number": 39,
      "verse_key": "20:39",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَنِ اقْذِفِيهِ فِي التَّابُوتِ فَاقْذِفِيهِ فِي الْيَمِّ فَلْيُلْقِهِ الْيَمُّ بِالسَّاحِلِ يَأْخُذْهُ عَدُوٌّ لِّي وَعَدُوٌّ لَّهُ ۚ وَأَلْقَيْتُ عَلَيْكَ مَحَبَّةً مِّنِّي وَلِتُصْنَعَ عَلَىٰ عَيْنِي",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2388,
      "verse_number": 40,
      "verse_key": "20:40",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ تَمْشِي أُخْتُكَ فَتَقُولُ هَلْ أَدُلُّكُمْ عَلَىٰ مَن يَكْفُلُهُ ۖ فَرَجَعْنَاكَ إِلَىٰ أُمِّكَ كَيْ تَقَرَّ عَيْنُهَا وَلَا تَحْزَنَ ۚ وَقَتَلْتَ نَفْسًا فَنَجَّيْنَاكَ مِنَ الْغَمِّ وَفَتَنَّاكَ فُتُونًا ۚ فَلَبِثْتَ سِنِينَ فِي أَهْلِ مَدْيَنَ ثُمَّ جِئْتَ عَلَىٰ قَدَرٍ يَا مُوسَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2389,
      "verse_number": 41,
      "verse_key": "20:41",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاصْطَنَعْتُكَ لِنَفْسِي",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2390,
      "verse_number": 42,
      "verse_key": "20:42",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اذْهَبْ أَنتَ وَأَخُوكَ بِآيَاتِي وَلَا تَنِيَا فِي ذِكْرِي",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2391,
      "verse_number": 43,
      "verse_key": "20:43",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "اذْهَبَا إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2392,
      "verse_number": 44,
      "verse_key": "20:44",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَقُولَا لَهُ قَوْلًا لَّيِّنًا لَّعَلَّهُ يَتَذَكَّرُ أَوْ يَخْشَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2393,
      "verse_number": 45,
      "verse_key": "20:45",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَا رَبَّنَا إِنَّنَا نَخَافُ أَن يَفْرُطَ عَلَيْنَا أَوْ أَن يَطْغَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2394,
      "verse_number": 46,
      "verse_key": "20:46",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَا تَخَافَا ۖ إِنَّنِي مَعَكُمَا أَسْمَعُ وَأَرَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2395,
      "verse_number": 47,
      "verse_key": "20:47",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأْتِيَاهُ فَقُولَا إِنَّا رَسُولَا رَبِّكَ فَأَرْسِلْ مَعَنَا بَنِي إِسْرَائِيلَ وَلَا تُعَذِّبْهُمْ ۖ قَدْ جِئْنَاكَ بِآيَةٍ مِّن رَّبِّكَ ۖ وَالسَّلَامُ عَلَىٰ مَنِ اتَّبَعَ الْهُدَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2396,
      "verse_number": 48,
      "verse_key": "20:48",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا قَدْ أُوحِيَ إِلَيْنَا أَنَّ الْعَذَابَ عَلَىٰ مَن كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2397,
      "verse_number": 49,
      "verse_key": "20:49",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَمَن رَّبُّكُمَا يَا مُوسَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2398,
      "verse_number": 50,
      "verse_key": "20:50",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبُّنَا الَّذِي أَعْطَىٰ كُلَّ شَيْءٍ خَلْقَهُ ثُمَّ هَدَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2399,
      "verse_number": 51,
      "verse_key": "20:51",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَمَا بَالُ الْقُرُونِ الْأُولَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2400,
      "verse_number": 52,
      "verse_key": "20:52",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ عِلْمُهَا عِندَ رَبِّي فِي كِتَابٍ ۖ لَّا يَضِلُّ رَبِّي وَلَا يَنسَى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2401,
      "verse_number": 53,
      "verse_key": "20:53",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي جَعَلَ لَكُمُ الْأَرْضَ مَهْدًا وَسَلَكَ لَكُمْ فِيهَا سُبُلًا وَأَنزَلَ مِنَ السَّمَاءِ مَاءً فَأَخْرَجْنَا بِهِ أَزْوَاجًا مِّن نَّبَاتٍ شَتَّىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2402,
      "verse_number": 54,
      "verse_key": "20:54",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كُلُوا وَارْعَوْا أَنْعَامَكُمْ ۗ إِنَّ فِي ذَٰلِكَ لَآيَاتٍ لِّأُولِي النُّهَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2403,
      "verse_number": 55,
      "verse_key": "20:55",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مِنْهَا خَلَقْنَاكُمْ وَفِيهَا نُعِيدُكُمْ وَمِنْهَا نُخْرِجُكُمْ تَارَةً أُخْرَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2404,
      "verse_number": 56,
      "verse_key": "20:56",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَرَيْنَاهُ آيَاتِنَا كُلَّهَا فَكَذَّبَ وَأَبَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2405,
      "verse_number": 57,
      "verse_key": "20:57",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَجِئْتَنَا لِتُخْرِجَنَا مِنْ أَرْضِنَا بِسِحْرِكَ يَا مُوسَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2406,
      "verse_number": 58,
      "verse_key": "20:58",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَنَأْتِيَنَّكَ بِسِحْرٍ مِّثْلِهِ فَاجْعَلْ بَيْنَنَا وَبَيْنَكَ مَوْعِدًا لَّا نُخْلِفُهُ نَحْنُ وَلَا أَنتَ مَكَانًا سُوًى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2407,
      "verse_number": 59,
      "verse_key": "20:59",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ مَوْعِدُكُمْ يَوْمُ الزِّينَةِ وَأَن يُحْشَرَ النَّاسُ ضُحًى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2408,
      "verse_number": 60,
      "verse_key": "20:60",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَتَوَلَّىٰ فِرْعَوْنُ فَجَمَعَ كَيْدَهُ ثُمَّ أَتَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2409,
      "verse_number": 61,
      "verse_key": "20:61",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَهُم مُّوسَىٰ وَيْلَكُمْ لَا تَفْتَرُوا عَلَى اللَّهِ كَذِبًا فَيُسْحِتَكُم بِعَذَابٍ ۖ وَقَدْ خَابَ مَنِ افْتَرَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2410,
      "verse_number": 62,
      "verse_key": "20:62",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَتَنَازَعُوا أَمْرَهُم بَيْنَهُمْ وَأَسَرُّوا النَّجْوَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2411,
      "verse_number": 63,
      "verse_key": "20:63",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنْ هَٰذَانِ لَسَاحِرَانِ يُرِيدَانِ أَن يُخْرِجَاكُم مِّنْ أَرْضِكُم بِسِحْرِهِمَا وَيَذْهَبَا بِطَرِيقَتِكُمُ الْمُثْلَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2412,
      "verse_number": 64,
      "verse_key": "20:64",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَجْمِعُوا كَيْدَكُمْ ثُمَّ ائْتُوا صَفًّا ۚ وَقَدْ أَفْلَحَ الْيَوْمَ مَنِ اسْتَعْلَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2413,
      "verse_number": 65,
      "verse_key": "20:65",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا يَا مُوسَىٰ إِمَّا أَن تُلْقِيَ وَإِمَّا أَن نَّكُونَ أَوَّلَ مَنْ أَلْقَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2414,
      "verse_number": 66,
      "verse_key": "20:66",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ بَلْ أَلْقُوا ۖ فَإِذَا حِبَالُهُمْ وَعِصِيُّهُمْ يُخَيَّلُ إِلَيْهِ مِن سِحْرِهِمْ أَنَّهَا تَسْعَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2415,
      "verse_number": 67,
      "verse_key": "20:67",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَوْجَسَ فِي نَفْسِهِ خِيفَةً مُّوسَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2416,
      "verse_number": 68,
      "verse_key": "20:68",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْنَا لَا تَخَفْ إِنَّكَ أَنتَ الْأَعْلَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2417,
      "verse_number": 69,
      "verse_key": "20:69",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَلْقِ مَا فِي يَمِينِكَ تَلْقَفْ مَا صَنَعُوا ۖ إِنَّمَا صَنَعُوا كَيْدُ سَاحِرٍ ۖ وَلَا يُفْلِحُ السَّاحِرُ حَيْثُ أَتَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2418,
      "verse_number": 70,
      "verse_key": "20:70",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأُلْقِيَ السَّحَرَةُ سُجَّدًا قَالُوا آمَنَّا بِرَبِّ هَارُونَ وَمُوسَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2419,
      "verse_number": 71,
      "verse_key": "20:71",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ آمَنتُمْ لَهُ قَبْلَ أَنْ آذَنَ لَكُمْ ۖ إِنَّهُ لَكَبِيرُكُمُ الَّذِي عَلَّمَكُمُ السِّحْرَ ۖ فَلَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَافٍ وَلَأُصَلِّبَنَّكُمْ فِي جُذُوعِ النَّخْلِ وَلَتَعْلَمُنَّ أَيُّنَا أَشَدُّ عَذَابًا وَأَبْقَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2420,
      "verse_number": 72,
      "verse_key": "20:72",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا لَن نُّؤْثِرَكَ عَلَىٰ مَا جَاءَنَا مِنَ الْبَيِّنَاتِ وَالَّذِي فَطَرَنَا ۖ فَاقْضِ مَا أَنتَ قَاضٍ ۖ إِنَّمَا تَقْضِي هَٰذِهِ الْحَيَاةَ الدُّنْيَا",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2421,
      "verse_number": 73,
      "verse_key": "20:73",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا آمَنَّا بِرَبِّنَا لِيَغْفِرَ لَنَا خَطَايَانَا وَمَا أَكْرَهْتَنَا عَلَيْهِ مِنَ السِّحْرِ ۗ وَاللَّهُ خَيْرٌ وَأَبْقَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2422,
      "verse_number": 74,
      "verse_key": "20:74",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ مَن يَأْتِ رَبَّهُ مُجْرِمًا فَإِنَّ لَهُ جَهَنَّمَ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2423,
      "verse_number": 75,
      "verse_key": "20:75",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَن يَأْتِهِ مُؤْمِنًا قَدْ عَمِلَ الصَّالِحَاتِ فَأُولَٰئِكَ لَهُمُ الدَّرَجَاتُ الْعُلَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2424,
      "verse_number": 76,
      "verse_key": "20:76",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "جَنَّاتُ عَدْنٍ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا ۚ وَذَٰلِكَ جَزَاءُ مَن تَزَكَّىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2425,
      "verse_number": 77,
      "verse_key": "20:77",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ بِعِبَادِي فَاضْرِبْ لَهُمْ طَرِيقًا فِي الْبَحْرِ يَبَسًا لَّا تَخَافُ دَرَكًا وَلَا تَخْشَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2426,
      "verse_number": 78,
      "verse_key": "20:78",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَتْبَعَهُمْ فِرْعَوْنُ بِجُنُودِهِ فَغَشِيَهُم مِّنَ الْيَمِّ مَا غَشِيَهُمْ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2427,
      "verse_number": 79,
      "verse_key": "20:79",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَضَلَّ فِرْعَوْنُ قَوْمَهُ وَمَا هَدَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2428,
      "verse_number": 80,
      "verse_key": "20:80",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا بَنِي إِسْرَائِيلَ قَدْ أَنجَيْنَاكُم مِّنْ عَدُوِّكُمْ وَوَاعَدْنَاكُمْ جَانِبَ الطُّورِ الْأَيْمَنَ وَنَزَّلْنَا عَلَيْكُمُ الْمَنَّ وَالسَّلْوَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2429,
      "verse_number": 81,
      "verse_key": "20:81",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كُلُوا مِن طَيِّبَاتِ مَا رَزَقْنَاكُمْ وَلَا تَطْغَوْا فِيهِ فَيَحِلَّ عَلَيْكُمْ غَضَبِي ۖ وَمَن يَحْلِلْ عَلَيْهِ غَضَبِي فَقَدْ هَوَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2430,
      "verse_number": 82,
      "verse_key": "20:82",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِنِّي لَغَفَّارٌ لِّمَن تَابَ وَآمَنَ وَعَمِلَ صَالِحًا ثُمَّ اهْتَدَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2431,
      "verse_number": 83,
      "verse_key": "20:83",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَعْجَلَكَ عَن قَوْمِكَ يَا مُوسَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2432,
      "verse_number": 84,
      "verse_key": "20:84",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ هُمْ أُولَاءِ عَلَىٰ أَثَرِي وَعَجِلْتُ إِلَيْكَ رَبِّ لِتَرْضَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2433,
      "verse_number": 85,
      "verse_key": "20:85",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَإِنَّا قَدْ فَتَنَّا قَوْمَكَ مِن بَعْدِكَ وَأَضَلَّهُمُ السَّامِرِيُّ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2434,
      "verse_number": 86,
      "verse_key": "20:86",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَرَجَعَ مُوسَىٰ إِلَىٰ قَوْمِهِ غَضْبَانَ أَسِفًا ۚ قَالَ يَا قَوْمِ أَلَمْ يَعِدْكُمْ رَبُّكُمْ وَعْدًا حَسَنًا ۚ أَفَطَالَ عَلَيْكُمُ الْعَهْدُ أَمْ أَرَدتُّمْ أَن يَحِلَّ عَلَيْكُمْ غَضَبٌ مِّن رَّبِّكُمْ فَأَخْلَفْتُم مَّوْعِدِي",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2435,
      "verse_number": 87,
      "verse_key": "20:87",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا مَا أَخْلَفْنَا مَوْعِدَكَ بِمَلْكِنَا وَلَٰكِنَّا حُمِّلْنَا أَوْزَارًا مِّن زِينَةِ الْقَوْمِ فَقَذَفْنَاهَا فَكَذَٰلِكَ أَلْقَى السَّامِرِيُّ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2436,
      "verse_number": 88,
      "verse_key": "20:88",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَخْرَجَ لَهُمْ عِجْلًا جَسَدًا لَّهُ خُوَارٌ فَقَالُوا هَٰذَا إِلَٰهُكُمْ وَإِلَٰهُ مُوسَىٰ فَنَسِيَ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2437,
      "verse_number": 89,
      "verse_key": "20:89",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَفَلَا يَرَوْنَ أَلَّا يَرْجِعُ إِلَيْهِمْ قَوْلًا وَلَا يَمْلِكُ لَهُمْ ضَرًّا وَلَا نَفْعًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2438,
      "verse_number": 90,
      "verse_key": "20:90",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ قَالَ لَهُمْ هَارُونُ مِن قَبْلُ يَا قَوْمِ إِنَّمَا فُتِنتُم بِهِ ۖ وَإِنَّ رَبَّكُمُ الرَّحْمَٰنُ فَاتَّبِعُونِي وَأَطِيعُوا أَمْرِي",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2439,
      "verse_number": 91,
      "verse_key": "20:91",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا لَن نَّبْرَحَ عَلَيْهِ عَاكِفِينَ حَتَّىٰ يَرْجِعَ إِلَيْنَا مُوسَىٰ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2440,
      "verse_number": 92,
      "verse_key": "20:92",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ يَا هَارُونُ مَا مَنَعَكَ إِذْ رَأَيْتَهُمْ ضَلُّوا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2441,
      "verse_number": 93,
      "verse_key": "20:93",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَلَّا تَتَّبِعَنِ ۖ أَفَعَصَيْتَ أَمْرِي",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2442,
      "verse_number": 94,
      "verse_key": "20:94",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ يَا ابْنَ أُمَّ لَا تَأْخُذْ بِلِحْيَتِي وَلَا بِرَأْسِي ۖ إِنِّي خَشِيتُ أَن تَقُولَ فَرَّقْتَ بَيْنَ بَنِي إِسْرَائِيلَ وَلَمْ تَرْقُبْ قَوْلِي",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2443,
      "verse_number": 95,
      "verse_key": "20:95",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَمَا خَطْبُكَ يَا سَامِرِيُّ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2444,
      "verse_number": 96,
      "verse_key": "20:96",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ بَصُرْتُ بِمَا لَمْ يَبْصُرُوا بِهِ فَقَبَضْتُ قَبْضَةً مِّنْ أَثَرِ الرَّسُولِ فَنَبَذْتُهَا وَكَذَٰلِكَ سَوَّلَتْ لِي نَفْسِي",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2445,
      "verse_number": 97,
      "verse_key": "20:97",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَاذْهَبْ فَإِنَّ لَكَ فِي الْحَيَاةِ أَن تَقُولَ لَا مِسَاسَ ۖ وَإِنَّ لَكَ مَوْعِدًا لَّن تُخْلَفَهُ ۖ وَانظُرْ إِلَىٰ إِلَٰهِكَ الَّذِي ظَلْتَ عَلَيْهِ عَاكِفًا ۖ لَّنُحَرِّقَنَّهُ ثُمَّ لَنَنسِفَنَّهُ فِي الْيَمِّ نَسْفًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2446,
      "verse_number": 98,
      "verse_key": "20:98",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّمَا إِلَٰهُكُمُ اللَّهُ الَّذِي لَا إِلَٰهَ إِلَّا هُوَ ۚ وَسِعَ كُلَّ شَيْءٍ عِلْمًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2447,
      "verse_number": 99,
      "verse_key": "20:99",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ نَقُصُّ عَلَيْكَ مِنْ أَنبَاءِ مَا قَدْ سَبَقَ ۚ وَقَدْ آتَيْنَاكَ مِن لَّدُنَّا ذِكْرًا",
      "page_number": 319,
      "juz_number": 16
    },
    {
      "id": 2448,
      "verse_number": 100,
      "verse_key": "20:100",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مَّنْ أَعْرَضَ عَنْهُ فَإِنَّهُ يَحْمِلُ يَوْمَ الْقِيَامَةِ وِزْرًا",
      "page_number": 319,
      "juz_number": 16
    }
  ]
}