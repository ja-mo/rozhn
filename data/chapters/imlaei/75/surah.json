{
  "id": 75,
  "name": "The Resurrection",
  "verses": [
    {
      "id": 5552,
      "verse_number": 1,
      "verse_key": "75:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَا أُقْسِمُ بِيَوْمِ الْقِيَامَةِ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5553,
      "verse_number": 2,
      "verse_key": "75:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا أُقْسِمُ بِالنَّفْسِ اللَّوَّامَةِ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5554,
      "verse_number": 3,
      "verse_key": "75:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَيَحْسَبُ الْإِنسَانُ أَلَّن نَّجْمَعَ عِظَامَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5555,
      "verse_number": 4,
      "verse_key": "75:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلَىٰ قَادِرِينَ عَلَىٰ أَن نُّسَوِّيَ بَنَانَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5556,
      "verse_number": 5,
      "verse_key": "75:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلْ يُرِيدُ الْإِنسَانُ لِيَفْجُرَ أَمَامَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5557,
      "verse_number": 6,
      "verse_key": "75:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَسْأَلُ أَيَّانَ يَوْمُ الْقِيَامَةِ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5558,
      "verse_number": 7,
      "verse_key": "75:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا بَرِقَ الْبَصَرُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5559,
      "verse_number": 8,
      "verse_key": "75:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَخَسَفَ الْقَمَرُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5560,
      "verse_number": 9,
      "verse_key": "75:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجُمِعَ الشَّمْسُ وَالْقَمَرُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5561,
      "verse_number": 10,
      "verse_key": "75:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَقُولُ الْإِنسَانُ يَوْمَئِذٍ أَيْنَ الْمَفَرُّ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5562,
      "verse_number": 11,
      "verse_key": "75:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا لَا وَزَرَ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5563,
      "verse_number": 12,
      "verse_key": "75:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمُسْتَقَرُّ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5564,
      "verse_number": 13,
      "verse_key": "75:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يُنَبَّأُ الْإِنسَانُ يَوْمَئِذٍ بِمَا قَدَّمَ وَأَخَّرَ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5565,
      "verse_number": 14,
      "verse_key": "75:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلِ الْإِنسَانُ عَلَىٰ نَفْسِهِ بَصِيرَةٌ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5566,
      "verse_number": 15,
      "verse_key": "75:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5567,
      "verse_number": 16,
      "verse_key": "75:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَا تُحَرِّكْ بِهِ لِسَانَكَ لِتَعْجَلَ بِهِ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5568,
      "verse_number": 17,
      "verse_key": "75:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ عَلَيْنَا جَمْعَهُ وَقُرْآنَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5569,
      "verse_number": 18,
      "verse_key": "75:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا قَرَأْنَاهُ فَاتَّبِعْ قُرْآنَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5570,
      "verse_number": 19,
      "verse_key": "75:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُ",
      "page_number": 577,
      "juz_number": 29
    },
    {
      "id": 5571,
      "verse_number": 20,
      "verse_key": "75:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا بَلْ تُحِبُّونَ الْعَاجِلَةَ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5572,
      "verse_number": 21,
      "verse_key": "75:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتَذَرُونَ الْآخِرَةَ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5573,
      "verse_number": 22,
      "verse_key": "75:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5574,
      "verse_number": 23,
      "verse_key": "75:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَىٰ رَبِّهَا نَاظِرَةٌ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5575,
      "verse_number": 24,
      "verse_key": "75:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَوُجُوهٌ يَوْمَئِذٍ بَاسِرَةٌ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5576,
      "verse_number": 25,
      "verse_key": "75:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5577,
      "verse_number": 26,
      "verse_key": "75:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِذَا بَلَغَتِ التَّرَاقِيَ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5578,
      "verse_number": 27,
      "verse_key": "75:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقِيلَ مَنْ ۜ رَاقٍ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5579,
      "verse_number": 28,
      "verse_key": "75:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَظَنَّ أَنَّهُ الْفِرَاقُ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5580,
      "verse_number": 29,
      "verse_key": "75:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْتَفَّتِ السَّاقُ بِالسَّاقِ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5581,
      "verse_number": 30,
      "verse_key": "75:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَىٰ رَبِّكَ يَوْمَئِذٍ الْمَسَاقُ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5582,
      "verse_number": 31,
      "verse_key": "75:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا صَدَّقَ وَلَا صَلَّىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5583,
      "verse_number": 32,
      "verse_key": "75:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5584,
      "verse_number": 33,
      "verse_key": "75:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ ذَهَبَ إِلَىٰ أَهْلِهِ يَتَمَطَّىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5585,
      "verse_number": 34,
      "verse_key": "75:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5586,
      "verse_number": 35,
      "verse_key": "75:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5587,
      "verse_number": 36,
      "verse_key": "75:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَيَحْسَبُ الْإِنسَانُ أَن يُتْرَكَ سُدًى",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5588,
      "verse_number": 37,
      "verse_key": "75:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِيٍّ يُمْنَىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5589,
      "verse_number": 38,
      "verse_key": "75:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5590,
      "verse_number": 39,
      "verse_key": "75:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَجَعَلَ مِنْهُ الزَّوْجَيْنِ الذَّكَرَ وَالْأُنثَىٰ",
      "page_number": 578,
      "juz_number": 29
    },
    {
      "id": 5591,
      "verse_number": 40,
      "verse_key": "75:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَيْسَ ذَٰلِكَ بِقَادِرٍ عَلَىٰ أَن يُحْيِيَ الْمَوْتَىٰ",
      "page_number": 578,
      "juz_number": 29
    }
  ]
}