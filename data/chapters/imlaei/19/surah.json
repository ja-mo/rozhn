{
  "id": 19,
  "name": "Mary",
  "verses": [
    {
      "id": 2251,
      "verse_number": 1,
      "verse_key": "19:1",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كهيعص",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2252,
      "verse_number": 2,
      "verse_key": "19:2",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ذِكْرُ رَحْمَتِ رَبِّكَ عَبْدَهُ زَكَرِيَّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2253,
      "verse_number": 3,
      "verse_key": "19:3",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ نَادَىٰ رَبَّهُ نِدَاءً خَفِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2254,
      "verse_number": 4,
      "verse_key": "19:4",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ إِنِّي وَهَنَ الْعَظْمُ مِنِّي وَاشْتَعَلَ الرَّأْسُ شَيْبًا وَلَمْ أَكُن بِدُعَائِكَ رَبِّ شَقِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2255,
      "verse_number": 5,
      "verse_key": "19:5",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِنِّي خِفْتُ الْمَوَالِيَ مِن وَرَائِي وَكَانَتِ امْرَأَتِي عَاقِرًا فَهَبْ لِي مِن لَّدُنكَ وَلِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2256,
      "verse_number": 6,
      "verse_key": "19:6",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَرِثُنِي وَيَرِثُ مِنْ آلِ يَعْقُوبَ ۖ وَاجْعَلْهُ رَبِّ رَضِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2257,
      "verse_number": 7,
      "verse_key": "19:7",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا زَكَرِيَّا إِنَّا نُبَشِّرُكَ بِغُلَامٍ اسْمُهُ يَحْيَىٰ لَمْ نَجْعَل لَّهُ مِن قَبْلُ سَمِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2258,
      "verse_number": 8,
      "verse_key": "19:8",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ أَنَّىٰ يَكُونُ لِي غُلَامٌ وَكَانَتِ امْرَأَتِي عَاقِرًا وَقَدْ بَلَغْتُ مِنَ الْكِبَرِ عِتِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2259,
      "verse_number": 9,
      "verse_key": "19:9",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ كَذَٰلِكَ قَالَ رَبُّكَ هُوَ عَلَيَّ هَيِّنٌ وَقَدْ خَلَقْتُكَ مِن قَبْلُ وَلَمْ تَكُ شَيْئًا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2260,
      "verse_number": 10,
      "verse_key": "19:10",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ اجْعَل لِّي آيَةً ۚ قَالَ آيَتُكَ أَلَّا تُكَلِّمَ النَّاسَ ثَلَاثَ لَيَالٍ سَوِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2261,
      "verse_number": 11,
      "verse_key": "19:11",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَخَرَجَ عَلَىٰ قَوْمِهِ مِنَ الْمِحْرَابِ فَأَوْحَىٰ إِلَيْهِمْ أَن سَبِّحُوا بُكْرَةً وَعَشِيًّا",
      "page_number": 305,
      "juz_number": 16
    },
    {
      "id": 2262,
      "verse_number": 12,
      "verse_key": "19:12",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا يَحْيَىٰ خُذِ الْكِتَابَ بِقُوَّةٍ ۖ وَآتَيْنَاهُ الْحُكْمَ صَبِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2263,
      "verse_number": 13,
      "verse_key": "19:13",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَحَنَانًا مِّن لَّدُنَّا وَزَكَاةً ۖ وَكَانَ تَقِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2264,
      "verse_number": 14,
      "verse_key": "19:14",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَبَرًّا بِوَالِدَيْهِ وَلَمْ يَكُن جَبَّارًا عَصِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2265,
      "verse_number": 15,
      "verse_key": "19:15",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 264,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَسَلَامٌ عَلَيْهِ يَوْمَ وُلِدَ وَيَوْمَ يَمُوتُ وَيَوْمَ يُبْعَثُ حَيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2266,
      "verse_number": 16,
      "verse_key": "19:16",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاذْكُرْ فِي الْكِتَابِ مَرْيَمَ إِذِ انتَبَذَتْ مِنْ أَهْلِهَا مَكَانًا شَرْقِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2267,
      "verse_number": 17,
      "verse_key": "19:17",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَاتَّخَذَتْ مِن دُونِهِمْ حِجَابًا فَأَرْسَلْنَا إِلَيْهَا رُوحَنَا فَتَمَثَّلَ لَهَا بَشَرًا سَوِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2268,
      "verse_number": 18,
      "verse_key": "19:18",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَتْ إِنِّي أَعُوذُ بِالرَّحْمَٰنِ مِنكَ إِن كُنتَ تَقِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2269,
      "verse_number": 19,
      "verse_key": "19:19",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ إِنَّمَا أَنَا رَسُولُ رَبِّكِ لِأَهَبَ لَكِ غُلَامًا زَكِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2270,
      "verse_number": 20,
      "verse_key": "19:20",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَتْ أَنَّىٰ يَكُونُ لِي غُلَامٌ وَلَمْ يَمْسَسْنِي بَشَرٌ وَلَمْ أَكُ بَغِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2271,
      "verse_number": 21,
      "verse_key": "19:21",
      "hizb_number": 31,
      "rub_el_hizb_number": 122,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ كَذَٰلِكِ قَالَ رَبُّكِ هُوَ عَلَيَّ هَيِّنٌ ۖ وَلِنَجْعَلَهُ آيَةً لِّلنَّاسِ وَرَحْمَةً مِّنَّا ۚ وَكَانَ أَمْرًا مَّقْضِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2272,
      "verse_number": 22,
      "verse_key": "19:22",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَحَمَلَتْهُ فَانتَبَذَتْ بِهِ مَكَانًا قَصِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2273,
      "verse_number": 23,
      "verse_key": "19:23",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَجَاءَهَا الْمَخَاضُ إِلَىٰ جِذْعِ النَّخْلَةِ قَالَتْ يَا لَيْتَنِي مِتُّ قَبْلَ هَٰذَا وَكُنتُ نَسْيًا مَّنسِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2274,
      "verse_number": 24,
      "verse_key": "19:24",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَنَادَاهَا مِن تَحْتِهَا أَلَّا تَحْزَنِي قَدْ جَعَلَ رَبُّكِ تَحْتَكِ سَرِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2275,
      "verse_number": 25,
      "verse_key": "19:25",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُزِّي إِلَيْكِ بِجِذْعِ النَّخْلَةِ تُسَاقِطْ عَلَيْكِ رُطَبًا جَنِيًّا",
      "page_number": 306,
      "juz_number": 16
    },
    {
      "id": 2276,
      "verse_number": 26,
      "verse_key": "19:26",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَكُلِي وَاشْرَبِي وَقَرِّي عَيْنًا ۖ فَإِمَّا تَرَيِنَّ مِنَ الْبَشَرِ أَحَدًا فَقُولِي إِنِّي نَذَرْتُ لِلرَّحْمَٰنِ صَوْمًا فَلَنْ أُكَلِّمَ الْيَوْمَ إِنسِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2277,
      "verse_number": 27,
      "verse_key": "19:27",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَتَتْ بِهِ قَوْمَهَا تَحْمِلُهُ ۖ قَالُوا يَا مَرْيَمُ لَقَدْ جِئْتِ شَيْئًا فَرِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2278,
      "verse_number": 28,
      "verse_key": "19:28",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا أُخْتَ هَارُونَ مَا كَانَ أَبُوكِ امْرَأَ سَوْءٍ وَمَا كَانَتْ أُمُّكِ بَغِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2279,
      "verse_number": 29,
      "verse_key": "19:29",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَأَشَارَتْ إِلَيْهِ ۖ قَالُوا كَيْفَ نُكَلِّمُ مَن كَانَ فِي الْمَهْدِ صَبِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2280,
      "verse_number": 30,
      "verse_key": "19:30",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ إِنِّي عَبْدُ اللَّهِ آتَانِيَ الْكِتَابَ وَجَعَلَنِي نَبِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2281,
      "verse_number": 31,
      "verse_key": "19:31",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلَنِي مُبَارَكًا أَيْنَ مَا كُنتُ وَأَوْصَانِي بِالصَّلَاةِ وَالزَّكَاةِ مَا دُمْتُ حَيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2282,
      "verse_number": 32,
      "verse_key": "19:32",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَبَرًّا بِوَالِدَتِي وَلَمْ يَجْعَلْنِي جَبَّارًا شَقِيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2283,
      "verse_number": 33,
      "verse_key": "19:33",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّلَامُ عَلَيَّ يَوْمَ وُلِدتُّ وَيَوْمَ أَمُوتُ وَيَوْمَ أُبْعَثُ حَيًّا",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2284,
      "verse_number": 34,
      "verse_key": "19:34",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ذَٰلِكَ عِيسَى ابْنُ مَرْيَمَ ۚ قَوْلَ الْحَقِّ الَّذِي فِيهِ يَمْتَرُونَ",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2285,
      "verse_number": 35,
      "verse_key": "19:35",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "مَا كَانَ لِلَّهِ أَن يَتَّخِذَ مِن وَلَدٍ ۖ سُبْحَانَهُ ۚ إِذَا قَضَىٰ أَمْرًا فَإِنَّمَا يَقُولُ لَهُ كُن فَيَكُونُ",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2286,
      "verse_number": 36,
      "verse_key": "19:36",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ اللَّهَ رَبِّي وَرَبُّكُمْ فَاعْبُدُوهُ ۚ هَٰذَا صِرَاطٌ مُّسْتَقِيمٌ",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2287,
      "verse_number": 37,
      "verse_key": "19:37",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَاخْتَلَفَ الْأَحْزَابُ مِن بَيْنِهِمْ ۖ فَوَيْلٌ لِّلَّذِينَ كَفَرُوا مِن مَّشْهَدِ يَوْمٍ عَظِيمٍ",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2288,
      "verse_number": 38,
      "verse_key": "19:38",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَسْمِعْ بِهِمْ وَأَبْصِرْ يَوْمَ يَأْتُونَنَا ۖ لَٰكِنِ الظَّالِمُونَ الْيَوْمَ فِي ضَلَالٍ مُّبِينٍ",
      "page_number": 307,
      "juz_number": 16
    },
    {
      "id": 2289,
      "verse_number": 39,
      "verse_key": "19:39",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَنذِرْهُمْ يَوْمَ الْحَسْرَةِ إِذْ قُضِيَ الْأَمْرُ وَهُمْ فِي غَفْلَةٍ وَهُمْ لَا يُؤْمِنُونَ",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2290,
      "verse_number": 40,
      "verse_key": "19:40",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 265,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا نَحْنُ نَرِثُ الْأَرْضَ وَمَنْ عَلَيْهَا وَإِلَيْنَا يُرْجَعُونَ",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2291,
      "verse_number": 41,
      "verse_key": "19:41",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاذْكُرْ فِي الْكِتَابِ إِبْرَاهِيمَ ۚ إِنَّهُ كَانَ صِدِّيقًا نَّبِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2292,
      "verse_number": 42,
      "verse_key": "19:42",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذْ قَالَ لِأَبِيهِ يَا أَبَتِ لِمَ تَعْبُدُ مَا لَا يَسْمَعُ وَلَا يُبْصِرُ وَلَا يُغْنِي عَنكَ شَيْئًا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2293,
      "verse_number": 43,
      "verse_key": "19:43",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا أَبَتِ إِنِّي قَدْ جَاءَنِي مِنَ الْعِلْمِ مَا لَمْ يَأْتِكَ فَاتَّبِعْنِي أَهْدِكَ صِرَاطًا سَوِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2294,
      "verse_number": 44,
      "verse_key": "19:44",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا أَبَتِ لَا تَعْبُدِ الشَّيْطَانَ ۖ إِنَّ الشَّيْطَانَ كَانَ لِلرَّحْمَٰنِ عَصِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2295,
      "verse_number": 45,
      "verse_key": "19:45",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا أَبَتِ إِنِّي أَخَافُ أَن يَمَسَّكَ عَذَابٌ مِّنَ الرَّحْمَٰنِ فَتَكُونَ لِلشَّيْطَانِ وَلِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2296,
      "verse_number": 46,
      "verse_key": "19:46",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَرَاغِبٌ أَنتَ عَنْ آلِهَتِي يَا إِبْرَاهِيمُ ۖ لَئِن لَّمْ تَنتَهِ لَأَرْجُمَنَّكَ ۖ وَاهْجُرْنِي مَلِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2297,
      "verse_number": 47,
      "verse_key": "19:47",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالَ سَلَامٌ عَلَيْكَ ۖ سَأَسْتَغْفِرُ لَكَ رَبِّي ۖ إِنَّهُ كَانَ بِي حَفِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2298,
      "verse_number": 48,
      "verse_key": "19:48",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَأَعْتَزِلُكُمْ وَمَا تَدْعُونَ مِن دُونِ اللَّهِ وَأَدْعُو رَبِّي عَسَىٰ أَلَّا أَكُونَ بِدُعَاءِ رَبِّي شَقِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2299,
      "verse_number": 49,
      "verse_key": "19:49",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا اعْتَزَلَهُمْ وَمَا يَعْبُدُونَ مِن دُونِ اللَّهِ وَهَبْنَا لَهُ إِسْحَاقَ وَيَعْقُوبَ ۖ وَكُلًّا جَعَلْنَا نَبِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2300,
      "verse_number": 50,
      "verse_key": "19:50",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 266,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَوَهَبْنَا لَهُم مِّن رَّحْمَتِنَا وَجَعَلْنَا لَهُمْ لِسَانَ صِدْقٍ عَلِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2301,
      "verse_number": 51,
      "verse_key": "19:51",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاذْكُرْ فِي الْكِتَابِ مُوسَىٰ ۚ إِنَّهُ كَانَ مُخْلَصًا وَكَانَ رَسُولًا نَّبِيًّا",
      "page_number": 308,
      "juz_number": 16
    },
    {
      "id": 2302,
      "verse_number": 52,
      "verse_key": "19:52",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَادَيْنَاهُ مِن جَانِبِ الطُّورِ الْأَيْمَنِ وَقَرَّبْنَاهُ نَجِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2303,
      "verse_number": 53,
      "verse_key": "19:53",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَوَهَبْنَا لَهُ مِن رَّحْمَتِنَا أَخَاهُ هَارُونَ نَبِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2304,
      "verse_number": 54,
      "verse_key": "19:54",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاذْكُرْ فِي الْكِتَابِ إِسْمَاعِيلَ ۚ إِنَّهُ كَانَ صَادِقَ الْوَعْدِ وَكَانَ رَسُولًا نَّبِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2305,
      "verse_number": 55,
      "verse_key": "19:55",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكَانَ يَأْمُرُ أَهْلَهُ بِالصَّلَاةِ وَالزَّكَاةِ وَكَانَ عِندَ رَبِّهِ مَرْضِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2306,
      "verse_number": 56,
      "verse_key": "19:56",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاذْكُرْ فِي الْكِتَابِ إِدْرِيسَ ۚ إِنَّهُ كَانَ صِدِّيقًا نَّبِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2307,
      "verse_number": 57,
      "verse_key": "19:57",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَرَفَعْنَاهُ مَكَانًا عَلِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2308,
      "verse_number": 58,
      "verse_key": "19:58",
      "hizb_number": 31,
      "rub_el_hizb_number": 123,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": 5,
      "text_imlaei": "أُولَٰئِكَ الَّذِينَ أَنْعَمَ اللَّهُ عَلَيْهِم مِّنَ النَّبِيِّينَ مِن ذُرِّيَّةِ آدَمَ وَمِمَّنْ حَمَلْنَا مَعَ نُوحٍ وَمِن ذُرِّيَّةِ إِبْرَاهِيمَ وَإِسْرَائِيلَ وَمِمَّنْ هَدَيْنَا وَاجْتَبَيْنَا ۚ إِذَا تُتْلَىٰ عَلَيْهِمْ آيَاتُ الرَّحْمَٰنِ خَرُّوا سُجَّدًا وَبُكِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2309,
      "verse_number": 59,
      "verse_key": "19:59",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَخَلَفَ مِن بَعْدِهِمْ خَلْفٌ أَضَاعُوا الصَّلَاةَ وَاتَّبَعُوا الشَّهَوَاتِ ۖ فَسَوْفَ يَلْقَوْنَ غَيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2310,
      "verse_number": 60,
      "verse_key": "19:60",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا مَن تَابَ وَآمَنَ وَعَمِلَ صَالِحًا فَأُولَٰئِكَ يَدْخُلُونَ الْجَنَّةَ وَلَا يُظْلَمُونَ شَيْئًا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2311,
      "verse_number": 61,
      "verse_key": "19:61",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "جَنَّاتِ عَدْنٍ الَّتِي وَعَدَ الرَّحْمَٰنُ عِبَادَهُ بِالْغَيْبِ ۚ إِنَّهُ كَانَ وَعْدُهُ مَأْتِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2312,
      "verse_number": 62,
      "verse_key": "19:62",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا إِلَّا سَلَامًا ۖ وَلَهُمْ رِزْقُهُمْ فِيهَا بُكْرَةً وَعَشِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2313,
      "verse_number": 63,
      "verse_key": "19:63",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تِلْكَ الْجَنَّةُ الَّتِي نُورِثُ مِنْ عِبَادِنَا مَن كَانَ تَقِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2314,
      "verse_number": 64,
      "verse_key": "19:64",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا نَتَنَزَّلُ إِلَّا بِأَمْرِ رَبِّكَ ۖ لَهُ مَا بَيْنَ أَيْدِينَا وَمَا خَلْفَنَا وَمَا بَيْنَ ذَٰلِكَ ۚ وَمَا كَانَ رَبُّكَ نَسِيًّا",
      "page_number": 309,
      "juz_number": 16
    },
    {
      "id": 2315,
      "verse_number": 65,
      "verse_key": "19:65",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 267,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "رَّبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا فَاعْبُدْهُ وَاصْطَبِرْ لِعِبَادَتِهِ ۚ هَلْ تَعْلَمُ لَهُ سَمِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2316,
      "verse_number": 66,
      "verse_key": "19:66",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَقُولُ الْإِنسَانُ أَإِذَا مَا مِتُّ لَسَوْفَ أُخْرَجُ حَيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2317,
      "verse_number": 67,
      "verse_key": "19:67",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَا يَذْكُرُ الْإِنسَانُ أَنَّا خَلَقْنَاهُ مِن قَبْلُ وَلَمْ يَكُ شَيْئًا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2318,
      "verse_number": 68,
      "verse_key": "19:68",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَوَرَبِّكَ لَنَحْشُرَنَّهُمْ وَالشَّيَاطِينَ ثُمَّ لَنُحْضِرَنَّهُمْ حَوْلَ جَهَنَّمَ جِثِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2319,
      "verse_number": 69,
      "verse_key": "19:69",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ لَنَنزِعَنَّ مِن كُلِّ شِيعَةٍ أَيُّهُمْ أَشَدُّ عَلَى الرَّحْمَٰنِ عِتِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2320,
      "verse_number": 70,
      "verse_key": "19:70",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ لَنَحْنُ أَعْلَمُ بِالَّذِينَ هُمْ أَوْلَىٰ بِهَا صِلِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2321,
      "verse_number": 71,
      "verse_key": "19:71",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِن مِّنكُمْ إِلَّا وَارِدُهَا ۚ كَانَ عَلَىٰ رَبِّكَ حَتْمًا مَّقْضِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2322,
      "verse_number": 72,
      "verse_key": "19:72",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ نُنَجِّي الَّذِينَ اتَّقَوا وَّنَذَرُ الظَّالِمِينَ فِيهَا جِثِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2323,
      "verse_number": 73,
      "verse_key": "19:73",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا تُتْلَىٰ عَلَيْهِمْ آيَاتُنَا بَيِّنَاتٍ قَالَ الَّذِينَ كَفَرُوا لِلَّذِينَ آمَنُوا أَيُّ الْفَرِيقَيْنِ خَيْرٌ مَّقَامًا وَأَحْسَنُ نَدِيًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2324,
      "verse_number": 74,
      "verse_key": "19:74",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هُمْ أَحْسَنُ أَثَاثًا وَرِئْيًا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2325,
      "verse_number": 75,
      "verse_key": "19:75",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ مَن كَانَ فِي الضَّلَالَةِ فَلْيَمْدُدْ لَهُ الرَّحْمَٰنُ مَدًّا ۚ حَتَّىٰ إِذَا رَأَوْا مَا يُوعَدُونَ إِمَّا الْعَذَابَ وَإِمَّا السَّاعَةَ فَسَيَعْلَمُونَ مَنْ هُوَ شَرٌّ مَّكَانًا وَأَضْعَفُ جُندًا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2326,
      "verse_number": 76,
      "verse_key": "19:76",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَزِيدُ اللَّهُ الَّذِينَ اهْتَدَوْا هُدًى ۗ وَالْبَاقِيَاتُ الصَّالِحَاتُ خَيْرٌ عِندَ رَبِّكَ ثَوَابًا وَخَيْرٌ مَّرَدًّا",
      "page_number": 310,
      "juz_number": 16
    },
    {
      "id": 2327,
      "verse_number": 77,
      "verse_key": "19:77",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَفَرَأَيْتَ الَّذِي كَفَرَ بِآيَاتِنَا وَقَالَ لَأُوتَيَنَّ مَالًا وَوَلَدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2328,
      "verse_number": 78,
      "verse_key": "19:78",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَطَّلَعَ الْغَيْبَ أَمِ اتَّخَذَ عِندَ الرَّحْمَٰنِ عَهْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2329,
      "verse_number": 79,
      "verse_key": "19:79",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۚ سَنَكْتُبُ مَا يَقُولُ وَنَمُدُّ لَهُ مِنَ الْعَذَابِ مَدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2330,
      "verse_number": 80,
      "verse_key": "19:80",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَرِثُهُ مَا يَقُولُ وَيَأْتِينَا فَرْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2331,
      "verse_number": 81,
      "verse_key": "19:81",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاتَّخَذُوا مِن دُونِ اللَّهِ آلِهَةً لِّيَكُونُوا لَهُمْ عِزًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2332,
      "verse_number": 82,
      "verse_key": "19:82",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 268,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۚ سَيَكْفُرُونَ بِعِبَادَتِهِمْ وَيَكُونُونَ عَلَيْهِمْ ضِدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2333,
      "verse_number": 83,
      "verse_key": "19:83",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ تَرَ أَنَّا أَرْسَلْنَا الشَّيَاطِينَ عَلَى الْكَافِرِينَ تَؤُزُّهُمْ أَزًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2334,
      "verse_number": 84,
      "verse_key": "19:84",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَا تَعْجَلْ عَلَيْهِمْ ۖ إِنَّمَا نَعُدُّ لَهُمْ عَدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2335,
      "verse_number": 85,
      "verse_key": "19:85",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ نَحْشُرُ الْمُتَّقِينَ إِلَى الرَّحْمَٰنِ وَفْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2336,
      "verse_number": 86,
      "verse_key": "19:86",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَنَسُوقُ الْمُجْرِمِينَ إِلَىٰ جَهَنَّمَ وِرْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2337,
      "verse_number": 87,
      "verse_key": "19:87",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّا يَمْلِكُونَ الشَّفَاعَةَ إِلَّا مَنِ اتَّخَذَ عِندَ الرَّحْمَٰنِ عَهْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2338,
      "verse_number": 88,
      "verse_key": "19:88",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا اتَّخَذَ الرَّحْمَٰنُ وَلَدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2339,
      "verse_number": 89,
      "verse_key": "19:89",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّقَدْ جِئْتُمْ شَيْئًا إِدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2340,
      "verse_number": 90,
      "verse_key": "19:90",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تَكَادُ السَّمَاوَاتُ يَتَفَطَّرْنَ مِنْهُ وَتَنشَقُّ الْأَرْضُ وَتَخِرُّ الْجِبَالُ هَدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2341,
      "verse_number": 91,
      "verse_key": "19:91",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَن دَعَوْا لِلرَّحْمَٰنِ وَلَدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2342,
      "verse_number": 92,
      "verse_key": "19:92",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يَنبَغِي لِلرَّحْمَٰنِ أَن يَتَّخِذَ وَلَدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2343,
      "verse_number": 93,
      "verse_key": "19:93",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِن كُلُّ مَن فِي السَّمَاوَاتِ وَالْأَرْضِ إِلَّا آتِي الرَّحْمَٰنِ عَبْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2344,
      "verse_number": 94,
      "verse_key": "19:94",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّقَدْ أَحْصَاهُمْ وَعَدَّهُمْ عَدًّا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2345,
      "verse_number": 95,
      "verse_key": "19:95",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكُلُّهُمْ آتِيهِ يَوْمَ الْقِيَامَةِ فَرْدًا",
      "page_number": 311,
      "juz_number": 16
    },
    {
      "id": 2346,
      "verse_number": 96,
      "verse_key": "19:96",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ سَيَجْعَلُ لَهُمُ الرَّحْمَٰنُ وُدًّا",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2347,
      "verse_number": 97,
      "verse_key": "19:97",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّمَا يَسَّرْنَاهُ بِلِسَانِكَ لِتُبَشِّرَ بِهِ الْمُتَّقِينَ وَتُنذِرَ بِهِ قَوْمًا لُّدًّا",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2348,
      "verse_number": 98,
      "verse_key": "19:98",
      "hizb_number": 31,
      "rub_el_hizb_number": 124,
      "ruku_number": 269,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هَلْ تُحِسُّ مِنْهُم مِّنْ أَحَدٍ أَوْ تَسْمَعُ لَهُمْ رِكْزًا",
      "page_number": 312,
      "juz_number": 16
    }
  ]
}