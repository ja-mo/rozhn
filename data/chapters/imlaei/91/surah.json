      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالشَّمْسِ وَضُحَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6045,
      "verse_number": 2,
      "verse_key": "91:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْقَمَرِ إِذَا تَلَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6046,
      "verse_number": 3,
      "verse_key": "91:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالنَّهَارِ إِذَا جَلَّاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6047,
      "verse_number": 4,
      "verse_key": "91:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّيْلِ إِذَا يَغْشَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6048,
      "verse_number": 5,
      "verse_key": "91:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءِ وَمَا بَنَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6049,
      "verse_number": 6,
      "verse_key": "91:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضِ وَمَا طَحَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6050,
      "verse_number": 7,
      "verse_key": "91:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَنَفْسٍ وَمَا سَوَّاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6051,
      "verse_number": 8,
      "verse_key": "91:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6052,
      "verse_number": 9,
      "verse_key": "91:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَدْ أَفْلَحَ مَن زَكَّاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6053,
      "verse_number": 10,
      "verse_key": "91:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَدْ خَابَ مَن دَسَّاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6054,
      "verse_number": 11,
      "verse_key": "91:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَذَّبَتْ ثَمُودُ بِطَغْوَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6055,
      "verse_number": 12,
      "verse_key": "91:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذِ انبَعَثَ أَشْقَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6056,
      "verse_number": 13,
      "verse_key": "91:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَقَالَ لَهُمْ رَسُولُ اللَّهِ نَاقَةَ اللَّهِ وَسُقْيَاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6057,
      "verse_number": 14,
      "verse_key": "91:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنبِهِمْ فَسَوَّاهَا",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6058,
      "verse_number": 15,
      "verse_key": "91:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 535,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَخَافُ عُقْبَاهَا",
      "page_number": 595,
      "juz_number": 30
    }
  ]
}