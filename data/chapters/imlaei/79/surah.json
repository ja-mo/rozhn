    },
    {
      "id": 5727,
      "verse_number": 15,
      "verse_key": "79:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ مُوسَىٰ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5728,
      "verse_number": 16,
      "verse_key": "79:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذْ نَادَاهُ رَبُّهُ بِالْوَادِ الْمُقَدَّسِ طُوًى",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5729,
      "verse_number": 17,
      "verse_key": "79:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "اذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُ طَغَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5730,
      "verse_number": 18,
      "verse_key": "79:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَقُلْ هَل لَّكَ إِلَىٰ أَن تَزَكَّىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5731,
      "verse_number": 19,
      "verse_key": "79:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5732,
      "verse_number": 20,
      "verse_key": "79:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَرَاهُ الْآيَةَ الْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5733,
      "verse_number": 21,
      "verse_key": "79:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَكَذَّبَ وَعَصَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5734,
      "verse_number": 22,
      "verse_key": "79:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5735,
      "verse_number": 23,
      "verse_key": "79:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَحَشَرَ فَنَادَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5736,
      "verse_number": 24,
      "verse_key": "79:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَقَالَ أَنَا رَبُّكُمُ الْأَعْلَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5737,
      "verse_number": 25,
      "verse_key": "79:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَخَذَهُ اللَّهُ نَكَالَ الْآخِرَةِ وَالْأُولَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5738,
      "verse_number": 26,
      "verse_key": "79:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5739,
      "verse_number": 27,
      "verse_key": "79:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَأَنتُمْ أَشَدُّ خَلْقًا أَمِ السَّمَاءُ ۚ بَنَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5740,
      "verse_number": 28,
      "verse_key": "79:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "رَفَعَ سَمْكَهَا فَسَوَّاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5741,
      "verse_number": 29,
      "verse_key": "79:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5742,
      "verse_number": 30,
      "verse_key": "79:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضَ بَعْدَ ذَٰلِكَ دَحَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5743,
      "verse_number": 31,
      "verse_key": "79:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَخْرَجَ مِنْهَا مَاءَهَا وَمَرْعَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5744,
      "verse_number": 32,
      "verse_key": "79:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْجِبَالَ أَرْسَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5745,
      "verse_number": 33,
      "verse_key": "79:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَتَاعًا لَّكُمْ وَلِأَنْعَامِكُمْ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5746,
      "verse_number": 34,
      "verse_key": "79:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا جَاءَتِ الطَّامَّةُ الْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5747,
      "verse_number": 35,
      "verse_key": "79:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَتَذَكَّرُ الْإِنسَانُ مَا سَعَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5748,
      "verse_number": 36,
      "verse_key": "79:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَبُرِّزَتِ الْجَحِيمُ لِمَن يَرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5749,
      "verse_number": 37,
      "verse_key": "79:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَمَّا مَن طَغَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5750,
      "verse_number": 38,
      "verse_key": "79:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَآثَرَ الْحَيَاةَ الدُّنْيَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5751,
      "verse_number": 39,
      "verse_key": "79:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّ الْجَحِيمَ هِيَ الْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5752,
      "verse_number": 40,
      "verse_key": "79:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِ وَنَهَى النَّفْسَ عَنِ الْهَوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5753,
      "verse_number": 41,
      "verse_key": "79:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّ الْجَنَّةَ هِيَ الْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5754,
      "verse_number": 42,
      "verse_key": "79:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَسْأَلُونَكَ عَنِ السَّاعَةِ أَيَّانَ مُرْسَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5755,
      "verse_number": 43,
      "verse_key": "79:43",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِيمَ أَنتَ مِن ذِكْرَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5756,
      "verse_number": 44,
      "verse_key": "79:44",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَىٰ رَبِّكَ مُنتَهَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5757,
      "verse_number": 45,
      "verse_key": "79:45",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّمَا أَنتَ مُنذِرُ مَن يَخْشَاهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5758,
      "verse_number": 46,
      "verse_key": "79:46",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوا إِلَّا عَشِيَّةً أَوْ ضُحَاهَا",
      "page_number": 584,
      "juz_number": 30
    }
