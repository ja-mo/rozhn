      "verse_key": "79:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
