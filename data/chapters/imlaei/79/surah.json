    },
    {
      "id": 5726,
      "verse_number": 14,
      "verse_key": "79:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا هُم بِالسَّاهِرَةِ",
      "page_number": 583,
