{
  "id": 89,
  "name": "The Dawn",
  "verses": [
    {
      "id": 5994,
      "verse_number": 1,
      "verse_key": "89:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْفَجْرِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 5995,
      "verse_number": 2,
      "verse_key": "89:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَيَالٍ عَشْرٍ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 5996,
      "verse_number": 3,
      "verse_key": "89:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالشَّفْعِ وَالْوَتْرِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 5997,
      "verse_number": 4,
      "verse_key": "89:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّيْلِ إِذَا يَسْرِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 5998,
      "verse_number": 5,
      "verse_key": "89:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَلْ فِي ذَٰلِكَ قَسَمٌ لِّذِي حِجْرٍ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 5999,
      "verse_number": 6,
      "verse_key": "89:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6000,
      "verse_number": 7,
      "verse_key": "89:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِرَمَ ذَاتِ الْعِمَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6001,
      "verse_number": 8,
      "verse_key": "89:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّتِي لَمْ يُخْلَقْ مِثْلُهَا فِي الْبِلَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6002,
      "verse_number": 9,
      "verse_key": "89:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَثَمُودَ الَّذِينَ جَابُوا الصَّخْرَ بِالْوَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6003,
      "verse_number": 10,
      "verse_key": "89:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِرْعَوْنَ ذِي الْأَوْتَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6004,
      "verse_number": 11,
      "verse_key": "89:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ طَغَوْا فِي الْبِلَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6005,
      "verse_number": 12,
      "verse_key": "89:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَكْثَرُوا فِيهَا الْفَسَادَ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6006,
      "verse_number": 13,
      "verse_key": "89:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6007,
      "verse_number": 14,
      "verse_key": "89:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ رَبَّكَ لَبِالْمِرْصَادِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6008,
      "verse_number": 15,
      "verse_key": "89:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَمَّا الْإِنسَانُ إِذَا مَا ابْتَلَاهُ رَبُّهُ فَأَكْرَمَهُ وَنَعَّمَهُ فَيَقُولُ رَبِّي أَكْرَمَنِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6009,
      "verse_number": 16,
      "verse_key": "89:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا إِذَا مَا ابْتَلَاهُ فَقَدَرَ عَلَيْهِ رِزْقَهُ فَيَقُولُ رَبِّي أَهَانَنِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6010,
      "verse_number": 17,
      "verse_key": "89:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۖ بَل لَّا تُكْرِمُونَ الْيَتِيمَ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6011,
      "verse_number": 18,
      "verse_key": "89:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا تَحَاضُّونَ عَلَىٰ طَعَامِ الْمِسْكِينِ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6012,
      "verse_number": 19,
      "verse_key": "89:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتَأْكُلُونَ التُّرَاثَ أَكْلًا لَّمًّا",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6013,
      "verse_number": 20,
      "verse_key": "89:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتُحِبُّونَ الْمَالَ حُبًّا جَمًّا",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6014,
      "verse_number": 21,
      "verse_key": "89:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِذَا دُكَّتِ الْأَرْضُ دَكًّا دَكًّا",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6015,
      "verse_number": 22,
      "verse_key": "89:22",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَاءَ رَبُّكَ وَالْمَلَكُ صَفًّا صَفًّا",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6016,
      "verse_number": 23,
      "verse_key": "89:23",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجِيءَ يَوْمَئِذٍ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ الْإِنسَانُ وَأَنَّىٰ لَهُ الذِّكْرَىٰ",
      "page_number": 593,
      "juz_number": 30
    },
    {
      "id": 6017,
      "verse_number": 24,
      "verse_key": "89:24",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَقُولُ يَا لَيْتَنِي قَدَّمْتُ لِحَيَاتِي",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6018,
      "verse_number": 25,
      "verse_key": "89:25",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُ أَحَدٌ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6019,
      "verse_number": 26,
      "verse_key": "89:26",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يُوثِقُ وَثَاقَهُ أَحَدٌ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6020,
      "verse_number": 27,
      "verse_key": "89:27",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَا أَيَّتُهَا النَّفْسُ الْمُطْمَئِنَّةُ",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6021,
      "verse_number": 28,
      "verse_key": "89:28",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ارْجِعِي إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6022,
      "verse_number": 29,
      "verse_key": "89:29",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَادْخُلِي فِي عِبَادِي",
      "page_number": 594,
      "juz_number": 30
    },
    {
      "id": 6023,
      "verse_number": 30,
      "verse_key": "89:30",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 533,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَادْخُلِي جَنَّتِي",
      "page_number": 594,
      "juz_number": 30
    }
  ]
}