{
  "id": 70,
  "name": "The Ascending Stairways",
  "verses": [
    {
      "id": 5376,
      "verse_number": 1,
      "verse_key": "70:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "سَأَلَ سَائِلٌ بِعَذَابٍ وَاقِعٍ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5377,
      "verse_number": 2,
      "verse_key": "70:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّلْكَافِرِينَ لَيْسَ لَهُ دَافِعٌ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5378,
      "verse_number": 3,
      "verse_key": "70:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مِّنَ اللَّهِ ذِي الْمَعَارِجِ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5379,
      "verse_number": 4,
      "verse_key": "70:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَعْرُجُ الْمَلَائِكَةُ وَالرُّوحُ إِلَيْهِ فِي يَوْمٍ كَانَ مِقْدَارُهُ خَمْسِينَ أَلْفَ سَنَةٍ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5380,
      "verse_number": 5,
      "verse_key": "70:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَاصْبِرْ صَبْرًا جَمِيلًا",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5381,
      "verse_number": 6,
      "verse_key": "70:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُمْ يَرَوْنَهُ بَعِيدًا",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5382,
      "verse_number": 7,
      "verse_key": "70:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَنَرَاهُ قَرِيبًا",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5383,
      "verse_number": 8,
      "verse_key": "70:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ تَكُونُ السَّمَاءُ كَالْمُهْلِ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5384,
      "verse_number": 9,
      "verse_key": "70:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتَكُونُ الْجِبَالُ كَالْعِهْنِ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5385,
      "verse_number": 10,
      "verse_key": "70:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَسْأَلُ حَمِيمٌ حَمِيمًا",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5386,
      "verse_number": 11,
      "verse_key": "70:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يُبَصَّرُونَهُمْ ۚ يَوَدُّ الْمُجْرِمُ لَوْ يَفْتَدِي مِنْ عَذَابِ يَوْمِئِذٍ بِبَنِيهِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5387,
      "verse_number": 12,
      "verse_key": "70:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَصَاحِبَتِهِ وَأَخِيهِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5388,
      "verse_number": 13,
      "verse_key": "70:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفَصِيلَتِهِ الَّتِي تُؤْوِيهِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5389,
      "verse_number": 14,
      "verse_key": "70:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَن فِي الْأَرْضِ جَمِيعًا ثُمَّ يُنجِيهِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5390,
      "verse_number": 15,
      "verse_key": "70:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۖ إِنَّهَا لَظَىٰ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5391,
      "verse_number": 16,
      "verse_key": "70:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "نَزَّاعَةً لِّلشَّوَىٰ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5392,
      "verse_number": 17,
      "verse_key": "70:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَدْعُو مَنْ أَدْبَرَ وَتَوَلَّىٰ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5393,
      "verse_number": 18,
      "verse_key": "70:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَمَعَ فَأَوْعَىٰ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5394,
      "verse_number": 19,
      "verse_key": "70:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْإِنسَانَ خُلِقَ هَلُوعًا",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5395,
      "verse_number": 20,
      "verse_key": "70:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا مَسَّهُ الشَّرُّ جَزُوعًا",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5396,
      "verse_number": 21,
      "verse_key": "70:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا مَسَّهُ الْخَيْرُ مَنُوعًا",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5397,
      "verse_number": 22,
      "verse_key": "70:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا الْمُصَلِّينَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5398,
      "verse_number": 23,
      "verse_key": "70:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَائِمُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5399,
      "verse_number": 24,
      "verse_key": "70:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ فِي أَمْوَالِهِمْ حَقٌّ مَّعْلُومٌ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5400,
      "verse_number": 25,
      "verse_key": "70:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّلسَّائِلِ وَالْمَحْرُومِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5401,
      "verse_number": 26,
      "verse_key": "70:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ يُصَدِّقُونَ بِيَوْمِ الدِّينِ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5402,
      "verse_number": 27,
      "verse_key": "70:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5403,
      "verse_number": 28,
      "verse_key": "70:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5404,
      "verse_number": 29,
      "verse_key": "70:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ هُمْ لِفُرُوجِهِمْ حَافِظُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5405,
      "verse_number": 30,
      "verse_key": "70:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا عَلَىٰ أَزْوَاجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَانُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5406,
      "verse_number": 31,
      "verse_key": "70:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَنِ ابْتَغَىٰ وَرَاءَ ذَٰلِكَ فَأُولَٰئِكَ هُمُ الْعَادُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5407,
      "verse_number": 32,
      "verse_key": "70:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ هُمْ لِأَمَانَاتِهِمْ وَعَهْدِهِمْ رَاعُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5408,
      "verse_number": 33,
      "verse_key": "70:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ هُم بِشَهَادَاتِهِمْ قَائِمُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5409,
      "verse_number": 34,
      "verse_key": "70:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5410,
      "verse_number": 35,
      "verse_key": "70:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 504,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أُولَٰئِكَ فِي جَنَّاتٍ مُّكْرَمُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5411,
      "verse_number": 36,
      "verse_key": "70:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَالِ الَّذِينَ كَفَرُوا قِبَلَكَ مُهْطِعِينَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5412,
      "verse_number": 37,
      "verse_key": "70:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ عِزِينَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5413,
      "verse_number": 38,
      "verse_key": "70:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَيَطْمَعُ كُلُّ امْرِئٍ مِّنْهُمْ أَن يُدْخَلَ جَنَّةَ نَعِيمٍ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5414,
      "verse_number": 39,
      "verse_key": "70:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۖ إِنَّا خَلَقْنَاهُم مِّمَّا يَعْلَمُونَ",
      "page_number": 569,
      "juz_number": 29
    },
    {
      "id": 5415,
      "verse_number": 40,
      "verse_key": "70:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا أُقْسِمُ بِرَبِّ الْمَشَارِقِ وَالْمَغَارِبِ إِنَّا لَقَادِرُونَ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5416,
      "verse_number": 41,
      "verse_key": "70:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلَىٰ أَن نُّبَدِّلَ خَيْرًا مِّنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5417,
      "verse_number": 42,
      "verse_key": "70:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَذَرْهُمْ يَخُوضُوا وَيَلْعَبُوا حَتَّىٰ يُلَاقُوا يَوْمَهُمُ الَّذِي يُوعَدُونَ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5418,
      "verse_number": 43,
      "verse_key": "70:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَخْرُجُونَ مِنَ الْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَىٰ نُصُبٍ يُوفِضُونَ",
      "page_number": 570,
      "juz_number": 29
    },
    {
      "id": 5419,
      "verse_number": 44,
      "verse_key": "70:44",
      "hizb_number": 57,
      "rub_el_hizb_number": 228,
      "ruku_number": 505,
