{
  "id": 84,
  "name": "The Sundering",
  "verses": [
    {
      "id": 5885,
      "verse_number": 1,
      "verse_key": "84:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا السَّمَاءُ انشَقَّتْ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5886,
      "verse_number": 2,
      "verse_key": "84:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5887,
      "verse_number": 3,
      "verse_key": "84:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا الْأَرْضُ مُدَّتْ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5888,
      "verse_number": 4,
      "verse_key": "84:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5889,
      "verse_number": 5,
      "verse_key": "84:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5890,
      "verse_number": 6,
      "verse_key": "84:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَا أَيُّهَا الْإِنسَانُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَاقِيهِ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5891,
      "verse_number": 7,
      "verse_key": "84:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَمَّا مَنْ أُوتِيَ كِتَابَهُ بِيَمِينِهِ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5892,
      "verse_number": 8,
      "verse_key": "84:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5893,
      "verse_number": 9,
      "verse_key": "84:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيَنقَلِبُ إِلَىٰ أَهْلِهِ مَسْرُورًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5894,
      "verse_number": 10,
      "verse_key": "84:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا مَنْ أُوتِيَ كِتَابَهُ وَرَاءَ ظَهْرِهِ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5895,
      "verse_number": 11,
      "verse_key": "84:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَسَوْفَ يَدْعُو ثُبُورًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5896,
      "verse_number": 12,
      "verse_key": "84:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيَصْلَىٰ سَعِيرًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5897,
      "verse_number": 13,
      "verse_key": "84:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ كَانَ فِي أَهْلِهِ مَسْرُورًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5898,
      "verse_number": 14,
      "verse_key": "84:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ ظَنَّ أَن لَّن يَحُورَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5899,
      "verse_number": 15,
      "verse_key": "84:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلَىٰ إِنَّ رَبَّهُ كَانَ بِهِ بَصِيرًا",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5900,
      "verse_number": 16,
      "verse_key": "84:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَا أُقْسِمُ بِالشَّفَقِ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5901,
      "verse_number": 17,
      "verse_key": "84:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّيْلِ وَمَا وَسَقَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5902,
      "verse_number": 18,
      "verse_key": "84:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْقَمَرِ إِذَا اتَّسَقَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5903,
      "verse_number": 19,
      "verse_key": "84:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5904,
      "verse_number": 20,
      "verse_key": "84:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَا لَهُمْ لَا يُؤْمِنُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5905,
      "verse_number": 21,
      "verse_key": "84:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": 13,
      "text_imlaei": "وَإِذَا قُرِئَ عَلَيْهِمُ الْقُرْآنُ لَا يَسْجُدُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5906,
      "verse_number": 22,
      "verse_key": "84:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلِ الَّذِينَ كَفَرُوا يُكَذِّبُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5907,
      "verse_number": 23,
      "verse_key": "84:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّهُ أَعْلَمُ بِمَا يُوعُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5908,
      "verse_number": 24,
      "verse_key": "84:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5909,
      "verse_number": 25,
      "verse_key": "84:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 528,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ",
      "page_number": 589,
      "juz_number": 30
    }
  ]
}