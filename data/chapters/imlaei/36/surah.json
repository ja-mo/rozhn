    },
    {
      "id": 3707,
      "verse_number": 2,
      "verse_key": "36:2",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالْقُرْآنِ الْحَكِيمِ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3708,
      "verse_number": 3,
      "verse_key": "36:3",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكَ لَمِنَ الْمُرْسَلِينَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3709,
      "verse_number": 4,
      "verse_key": "36:4",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "عَلَىٰ صِرَاطٍ مُّسْتَقِيمٍ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3710,
      "verse_number": 5,
      "verse_key": "36:5",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "تَنزِيلَ الْعَزِيزِ الرَّحِيمِ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3711,
      "verse_number": 6,
      "verse_key": "36:6",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لِتُنذِرَ قَوْمًا مَّا أُنذِرَ آبَاؤُهُمْ فَهُمْ غَافِلُونَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3712,
      "verse_number": 7,
      "verse_key": "36:7",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَقَدْ حَقَّ الْقَوْلُ عَلَىٰ أَكْثَرِهِمْ فَهُمْ لَا يُؤْمِنُونَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3713,
      "verse_number": 8,
      "verse_key": "36:8",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا جَعَلْنَا فِي أَعْنَاقِهِمْ أَغْلَالًا فَهِيَ إِلَى الْأَذْقَانِ فَهُم مُّقْمَحُونَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3714,
      "verse_number": 9,
      "verse_key": "36:9",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا مِن بَيْنِ أَيْدِيهِمْ سَدًّا وَمِنْ خَلْفِهِمْ سَدًّا فَأَغْشَيْنَاهُمْ فَهُمْ لَا يُبْصِرُونَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3715,
      "verse_number": 10,
      "verse_key": "36:10",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَسَوَاءٌ عَلَيْهِمْ أَأَنذَرْتَهُمْ أَمْ لَمْ تُنذِرْهُمْ لَا يُؤْمِنُونَ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3716,
      "verse_number": 11,
      "verse_key": "36:11",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّمَا تُنذِرُ مَنِ اتَّبَعَ الذِّكْرَ وَخَشِيَ الرَّحْمَٰنَ بِالْغَيْبِ ۖ فَبَشِّرْهُ بِمَغْفِرَةٍ وَأَجْرٍ كَرِيمٍ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3717,
      "verse_number": 12,
      "verse_key": "36:12",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 383,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا نَحْنُ نُحْيِي الْمَوْتَىٰ وَنَكْتُبُ مَا قَدَّمُوا وَآثَارَهُمْ ۚ وَكُلَّ شَيْءٍ أَحْصَيْنَاهُ فِي إِمَامٍ مُّبِينٍ",
      "page_number": 440,
      "juz_number": 22
    },
    {
      "id": 3718,
      "verse_number": 13,
      "verse_key": "36:13",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَاضْرِبْ لَهُم مَّثَلًا أَصْحَابَ الْقَرْيَةِ إِذْ جَاءَهَا الْمُرْسَلُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3719,
      "verse_number": 14,
      "verse_key": "36:14",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِذْ أَرْسَلْنَا إِلَيْهِمُ اثْنَيْنِ فَكَذَّبُوهُمَا فَعَزَّزْنَا بِثَالِثٍ فَقَالُوا إِنَّا إِلَيْكُم مُّرْسَلُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3720,
      "verse_number": 15,
      "verse_key": "36:15",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا مَا أَنتُمْ إِلَّا بَشَرٌ مِّثْلُنَا وَمَا أَنزَلَ الرَّحْمَٰنُ مِن شَيْءٍ إِنْ أَنتُمْ إِلَّا تَكْذِبُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3721,
      "verse_number": 16,
      "verse_key": "36:16",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا رَبُّنَا يَعْلَمُ إِنَّا إِلَيْكُمْ لَمُرْسَلُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3722,
      "verse_number": 17,
      "verse_key": "36:17",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا عَلَيْنَا إِلَّا الْبَلَاغُ الْمُبِينُ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3723,
      "verse_number": 18,
      "verse_key": "36:18",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنَّا تَطَيَّرْنَا بِكُمْ ۖ لَئِن لَّمْ تَنتَهُوا لَنَرْجُمَنَّكُمْ وَلَيَمَسَّنَّكُم مِّنَّا عَذَابٌ أَلِيمٌ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3724,
      "verse_number": 19,
      "verse_key": "36:19",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا طَائِرُكُم مَّعَكُمْ ۚ أَئِن ذُكِّرْتُم ۚ بَلْ أَنتُمْ قَوْمٌ مُّسْرِفُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3725,
      "verse_number": 20,
      "verse_key": "36:20",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَجَاءَ مِنْ أَقْصَى الْمَدِينَةِ رَجُلٌ يَسْعَىٰ قَالَ يَا قَوْمِ اتَّبِعُوا الْمُرْسَلِينَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3726,
      "verse_number": 21,
      "verse_key": "36:21",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "اتَّبِعُوا مَن لَّا يَسْأَلُكُمْ أَجْرًا وَهُم مُّهْتَدُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3727,
      "verse_number": 22,
      "verse_key": "36:22",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا لِيَ لَا أَعْبُدُ الَّذِي فَطَرَنِي وَإِلَيْهِ تُرْجَعُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3728,
      "verse_number": 23,
      "verse_key": "36:23",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَأَتَّخِذُ مِن دُونِهِ آلِهَةً إِن يُرِدْنِ الرَّحْمَٰنُ بِضُرٍّ لَّا تُغْنِ عَنِّي شَفَاعَتُهُمْ شَيْئًا وَلَا يُنقِذُونِ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3729,
      "verse_number": 24,
      "verse_key": "36:24",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنِّي إِذًا لَّفِي ضَلَالٍ مُّبِينٍ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3730,
      "verse_number": 25,
      "verse_key": "36:25",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنِّي آمَنتُ بِرَبِّكُمْ فَاسْمَعُونِ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3731,
      "verse_number": 26,
      "verse_key": "36:26",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قِيلَ ادْخُلِ الْجَنَّةَ ۖ قَالَ يَا لَيْتَ قَوْمِي يَعْلَمُونَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3732,
      "verse_number": 27,
      "verse_key": "36:27",
      "hizb_number": 44,
      "rub_el_hizb_number": 176,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "بِمَا غَفَرَ لِي رَبِّي وَجَعَلَنِي مِنَ الْمُكْرَمِينَ",
      "page_number": 441,
      "juz_number": 22
    },
    {
      "id": 3733,
      "verse_number": 28,
      "verse_key": "36:28",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَنزَلْنَا عَلَىٰ قَوْمِهِ مِن بَعْدِهِ مِن جُندٍ مِّنَ السَّمَاءِ وَمَا كُنَّا مُنزِلِينَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3734,
      "verse_number": 29,
      "verse_key": "36:29",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِن كَانَتْ إِلَّا صَيْحَةً وَاحِدَةً فَإِذَا هُمْ خَامِدُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3735,
      "verse_number": 30,
      "verse_key": "36:30",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "يَا حَسْرَةً عَلَى الْعِبَادِ ۚ مَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا بِهِ يَسْتَهْزِئُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3736,
      "verse_number": 31,
      "verse_key": "36:31",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ يَرَوْا كَمْ أَهْلَكْنَا قَبْلَهُم مِّنَ الْقُرُونِ أَنَّهُمْ إِلَيْهِمْ لَا يَرْجِعُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3737,
      "verse_number": 32,
      "verse_key": "36:32",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 384,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِن كُلٌّ لَّمَّا جَمِيعٌ لَّدَيْنَا مُحْضَرُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3738,
      "verse_number": 33,
      "verse_key": "36:33",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَآيَةٌ لَّهُمُ الْأَرْضُ الْمَيْتَةُ أَحْيَيْنَاهَا وَأَخْرَجْنَا مِنْهَا حَبًّا فَمِنْهُ يَأْكُلُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3739,
      "verse_number": 34,
      "verse_key": "36:34",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا فِيهَا جَنَّاتٍ مِّن نَّخِيلٍ وَأَعْنَابٍ وَفَجَّرْنَا فِيهَا مِنَ الْعُيُونِ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3740,
      "verse_number": 35,
      "verse_key": "36:35",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لِيَأْكُلُوا مِن ثَمَرِهِ وَمَا عَمِلَتْهُ أَيْدِيهِمْ ۖ أَفَلَا يَشْكُرُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3741,
      "verse_number": 36,
      "verse_key": "36:36",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "سُبْحَانَ الَّذِي خَلَقَ الْأَزْوَاجَ كُلَّهَا مِمَّا تُنبِتُ الْأَرْضُ وَمِنْ أَنفُسِهِمْ وَمِمَّا لَا يَعْلَمُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3742,
      "verse_number": 37,
      "verse_key": "36:37",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَآيَةٌ لَّهُمُ اللَّيْلُ نَسْلَخُ مِنْهُ النَّهَارَ فَإِذَا هُم مُّظْلِمُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3743,
      "verse_number": 38,
      "verse_key": "36:38",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالشَّمْسُ تَجْرِي لِمُسْتَقَرٍّ لَّهَا ۚ ذَٰلِكَ تَقْدِيرُ الْعَزِيزِ الْعَلِيمِ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3744,
      "verse_number": 39,
      "verse_key": "36:39",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالْقَمَرَ قَدَّرْنَاهُ مَنَازِلَ حَتَّىٰ عَادَ كَالْعُرْجُونِ الْقَدِيمِ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3745,
      "verse_number": 40,
      "verse_key": "36:40",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَا الشَّمْسُ يَنبَغِي لَهَا أَن تُدْرِكَ الْقَمَرَ وَلَا اللَّيْلُ سَابِقُ النَّهَارِ ۚ وَكُلٌّ فِي فَلَكٍ يَسْبَحُونَ",
      "page_number": 442,
      "juz_number": 23
    },
    {
      "id": 3746,
      "verse_number": 41,
      "verse_key": "36:41",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَآيَةٌ لَّهُمْ أَنَّا حَمَلْنَا ذُرِّيَّتَهُمْ فِي الْفُلْكِ الْمَشْحُونِ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3747,
      "verse_number": 42,
      "verse_key": "36:42",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَخَلَقْنَا لَهُم مِّن مِّثْلِهِ مَا يَرْكَبُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3748,
      "verse_number": 43,
      "verse_key": "36:43",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِن نَّشَأْ نُغْرِقْهُمْ فَلَا صَرِيخَ لَهُمْ وَلَا هُمْ يُنقَذُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3749,
      "verse_number": 44,
      "verse_key": "36:44",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا رَحْمَةً مِّنَّا وَمَتَاعًا إِلَىٰ حِينٍ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3750,
      "verse_number": 45,
      "verse_key": "36:45",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا قِيلَ لَهُمُ اتَّقُوا مَا بَيْنَ أَيْدِيكُمْ وَمَا خَلْفَكُمْ لَعَلَّكُمْ تُرْحَمُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3751,
      "verse_number": 46,
      "verse_key": "36:46",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا تَأْتِيهِم مِّنْ آيَةٍ مِّنْ آيَاتِ رَبِّهِمْ إِلَّا كَانُوا عَنْهَا مُعْرِضِينَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3752,
      "verse_number": 47,
      "verse_key": "36:47",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا قِيلَ لَهُمْ أَنفِقُوا مِمَّا رَزَقَكُمُ اللَّهُ قَالَ الَّذِينَ كَفَرُوا لِلَّذِينَ آمَنُوا أَنُطْعِمُ مَن لَّوْ يَشَاءُ اللَّهُ أَطْعَمَهُ إِنْ أَنتُمْ إِلَّا فِي ضَلَالٍ مُّبِينٍ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3753,
      "verse_number": 48,
      "verse_key": "36:48",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَيَقُولُونَ مَتَىٰ هَٰذَا الْوَعْدُ إِن كُنتُمْ صَادِقِينَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3754,
      "verse_number": 49,
      "verse_key": "36:49",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "مَا يَنظُرُونَ إِلَّا صَيْحَةً وَاحِدَةً تَأْخُذُهُمْ وَهُمْ يَخِصِّمُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3755,
      "verse_number": 50,
      "verse_key": "36:50",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 385,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَلَا يَسْتَطِيعُونَ تَوْصِيَةً وَلَا إِلَىٰ أَهْلِهِمْ يَرْجِعُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3756,
      "verse_number": 51,
      "verse_key": "36:51",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَنُفِخَ فِي الصُّورِ فَإِذَا هُم مِّنَ الْأَجْدَاثِ إِلَىٰ رَبِّهِمْ يَنسِلُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3757,
      "verse_number": 52,
      "verse_key": "36:52",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا يَا وَيْلَنَا مَن بَعَثَنَا مِن مَّرْقَدِنَا ۜ ۗ هَٰذَا مَا وَعَدَ الرَّحْمَٰنُ وَصَدَقَ الْمُرْسَلُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3758,
      "verse_number": 53,
      "verse_key": "36:53",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِن كَانَتْ إِلَّا صَيْحَةً وَاحِدَةً فَإِذَا هُمْ جَمِيعٌ لَّدَيْنَا مُحْضَرُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3759,
      "verse_number": 54,
      "verse_key": "36:54",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَالْيَوْمَ لَا تُظْلَمُ نَفْسٌ شَيْئًا وَلَا تُجْزَوْنَ إِلَّا مَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 443,
      "juz_number": 23
    },
    {
      "id": 3760,
      "verse_number": 55,
      "verse_key": "36:55",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ أَصْحَابَ الْجَنَّةِ الْيَوْمَ فِي شُغُلٍ فَاكِهُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3761,
      "verse_number": 56,
      "verse_key": "36:56",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "هُمْ وَأَزْوَاجُهُمْ فِي ظِلَالٍ عَلَى الْأَرَائِكِ مُتَّكِئُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3762,
      "verse_number": 57,
      "verse_key": "36:57",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَهُمْ فِيهَا فَاكِهَةٌ وَلَهُم مَّا يَدَّعُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3763,
      "verse_number": 58,
      "verse_key": "36:58",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "سَلَامٌ قَوْلًا مِّن رَّبٍّ رَّحِيمٍ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3764,
      "verse_number": 59,
      "verse_key": "36:59",
      "hizb_number": 45,
      "rub_el_hizb_number": 177,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَامْتَازُوا الْيَوْمَ أَيُّهَا الْمُجْرِمُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3765,
      "verse_number": 60,
      "verse_key": "36:60",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ أَعْهَدْ إِلَيْكُمْ يَا بَنِي آدَمَ أَن لَّا تَعْبُدُوا الشَّيْطَانَ ۖ إِنَّهُ لَكُمْ عَدُوٌّ مُّبِينٌ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3766,
      "verse_number": 61,
      "verse_key": "36:61",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَأَنِ اعْبُدُونِي ۚ هَٰذَا صِرَاطٌ مُّسْتَقِيمٌ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3767,
      "verse_number": 62,
      "verse_key": "36:62",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَضَلَّ مِنكُمْ جِبِلًّا كَثِيرًا ۖ أَفَلَمْ تَكُونُوا تَعْقِلُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3768,
      "verse_number": 63,
      "verse_key": "36:63",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "هَٰذِهِ جَهَنَّمُ الَّتِي كُنتُمْ تُوعَدُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3769,
      "verse_number": 64,
      "verse_key": "36:64",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "اصْلَوْهَا الْيَوْمَ بِمَا كُنتُمْ تَكْفُرُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3770,
      "verse_number": 65,
      "verse_key": "36:65",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "الْيَوْمَ نَخْتِمُ عَلَىٰ أَفْوَاهِهِمْ وَتُكَلِّمُنَا أَيْدِيهِمْ وَتَشْهَدُ أَرْجُلُهُم بِمَا كَانُوا يَكْسِبُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3771,
      "verse_number": 66,
      "verse_key": "36:66",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ نَشَاءُ لَطَمَسْنَا عَلَىٰ أَعْيُنِهِمْ فَاسْتَبَقُوا الصِّرَاطَ فَأَنَّىٰ يُبْصِرُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3772,
      "verse_number": 67,
      "verse_key": "36:67",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 386,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ نَشَاءُ لَمَسَخْنَاهُمْ عَلَىٰ مَكَانَتِهِمْ فَمَا اسْتَطَاعُوا مُضِيًّا وَلَا يَرْجِعُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3773,
      "verse_number": 68,
      "verse_key": "36:68",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَن نُّعَمِّرْهُ نُنَكِّسْهُ فِي الْخَلْقِ ۖ أَفَلَا يَعْقِلُونَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3774,
      "verse_number": 69,
      "verse_key": "36:69",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا عَلَّمْنَاهُ الشِّعْرَ وَمَا يَنبَغِي لَهُ ۚ إِنْ هُوَ إِلَّا ذِكْرٌ وَقُرْآنٌ مُّبِينٌ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3775,
      "verse_number": 70,
      "verse_key": "36:70",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لِّيُنذِرَ مَن كَانَ حَيًّا وَيَحِقَّ الْقَوْلُ عَلَى الْكَافِرِينَ",
      "page_number": 444,
      "juz_number": 23
    },
    {
      "id": 3776,
      "verse_number": 71,
      "verse_key": "36:71",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَمْ يَرَوْا أَنَّا خَلَقْنَا لَهُم مِّمَّا عَمِلَتْ أَيْدِينَا أَنْعَامًا فَهُمْ لَهَا مَالِكُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3777,
      "verse_number": 72,
      "verse_key": "36:72",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَذَلَّلْنَاهَا لَهُمْ فَمِنْهَا رَكُوبُهُمْ وَمِنْهَا يَأْكُلُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3778,
      "verse_number": 73,
      "verse_key": "36:73",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَلَهُمْ فِيهَا مَنَافِعُ وَمَشَارِبُ ۖ أَفَلَا يَشْكُرُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3779,
      "verse_number": 74,
      "verse_key": "36:74",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَاتَّخَذُوا مِن دُونِ اللَّهِ آلِهَةً لَّعَلَّهُمْ يُنصَرُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3780,
      "verse_number": 75,
      "verse_key": "36:75",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَا يَسْتَطِيعُونَ نَصْرَهُمْ وَهُمْ لَهُمْ جُندٌ مُّحْضَرُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3781,
      "verse_number": 76,
      "verse_key": "36:76",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَلَا يَحْزُنكَ قَوْلُهُمْ ۘ إِنَّا نَعْلَمُ مَا يُسِرُّونَ وَمَا يُعْلِنُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3782,
      "verse_number": 77,
      "verse_key": "36:77",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَمْ يَرَ الْإِنسَانُ أَنَّا خَلَقْنَاهُ مِن نُّطْفَةٍ فَإِذَا هُوَ خَصِيمٌ مُّبِينٌ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3783,
      "verse_number": 78,
      "verse_key": "36:78",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَضَرَبَ لَنَا مَثَلًا وَنَسِيَ خَلْقَهُ ۖ قَالَ مَن يُحْيِي الْعِظَامَ وَهِيَ رَمِيمٌ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3784,
      "verse_number": 79,
      "verse_key": "36:79",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قُلْ يُحْيِيهَا الَّذِي أَنشَأَهَا أَوَّلَ مَرَّةٍ ۖ وَهُوَ بِكُلِّ خَلْقٍ عَلِيمٌ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3785,
      "verse_number": 80,
      "verse_key": "36:80",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي جَعَلَ لَكُم مِّنَ الشَّجَرِ الْأَخْضَرِ نَارًا فَإِذَا أَنتُم مِّنْهُ تُوقِدُونَ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3786,
      "verse_number": 81,
      "verse_key": "36:81",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَيْسَ الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ بِقَادِرٍ عَلَىٰ أَن يَخْلُقَ مِثْلَهُم ۚ بَلَىٰ وَهُوَ الْخَلَّاقُ الْعَلِيمُ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3787,
      "verse_number": 82,
      "verse_key": "36:82",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّمَا أَمْرُهُ إِذَا أَرَادَ شَيْئًا أَن يَقُولَ لَهُ كُن فَيَكُونُ",
      "page_number": 445,
      "juz_number": 23
    },
    {
      "id": 3788,
      "verse_number": 83,
      "verse_key": "36:83",
      "hizb_number": 45,
      "rub_el_hizb_number": 178,
      "ruku_number": 387,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَسُبْحَانَ الَّذِي بِيَدِهِ مَلَكُوتُ كُلِّ شَيْءٍ وَإِلَيْهِ تُرْجَعُونَ",
      "page_number": 445,
      "juz_number": 23
    }
  ]
}