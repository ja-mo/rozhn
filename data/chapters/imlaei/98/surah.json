{
  "id": 98,
  "name": "The Clear Proof",
  "verses": [
    {
      "id": 6131,
      "verse_number": 1,
      "verse_key": "98:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَمْ يَكُنِ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ مُنفَكِّينَ حَتَّىٰ تَأْتِيَهُمُ الْبَيِّنَةُ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6132,
      "verse_number": 2,
      "verse_key": "98:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "رَسُولٌ مِّنَ اللَّهِ يَتْلُو صُحُفًا مُّطَهَّرَةً",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6133,
      "verse_number": 3,
      "verse_key": "98:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِيهَا كُتُبٌ قَيِّمَةٌ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6134,
      "verse_number": 4,
      "verse_key": "98:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا تَفَرَّقَ الَّذِينَ أُوتُوا الْكِتَابَ إِلَّا مِن بَعْدِ مَا جَاءَتْهُمُ الْبَيِّنَةُ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6135,
      "verse_number": 5,
      "verse_key": "98:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أُمِرُوا إِلَّا لِيَعْبُدُوا اللَّهَ مُخْلِصِينَ لَهُ الدِّينَ حُنَفَاءَ وَيُقِيمُوا الصَّلَاةَ وَيُؤْتُوا الزَّكَاةَ ۚ وَذَٰلِكَ دِينُ الْقَيِّمَةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6136,
      "verse_number": 6,
      "verse_key": "98:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ كَفَرُوا مِنْ أَهْلِ الْكِتَابِ وَالْمُشْرِكِينَ فِي نَارِ جَهَنَّمَ خَالِدِينَ فِيهَا ۚ أُولَٰئِكَ هُمْ شَرُّ الْبَرِيَّةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6137,
      "verse_number": 7,
      "verse_key": "98:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ أُولَٰئِكَ هُمْ خَيْرُ الْبَرِيَّةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6138,
      "verse_number": 8,
      "verse_key": "98:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "جَزَاؤُهُمْ عِندَ رَبِّهِمْ جَنَّاتُ عَدْنٍ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ خَالِدِينَ فِيهَا أَبَدًا ۖ رَّضِيَ اللَّهُ عَنْهُمْ وَرَضُوا عَنْهُ ۚ ذَٰلِكَ لِمَنْ خَشِيَ رَبَّهُ",
      "page_number": 599,
      "juz_number": 30
    }
  ]
}