      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالذَّارِيَاتِ ذَرْوًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4677,
      "verse_number": 2,
      "verse_key": "51:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَالْحَامِلَاتِ وِقْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4678,
      "verse_number": 3,
      "verse_key": "51:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَالْجَارِيَاتِ يُسْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4679,
      "verse_number": 4,
      "verse_key": "51:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَالْمُقَسِّمَاتِ أَمْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4680,
      "verse_number": 5,
      "verse_key": "51:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّمَا تُوعَدُونَ لَصَادِقٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4681,
      "verse_number": 6,
      "verse_key": "51:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ الدِّينَ لَوَاقِعٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4682,
      "verse_number": 7,
      "verse_key": "51:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءِ ذَاتِ الْحُبُكِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4683,
      "verse_number": 8,
      "verse_key": "51:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّكُمْ لَفِي قَوْلٍ مُّخْتَلِفٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4684,
      "verse_number": 9,
      "verse_key": "51:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يُؤْفَكُ عَنْهُ مَنْ أُفِكَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4685,
      "verse_number": 10,
      "verse_key": "51:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُتِلَ الْخَرَّاصُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4686,
      "verse_number": 11,
      "verse_key": "51:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ هُمْ فِي غَمْرَةٍ سَاهُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4687,
      "verse_number": 12,
      "verse_key": "51:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَسْأَلُونَ أَيَّانَ يَوْمُ الدِّينِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4688,
      "verse_number": 13,
      "verse_key": "51:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ هُمْ عَلَى النَّارِ يُفْتَنُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4689,
      "verse_number": 14,
      "verse_key": "51:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذُوقُوا فِتْنَتَكُمْ هَٰذَا الَّذِي كُنتُم بِهِ تَسْتَعْجِلُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4690,
      "verse_number": 15,
      "verse_key": "51:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَعُيُونٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4691,
      "verse_number": 16,
      "verse_key": "51:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "آخِذِينَ مَا آتَاهُمْ رَبُّهُمْ ۚ إِنَّهُمْ كَانُوا قَبْلَ ذَٰلِكَ مُحْسِنِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4692,
      "verse_number": 17,
      "verse_key": "51:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَانُوا قَلِيلًا مِّنَ اللَّيْلِ مَا يَهْجَعُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4693,
      "verse_number": 18,
      "verse_key": "51:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَبِالْأَسْحَارِ هُمْ يَسْتَغْفِرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4694,
      "verse_number": 19,
      "verse_key": "51:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي أَمْوَالِهِمْ حَقٌّ لِّلسَّائِلِ وَالْمَحْرُومِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4695,
      "verse_number": 20,
      "verse_key": "51:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي الْأَرْضِ آيَاتٌ لِّلْمُوقِنِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4696,
      "verse_number": 21,
      "verse_key": "51:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي أَنفُسِكُمْ ۚ أَفَلَا تُبْصِرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4697,
      "verse_number": 22,
      "verse_key": "51:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي السَّمَاءِ رِزْقُكُمْ وَمَا تُوعَدُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4698,
      "verse_number": 23,
      "verse_key": "51:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَوَرَبِّ السَّمَاءِ وَالْأَرْضِ إِنَّهُ لَحَقٌّ مِّثْلَ مَا أَنَّكُمْ تَنطِقُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4699,
      "verse_number": 24,
      "verse_key": "51:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ ضَيْفِ إِبْرَاهِيمَ الْمُكْرَمِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4700,
      "verse_number": 25,
      "verse_key": "51:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذْ دَخَلُوا عَلَيْهِ فَقَالُوا سَلَامًا ۖ قَالَ سَلَامٌ قَوْمٌ مُّنكَرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4701,
      "verse_number": 26,
      "verse_key": "51:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَرَاغَ إِلَىٰ أَهْلِهِ فَجَاءَ بِعِجْلٍ سَمِينٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4702,
      "verse_number": 27,
      "verse_key": "51:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَقَرَّبَهُ إِلَيْهِمْ قَالَ أَلَا تَأْكُلُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4703,
      "verse_number": 28,
      "verse_key": "51:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَوْجَسَ مِنْهُمْ خِيفَةً ۖ قَالُوا لَا تَخَفْ ۖ وَبَشَّرُوهُ بِغُلَامٍ عَلِيمٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4704,
      "verse_number": 29,
      "verse_key": "51:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَقْبَلَتِ امْرَأَتُهُ فِي صَرَّةٍ فَصَكَّتْ وَجْهَهَا وَقَالَتْ عَجُوزٌ عَقِيمٌ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4705,
      "verse_number": 30,
      "verse_key": "51:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا كَذَٰلِكِ قَالَ رَبُّكِ ۖ إِنَّهُ هُوَ الْحَكِيمُ الْعَلِيمُ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4706,
      "verse_number": 31,
      "verse_key": "51:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا الْمُرْسَلُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4707,
      "verse_number": 32,
      "verse_key": "51:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنَّا أُرْسِلْنَا إِلَىٰ قَوْمٍ مُّجْرِمِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4708,
      "verse_number": 33,
      "verse_key": "51:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِنُرْسِلَ عَلَيْهِمْ حِجَارَةً مِّن طِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4709,
      "verse_number": 34,
      "verse_key": "51:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مُّسَوَّمَةً عِندَ رَبِّكَ لِلْمُسْرِفِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4710,
      "verse_number": 35,
      "verse_key": "51:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَخْرَجْنَا مَن كَانَ فِيهَا مِنَ الْمُؤْمِنِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4711,
      "verse_number": 36,
      "verse_key": "51:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَا وَجَدْنَا فِيهَا غَيْرَ بَيْتٍ مِّنَ الْمُسْلِمِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4712,
      "verse_number": 37,
      "verse_key": "51:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَتَرَكْنَا فِيهَا آيَةً لِّلَّذِينَ يَخَافُونَ الْعَذَابَ الْأَلِيمَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4713,
      "verse_number": 38,
      "verse_key": "51:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي مُوسَىٰ إِذْ أَرْسَلْنَاهُ إِلَىٰ فِرْعَوْنَ بِسُلْطَانٍ مُّبِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4714,
      "verse_number": 39,
      "verse_key": "51:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَتَوَلَّىٰ بِرُكْنِهِ وَقَالَ سَاحِرٌ أَوْ مَجْنُونٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4715,
      "verse_number": 40,
      "verse_key": "51:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَخَذْنَاهُ وَجُنُودَهُ فَنَبَذْنَاهُمْ فِي الْيَمِّ وَهُوَ مُلِيمٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4716,
      "verse_number": 41,
      "verse_key": "51:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي عَادٍ إِذْ أَرْسَلْنَا عَلَيْهِمُ الرِّيحَ الْعَقِيمَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4717,
      "verse_number": 42,
      "verse_key": "51:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا تَذَرُ مِن شَيْءٍ أَتَتْ عَلَيْهِ إِلَّا جَعَلَتْهُ كَالرَّمِيمِ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4718,
      "verse_number": 43,
      "verse_key": "51:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَفِي ثَمُودَ إِذْ قِيلَ لَهُمْ تَمَتَّعُوا حَتَّىٰ حِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4719,
      "verse_number": 44,
      "verse_key": "51:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَعَتَوْا عَنْ أَمْرِ رَبِّهِمْ فَأَخَذَتْهُمُ الصَّاعِقَةُ وَهُمْ يَنظُرُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4720,
      "verse_number": 45,
      "verse_key": "51:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَا اسْتَطَاعُوا مِن قِيَامٍ وَمَا كَانُوا مُنتَصِرِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4721,
      "verse_number": 46,
      "verse_key": "51:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَوْمَ نُوحٍ مِّن قَبْلُ ۖ إِنَّهُمْ كَانُوا قَوْمًا فَاسِقِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4722,
      "verse_number": 47,
      "verse_key": "51:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءَ بَنَيْنَاهَا بِأَيْدٍ وَإِنَّا لَمُوسِعُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4723,
      "verse_number": 48,
      "verse_key": "51:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضَ فَرَشْنَاهَا فَنِعْمَ الْمَاهِدُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4724,
      "verse_number": 49,
      "verse_key": "51:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمِن كُلِّ شَيْءٍ خَلَقْنَا زَوْجَيْنِ لَعَلَّكُمْ تَذَكَّرُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4725,
      "verse_number": 50,
      "verse_key": "51:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَفِرُّوا إِلَى اللَّهِ ۖ إِنِّي لَكُم مِّنْهُ نَذِيرٌ مُّبِينٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4726,
      "verse_number": 51,
      "verse_key": "51:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَا تَجْعَلُوا مَعَ اللَّهِ إِلَٰهًا آخَرَ ۖ إِنِّي لَكُم مِّنْهُ نَذِيرٌ مُّبِينٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4727,
      "verse_number": 52,
      "verse_key": "51:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ مَا أَتَى الَّذِينَ مِن قَبْلِهِم مِّن رَّسُولٍ إِلَّا قَالُوا سَاحِرٌ أَوْ مَجْنُونٌ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4728,
      "verse_number": 53,
      "verse_key": "51:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَتَوَاصَوْا بِهِ ۚ بَلْ هُمْ قَوْمٌ طَاغُونَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4729,
      "verse_number": 54,
      "verse_key": "51:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَتَوَلَّ عَنْهُمْ فَمَا أَنتَ بِمَلُومٍ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4730,
      "verse_number": 55,
      "verse_key": "51:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَذَكِّرْ فَإِنَّ الذِّكْرَىٰ تَنفَعُ الْمُؤْمِنِينَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4731,
      "verse_number": 56,
      "verse_key": "51:56",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا خَلَقْتُ الْجِنَّ وَالْإِنسَ إِلَّا لِيَعْبُدُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4732,
      "verse_number": 57,
      "verse_key": "51:57",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا أُرِيدُ مِنْهُم مِّن رِّزْقٍ وَمَا أُرِيدُ أَن يُطْعِمُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4733,
      "verse_number": 58,
      "verse_key": "51:58",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ اللَّهَ هُوَ الرَّزَّاقُ ذُو الْقُوَّةِ الْمَتِينُ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4734,
      "verse_number": 59,
      "verse_key": "51:59",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّ لِلَّذِينَ ظَلَمُوا ذَنُوبًا مِّثْلَ ذَنُوبِ أَصْحَابِهِمْ فَلَا يَسْتَعْجِلُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4735,
      "verse_number": 60,
      "verse_key": "51:60",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَوَيْلٌ لِّلَّذِينَ كَفَرُوا مِن يَوْمِهِمُ الَّذِي يُوعَدُونَ",
      "page_number": 523,
      "juz_number": 27
    }
  ]
}