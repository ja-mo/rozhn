{
  "id": 43,
  "name": "The Ornaments of Gold",
  "verses": [
    {
      "id": 4326,
      "verse_number": 1,
      "verse_key": "43:1",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "حم",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4327,
      "verse_number": 2,
      "verse_key": "43:2",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَالْكِتَابِ الْمُبِينِ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4328,
      "verse_number": 3,
      "verse_key": "43:3",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا جَعَلْنَاهُ قُرْآنًا عَرَبِيًّا لَّعَلَّكُمْ تَعْقِلُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4329,
      "verse_number": 4,
      "verse_key": "43:4",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهُ فِي أُمِّ الْكِتَابِ لَدَيْنَا لَعَلِيٌّ حَكِيمٌ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4330,
      "verse_number": 5,
      "verse_key": "43:5",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَفَنَضْرِبُ عَنكُمُ الذِّكْرَ صَفْحًا أَن كُنتُمْ قَوْمًا مُّسْرِفِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4331,
      "verse_number": 6,
      "verse_key": "43:6",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَكَمْ أَرْسَلْنَا مِن نَّبِيٍّ فِي الْأَوَّلِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4332,
      "verse_number": 7,
      "verse_key": "43:7",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يَأْتِيهِم مِّن نَّبِيٍّ إِلَّا كَانُوا بِهِ يَسْتَهْزِئُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4333,
      "verse_number": 8,
      "verse_key": "43:8",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَأَهْلَكْنَا أَشَدَّ مِنْهُم بَطْشًا وَمَضَىٰ مَثَلُ الْأَوَّلِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4334,
      "verse_number": 9,
      "verse_key": "43:9",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَئِن سَأَلْتَهُم مَّنْ خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ لَيَقُولُنَّ خَلَقَهُنَّ الْعَزِيزُ الْعَلِيمُ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4335,
      "verse_number": 10,
      "verse_key": "43:10",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي جَعَلَ لَكُمُ الْأَرْضَ مَهْدًا وَجَعَلَ لَكُمْ فِيهَا سُبُلًا لَّعَلَّكُمْ تَهْتَدُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4336,
      "verse_number": 11,
      "verse_key": "43:11",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِي نَزَّلَ مِنَ السَّمَاءِ مَاءً بِقَدَرٍ فَأَنشَرْنَا بِهِ بَلْدَةً مَّيْتًا ۚ كَذَٰلِكَ تُخْرَجُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4337,
      "verse_number": 12,
      "verse_key": "43:12",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِي خَلَقَ الْأَزْوَاجَ كُلَّهَا وَجَعَلَ لَكُم مِّنَ الْفُلْكِ وَالْأَنْعَامِ مَا تَرْكَبُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4338,
      "verse_number": 13,
      "verse_key": "43:13",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لِتَسْتَوُوا عَلَىٰ ظُهُورِهِ ثُمَّ تَذْكُرُوا نِعْمَةَ رَبِّكُمْ إِذَا اسْتَوَيْتُمْ عَلَيْهِ وَتَقُولُوا سُبْحَانَ الَّذِي سَخَّرَ لَنَا هَٰذَا وَمَا كُنَّا لَهُ مُقْرِنِينَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4339,
      "verse_number": 14,
      "verse_key": "43:14",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّا إِلَىٰ رَبِّنَا لَمُنقَلِبُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4340,
      "verse_number": 15,
      "verse_key": "43:15",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلُوا لَهُ مِنْ عِبَادِهِ جُزْءًا ۚ إِنَّ الْإِنسَانَ لَكَفُورٌ مُّبِينٌ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4341,
      "verse_number": 16,
      "verse_key": "43:16",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمِ اتَّخَذَ مِمَّا يَخْلُقُ بَنَاتٍ وَأَصْفَاكُم بِالْبَنِينَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4342,
      "verse_number": 17,
      "verse_key": "43:17",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا بُشِّرَ أَحَدُهُم بِمَا ضَرَبَ لِلرَّحْمَٰنِ مَثَلًا ظَلَّ وَجْهُهُ مُسْوَدًّا وَهُوَ كَظِيمٌ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4343,
      "verse_number": 18,
      "verse_key": "43:18",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَوَمَن يُنَشَّأُ فِي الْحِلْيَةِ وَهُوَ فِي الْخِصَامِ غَيْرُ مُبِينٍ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4344,
      "verse_number": 19,
      "verse_key": "43:19",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلُوا الْمَلَائِكَةَ الَّذِينَ هُمْ عِبَادُ الرَّحْمَٰنِ إِنَاثًا ۚ أَشَهِدُوا خَلْقَهُمْ ۚ سَتُكْتَبُ شَهَادَتُهُمْ وَيُسْأَلُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4345,
      "verse_number": 20,
      "verse_key": "43:20",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا لَوْ شَاءَ الرَّحْمَٰنُ مَا عَبَدْنَاهُم ۗ مَّا لَهُم بِذَٰلِكَ مِنْ عِلْمٍ ۖ إِنْ هُمْ إِلَّا يَخْرُصُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4346,
      "verse_number": 21,
      "verse_key": "43:21",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمْ آتَيْنَاهُمْ كِتَابًا مِّن قَبْلِهِ فَهُم بِهِ مُسْتَمْسِكُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4347,
      "verse_number": 22,
      "verse_key": "43:22",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ قَالُوا إِنَّا وَجَدْنَا آبَاءَنَا عَلَىٰ أُمَّةٍ وَإِنَّا عَلَىٰ آثَارِهِم مُّهْتَدُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4348,
      "verse_number": 23,
      "verse_key": "43:23",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَكَذَٰلِكَ مَا أَرْسَلْنَا مِن قَبْلِكَ فِي قَرْيَةٍ مِّن نَّذِيرٍ إِلَّا قَالَ مُتْرَفُوهَا إِنَّا وَجَدْنَا آبَاءَنَا عَلَىٰ أُمَّةٍ وَإِنَّا عَلَىٰ آثَارِهِم مُّقْتَدُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4349,
      "verse_number": 24,
      "verse_key": "43:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَوَلَوْ جِئْتُكُم بِأَهْدَىٰ مِمَّا وَجَدتُّمْ عَلَيْهِ آبَاءَكُمْ ۖ قَالُوا إِنَّا بِمَا أُرْسِلْتُم بِهِ كَافِرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4350,
      "verse_number": 25,
      "verse_key": "43:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَانتَقَمْنَا مِنْهُمْ ۖ فَانظُرْ كَيْفَ كَانَ عَاقِبَةُ الْمُكَذِّبِينَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4351,
      "verse_number": 26,
      "verse_key": "43:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِذْ قَالَ إِبْرَاهِيمُ لِأَبِيهِ وَقَوْمِهِ إِنَّنِي بَرَاءٌ مِّمَّا تَعْبُدُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4352,
      "verse_number": 27,
      "verse_key": "43:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا الَّذِي فَطَرَنِي فَإِنَّهُ سَيَهْدِينِ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4353,
      "verse_number": 28,
      "verse_key": "43:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلَهَا كَلِمَةً بَاقِيَةً فِي عَقِبِهِ لَعَلَّهُمْ يَرْجِعُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4354,
      "verse_number": 29,
      "verse_key": "43:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "بَلْ مَتَّعْتُ هَٰؤُلَاءِ وَآبَاءَهُمْ حَتَّىٰ جَاءَهُمُ الْحَقُّ وَرَسُولٌ مُّبِينٌ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4355,
      "verse_number": 30,
      "verse_key": "43:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَمَّا جَاءَهُمُ الْحَقُّ قَالُوا هَٰذَا سِحْرٌ وَإِنَّا بِهِ كَافِرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4356,
      "verse_number": 31,
      "verse_key": "43:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا لَوْلَا نُزِّلَ هَٰذَا الْقُرْآنُ عَلَىٰ رَجُلٍ مِّنَ الْقَرْيَتَيْنِ عَظِيمٍ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4357,
      "verse_number": 32,
      "verse_key": "43:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَهُمْ يَقْسِمُونَ رَحْمَتَ رَبِّكَ ۚ نَحْنُ قَسَمْنَا بَيْنَهُم مَّعِيشَتَهُمْ فِي الْحَيَاةِ الدُّنْيَا ۚ وَرَفَعْنَا بَعْضَهُمْ فَوْقَ بَعْضٍ دَرَجَاتٍ لِّيَتَّخِذَ بَعْضُهُم بَعْضًا سُخْرِيًّا ۗ وَرَحْمَتُ رَبِّكَ خَيْرٌ مِّمَّا يَجْمَعُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4358,
      "verse_number": 33,
      "verse_key": "43:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْلَا أَن يَكُونَ النَّاسُ أُمَّةً وَاحِدَةً لَّجَعَلْنَا لِمَن يَكْفُرُ بِالرَّحْمَٰنِ لِبُيُوتِهِمْ سُقُفًا مِّن فِضَّةٍ وَمَعَارِجَ عَلَيْهَا يَظْهَرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4359,
      "verse_number": 34,
      "verse_key": "43:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلِبُيُوتِهِمْ أَبْوَابًا وَسُرُرًا عَلَيْهَا يَتَّكِئُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4360,
      "verse_number": 35,
      "verse_key": "43:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَزُخْرُفًا ۚ وَإِن كُلُّ ذَٰلِكَ لَمَّا مَتَاعُ الْحَيَاةِ الدُّنْيَا ۚ وَالْآخِرَةُ عِندَ رَبِّكَ لِلْمُتَّقِينَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4361,
      "verse_number": 36,
      "verse_key": "43:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَن يَعْشُ عَن ذِكْرِ الرَّحْمَٰنِ نُقَيِّضْ لَهُ شَيْطَانًا فَهُوَ لَهُ قَرِينٌ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4362,
      "verse_number": 37,
      "verse_key": "43:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهُمْ لَيَصُدُّونَهُمْ عَنِ السَّبِيلِ وَيَحْسَبُونَ أَنَّهُم مُّهْتَدُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4363,
      "verse_number": 38,
      "verse_key": "43:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "حَتَّىٰ إِذَا جَاءَنَا قَالَ يَا لَيْتَ بَيْنِي وَبَيْنَكَ بُعْدَ الْمَشْرِقَيْنِ فَبِئْسَ الْقَرِينُ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4364,
      "verse_number": 39,
      "verse_key": "43:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَن يَنفَعَكُمُ الْيَوْمَ إِذ ظَّلَمْتُمْ أَنَّكُمْ فِي الْعَذَابِ مُشْتَرِكُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4365,
      "verse_number": 40,
      "verse_key": "43:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَفَأَنتَ تُسْمِعُ الصُّمَّ أَوْ تَهْدِي الْعُمْيَ وَمَن كَانَ فِي ضَلَالٍ مُّبِينٍ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4366,
      "verse_number": 41,
      "verse_key": "43:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَإِمَّا نَذْهَبَنَّ بِكَ فَإِنَّا مِنْهُم مُّنتَقِمُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4367,
      "verse_number": 42,
      "verse_key": "43:42",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَوْ نُرِيَنَّكَ الَّذِي وَعَدْنَاهُمْ فَإِنَّا عَلَيْهِم مُّقْتَدِرُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4368,
      "verse_number": 43,
      "verse_key": "43:43",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَمْسِكْ بِالَّذِي أُوحِيَ إِلَيْكَ ۖ إِنَّكَ عَلَىٰ صِرَاطٍ مُّسْتَقِيمٍ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4369,
      "verse_number": 44,
      "verse_key": "43:44",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهُ لَذِكْرٌ لَّكَ وَلِقَوْمِكَ ۖ وَسَوْفَ تُسْأَلُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4370,
      "verse_number": 45,
      "verse_key": "43:45",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَاسْأَلْ مَنْ أَرْسَلْنَا مِن قَبْلِكَ مِن رُّسُلِنَا أَجَعَلْنَا مِن دُونِ الرَّحْمَٰنِ آلِهَةً يُعْبَدُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4371,
      "verse_number": 46,
      "verse_key": "43:46",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَرْسَلْنَا مُوسَىٰ بِآيَاتِنَا إِلَىٰ فِرْعَوْنَ وَمَلَئِهِ فَقَالَ إِنِّي رَسُولُ رَبِّ الْعَالَمِينَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4372,
      "verse_number": 47,
      "verse_key": "43:47",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا جَاءَهُم بِآيَاتِنَا إِذَا هُم مِّنْهَا يَضْحَكُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4373,
      "verse_number": 48,
      "verse_key": "43:48",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا نُرِيهِم مِّنْ آيَةٍ إِلَّا هِيَ أَكْبَرُ مِنْ أُخْتِهَا ۖ وَأَخَذْنَاهُم بِالْعَذَابِ لَعَلَّهُمْ يَرْجِعُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4374,
      "verse_number": 49,
      "verse_key": "43:49",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا يَا أَيُّهَ السَّاحِرُ ادْعُ لَنَا رَبَّكَ بِمَا عَهِدَ عِندَكَ إِنَّنَا لَمُهْتَدُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4375,
      "verse_number": 50,
      "verse_key": "43:50",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا كَشَفْنَا عَنْهُمُ الْعَذَابَ إِذَا هُمْ يَنكُثُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4376,
      "verse_number": 51,
      "verse_key": "43:51",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَنَادَىٰ فِرْعَوْنُ فِي قَوْمِهِ قَالَ يَا قَوْمِ أَلَيْسَ لِي مُلْكُ مِصْرَ وَهَٰذِهِ الْأَنْهَارُ تَجْرِي مِن تَحْتِي ۖ أَفَلَا تُبْصِرُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4377,
      "verse_number": 52,
      "verse_key": "43:52",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمْ أَنَا خَيْرٌ مِّنْ هَٰذَا الَّذِي هُوَ مَهِينٌ وَلَا يَكَادُ يُبِينُ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4378,
      "verse_number": 53,
      "verse_key": "43:53",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَلَوْلَا أُلْقِيَ عَلَيْهِ أَسْوِرَةٌ مِّن ذَهَبٍ أَوْ جَاءَ مَعَهُ الْمَلَائِكَةُ مُقْتَرِنِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4379,
      "verse_number": 54,
      "verse_key": "43:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَاسْتَخَفَّ قَوْمَهُ فَأَطَاعُوهُ ۚ إِنَّهُمْ كَانُوا قَوْمًا فَاسِقِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4380,
      "verse_number": 55,
      "verse_key": "43:55",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا آسَفُونَا انتَقَمْنَا مِنْهُمْ فَأَغْرَقْنَاهُمْ أَجْمَعِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4381,
      "verse_number": 56,
      "verse_key": "43:56",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَجَعَلْنَاهُمْ سَلَفًا وَمَثَلًا لِّلْآخِرِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4382,
      "verse_number": 57,
      "verse_key": "43:57",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَمَّا ضُرِبَ ابْنُ مَرْيَمَ مَثَلًا إِذَا قَوْمُكَ مِنْهُ يَصِدُّونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4383,
      "verse_number": 58,
      "verse_key": "43:58",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا أَآلِهَتُنَا خَيْرٌ أَمْ هُوَ ۚ مَا ضَرَبُوهُ لَكَ إِلَّا جَدَلًا ۚ بَلْ هُمْ قَوْمٌ خَصِمُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4384,
      "verse_number": 59,
      "verse_key": "43:59",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنْ هُوَ إِلَّا عَبْدٌ أَنْعَمْنَا عَلَيْهِ وَجَعَلْنَاهُ مَثَلًا لِّبَنِي إِسْرَائِيلَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4385,
      "verse_number": 60,
      "verse_key": "43:60",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ نَشَاءُ لَجَعَلْنَا مِنكُم مَّلَائِكَةً فِي الْأَرْضِ يَخْلُفُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4386,
      "verse_number": 61,
      "verse_key": "43:61",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهُ لَعِلْمٌ لِّلسَّاعَةِ فَلَا تَمْتَرُنَّ بِهَا وَاتَّبِعُونِ ۚ هَٰذَا صِرَاطٌ مُّسْتَقِيمٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4387,
      "verse_number": 62,
      "verse_key": "43:62",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَصُدَّنَّكُمُ الشَّيْطَانُ ۖ إِنَّهُ لَكُمْ عَدُوٌّ مُّبِينٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4388,
      "verse_number": 63,
      "verse_key": "43:63",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَمَّا جَاءَ عِيسَىٰ بِالْبَيِّنَاتِ قَالَ قَدْ جِئْتُكُم بِالْحِكْمَةِ وَلِأُبَيِّنَ لَكُم بَعْضَ الَّذِي تَخْتَلِفُونَ فِيهِ ۖ فَاتَّقُوا اللَّهَ وَأَطِيعُونِ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4389,
      "verse_number": 64,
      "verse_key": "43:64",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ اللَّهَ هُوَ رَبِّي وَرَبُّكُمْ فَاعْبُدُوهُ ۚ هَٰذَا صِرَاطٌ مُّسْتَقِيمٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4390,
      "verse_number": 65,
      "verse_key": "43:65",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَاخْتَلَفَ الْأَحْزَابُ مِن بَيْنِهِمْ ۖ فَوَيْلٌ لِّلَّذِينَ ظَلَمُوا مِنْ عَذَابِ يَوْمٍ أَلِيمٍ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4391,
      "verse_number": 66,
      "verse_key": "43:66",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "هَلْ يَنظُرُونَ إِلَّا السَّاعَةَ أَن تَأْتِيَهُم بَغْتَةً وَهُمْ لَا يَشْعُرُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4392,
      "verse_number": 67,
      "verse_key": "43:67",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "الْأَخِلَّاءُ يَوْمَئِذٍ بَعْضُهُمْ لِبَعْضٍ عَدُوٌّ إِلَّا الْمُتَّقِينَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4393,
      "verse_number": 68,
      "verse_key": "43:68",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "يَا عِبَادِ لَا خَوْفٌ عَلَيْكُمُ الْيَوْمَ وَلَا أَنتُمْ تَحْزَنُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4394,
      "verse_number": 69,
      "verse_key": "43:69",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ آمَنُوا بِآيَاتِنَا وَكَانُوا مُسْلِمِينَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4395,
      "verse_number": 70,
      "verse_key": "43:70",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "ادْخُلُوا الْجَنَّةَ أَنتُمْ وَأَزْوَاجُكُمْ تُحْبَرُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4396,
      "verse_number": 71,
      "verse_key": "43:71",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "يُطَافُ عَلَيْهِم بِصِحَافٍ مِّن ذَهَبٍ وَأَكْوَابٍ ۖ وَفِيهَا مَا تَشْتَهِيهِ الْأَنفُسُ وَتَلَذُّ الْأَعْيُنُ ۖ وَأَنتُمْ فِيهَا خَالِدُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4397,
      "verse_number": 72,
      "verse_key": "43:72",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَتِلْكَ الْجَنَّةُ الَّتِي أُورِثْتُمُوهَا بِمَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4398,
      "verse_number": 73,
      "verse_key": "43:73",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لَكُمْ فِيهَا فَاكِهَةٌ كَثِيرَةٌ مِّنْهَا تَأْكُلُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4399,
      "verse_number": 74,
      "verse_key": "43:74",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْمُجْرِمِينَ فِي عَذَابِ جَهَنَّمَ خَالِدُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4400,
      "verse_number": 75,
      "verse_key": "43:75",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لَا يُفَتَّرُ عَنْهُمْ وَهُمْ فِيهِ مُبْلِسُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4401,
      "verse_number": 76,
      "verse_key": "43:76",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَمَا ظَلَمْنَاهُمْ وَلَٰكِن كَانُوا هُمُ الظَّالِمِينَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4402,
      "verse_number": 77,
      "verse_key": "43:77",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَنَادَوْا يَا مَالِكُ لِيَقْضِ عَلَيْنَا رَبُّكَ ۖ قَالَ إِنَّكُم مَّاكِثُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4403,
      "verse_number": 78,
      "verse_key": "43:78",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "لَقَدْ جِئْنَاكُم بِالْحَقِّ وَلَٰكِنَّ أَكْثَرَكُمْ لِلْحَقِّ كَارِهُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4404,
      "verse_number": 79,
      "verse_key": "43:79",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمْ أَبْرَمُوا أَمْرًا فَإِنَّا مُبْرِمُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4405,
      "verse_number": 80,
      "verse_key": "43:80",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "أَمْ يَحْسَبُونَ أَنَّا لَا نَسْمَعُ سِرَّهُمْ وَنَجْوَاهُم ۚ بَلَىٰ وَرُسُلُنَا لَدَيْهِمْ يَكْتُبُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4406,
      "verse_number": 81,
      "verse_key": "43:81",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِن كَانَ لِلرَّحْمَٰنِ وَلَدٌ فَأَنَا أَوَّلُ الْعَابِدِينَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4407,
      "verse_number": 82,
      "verse_key": "43:82",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "سُبْحَانَ رَبِّ السَّمَاوَاتِ وَالْأَرْضِ رَبِّ الْعَرْشِ عَمَّا يَصِفُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4408,
      "verse_number": 83,
      "verse_key": "43:83",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَذَرْهُمْ يَخُوضُوا وَيَلْعَبُوا حَتَّىٰ يُلَاقُوا يَوْمَهُمُ الَّذِي يُوعَدُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4409,
      "verse_number": 84,
      "verse_key": "43:84",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي فِي السَّمَاءِ إِلَٰهٌ وَفِي الْأَرْضِ إِلَٰهٌ ۚ وَهُوَ الْحَكِيمُ الْعَلِيمُ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4410,
      "verse_number": 85,
      "verse_key": "43:85",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَتَبَارَكَ الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا وَعِندَهُ عِلْمُ السَّاعَةِ وَإِلَيْهِ تُرْجَعُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4411,
      "verse_number": 86,
      "verse_key": "43:86",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَمْلِكُ الَّذِينَ يَدْعُونَ مِن دُونِهِ الشَّفَاعَةَ إِلَّا مَن شَهِدَ بِالْحَقِّ وَهُمْ يَعْلَمُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4412,
      "verse_number": 87,
      "verse_key": "43:87",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَلَئِن سَأَلْتَهُم مَّنْ خَلَقَهُمْ لَيَقُولُنَّ اللَّهُ ۖ فَأَنَّىٰ يُؤْفَكُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4413,
      "verse_number": 88,
      "verse_key": "43:88",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "وَقِيلِهِ يَا رَبِّ إِنَّ هَٰؤُلَاءِ قَوْمٌ لَّا يُؤْمِنُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4414,
      "verse_number": 89,
      "verse_key": "43:89",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_imlaei": "فَاصْفَحْ عَنْهُمْ وَقُلْ سَلَامٌ ۚ فَسَوْفَ يَعْلَمُونَ",
      "page_number": 495,
      "juz_number": 25
    }
  ]
}