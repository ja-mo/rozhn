{
  "id": 72,
  "name": "The Jinn",
  "verses": [
    {
      "id": 5448,
      "verse_number": 1,
      "verse_key": "72:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ أُوحِيَ إِلَيَّ أَنَّهُ اسْتَمَعَ نَفَرٌ مِّنَ الْجِنِّ فَقَالُوا إِنَّا سَمِعْنَا قُرْآنًا عَجَبًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5449,
      "verse_number": 2,
      "verse_key": "72:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَهْدِي إِلَى الرُّشْدِ فَآمَنَّا بِهِ ۖ وَلَن نُّشْرِكَ بِرَبِّنَا أَحَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5450,
      "verse_number": 3,
      "verse_key": "72:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّهُ تَعَالَىٰ جَدُّ رَبِّنَا مَا اتَّخَذَ صَاحِبَةً وَلَا وَلَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5451,
      "verse_number": 4,
      "verse_key": "72:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّهُ كَانَ يَقُولُ سَفِيهُنَا عَلَى اللَّهِ شَطَطًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5452,
      "verse_number": 5,
      "verse_key": "72:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا ظَنَنَّا أَن لَّن تَقُولَ الْإِنسُ وَالْجِنُّ عَلَى اللَّهِ كَذِبًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5453,
      "verse_number": 6,
      "verse_key": "72:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّهُ كَانَ رِجَالٌ مِّنَ الْإِنسِ يَعُوذُونَ بِرِجَالٍ مِّنَ الْجِنِّ فَزَادُوهُمْ رَهَقًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5454,
      "verse_number": 7,
      "verse_key": "72:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّهُمْ ظَنُّوا كَمَا ظَنَنتُمْ أَن لَّن يَبْعَثَ اللَّهُ أَحَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5455,
      "verse_number": 8,
      "verse_key": "72:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا لَمَسْنَا السَّمَاءَ فَوَجَدْنَاهَا مُلِئَتْ حَرَسًا شَدِيدًا وَشُهُبًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5456,
      "verse_number": 9,
      "verse_key": "72:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا كُنَّا نَقْعُدُ مِنْهَا مَقَاعِدَ لِلسَّمْعِ ۖ فَمَن يَسْتَمِعِ الْآنَ يَجِدْ لَهُ شِهَابًا رَّصَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5457,
      "verse_number": 10,
      "verse_key": "72:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا لَا نَدْرِي أَشَرٌّ أُرِيدَ بِمَن فِي الْأَرْضِ أَمْ أَرَادَ بِهِمْ رَبُّهُمْ رَشَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5458,
      "verse_number": 11,
      "verse_key": "72:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا مِنَّا الصَّالِحُونَ وَمِنَّا دُونَ ذَٰلِكَ ۖ كُنَّا طَرَائِقَ قِدَدًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5459,
      "verse_number": 12,
      "verse_key": "72:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا ظَنَنَّا أَن لَّن نُّعْجِزَ اللَّهَ فِي الْأَرْضِ وَلَن نُّعْجِزَهُ هَرَبًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5460,
      "verse_number": 13,
      "verse_key": "72:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا لَمَّا سَمِعْنَا الْهُدَىٰ آمَنَّا بِهِ ۖ فَمَن يُؤْمِن بِرَبِّهِ فَلَا يَخَافُ بَخْسًا وَلَا رَهَقًا",
      "page_number": 572,
      "juz_number": 29
    },
    {
      "id": 5461,
      "verse_number": 14,
      "verse_key": "72:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّا مِنَّا الْمُسْلِمُونَ وَمِنَّا الْقَاسِطُونَ ۖ فَمَنْ أَسْلَمَ فَأُولَٰئِكَ تَحَرَّوْا رَشَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5462,
      "verse_number": 15,
      "verse_key": "72:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا الْقَاسِطُونَ فَكَانُوا لِجَهَنَّمَ حَطَبًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5463,
      "verse_number": 16,
      "verse_key": "72:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَن لَّوِ اسْتَقَامُوا عَلَى الطَّرِيقَةِ لَأَسْقَيْنَاهُم مَّاءً غَدَقًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5464,
      "verse_number": 17,
      "verse_key": "72:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّنَفْتِنَهُمْ فِيهِ ۚ وَمَن يُعْرِضْ عَن ذِكْرِ رَبِّهِ يَسْلُكْهُ عَذَابًا صَعَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5465,
      "verse_number": 18,
      "verse_key": "72:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّ الْمَسَاجِدَ لِلَّهِ فَلَا تَدْعُوا مَعَ اللَّهِ أَحَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5466,
      "verse_number": 19,
      "verse_key": "72:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 508,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّهُ لَمَّا قَامَ عَبْدُ اللَّهِ يَدْعُوهُ كَادُوا يَكُونُونَ عَلَيْهِ لِبَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5467,
      "verse_number": 20,
      "verse_key": "72:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنَّمَا أَدْعُو رَبِّي وَلَا أُشْرِكُ بِهِ أَحَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5468,
      "verse_number": 21,
      "verse_key": "72:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنِّي لَا أَمْلِكُ لَكُمْ ضَرًّا وَلَا رَشَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5469,
      "verse_number": 22,
      "verse_key": "72:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنِّي لَن يُجِيرَنِي مِنَ اللَّهِ أَحَدٌ وَلَنْ أَجِدَ مِن دُونِهِ مُلْتَحَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5470,
      "verse_number": 23,
      "verse_key": "72:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا بَلَاغًا مِّنَ اللَّهِ وَرِسَالَاتِهِ ۚ وَمَن يَعْصِ اللَّهَ وَرَسُولَهُ فَإِنَّ لَهُ نَارَ جَهَنَّمَ خَالِدِينَ فِيهَا أَبَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5471,
      "verse_number": 24,
      "verse_key": "72:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "حَتَّىٰ إِذَا رَأَوْا مَا يُوعَدُونَ فَسَيَعْلَمُونَ مَنْ أَضْعَفُ نَاصِرًا وَأَقَلُّ عَدَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5472,
      "verse_number": 25,
      "verse_key": "72:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنْ أَدْرِي أَقَرِيبٌ مَّا تُوعَدُونَ أَمْ يَجْعَلُ لَهُ رَبِّي أَمَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5473,
      "verse_number": 26,
      "verse_key": "72:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَالِمُ الْغَيْبِ فَلَا يُظْهِرُ عَلَىٰ غَيْبِهِ أَحَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5474,
      "verse_number": 27,
      "verse_key": "72:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا مَنِ ارْتَضَىٰ مِن رَّسُولٍ فَإِنَّهُ يَسْلُكُ مِن بَيْنِ يَدَيْهِ وَمِنْ خَلْفِهِ رَصَدًا",
      "page_number": 573,
      "juz_number": 29
    },
    {
      "id": 5475,
      "verse_number": 28,
      "verse_key": "72:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 229,
      "ruku_number": 509,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِّيَعْلَمَ أَن قَدْ أَبْلَغُوا رِسَالَاتِ رَبِّهِمْ وَأَحَاطَ بِمَا لَدَيْهِمْ وَأَحْصَىٰ كُلَّ شَيْءٍ عَدَدًا",
      "page_number": 573,
      "juz_number": 29
    }
  ]
}