{
  "id": 50,
  "name": "The Letter \"Qaf\"",
  "verses": [
    {
      "id": 4631,
      "verse_number": 1,
      "verse_key": "50:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ق ۚ وَالْقُرْآنِ الْمَجِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4632,
      "verse_number": 2,
      "verse_key": "50:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلْ عَجِبُوا أَن جَاءَهُم مُّنذِرٌ مِّنْهُمْ فَقَالَ الْكَافِرُونَ هَٰذَا شَيْءٌ عَجِيبٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4633,
      "verse_number": 3,
      "verse_key": "50:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَإِذَا مِتْنَا وَكُنَّا تُرَابًا ۖ ذَٰلِكَ رَجْعٌ بَعِيدٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4634,
      "verse_number": 4,
      "verse_key": "50:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَدْ عَلِمْنَا مَا تَنقُصُ الْأَرْضُ مِنْهُمْ ۖ وَعِندَنَا كِتَابٌ حَفِيظٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4635,
      "verse_number": 5,
      "verse_key": "50:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلْ كَذَّبُوا بِالْحَقِّ لَمَّا جَاءَهُمْ فَهُمْ فِي أَمْرٍ مَّرِيجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4636,
      "verse_number": 6,
      "verse_key": "50:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَلَمْ يَنظُرُوا إِلَى السَّمَاءِ فَوْقَهُمْ كَيْفَ بَنَيْنَاهَا وَزَيَّنَّاهَا وَمَا لَهَا مِن فُرُوجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4637,
      "verse_number": 7,
      "verse_key": "50:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضَ مَدَدْنَاهَا وَأَلْقَيْنَا فِيهَا رَوَاسِيَ وَأَنبَتْنَا فِيهَا مِن كُلِّ زَوْجٍ بَهِيجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4638,
      "verse_number": 8,
      "verse_key": "50:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَبْصِرَةً وَذِكْرَىٰ لِكُلِّ عَبْدٍ مُّنِيبٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4639,
      "verse_number": 9,
      "verse_key": "50:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَنَزَّلْنَا مِنَ السَّمَاءِ مَاءً مُّبَارَكًا فَأَنبَتْنَا بِهِ جَنَّاتٍ وَحَبَّ الْحَصِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4640,
      "verse_number": 10,
      "verse_key": "50:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالنَّخْلَ بَاسِقَاتٍ لَّهَا طَلْعٌ نَّضِيدٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4641,
      "verse_number": 11,
      "verse_key": "50:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "رِّزْقًا لِّلْعِبَادِ ۖ وَأَحْيَيْنَا بِهِ بَلْدَةً مَّيْتًا ۚ كَذَٰلِكَ الْخُرُوجُ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4642,
      "verse_number": 12,
      "verse_key": "50:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَأَصْحَابُ الرَّسِّ وَثَمُودُ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4643,
      "verse_number": 13,
      "verse_key": "50:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَعَادٌ وَفِرْعَوْنُ وَإِخْوَانُ لُوطٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4644,
      "verse_number": 14,
      "verse_key": "50:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَصْحَابُ الْأَيْكَةِ وَقَوْمُ تُبَّعٍ ۚ كُلٌّ كَذَّبَ الرُّسُلَ فَحَقَّ وَعِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4645,
      "verse_number": 15,
      "verse_key": "50:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَعَيِينَا بِالْخَلْقِ الْأَوَّلِ ۚ بَلْ هُمْ فِي لَبْسٍ مِّنْ خَلْقٍ جَدِيدٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4646,
      "verse_number": 16,
      "verse_key": "50:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ خَلَقْنَا الْإِنسَانَ وَنَعْلَمُ مَا تُوَسْوِسُ بِهِ نَفْسُهُ ۖ وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْ حَبْلِ الْوَرِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4647,
      "verse_number": 17,
      "verse_key": "50:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذْ يَتَلَقَّى الْمُتَلَقِّيَانِ عَنِ الْيَمِينِ وَعَنِ الشِّمَالِ قَعِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4648,
      "verse_number": 18,
      "verse_key": "50:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّا يَلْفِظُ مِن قَوْلٍ إِلَّا لَدَيْهِ رَقِيبٌ عَتِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4649,
      "verse_number": 19,
      "verse_key": "50:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَاءَتْ سَكْرَةُ الْمَوْتِ بِالْحَقِّ ۖ ذَٰلِكَ مَا كُنتَ مِنْهُ تَحِيدُ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4650,
      "verse_number": 20,
      "verse_key": "50:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَنُفِخَ فِي الصُّورِ ۚ ذَٰلِكَ يَوْمُ الْوَعِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4651,
      "verse_number": 21,
      "verse_key": "50:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَجَاءَتْ كُلُّ نَفْسٍ مَّعَهَا سَائِقٌ وَشَهِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4652,
      "verse_number": 22,
      "verse_key": "50:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَّقَدْ كُنتَ فِي غَفْلَةٍ مِّنْ هَٰذَا فَكَشَفْنَا عَنكَ غِطَاءَكَ فَبَصَرُكَ الْيَوْمَ حَدِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4653,
      "verse_number": 23,
      "verse_key": "50:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ قَرِينُهُ هَٰذَا مَا لَدَيَّ عَتِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4654,
      "verse_number": 24,
      "verse_key": "50:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلْقِيَا فِي جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4655,
      "verse_number": 25,
      "verse_key": "50:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ مُّرِيبٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4656,
      "verse_number": 26,
      "verse_key": "50:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي جَعَلَ مَعَ اللَّهِ إِلَٰهًا آخَرَ فَأَلْقِيَاهُ فِي الْعَذَابِ الشَّدِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4657,
      "verse_number": 27,
      "verse_key": "50:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ قَرِينُهُ رَبَّنَا مَا أَطْغَيْتُهُ وَلَٰكِن كَانَ فِي ضَلَالٍ بَعِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4658,
      "verse_number": 28,
      "verse_key": "50:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَا تَخْتَصِمُوا لَدَيَّ وَقَدْ قَدَّمْتُ إِلَيْكُم بِالْوَعِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4659,
      "verse_number": 29,
      "verse_key": "50:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَا يُبَدَّلُ الْقَوْلُ لَدَيَّ وَمَا أَنَا بِظَلَّامٍ لِّلْعَبِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4660,
      "verse_number": 30,
      "verse_key": "50:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ نَقُولُ لِجَهَنَّمَ هَلِ امْتَلَأْتِ وَتَقُولُ هَلْ مِن مَّزِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4661,
      "verse_number": 31,
      "verse_key": "50:31",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأُزْلِفَتِ الْجَنَّةُ لِلْمُتَّقِينَ غَيْرَ بَعِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4662,
      "verse_number": 32,
      "verse_key": "50:32",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَٰذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4663,
      "verse_number": 33,
      "verse_key": "50:33",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "مَّنْ خَشِيَ الرَّحْمَٰنَ بِالْغَيْبِ وَجَاءَ بِقَلْبٍ مُّنِيبٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4664,
      "verse_number": 34,
      "verse_key": "50:34",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ادْخُلُوهَا بِسَلَامٍ ۖ ذَٰلِكَ يَوْمُ الْخُلُودِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4665,
      "verse_number": 35,
      "verse_key": "50:35",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَهُم مَّا يَشَاءُونَ فِيهَا وَلَدَيْنَا مَزِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4666,
      "verse_number": 36,
      "verse_key": "50:36",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هُمْ أَشَدُّ مِنْهُم بَطْشًا فَنَقَّبُوا فِي الْبِلَادِ هَلْ مِن مَّحِيصٍ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4667,
      "verse_number": 37,
      "verse_key": "50:37",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَذِكْرَىٰ لِمَن كَانَ لَهُ قَلْبٌ أَوْ أَلْقَى السَّمْعَ وَهُوَ شَهِيدٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4668,
      "verse_number": 38,
      "verse_key": "50:38",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ خَلَقْنَا السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ وَمَا مَسَّنَا مِن لُّغُوبٍ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4669,
      "verse_number": 39,
      "verse_key": "50:39",
