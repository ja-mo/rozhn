{
  "id": 83,
  "name": "The Defrauding",
  "verses": [
    {
      "id": 5849,
      "verse_number": 1,
      "verse_key": "83:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيْلٌ لِّلْمُطَفِّفِينَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5850,
      "verse_number": 2,
      "verse_key": "83:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ إِذَا اكْتَالُوا عَلَى النَّاسِ يَسْتَوْفُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5851,
      "verse_number": 3,
      "verse_key": "83:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5852,
      "verse_number": 4,
      "verse_key": "83:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَا يَظُنُّ أُولَٰئِكَ أَنَّهُم مَّبْعُوثُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5853,
      "verse_number": 5,
      "verse_key": "83:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لِيَوْمٍ عَظِيمٍ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5854,
      "verse_number": 6,
      "verse_key": "83:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَقُومُ النَّاسُ لِرَبِّ الْعَالَمِينَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5855,
      "verse_number": 7,
      "verse_key": "83:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِنَّ كِتَابَ الْفُجَّارِ لَفِي سِجِّينٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5856,
      "verse_number": 8,
      "verse_key": "83:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَدْرَاكَ مَا سِجِّينٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5857,
      "verse_number": 9,
      "verse_key": "83:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كِتَابٌ مَّرْقُومٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5858,
      "verse_number": 10,
      "verse_key": "83:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5859,
      "verse_number": 11,
      "verse_key": "83:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ يُكَذِّبُونَ بِيَوْمِ الدِّينِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5860,
      "verse_number": 12,
      "verse_key": "83:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يُكَذِّبُ بِهِ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5861,
      "verse_number": 13,
      "verse_key": "83:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا تُتْلَىٰ عَلَيْهِ آيَاتُنَا قَالَ أَسَاطِيرُ الْأَوَّلِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5862,
      "verse_number": 14,
      "verse_key": "83:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا يَكْسِبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5863,
      "verse_number": 15,
      "verse_key": "83:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍ لَّمَحْجُوبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5864,
      "verse_number": 16,
      "verse_key": "83:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ إِنَّهُمْ لَصَالُو الْجَحِيمِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5865,
      "verse_number": 17,
      "verse_key": "83:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ يُقَالُ هَٰذَا الَّذِي كُنتُم بِهِ تُكَذِّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5866,
      "verse_number": 18,
      "verse_key": "83:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كَلَّا إِنَّ كِتَابَ الْأَبْرَارِ لَفِي عِلِّيِّينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5867,
      "verse_number": 19,
      "verse_key": "83:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَدْرَاكَ مَا عِلِّيُّونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5868,
      "verse_number": 20,
      "verse_key": "83:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "كِتَابٌ مَّرْقُومٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5869,
      "verse_number": 21,
      "verse_key": "83:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَشْهَدُهُ الْمُقَرَّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5870,
      "verse_number": 22,
      "verse_key": "83:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْأَبْرَارَ لَفِي نَعِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5871,
      "verse_number": 23,
      "verse_key": "83:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلَى الْأَرَائِكِ يَنظُرُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5872,
      "verse_number": 24,
      "verse_key": "83:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَعْرِفُ فِي وُجُوهِهِمْ نَضْرَةَ النَّعِيمِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5873,
      "verse_number": 25,
      "verse_key": "83:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يُسْقَوْنَ مِن رَّحِيقٍ مَّخْتُومٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5874,
      "verse_number": 26,
      "verse_key": "83:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "خِتَامُهُ مِسْكٌ ۚ وَفِي ذَٰلِكَ فَلْيَتَنَافَسِ الْمُتَنَافِسُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5875,
      "verse_number": 27,
      "verse_key": "83:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمِزَاجُهُ مِن تَسْنِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5876,
      "verse_number": 28,
      "verse_key": "83:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَيْنًا يَشْرَبُ بِهَا الْمُقَرَّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5877,
      "verse_number": 29,
      "verse_key": "83:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ أَجْرَمُوا كَانُوا مِنَ الَّذِينَ آمَنُوا يَضْحَكُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5878,
      "verse_number": 30,
      "verse_key": "83:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا مَرُّوا بِهِمْ يَتَغَامَزُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5879,
      "verse_number": 31,
      "verse_key": "83:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا انقَلَبُوا إِلَىٰ أَهْلِهِمُ انقَلَبُوا فَكِهِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5880,
      "verse_number": 32,
      "verse_key": "83:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا رَأَوْهُمْ قَالُوا إِنَّ هَٰؤُلَاءِ لَضَالُّونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5881,
      "verse_number": 33,
      "verse_key": "83:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أُرْسِلُوا عَلَيْهِمْ حَافِظِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5882,
      "verse_number": 34,
      "verse_key": "83:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَالْيَوْمَ الَّذِينَ آمَنُوا مِنَ الْكُفَّارِ يَضْحَكُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5883,
      "verse_number": 35,
      "verse_key": "83:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "عَلَى الْأَرَائِكِ يَنظُرُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5884,
      "verse_number": 36,
      "verse_key": "83:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَلْ ثُوِّبَ الْكُفَّارُ مَا كَانُوا يَفْعَلُونَ",
      "page_number": 589,
      "juz_number": 30
    }
  ]
}