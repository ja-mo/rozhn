      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءِ وَالطَّارِقِ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5933,
      "verse_number": 2,
      "verse_key": "86:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَدْرَاكَ مَا الطَّارِقُ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5934,
      "verse_number": 3,
      "verse_key": "86:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "النَّجْمُ الثَّاقِبُ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5935,
      "verse_number": 4,
      "verse_key": "86:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5936,
      "verse_number": 5,
      "verse_key": "86:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلْيَنظُرِ الْإِنسَانُ مِمَّ خُلِقَ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5937,
      "verse_number": 6,
      "verse_key": "86:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "خُلِقَ مِن مَّاءٍ دَافِقٍ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5938,
      "verse_number": 7,
      "verse_key": "86:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَخْرُجُ مِن بَيْنِ الصُّلْبِ وَالتَّرَائِبِ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5939,
      "verse_number": 8,
      "verse_key": "86:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ عَلَىٰ رَجْعِهِ لَقَادِرٌ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5940,
      "verse_number": 9,
      "verse_key": "86:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ تُبْلَى السَّرَائِرُ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5941,
      "verse_number": 10,
      "verse_key": "86:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَا لَهُ مِن قُوَّةٍ وَلَا نَاصِرٍ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5942,
      "verse_number": 11,
      "verse_key": "86:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءِ ذَاتِ الرَّجْعِ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5943,
      "verse_number": 12,
      "verse_key": "86:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضِ ذَاتِ الصَّدْعِ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5944,
      "verse_number": 13,
      "verse_key": "86:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ لَقَوْلٌ فَصْلٌ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5945,
      "verse_number": 14,
      "verse_key": "86:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا هُوَ بِالْهَزْلِ",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5946,
      "verse_number": 15,
      "verse_key": "86:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُمْ يَكِيدُونَ كَيْدًا",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5947,
      "verse_number": 16,
      "verse_key": "86:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَكِيدُ كَيْدًا",
      "page_number": 591,
      "juz_number": 30
    },
    {
      "id": 5948,
      "verse_number": 17,
      "verse_key": "86:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 530,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَمَهِّلِ الْكَافِرِينَ أَمْهِلْهُمْ رُوَيْدًا",
      "page_number": 591,
