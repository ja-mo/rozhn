{
  "id": 92,
  "name": "The Night",
  "verses": [
    {
      "id": 6059,
      "verse_number": 1,
      "verse_key": "92:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّيْلِ إِذَا يَغْشَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6060,
      "verse_number": 2,
      "verse_key": "92:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالنَّهَارِ إِذَا تَجَلَّىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6061,
      "verse_number": 3,
      "verse_key": "92:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا خَلَقَ الذَّكَرَ وَالْأُنثَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6062,
      "verse_number": 4,
      "verse_key": "92:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ سَعْيَكُمْ لَشَتَّىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6063,
      "verse_number": 5,
      "verse_key": "92:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَمَّا مَنْ أَعْطَىٰ وَاتَّقَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6064,
      "verse_number": 6,
      "verse_key": "92:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَصَدَّقَ بِالْحُسْنَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6065,
      "verse_number": 7,
      "verse_key": "92:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَسَنُيَسِّرُهُ لِلْيُسْرَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6066,
      "verse_number": 8,
      "verse_key": "92:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَمَّا مَن بَخِلَ وَاسْتَغْنَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6067,
      "verse_number": 9,
      "verse_key": "92:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَكَذَّبَ بِالْحُسْنَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6068,
      "verse_number": 10,
      "verse_key": "92:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَسَنُيَسِّرُهُ لِلْعُسْرَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6069,
      "verse_number": 11,
      "verse_key": "92:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يُغْنِي عَنْهُ مَالُهُ إِذَا تَرَدَّىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6070,
      "verse_number": 12,
      "verse_key": "92:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ عَلَيْنَا لَلْهُدَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6071,
      "verse_number": 13,
      "verse_key": "92:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ لَنَا لَلْآخِرَةَ وَالْأُولَىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6072,
      "verse_number": 14,
      "verse_key": "92:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ",
      "page_number": 595,
      "juz_number": 30
    },
    {
      "id": 6073,
      "verse_number": 15,
      "verse_key": "92:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "لَا يَصْلَاهَا إِلَّا الْأَشْقَى",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6074,
      "verse_number": 16,
      "verse_key": "92:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6075,
      "verse_number": 17,
      "verse_key": "92:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَسَيُجَنَّبُهَا الْأَتْقَى",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6076,
      "verse_number": 18,
      "verse_key": "92:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي يُؤْتِي مَالَهُ يَتَزَكَّىٰ",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6077,
      "verse_number": 19,
      "verse_key": "92:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا لِأَحَدٍ عِندَهُ مِن نِّعْمَةٍ تُجْزَىٰ",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6078,
      "verse_number": 20,
      "verse_key": "92:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا ابْتِغَاءَ وَجْهِ رَبِّهِ الْأَعْلَىٰ",
      "page_number": 596,
      "juz_number": 30
    },
    {
      "id": 6079,
      "verse_number": 21,
      "verse_key": "92:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 536,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَسَوْفَ يَرْضَىٰ",
      "page_number": 596,
      "juz_number": 30
    }
  ]
}