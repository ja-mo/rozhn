{
  "id": 15,
  "name": "The Rocky Tract",
  "verses": [
    {
      "id": 1803,
      "verse_number": 1,
      "verse_key": "15:1",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "الر ۚ تِلْكَ آيَاتُ الْكِتَابِ وَقُرْآنٍ مُّبِينٍ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1804,
      "verse_number": 2,
      "verse_key": "15:2",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "رُّبَمَا يَوَدُّ الَّذِينَ كَفَرُوا لَوْ كَانُوا مُسْلِمِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1805,
      "verse_number": 3,
      "verse_key": "15:3",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "ذَرْهُمْ يَأْكُلُوا وَيَتَمَتَّعُوا وَيُلْهِهِمُ الْأَمَلُ ۖ فَسَوْفَ يَعْلَمُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1806,
      "verse_number": 4,
      "verse_key": "15:4",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَهْلَكْنَا مِن قَرْيَةٍ إِلَّا وَلَهَا كِتَابٌ مَّعْلُومٌ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1807,
      "verse_number": 5,
      "verse_key": "15:5",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "مَّا تَسْبِقُ مِنْ أُمَّةٍ أَجَلَهَا وَمَا يَسْتَأْخِرُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1808,
      "verse_number": 6,
      "verse_key": "15:6",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا يَا أَيُّهَا الَّذِي نُزِّلَ عَلَيْهِ الذِّكْرُ إِنَّكَ لَمَجْنُونٌ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1809,
      "verse_number": 7,
      "verse_key": "15:7",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَّوْ مَا تَأْتِينَا بِالْمَلَائِكَةِ إِن كُنتَ مِنَ الصَّادِقِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1810,
      "verse_number": 8,
      "verse_key": "15:8",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "مَا نُنَزِّلُ الْمَلَائِكَةَ إِلَّا بِالْحَقِّ وَمَا كَانُوا إِذًا مُّنظَرِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1811,
      "verse_number": 9,
      "verse_key": "15:9",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا نَحْنُ نَزَّلْنَا الذِّكْرَ وَإِنَّا لَهُ لَحَافِظُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1812,
      "verse_number": 10,
      "verse_key": "15:10",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَرْسَلْنَا مِن قَبْلِكَ فِي شِيَعِ الْأَوَّلِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1813,
      "verse_number": 11,
      "verse_key": "15:11",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يَأْتِيهِم مِّن رَّسُولٍ إِلَّا كَانُوا بِهِ يَسْتَهْزِئُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1814,
      "verse_number": 12,
      "verse_key": "15:12",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ نَسْلُكُهُ فِي قُلُوبِ الْمُجْرِمِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1815,
      "verse_number": 13,
      "verse_key": "15:13",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَا يُؤْمِنُونَ بِهِ ۖ وَقَدْ خَلَتْ سُنَّةُ الْأَوَّلِينَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1816,
      "verse_number": 14,
      "verse_key": "15:14",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ فَتَحْنَا عَلَيْهِم بَابًا مِّنَ السَّمَاءِ فَظَلُّوا فِيهِ يَعْرُجُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1817,
      "verse_number": 15,
      "verse_key": "15:15",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 218,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَقَالُوا إِنَّمَا سُكِّرَتْ أَبْصَارُنَا بَلْ نَحْنُ قَوْمٌ مَّسْحُورُونَ",
      "page_number": 262,
      "juz_number": 14
    },
    {
      "id": 1818,
      "verse_number": 16,
      "verse_key": "15:16",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ جَعَلْنَا فِي السَّمَاءِ بُرُوجًا وَزَيَّنَّاهَا لِلنَّاظِرِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1819,
      "verse_number": 17,
      "verse_key": "15:17",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَحَفِظْنَاهَا مِن كُلِّ شَيْطَانٍ رَّجِيمٍ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1820,
      "verse_number": 18,
      "verse_key": "15:18",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا مَنِ اسْتَرَقَ السَّمْعَ فَأَتْبَعَهُ شِهَابٌ مُّبِينٌ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1821,
      "verse_number": 19,
      "verse_key": "15:19",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَالْأَرْضَ مَدَدْنَاهَا وَأَلْقَيْنَا فِيهَا رَوَاسِيَ وَأَنبَتْنَا فِيهَا مِن كُلِّ شَيْءٍ مَّوْزُونٍ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1822,
      "verse_number": 20,
      "verse_key": "15:20",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَجَعَلْنَا لَكُمْ فِيهَا مَعَايِشَ وَمَن لَّسْتُمْ لَهُ بِرَازِقِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1823,
      "verse_number": 21,
      "verse_key": "15:21",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِن مِّن شَيْءٍ إِلَّا عِندَنَا خَزَائِنُهُ وَمَا نُنَزِّلُهُ إِلَّا بِقَدَرٍ مَّعْلُومٍ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1824,
      "verse_number": 22,
      "verse_key": "15:22",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَأَرْسَلْنَا الرِّيَاحَ لَوَاقِحَ فَأَنزَلْنَا مِنَ السَّمَاءِ مَاءً فَأَسْقَيْنَاكُمُوهُ وَمَا أَنتُمْ لَهُ بِخَازِنِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1825,
      "verse_number": 23,
      "verse_key": "15:23",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّا لَنَحْنُ نُحْيِي وَنُمِيتُ وَنَحْنُ الْوَارِثُونَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1826,
      "verse_number": 24,
      "verse_key": "15:24",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ عَلِمْنَا الْمُسْتَقْدِمِينَ مِنكُمْ وَلَقَدْ عَلِمْنَا الْمُسْتَأْخِرِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1827,
      "verse_number": 25,
      "verse_key": "15:25",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 219,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ رَبَّكَ هُوَ يَحْشُرُهُمْ ۚ إِنَّهُ حَكِيمٌ عَلِيمٌ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1828,
      "verse_number": 26,
      "verse_key": "15:26",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ خَلَقْنَا الْإِنسَانَ مِن صَلْصَالٍ مِّنْ حَمَإٍ مَّسْنُونٍ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1829,
      "verse_number": 27,
      "verse_key": "15:27",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَالْجَانَّ خَلَقْنَاهُ مِن قَبْلُ مِن نَّارِ السَّمُومِ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1830,
      "verse_number": 28,
      "verse_key": "15:28",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِذْ قَالَ رَبُّكَ لِلْمَلَائِكَةِ إِنِّي خَالِقٌ بَشَرًا مِّن صَلْصَالٍ مِّنْ حَمَإٍ مَّسْنُونٍ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1831,
      "verse_number": 29,
      "verse_key": "15:29",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَإِذَا سَوَّيْتُهُ وَنَفَخْتُ فِيهِ مِن رُّوحِي فَقَعُوا لَهُ سَاجِدِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1832,
      "verse_number": 30,
      "verse_key": "15:30",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَسَجَدَ الْمَلَائِكَةُ كُلُّهُمْ أَجْمَعُونَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1833,
      "verse_number": 31,
      "verse_key": "15:31",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا إِبْلِيسَ أَبَىٰ أَن يَكُونَ مَعَ السَّاجِدِينَ",
      "page_number": 263,
      "juz_number": 14
    },
    {
      "id": 1834,
      "verse_number": 32,
      "verse_key": "15:32",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ يَا إِبْلِيسُ مَا لَكَ أَلَّا تَكُونَ مَعَ السَّاجِدِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1835,
      "verse_number": 33,
      "verse_key": "15:33",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَمْ أَكُن لِّأَسْجُدَ لِبَشَرٍ خَلَقْتَهُ مِن صَلْصَالٍ مِّنْ حَمَإٍ مَّسْنُونٍ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1836,
      "verse_number": 34,
      "verse_key": "15:34",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَاخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1837,
      "verse_number": 35,
      "verse_key": "15:35",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ عَلَيْكَ اللَّعْنَةَ إِلَىٰ يَوْمِ الدِّينِ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1838,
      "verse_number": 36,
      "verse_key": "15:36",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ فَأَنظِرْنِي إِلَىٰ يَوْمِ يُبْعَثُونَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1839,
      "verse_number": 37,
      "verse_key": "15:37",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَإِنَّكَ مِنَ الْمُنظَرِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1840,
      "verse_number": 38,
      "verse_key": "15:38",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَىٰ يَوْمِ الْوَقْتِ الْمَعْلُومِ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1841,
      "verse_number": 39,
      "verse_key": "15:39",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ بِمَا أَغْوَيْتَنِي لَأُزَيِّنَنَّ لَهُمْ فِي الْأَرْضِ وَلَأُغْوِيَنَّهُمْ أَجْمَعِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1842,
      "verse_number": 40,
      "verse_key": "15:40",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا عِبَادَكَ مِنْهُمُ الْمُخْلَصِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1843,
      "verse_number": 41,
      "verse_key": "15:41",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ هَٰذَا صِرَاطٌ عَلَيَّ مُسْتَقِيمٌ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1844,
      "verse_number": 42,
      "verse_key": "15:42",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ عِبَادِي لَيْسَ لَكَ عَلَيْهِمْ سُلْطَانٌ إِلَّا مَنِ اتَّبَعَكَ مِنَ الْغَاوِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1845,
      "verse_number": 43,
      "verse_key": "15:43",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ جَهَنَّمَ لَمَوْعِدُهُمْ أَجْمَعِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1846,
      "verse_number": 44,
      "verse_key": "15:44",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 220,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَهَا سَبْعَةُ أَبْوَابٍ لِّكُلِّ بَابٍ مِّنْهُمْ جُزْءٌ مَّقْسُومٌ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1847,
      "verse_number": 45,
      "verse_key": "15:45",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الْمُتَّقِينَ فِي جَنَّاتٍ وَعُيُونٍ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1848,
      "verse_number": 46,
      "verse_key": "15:46",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "ادْخُلُوهَا بِسَلَامٍ آمِنِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1849,
      "verse_number": 47,
      "verse_key": "15:47",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَنَزَعْنَا مَا فِي صُدُورِهِم مِّنْ غِلٍّ إِخْوَانًا عَلَىٰ سُرُرٍ مُّتَقَابِلِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1850,
      "verse_number": 48,
      "verse_key": "15:48",
      "hizb_number": 27,
      "rub_el_hizb_number": 105,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَا يَمَسُّهُمْ فِيهَا نَصَبٌ وَمَا هُم مِّنْهَا بِمُخْرَجِينَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1851,
      "verse_number": 49,
      "verse_key": "15:49",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "نَبِّئْ عِبَادِي أَنِّي أَنَا الْغَفُورُ الرَّحِيمُ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1852,
      "verse_number": 50,
      "verse_key": "15:50",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَأَنَّ عَذَابِي هُوَ الْعَذَابُ الْأَلِيمُ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1853,
      "verse_number": 51,
      "verse_key": "15:51",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَنَبِّئْهُمْ عَن ضَيْفِ إِبْرَاهِيمَ",
      "page_number": 264,
      "juz_number": 14
    },
    {
      "id": 1854,
      "verse_number": 52,
      "verse_key": "15:52",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِذْ دَخَلُوا عَلَيْهِ فَقَالُوا سَلَامًا قَالَ إِنَّا مِنكُمْ وَجِلُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1855,
      "verse_number": 53,
      "verse_key": "15:53",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا لَا تَوْجَلْ إِنَّا نُبَشِّرُكَ بِغُلَامٍ عَلِيمٍ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1856,
      "verse_number": 54,
      "verse_key": "15:54",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَبَشَّرْتُمُونِي عَلَىٰ أَن مَّسَّنِيَ الْكِبَرُ فَبِمَ تُبَشِّرُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1857,
      "verse_number": 55,
      "verse_key": "15:55",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا بَشَّرْنَاكَ بِالْحَقِّ فَلَا تَكُن مِّنَ الْقَانِطِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1858,
      "verse_number": 56,
      "verse_key": "15:56",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ وَمَن يَقْنَطُ مِن رَّحْمَةِ رَبِّهِ إِلَّا الضَّالُّونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1859,
      "verse_number": 57,
      "verse_key": "15:57",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَمَا خَطْبُكُمْ أَيُّهَا الْمُرْسَلُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1860,
      "verse_number": 58,
      "verse_key": "15:58",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا إِنَّا أُرْسِلْنَا إِلَىٰ قَوْمٍ مُّجْرِمِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1861,
      "verse_number": 59,
      "verse_key": "15:59",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا آلَ لُوطٍ إِنَّا لَمُنَجُّوهُمْ أَجْمَعِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1862,
      "verse_number": 60,
      "verse_key": "15:60",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 221,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا امْرَأَتَهُ قَدَّرْنَا ۙ إِنَّهَا لَمِنَ الْغَابِرِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1863,
      "verse_number": 61,
      "verse_key": "15:61",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا جَاءَ آلَ لُوطٍ الْمُرْسَلُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1864,
      "verse_number": 62,
      "verse_key": "15:62",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ إِنَّكُمْ قَوْمٌ مُّنكَرُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1865,
      "verse_number": 63,
      "verse_key": "15:63",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا بَلْ جِئْنَاكَ بِمَا كَانُوا فِيهِ يَمْتَرُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1866,
      "verse_number": 64,
      "verse_key": "15:64",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَأَتَيْنَاكَ بِالْحَقِّ وَإِنَّا لَصَادِقُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1867,
      "verse_number": 65,
      "verse_key": "15:65",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَأَسْرِ بِأَهْلِكَ بِقِطْعٍ مِّنَ اللَّيْلِ وَاتَّبِعْ أَدْبَارَهُمْ وَلَا يَلْتَفِتْ مِنكُمْ أَحَدٌ وَامْضُوا حَيْثُ تُؤْمَرُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1868,
      "verse_number": 66,
      "verse_key": "15:66",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَقَضَيْنَا إِلَيْهِ ذَٰلِكَ الْأَمْرَ أَنَّ دَابِرَ هَٰؤُلَاءِ مَقْطُوعٌ مُّصْبِحِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1869,
      "verse_number": 67,
      "verse_key": "15:67",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَجَاءَ أَهْلُ الْمَدِينَةِ يَسْتَبْشِرُونَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1870,
      "verse_number": 68,
      "verse_key": "15:68",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ إِنَّ هَٰؤُلَاءِ ضَيْفِي فَلَا تَفْضَحُونِ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1871,
      "verse_number": 69,
      "verse_key": "15:69",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَاتَّقُوا اللَّهَ وَلَا تُخْزُونِ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1872,
      "verse_number": 70,
      "verse_key": "15:70",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا أَوَلَمْ نَنْهَكَ عَنِ الْعَالَمِينَ",
      "page_number": 265,
      "juz_number": 14
    },
    {
      "id": 1873,
      "verse_number": 71,
      "verse_key": "15:71",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "قَالَ هَٰؤُلَاءِ بَنَاتِي إِن كُنتُمْ فَاعِلِينَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1874,
      "verse_number": 72,
      "verse_key": "15:72",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "لَعَمْرُكَ إِنَّهُمْ لَفِي سَكْرَتِهِمْ يَعْمَهُونَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1875,
      "verse_number": 73,
      "verse_key": "15:73",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَأَخَذَتْهُمُ الصَّيْحَةُ مُشْرِقِينَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1876,
      "verse_number": 74,
      "verse_key": "15:74",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَجَعَلْنَا عَالِيَهَا سَافِلَهَا وَأَمْطَرْنَا عَلَيْهِمْ حِجَارَةً مِّن سِجِّيلٍ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1877,
      "verse_number": 75,
      "verse_key": "15:75",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَآيَاتٍ لِّلْمُتَوَسِّمِينَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1878,
      "verse_number": 76,
      "verse_key": "15:76",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهَا لَبِسَبِيلٍ مُّقِيمٍ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1879,
      "verse_number": 77,
      "verse_key": "15:77",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَآيَةً لِّلْمُؤْمِنِينَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1880,
      "verse_number": 78,
      "verse_key": "15:78",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَإِن كَانَ أَصْحَابُ الْأَيْكَةِ لَظَالِمِينَ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1881,
      "verse_number": 79,
      "verse_key": "15:79",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 222,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "فَانتَقَمْنَا مِنْهُمْ وَإِنَّهُمَا لَبِإِمَامٍ مُّبِينٍ",
      "page_number": 266,
      "juz_number": 14
    },
    {
      "id": 1882,
      "verse_number": 80,
      "verse_key": "15:80",
      "hizb_number": 27,
      "rub_el_hizb_number": 106,
      "ruku_number": 223,
      "manzil_number": 3,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ كَذَّبَ أَصْحَابُ الْحِجْرِ الْمُرْسَلِينَ",
