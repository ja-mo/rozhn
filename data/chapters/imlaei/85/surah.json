      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالسَّمَاءِ ذَاتِ الْبُرُوجِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5911,
      "verse_number": 2,
      "verse_key": "85:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَالْيَوْمِ الْمَوْعُودِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5912,
      "verse_number": 3,
      "verse_key": "85:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَشَاهِدٍ وَمَشْهُودٍ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5913,
      "verse_number": 4,
      "verse_key": "85:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُتِلَ أَصْحَابُ الْأُخْدُودِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5914,
      "verse_number": 5,
      "verse_key": "85:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "النَّارِ ذَاتِ الْوَقُودِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5915,
      "verse_number": 6,
      "verse_key": "85:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذْ هُمْ عَلَيْهَا قُعُودٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5916,
      "verse_number": 7,
      "verse_key": "85:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِالْمُؤْمِنِينَ شُهُودٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5917,
      "verse_number": 8,
      "verse_key": "85:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَمَا نَقَمُوا مِنْهُمْ إِلَّا أَن يُؤْمِنُوا بِاللَّهِ الْعَزِيزِ الْحَمِيدِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5918,
      "verse_number": 9,
      "verse_key": "85:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ ۚ وَاللَّهُ عَلَىٰ كُلِّ شَيْءٍ شَهِيدٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5919,
      "verse_number": 10,
      "verse_key": "85:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ فَتَنُوا الْمُؤْمِنِينَ وَالْمُؤْمِنَاتِ ثُمَّ لَمْ يَتُوبُوا فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ الْحَرِيقِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5920,
      "verse_number": 11,
      "verse_key": "85:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ آمَنُوا وَعَمِلُوا الصَّالِحَاتِ لَهُمْ جَنَّاتٌ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ ۚ ذَٰلِكَ الْفَوْزُ الْكَبِيرُ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5921,
      "verse_number": 12,
      "verse_key": "85:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5922,
      "verse_number": 13,
      "verse_key": "85:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهُ هُوَ يُبْدِئُ وَيُعِيدُ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5923,
      "verse_number": 14,
      "verse_key": "85:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الْغَفُورُ الْوَدُودُ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5924,
      "verse_number": 15,
      "verse_key": "85:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ذُو الْعَرْشِ الْمَجِيدُ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5925,
      "verse_number": 16,
      "verse_key": "85:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَعَّالٌ لِّمَا يُرِيدُ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5926,
      "verse_number": 17,
      "verse_key": "85:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هَلْ أَتَاكَ حَدِيثُ الْجُنُودِ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5927,
      "verse_number": 18,
      "verse_key": "85:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِرْعَوْنَ وَثَمُودَ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5928,
      "verse_number": 19,
      "verse_key": "85:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلِ الَّذِينَ كَفَرُوا فِي تَكْذِيبٍ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5929,
      "verse_number": 20,
      "verse_key": "85:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَاللَّهُ مِن وَرَائِهِم مُّحِيطٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5930,
      "verse_number": 21,
      "verse_key": "85:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "بَلْ هُوَ قُرْآنٌ مَّجِيدٌ",
      "page_number": 590,
      "juz_number": 30
    },
    {
      "id": 5931,
      "verse_number": 22,
      "verse_key": "85:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 236,
      "ruku_number": 529,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فِي لَوْحٍ مَّحْفُوظٍ",
      "page_number": 590,
      "juz_number": 30
    }
  ]
