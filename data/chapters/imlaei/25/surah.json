{
  "id": 25,
  "name": "The Criterion",
  "verses": [
    {
      "id": 2856,
      "verse_number": 1,
      "verse_key": "25:1",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تَبَارَكَ الَّذِي نَزَّلَ الْفُرْقَانَ عَلَىٰ عَبْدِهِ لِيَكُونَ لِلْعَالَمِينَ نَذِيرًا",
      "page_number": 359,
      "juz_number": 18
    },
    {
      "id": 2857,
      "verse_number": 2,
      "verse_key": "25:2",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي لَهُ مُلْكُ السَّمَاوَاتِ وَالْأَرْضِ وَلَمْ يَتَّخِذْ وَلَدًا وَلَمْ يَكُن لَّهُ شَرِيكٌ فِي الْمُلْكِ وَخَلَقَ كُلَّ شَيْءٍ فَقَدَّرَهُ تَقْدِيرًا",
      "page_number": 359,
      "juz_number": 18
    },
    {
      "id": 2858,
      "verse_number": 3,
      "verse_key": "25:3",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَاتَّخَذُوا مِن دُونِهِ آلِهَةً لَّا يَخْلُقُونَ شَيْئًا وَهُمْ يُخْلَقُونَ وَلَا يَمْلِكُونَ لِأَنفُسِهِمْ ضَرًّا وَلَا نَفْعًا وَلَا يَمْلِكُونَ مَوْتًا وَلَا حَيَاةً وَلَا نُشُورًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2859,
      "verse_number": 4,
      "verse_key": "25:4",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ الَّذِينَ كَفَرُوا إِنْ هَٰذَا إِلَّا إِفْكٌ افْتَرَاهُ وَأَعَانَهُ عَلَيْهِ قَوْمٌ آخَرُونَ ۖ فَقَدْ جَاءُوا ظُلْمًا وَزُورًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2860,
      "verse_number": 5,
      "verse_key": "25:5",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا أَسَاطِيرُ الْأَوَّلِينَ اكْتَتَبَهَا فَهِيَ تُمْلَىٰ عَلَيْهِ بُكْرَةً وَأَصِيلًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2861,
      "verse_number": 6,
      "verse_key": "25:6",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ أَنزَلَهُ الَّذِي يَعْلَمُ السِّرَّ فِي السَّمَاوَاتِ وَالْأَرْضِ ۚ إِنَّهُ كَانَ غَفُورًا رَّحِيمًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2862,
      "verse_number": 7,
      "verse_key": "25:7",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا مَالِ هَٰذَا الرَّسُولِ يَأْكُلُ الطَّعَامَ وَيَمْشِي فِي الْأَسْوَاقِ ۙ لَوْلَا أُنزِلَ إِلَيْهِ مَلَكٌ فَيَكُونَ مَعَهُ نَذِيرًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2863,
      "verse_number": 8,
      "verse_key": "25:8",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَوْ يُلْقَىٰ إِلَيْهِ كَنزٌ أَوْ تَكُونُ لَهُ جَنَّةٌ يَأْكُلُ مِنْهَا ۚ وَقَالَ الظَّالِمُونَ إِن تَتَّبِعُونَ إِلَّا رَجُلًا مَّسْحُورًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2864,
      "verse_number": 9,
      "verse_key": "25:9",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 310,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "انظُرْ كَيْفَ ضَرَبُوا لَكَ الْأَمْثَالَ فَضَلُّوا فَلَا يَسْتَطِيعُونَ سَبِيلًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2865,
      "verse_number": 10,
      "verse_key": "25:10",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تَبَارَكَ الَّذِي إِن شَاءَ جَعَلَ لَكَ خَيْرًا مِّن ذَٰلِكَ جَنَّاتٍ تَجْرِي مِن تَحْتِهَا الْأَنْهَارُ وَيَجْعَل لَّكَ قُصُورًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2866,
      "verse_number": 11,
      "verse_key": "25:11",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "بَلْ كَذَّبُوا بِالسَّاعَةِ ۖ وَأَعْتَدْنَا لِمَن كَذَّبَ بِالسَّاعَةِ سَعِيرًا",
      "page_number": 360,
      "juz_number": 18
    },
    {
      "id": 2867,
      "verse_number": 12,
      "verse_key": "25:12",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِذَا رَأَتْهُم مِّن مَّكَانٍ بَعِيدٍ سَمِعُوا لَهَا تَغَيُّظًا وَزَفِيرًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2868,
      "verse_number": 13,
      "verse_key": "25:13",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا أُلْقُوا مِنْهَا مَكَانًا ضَيِّقًا مُّقَرَّنِينَ دَعَوْا هُنَالِكَ ثُبُورًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2869,
      "verse_number": 14,
      "verse_key": "25:14",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّا تَدْعُوا الْيَوْمَ ثُبُورًا وَاحِدًا وَادْعُوا ثُبُورًا كَثِيرًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2870,
      "verse_number": 15,
      "verse_key": "25:15",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ أَذَٰلِكَ خَيْرٌ أَمْ جَنَّةُ الْخُلْدِ الَّتِي وُعِدَ الْمُتَّقُونَ ۚ كَانَتْ لَهُمْ جَزَاءً وَمَصِيرًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2871,
      "verse_number": 16,
      "verse_key": "25:16",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّهُمْ فِيهَا مَا يَشَاءُونَ خَالِدِينَ ۚ كَانَ عَلَىٰ رَبِّكَ وَعْدًا مَّسْئُولًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2872,
      "verse_number": 17,
      "verse_key": "25:17",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَوْمَ يَحْشُرُهُمْ وَمَا يَعْبُدُونَ مِن دُونِ اللَّهِ فَيَقُولُ أَأَنتُمْ أَضْلَلْتُمْ عِبَادِي هَٰؤُلَاءِ أَمْ هُمْ ضَلُّوا السَّبِيلَ",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2873,
      "verse_number": 18,
      "verse_key": "25:18",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا سُبْحَانَكَ مَا كَانَ يَنبَغِي لَنَا أَن نَّتَّخِذَ مِن دُونِكَ مِنْ أَوْلِيَاءَ وَلَٰكِن مَّتَّعْتَهُمْ وَآبَاءَهُمْ حَتَّىٰ نَسُوا الذِّكْرَ وَكَانُوا قَوْمًا بُورًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2874,
      "verse_number": 19,
      "verse_key": "25:19",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَقَدْ كَذَّبُوكُم بِمَا تَقُولُونَ فَمَا تَسْتَطِيعُونَ صَرْفًا وَلَا نَصْرًا ۚ وَمَن يَظْلِم مِّنكُمْ نُذِقْهُ عَذَابًا كَبِيرًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2875,
      "verse_number": 20,
      "verse_key": "25:20",
      "hizb_number": 36,
      "rub_el_hizb_number": 144,
      "ruku_number": 311,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَرْسَلْنَا قَبْلَكَ مِنَ الْمُرْسَلِينَ إِلَّا إِنَّهُمْ لَيَأْكُلُونَ الطَّعَامَ وَيَمْشُونَ فِي الْأَسْوَاقِ ۗ وَجَعَلْنَا بَعْضَكُمْ لِبَعْضٍ فِتْنَةً أَتَصْبِرُونَ ۗ وَكَانَ رَبُّكَ بَصِيرًا",
      "page_number": 361,
      "juz_number": 18
    },
    {
      "id": 2876,
      "verse_number": 21,
      "verse_key": "25:21",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ الَّذِينَ لَا يَرْجُونَ لِقَاءَنَا لَوْلَا أُنزِلَ عَلَيْنَا الْمَلَائِكَةُ أَوْ نَرَىٰ رَبَّنَا ۗ لَقَدِ اسْتَكْبَرُوا فِي أَنفُسِهِمْ وَعَتَوْا عُتُوًّا كَبِيرًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2877,
      "verse_number": 22,
      "verse_key": "25:22",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَوْمَ يَرَوْنَ الْمَلَائِكَةَ لَا بُشْرَىٰ يَوْمَئِذٍ لِّلْمُجْرِمِينَ وَيَقُولُونَ حِجْرًا مَّحْجُورًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2878,
      "verse_number": 23,
      "verse_key": "25:23",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَدِمْنَا إِلَىٰ مَا عَمِلُوا مِنْ عَمَلٍ فَجَعَلْنَاهُ هَبَاءً مَّنثُورًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2879,
      "verse_number": 24,
      "verse_key": "25:24",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَصْحَابُ الْجَنَّةِ يَوْمَئِذٍ خَيْرٌ مُّسْتَقَرًّا وَأَحْسَنُ مَقِيلًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2880,
      "verse_number": 25,
      "verse_key": "25:25",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَوْمَ تَشَقَّقُ السَّمَاءُ بِالْغَمَامِ وَنُزِّلَ الْمَلَائِكَةُ تَنزِيلًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2881,
      "verse_number": 26,
      "verse_key": "25:26",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الْمُلْكُ يَوْمَئِذٍ الْحَقُّ لِلرَّحْمَٰنِ ۚ وَكَانَ يَوْمًا عَلَى الْكَافِرِينَ عَسِيرًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2882,
      "verse_number": 27,
      "verse_key": "25:27",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَوْمَ يَعَضُّ الظَّالِمُ عَلَىٰ يَدَيْهِ يَقُولُ يَا لَيْتَنِي اتَّخَذْتُ مَعَ الرَّسُولِ سَبِيلًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2883,
      "verse_number": 28,
      "verse_key": "25:28",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يَا وَيْلَتَىٰ لَيْتَنِي لَمْ أَتَّخِذْ فُلَانًا خَلِيلًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2884,
      "verse_number": 29,
      "verse_key": "25:29",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لَّقَدْ أَضَلَّنِي عَنِ الذِّكْرِ بَعْدَ إِذْ جَاءَنِي ۗ وَكَانَ الشَّيْطَانُ لِلْإِنسَانِ خَذُولًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2885,
      "verse_number": 30,
      "verse_key": "25:30",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ الرَّسُولُ يَا رَبِّ إِنَّ قَوْمِي اتَّخَذُوا هَٰذَا الْقُرْآنَ مَهْجُورًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2886,
      "verse_number": 31,
      "verse_key": "25:31",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكَذَٰلِكَ جَعَلْنَا لِكُلِّ نَبِيٍّ عَدُوًّا مِّنَ الْمُجْرِمِينَ ۗ وَكَفَىٰ بِرَبِّكَ هَادِيًا وَنَصِيرًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2887,
      "verse_number": 32,
      "verse_key": "25:32",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَالَ الَّذِينَ كَفَرُوا لَوْلَا نُزِّلَ عَلَيْهِ الْقُرْآنُ جُمْلَةً وَاحِدَةً ۚ كَذَٰلِكَ لِنُثَبِّتَ بِهِ فُؤَادَكَ ۖ وَرَتَّلْنَاهُ تَرْتِيلًا",
      "page_number": 362,
      "juz_number": 19
    },
    {
      "id": 2888,
      "verse_number": 33,
      "verse_key": "25:33",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَا يَأْتُونَكَ بِمَثَلٍ إِلَّا جِئْنَاكَ بِالْحَقِّ وَأَحْسَنَ تَفْسِيرًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2889,
      "verse_number": 34,
      "verse_key": "25:34",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 312,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الَّذِينَ يُحْشَرُونَ عَلَىٰ وُجُوهِهِمْ إِلَىٰ جَهَنَّمَ أُولَٰئِكَ شَرٌّ مَّكَانًا وَأَضَلُّ سَبِيلًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2890,
      "verse_number": 35,
      "verse_key": "25:35",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ آتَيْنَا مُوسَى الْكِتَابَ وَجَعَلْنَا مَعَهُ أَخَاهُ هَارُونَ وَزِيرًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2891,
      "verse_number": 36,
      "verse_key": "25:36",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَقُلْنَا اذْهَبَا إِلَى الْقَوْمِ الَّذِينَ كَذَّبُوا بِآيَاتِنَا فَدَمَّرْنَاهُمْ تَدْمِيرًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2892,
      "verse_number": 37,
      "verse_key": "25:37",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَقَوْمَ نُوحٍ لَّمَّا كَذَّبُوا الرُّسُلَ أَغْرَقْنَاهُمْ وَجَعَلْنَاهُمْ لِلنَّاسِ آيَةً ۖ وَأَعْتَدْنَا لِلظَّالِمِينَ عَذَابًا أَلِيمًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2893,
      "verse_number": 38,
      "verse_key": "25:38",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَعَادًا وَثَمُودَ وَأَصْحَابَ الرَّسِّ وَقُرُونًا بَيْنَ ذَٰلِكَ كَثِيرًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2894,
      "verse_number": 39,
      "verse_key": "25:39",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَكُلًّا ضَرَبْنَا لَهُ الْأَمْثَالَ ۖ وَكُلًّا تَبَّرْنَا تَتْبِيرًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2895,
      "verse_number": 40,
      "verse_key": "25:40",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ أَتَوْا عَلَى الْقَرْيَةِ الَّتِي أُمْطِرَتْ مَطَرَ السَّوْءِ ۚ أَفَلَمْ يَكُونُوا يَرَوْنَهَا ۚ بَلْ كَانُوا لَا يَرْجُونَ نُشُورًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2896,
      "verse_number": 41,
      "verse_key": "25:41",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا رَأَوْكَ إِن يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَٰذَا الَّذِي بَعَثَ اللَّهُ رَسُولًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2897,
      "verse_number": 42,
      "verse_key": "25:42",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِن كَادَ لَيُضِلُّنَا عَنْ آلِهَتِنَا لَوْلَا أَن صَبَرْنَا عَلَيْهَا ۚ وَسَوْفَ يَعْلَمُونَ حِينَ يَرَوْنَ الْعَذَابَ مَنْ أَضَلُّ سَبِيلًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2898,
      "verse_number": 43,
      "verse_key": "25:43",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَرَأَيْتَ مَنِ اتَّخَذَ إِلَٰهَهُ هَوَاهُ أَفَأَنتَ تَكُونُ عَلَيْهِ وَكِيلًا",
      "page_number": 363,
      "juz_number": 19
    },
    {
      "id": 2899,
      "verse_number": 44,
      "verse_key": "25:44",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 313,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَمْ تَحْسَبُ أَنَّ أَكْثَرَهُمْ يَسْمَعُونَ أَوْ يَعْقِلُونَ ۚ إِنْ هُمْ إِلَّا كَالْأَنْعَامِ ۖ بَلْ هُمْ أَضَلُّ سَبِيلًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2900,
      "verse_number": 45,
      "verse_key": "25:45",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أَلَمْ تَرَ إِلَىٰ رَبِّكَ كَيْفَ مَدَّ الظِّلَّ وَلَوْ شَاءَ لَجَعَلَهُ سَاكِنًا ثُمَّ جَعَلْنَا الشَّمْسَ عَلَيْهِ دَلِيلًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2901,
      "verse_number": 46,
      "verse_key": "25:46",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ قَبَضْنَاهُ إِلَيْنَا قَبْضًا يَسِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2902,
      "verse_number": 47,
      "verse_key": "25:47",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي جَعَلَ لَكُمُ اللَّيْلَ لِبَاسًا وَالنَّوْمَ سُبَاتًا وَجَعَلَ النَّهَارَ نُشُورًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2903,
      "verse_number": 48,
      "verse_key": "25:48",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي أَرْسَلَ الرِّيَاحَ بُشْرًا بَيْنَ يَدَيْ رَحْمَتِهِ ۚ وَأَنزَلْنَا مِنَ السَّمَاءِ مَاءً طَهُورًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2904,
      "verse_number": 49,
      "verse_key": "25:49",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "لِّنُحْيِيَ بِهِ بَلْدَةً مَّيْتًا وَنُسْقِيَهُ مِمَّا خَلَقْنَا أَنْعَامًا وَأَنَاسِيَّ كَثِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2905,
      "verse_number": 50,
      "verse_key": "25:50",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ صَرَّفْنَاهُ بَيْنَهُمْ لِيَذَّكَّرُوا فَأَبَىٰ أَكْثَرُ النَّاسِ إِلَّا كُفُورًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2906,
      "verse_number": 51,
      "verse_key": "25:51",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَلَوْ شِئْنَا لَبَعَثْنَا فِي كُلِّ قَرْيَةٍ نَّذِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2907,
      "verse_number": 52,
      "verse_key": "25:52",
      "hizb_number": 37,
      "rub_el_hizb_number": 145,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "فَلَا تُطِعِ الْكَافِرِينَ وَجَاهِدْهُم بِهِ جِهَادًا كَبِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2908,
      "verse_number": 53,
      "verse_key": "25:53",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي مَرَجَ الْبَحْرَيْنِ هَٰذَا عَذْبٌ فُرَاتٌ وَهَٰذَا مِلْحٌ أُجَاجٌ وَجَعَلَ بَيْنَهُمَا بَرْزَخًا وَحِجْرًا مَّحْجُورًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2909,
      "verse_number": 54,
      "verse_key": "25:54",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي خَلَقَ مِنَ الْمَاءِ بَشَرًا فَجَعَلَهُ نَسَبًا وَصِهْرًا ۗ وَكَانَ رَبُّكَ قَدِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2910,
      "verse_number": 55,
      "verse_key": "25:55",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَيَعْبُدُونَ مِن دُونِ اللَّهِ مَا لَا يَنفَعُهُمْ وَلَا يَضُرُّهُمْ ۗ وَكَانَ الْكَافِرُ عَلَىٰ رَبِّهِ ظَهِيرًا",
      "page_number": 364,
      "juz_number": 19
    },
    {
      "id": 2911,
      "verse_number": 56,
      "verse_key": "25:56",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَا أَرْسَلْنَاكَ إِلَّا مُبَشِّرًا وَنَذِيرًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2912,
      "verse_number": 57,
      "verse_key": "25:57",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ مَا أَسْأَلُكُمْ عَلَيْهِ مِنْ أَجْرٍ إِلَّا مَن شَاءَ أَن يَتَّخِذَ إِلَىٰ رَبِّهِ سَبِيلًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2913,
      "verse_number": 58,
      "verse_key": "25:58",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَتَوَكَّلْ عَلَى الْحَيِّ الَّذِي لَا يَمُوتُ وَسَبِّحْ بِحَمْدِهِ ۚ وَكَفَىٰ بِهِ بِذُنُوبِ عِبَادِهِ خَبِيرًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2914,
      "verse_number": 59,
      "verse_key": "25:59",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي خَلَقَ السَّمَاوَاتِ وَالْأَرْضَ وَمَا بَيْنَهُمَا فِي سِتَّةِ أَيَّامٍ ثُمَّ اسْتَوَىٰ عَلَى الْعَرْشِ ۚ الرَّحْمَٰنُ فَاسْأَلْ بِهِ خَبِيرًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2915,
      "verse_number": 60,
      "verse_key": "25:60",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 314,
      "manzil_number": 4,
      "sajdah_number": 7,
      "text_imlaei": "وَإِذَا قِيلَ لَهُمُ اسْجُدُوا لِلرَّحْمَٰنِ قَالُوا وَمَا الرَّحْمَٰنُ أَنَسْجُدُ لِمَا تَأْمُرُنَا وَزَادَهُمْ نُفُورًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2916,
      "verse_number": 61,
      "verse_key": "25:61",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "تَبَارَكَ الَّذِي جَعَلَ فِي السَّمَاءِ بُرُوجًا وَجَعَلَ فِيهَا سِرَاجًا وَقَمَرًا مُّنِيرًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2917,
      "verse_number": 62,
      "verse_key": "25:62",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَهُوَ الَّذِي جَعَلَ اللَّيْلَ وَالنَّهَارَ خِلْفَةً لِّمَنْ أَرَادَ أَن يَذَّكَّرَ أَوْ أَرَادَ شُكُورًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2918,
      "verse_number": 63,
      "verse_key": "25:63",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَعِبَادُ الرَّحْمَٰنِ الَّذِينَ يَمْشُونَ عَلَى الْأَرْضِ هَوْنًا وَإِذَا خَاطَبَهُمُ الْجَاهِلُونَ قَالُوا سَلَامًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2919,
      "verse_number": 64,
      "verse_key": "25:64",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ يَبِيتُونَ لِرَبِّهِمْ سُجَّدًا وَقِيَامًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2920,
      "verse_number": 65,
      "verse_key": "25:65",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ يَقُولُونَ رَبَّنَا اصْرِفْ عَنَّا عَذَابَ جَهَنَّمَ ۖ إِنَّ عَذَابَهَا كَانَ غَرَامًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2921,
      "verse_number": 66,
      "verse_key": "25:66",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِنَّهَا سَاءَتْ مُسْتَقَرًّا وَمُقَامًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2922,
      "verse_number": 67,
      "verse_key": "25:67",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ إِذَا أَنفَقُوا لَمْ يُسْرِفُوا وَلَمْ يَقْتُرُوا وَكَانَ بَيْنَ ذَٰلِكَ قَوَامًا",
      "page_number": 365,
      "juz_number": 19
    },
    {
      "id": 2923,
      "verse_number": 68,
      "verse_key": "25:68",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ لَا يَدْعُونَ مَعَ اللَّهِ إِلَٰهًا آخَرَ وَلَا يَقْتُلُونَ النَّفْسَ الَّتِي حَرَّمَ اللَّهُ إِلَّا بِالْحَقِّ وَلَا يَزْنُونَ ۚ وَمَن يَفْعَلْ ذَٰلِكَ يَلْقَ أَثَامًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2924,
      "verse_number": 69,
      "verse_key": "25:69",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "يُضَاعَفْ لَهُ الْعَذَابُ يَوْمَ الْقِيَامَةِ وَيَخْلُدْ فِيهِ مُهَانًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2925,
      "verse_number": 70,
      "verse_key": "25:70",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "إِلَّا مَن تَابَ وَآمَنَ وَعَمِلَ عَمَلًا صَالِحًا فَأُولَٰئِكَ يُبَدِّلُ اللَّهُ سَيِّئَاتِهِمْ حَسَنَاتٍ ۗ وَكَانَ اللَّهُ غَفُورًا رَّحِيمًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2926,
      "verse_number": 71,
      "verse_key": "25:71",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَمَن تَابَ وَعَمِلَ صَالِحًا فَإِنَّهُ يَتُوبُ إِلَى اللَّهِ مَتَابًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2927,
      "verse_number": 72,
      "verse_key": "25:72",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ لَا يَشْهَدُونَ الزُّورَ وَإِذَا مَرُّوا بِاللَّغْوِ مَرُّوا كِرَامًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2928,
      "verse_number": 73,
      "verse_key": "25:73",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ إِذَا ذُكِّرُوا بِآيَاتِ رَبِّهِمْ لَمْ يَخِرُّوا عَلَيْهَا صُمًّا وَعُمْيَانًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2929,
      "verse_number": 74,
      "verse_key": "25:74",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِينَ يَقُولُونَ رَبَّنَا هَبْ لَنَا مِنْ أَزْوَاجِنَا وَذُرِّيَّاتِنَا قُرَّةَ أَعْيُنٍ وَاجْعَلْنَا لِلْمُتَّقِينَ إِمَامًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2930,
      "verse_number": 75,
      "verse_key": "25:75",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "أُولَٰئِكَ يُجْزَوْنَ الْغُرْفَةَ بِمَا صَبَرُوا وَيُلَقَّوْنَ فِيهَا تَحِيَّةً وَسَلَامًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2931,
      "verse_number": 76,
      "verse_key": "25:76",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "خَالِدِينَ فِيهَا ۚ حَسُنَتْ مُسْتَقَرًّا وَمُقَامًا",
      "page_number": 366,
      "juz_number": 19
    },
    {
      "id": 2932,
      "verse_number": 77,
      "verse_key": "25:77",
      "hizb_number": 37,
      "rub_el_hizb_number": 146,
      "ruku_number": 315,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_imlaei": "قُلْ مَا يَعْبَأُ بِكُمْ رَبِّي لَوْلَا دُعَاؤُكُمْ ۖ فَقَدْ كَذَّبْتُمْ فَسَوْفَ يَكُونُ لِزَامًا",
      "page_number": 366,
