{
  "id": 67,
  "name": "The Sovereignty",
  "verses": [
    {
      "id": 5242,
      "verse_number": 1,
      "verse_key": "67:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَبَارَكَ الَّذِي بِيَدِهِ الْمُلْكُ وَهُوَ عَلَىٰ كُلِّ شَيْءٍ قَدِيرٌ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5243,
      "verse_number": 2,
      "verse_key": "67:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي خَلَقَ الْمَوْتَ وَالْحَيَاةَ لِيَبْلُوَكُمْ أَيُّكُمْ أَحْسَنُ عَمَلًا ۚ وَهُوَ الْعَزِيزُ الْغَفُورُ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5244,
      "verse_number": 3,
      "verse_key": "67:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي خَلَقَ سَبْعَ سَمَاوَاتٍ طِبَاقًا ۖ مَّا تَرَىٰ فِي خَلْقِ الرَّحْمَٰنِ مِن تَفَاوُتٍ ۖ فَارْجِعِ الْبَصَرَ هَلْ تَرَىٰ مِن فُطُورٍ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5245,
      "verse_number": 4,
      "verse_key": "67:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ ارْجِعِ الْبَصَرَ كَرَّتَيْنِ يَنقَلِبْ إِلَيْكَ الْبَصَرُ خَاسِئًا وَهُوَ حَسِيرٌ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5246,
      "verse_number": 5,
      "verse_key": "67:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ زَيَّنَّا السَّمَاءَ الدُّنْيَا بِمَصَابِيحَ وَجَعَلْنَاهَا رُجُومًا لِّلشَّيَاطِينِ ۖ وَأَعْتَدْنَا لَهُمْ عَذَابَ السَّعِيرِ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5247,
      "verse_number": 6,
      "verse_key": "67:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلِلَّذِينَ كَفَرُوا بِرَبِّهِمْ عَذَابُ جَهَنَّمَ ۖ وَبِئْسَ الْمَصِيرُ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5248,
      "verse_number": 7,
      "verse_key": "67:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِذَا أُلْقُوا فِيهَا سَمِعُوا لَهَا شَهِيقًا وَهِيَ تَفُورُ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5249,
      "verse_number": 8,
      "verse_key": "67:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "تَكَادُ تَمَيَّزُ مِنَ الْغَيْظِ ۖ كُلَّمَا أُلْقِيَ فِيهَا فَوْجٌ سَأَلَهُمْ خَزَنَتُهَا أَلَمْ يَأْتِكُمْ نَذِيرٌ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5250,
      "verse_number": 9,
      "verse_key": "67:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا بَلَىٰ قَدْ جَاءَنَا نَذِيرٌ فَكَذَّبْنَا وَقُلْنَا مَا نَزَّلَ اللَّهُ مِن شَيْءٍ إِنْ أَنتُمْ إِلَّا فِي ضَلَالٍ كَبِيرٍ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5251,
      "verse_number": 10,
      "verse_key": "67:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَقَالُوا لَوْ كُنَّا نَسْمَعُ أَوْ نَعْقِلُ مَا كُنَّا فِي أَصْحَابِ السَّعِيرِ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5252,
      "verse_number": 11,
      "verse_key": "67:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَاعْتَرَفُوا بِذَنبِهِمْ فَسُحْقًا لِّأَصْحَابِ السَّعِيرِ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5253,
      "verse_number": 12,
      "verse_key": "67:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ الَّذِينَ يَخْشَوْنَ رَبَّهُم بِالْغَيْبِ لَهُم مَّغْفِرَةٌ وَأَجْرٌ كَبِيرٌ",
      "page_number": 562,
      "juz_number": 29
    },
    {
      "id": 5254,
      "verse_number": 13,
      "verse_key": "67:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَأَسِرُّوا قَوْلَكُمْ أَوِ اجْهَرُوا بِهِ ۖ إِنَّهُ عَلِيمٌ بِذَاتِ الصُّدُورِ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5255,
      "verse_number": 14,
      "verse_key": "67:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 498,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَلَا يَعْلَمُ مَنْ خَلَقَ وَهُوَ اللَّطِيفُ الْخَبِيرُ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5256,
      "verse_number": 15,
      "verse_key": "67:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "هُوَ الَّذِي جَعَلَ لَكُمُ الْأَرْضَ ذَلُولًا فَامْشُوا فِي مَنَاكِبِهَا وَكُلُوا مِن رِّزْقِهِ ۖ وَإِلَيْهِ النُّشُورُ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5257,
      "verse_number": 16,
      "verse_key": "67:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَأَمِنتُم مَّن فِي السَّمَاءِ أَن يَخْسِفَ بِكُمُ الْأَرْضَ فَإِذَا هِيَ تَمُورُ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5258,
      "verse_number": 17,
      "verse_key": "67:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمْ أَمِنتُم مَّن فِي السَّمَاءِ أَن يُرْسِلَ عَلَيْكُمْ حَاصِبًا ۖ فَسَتَعْلَمُونَ كَيْفَ نَذِيرِ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5259,
      "verse_number": 18,
      "verse_key": "67:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَلَقَدْ كَذَّبَ الَّذِينَ مِن قَبْلِهِمْ فَكَيْفَ كَانَ نَكِيرِ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5260,
      "verse_number": 19,
      "verse_key": "67:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَمْ يَرَوْا إِلَى الطَّيْرِ فَوْقَهُمْ صَافَّاتٍ وَيَقْبِضْنَ ۚ مَا يُمْسِكُهُنَّ إِلَّا الرَّحْمَٰنُ ۚ إِنَّهُ بِكُلِّ شَيْءٍ بَصِيرٌ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5261,
      "verse_number": 20,
      "verse_key": "67:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمَّنْ هَٰذَا الَّذِي هُوَ جُندٌ لَّكُمْ يَنصُرُكُم مِّن دُونِ الرَّحْمَٰنِ ۚ إِنِ الْكَافِرُونَ إِلَّا فِي غُرُورٍ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5262,
      "verse_number": 21,
      "verse_key": "67:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَمَّنْ هَٰذَا الَّذِي يَرْزُقُكُمْ إِنْ أَمْسَكَ رِزْقَهُ ۚ بَل لَّجُّوا فِي عُتُوٍّ وَنُفُورٍ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5263,
      "verse_number": 22,
      "verse_key": "67:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "أَفَمَن يَمْشِي مُكِبًّا عَلَىٰ وَجْهِهِ أَهْدَىٰ أَمَّن يَمْشِي سَوِيًّا عَلَىٰ صِرَاطٍ مُّسْتَقِيمٍ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5264,
      "verse_number": 23,
      "verse_key": "67:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ هُوَ الَّذِي أَنشَأَكُمْ وَجَعَلَ لَكُمُ السَّمْعَ وَالْأَبْصَارَ وَالْأَفْئِدَةَ ۖ قَلِيلًا مَّا تَشْكُرُونَ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5265,
      "verse_number": 24,
      "verse_key": "67:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ هُوَ الَّذِي ذَرَأَكُمْ فِي الْأَرْضِ وَإِلَيْهِ تُحْشَرُونَ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5266,
      "verse_number": 25,
      "verse_key": "67:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "وَيَقُولُونَ مَتَىٰ هَٰذَا الْوَعْدُ إِن كُنتُمْ صَادِقِينَ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5267,
      "verse_number": 26,
      "verse_key": "67:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ إِنَّمَا الْعِلْمُ عِندَ اللَّهِ وَإِنَّمَا أَنَا نَذِيرٌ مُّبِينٌ",
      "page_number": 563,
      "juz_number": 29
    },
    {
      "id": 5268,
      "verse_number": 27,
      "verse_key": "67:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا رَأَوْهُ زُلْفَةً سِيئَتْ وُجُوهُ الَّذِينَ كَفَرُوا وَقِيلَ هَٰذَا الَّذِي كُنتُم بِهِ تَدَّعُونَ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5269,
      "verse_number": 28,
      "verse_key": "67:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ أَرَأَيْتُمْ إِنْ أَهْلَكَنِيَ اللَّهُ وَمَن مَّعِيَ أَوْ رَحِمَنَا فَمَن يُجِيرُ الْكَافِرِينَ مِنْ عَذَابٍ أَلِيمٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5270,
      "verse_number": 29,
      "verse_key": "67:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ هُوَ الرَّحْمَٰنُ آمَنَّا بِهِ وَعَلَيْهِ تَوَكَّلْنَا ۖ فَسَتَعْلَمُونَ مَنْ هُوَ فِي ضَلَالٍ مُّبِينٍ",
      "page_number": 564,
      "juz_number": 29
    },
    {
      "id": 5271,
      "verse_number": 30,
      "verse_key": "67:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 225,
      "ruku_number": 499,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_imlaei": "قُلْ أَرَأَيْتُمْ إِنْ أَصْبَحَ مَاؤُكُمْ غَوْرًا فَمَن يَأْتِيكُم بِمَاءٍ مَّعِينٍ",
      "page_number": 564,
      "juz_number": 29
    }
  ]
}