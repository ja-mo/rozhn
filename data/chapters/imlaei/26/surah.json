{
  "id": 26,
  "name": "The Poets",
  "verses": [
    {
      "id": 2933,
      "verse_number": 1,
      "verse_key": "26:1",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "طسم",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2934,
      "verse_number": 2,
      "verse_key": "26:2",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "تِلْكَ آيَاتُ الْكِتَابِ الْمُبِينِ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2935,
      "verse_number": 3,
      "verse_key": "26:3",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَعَلَّكَ بَاخِعٌ نَّفْسَكَ أَلَّا يَكُونُوا مُؤْمِنِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2936,
      "verse_number": 4,
      "verse_key": "26:4",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِن نَّشَأْ نُنَزِّلْ عَلَيْهِم مِّنَ السَّمَاءِ آيَةً فَظَلَّتْ أَعْنَاقُهُمْ لَهَا خَاضِعِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2937,
      "verse_number": 5,
      "verse_key": "26:5",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَمَا يَأْتِيهِم مِّن ذِكْرٍ مِّنَ الرَّحْمَٰنِ مُحْدَثٍ إِلَّا كَانُوا عَنْهُ مُعْرِضِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2938,
      "verse_number": 6,
      "verse_key": "26:6",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَقَدْ كَذَّبُوا فَسَيَأْتِيهِمْ أَنبَاءُ مَا كَانُوا بِهِ يَسْتَهْزِئُونَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2939,
      "verse_number": 7,
      "verse_key": "26:7",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَوَلَمْ يَرَوْا إِلَى الْأَرْضِ كَمْ أَنبَتْنَا فِيهَا مِن كُلِّ زَوْجٍ كَرِيمٍ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2940,
      "verse_number": 8,
      "verse_key": "26:8",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2941,
      "verse_number": 9,
      "verse_key": "26:9",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 316,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2942,
      "verse_number": 10,
      "verse_key": "26:10",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِذْ نَادَىٰ رَبُّكَ مُوسَىٰ أَنِ ائْتِ الْقَوْمَ الظَّالِمِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2943,
      "verse_number": 11,
      "verse_key": "26:11",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَوْمَ فِرْعَوْنَ ۚ أَلَا يَتَّقُونَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2944,
      "verse_number": 12,
      "verse_key": "26:12",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبِّ إِنِّي أَخَافُ أَن يُكَذِّبُونِ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2945,
      "verse_number": 13,
      "verse_key": "26:13",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَيَضِيقُ صَدْرِي وَلَا يَنطَلِقُ لِسَانِي فَأَرْسِلْ إِلَىٰ هَارُونَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2946,
      "verse_number": 14,
      "verse_key": "26:14",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَلَهُمْ عَلَيَّ ذَنبٌ فَأَخَافُ أَن يَقْتُلُونِ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2947,
      "verse_number": 15,
      "verse_key": "26:15",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ كَلَّا ۖ فَاذْهَبَا بِآيَاتِنَا ۖ إِنَّا مَعَكُم مُّسْتَمِعُونَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2948,
      "verse_number": 16,
      "verse_key": "26:16",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأْتِيَا فِرْعَوْنَ فَقُولَا إِنَّا رَسُولُ رَبِّ الْعَالَمِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2949,
      "verse_number": 17,
      "verse_key": "26:17",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَنْ أَرْسِلْ مَعَنَا بَنِي إِسْرَائِيلَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2950,
      "verse_number": 18,
      "verse_key": "26:18",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَلَمْ نُرَبِّكَ فِينَا وَلِيدًا وَلَبِثْتَ فِينَا مِنْ عُمُرِكَ سِنِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2951,
      "verse_number": 19,
      "verse_key": "26:19",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَفَعَلْتَ فَعْلَتَكَ الَّتِي فَعَلْتَ وَأَنتَ مِنَ الْكَافِرِينَ",
      "page_number": 367,
      "juz_number": 19
    },
    {
      "id": 2952,
      "verse_number": 20,
      "verse_key": "26:20",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَعَلْتُهَا إِذًا وَأَنَا مِنَ الضَّالِّينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2953,
      "verse_number": 21,
      "verse_key": "26:21",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَفَرَرْتُ مِنكُمْ لَمَّا خِفْتُكُمْ فَوَهَبَ لِي رَبِّي حُكْمًا وَجَعَلَنِي مِنَ الْمُرْسَلِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2954,
      "verse_number": 22,
      "verse_key": "26:22",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَتِلْكَ نِعْمَةٌ تَمُنُّهَا عَلَيَّ أَنْ عَبَّدتَّ بَنِي إِسْرَائِيلَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2955,
      "verse_number": 23,
      "verse_key": "26:23",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فِرْعَوْنُ وَمَا رَبُّ الْعَالَمِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2956,
      "verse_number": 24,
      "verse_key": "26:24",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبُّ السَّمَاوَاتِ وَالْأَرْضِ وَمَا بَيْنَهُمَا ۖ إِن كُنتُم مُّوقِنِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2957,
      "verse_number": 25,
      "verse_key": "26:25",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لِمَنْ حَوْلَهُ أَلَا تَسْتَمِعُونَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2958,
      "verse_number": 26,
      "verse_key": "26:26",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبُّكُمْ وَرَبُّ آبَائِكُمُ الْأَوَّلِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2959,
      "verse_number": 27,
      "verse_key": "26:27",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ إِنَّ رَسُولَكُمُ الَّذِي أُرْسِلَ إِلَيْكُمْ لَمَجْنُونٌ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2960,
      "verse_number": 28,
      "verse_key": "26:28",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ رَبُّ الْمَشْرِقِ وَالْمَغْرِبِ وَمَا بَيْنَهُمَا ۖ إِن كُنتُمْ تَعْقِلُونَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2961,
      "verse_number": 29,
      "verse_key": "26:29",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَئِنِ اتَّخَذْتَ إِلَٰهًا غَيْرِي لَأَجْعَلَنَّكَ مِنَ الْمَسْجُونِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2962,
      "verse_number": 30,
      "verse_key": "26:30",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَوَلَوْ جِئْتُكَ بِشَيْءٍ مُّبِينٍ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2963,
      "verse_number": 31,
      "verse_key": "26:31",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ فَأْتِ بِهِ إِن كُنتَ مِنَ الصَّادِقِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2964,
      "verse_number": 32,
      "verse_key": "26:32",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَلْقَىٰ عَصَاهُ فَإِذَا هِيَ ثُعْبَانٌ مُّبِينٌ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2965,
      "verse_number": 33,
      "verse_key": "26:33",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 317,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَنَزَعَ يَدَهُ فَإِذَا هِيَ بَيْضَاءُ لِلنَّاظِرِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2966,
      "verse_number": 34,
      "verse_key": "26:34",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لِلْمَلَإِ حَوْلَهُ إِنَّ هَٰذَا لَسَاحِرٌ عَلِيمٌ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2967,
      "verse_number": 35,
      "verse_key": "26:35",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "يُرِيدُ أَن يُخْرِجَكُم مِّنْ أَرْضِكُم بِسِحْرِهِ فَمَاذَا تَأْمُرُونَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2968,
      "verse_number": 36,
      "verse_key": "26:36",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا أَرْجِهْ وَأَخَاهُ وَابْعَثْ فِي الْمَدَائِنِ حَاشِرِينَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2969,
      "verse_number": 37,
      "verse_key": "26:37",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "يَأْتُوكَ بِكُلِّ سَحَّارٍ عَلِيمٍ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2970,
      "verse_number": 38,
      "verse_key": "26:38",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَجُمِعَ السَّحَرَةُ لِمِيقَاتِ يَوْمٍ مَّعْلُومٍ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2971,
      "verse_number": 39,
      "verse_key": "26:39",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَقِيلَ لِلنَّاسِ هَلْ أَنتُم مُّجْتَمِعُونَ",
      "page_number": 368,
      "juz_number": 19
    },
    {
      "id": 2972,
      "verse_number": 40,
      "verse_key": "26:40",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "لَعَلَّنَا نَتَّبِعُ السَّحَرَةَ إِن كَانُوا هُمُ الْغَالِبِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2973,
      "verse_number": 41,
      "verse_key": "26:41",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا جَاءَ السَّحَرَةُ قَالُوا لِفِرْعَوْنَ أَئِنَّ لَنَا لَأَجْرًا إِن كُنَّا نَحْنُ الْغَالِبِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2974,
      "verse_number": 42,
      "verse_key": "26:42",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ نَعَمْ وَإِنَّكُمْ إِذًا لَّمِنَ الْمُقَرَّبِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2975,
      "verse_number": 43,
      "verse_key": "26:43",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ لَهُم مُّوسَىٰ أَلْقُوا مَا أَنتُم مُّلْقُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2976,
      "verse_number": 44,
      "verse_key": "26:44",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَلْقَوْا حِبَالَهُمْ وَعِصِيَّهُمْ وَقَالُوا بِعِزَّةِ فِرْعَوْنَ إِنَّا لَنَحْنُ الْغَالِبُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2977,
      "verse_number": 45,
      "verse_key": "26:45",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَلْقَىٰ مُوسَىٰ عَصَاهُ فَإِذَا هِيَ تَلْقَفُ مَا يَأْفِكُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2978,
      "verse_number": 46,
      "verse_key": "26:46",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأُلْقِيَ السَّحَرَةُ سَاجِدِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2979,
      "verse_number": 47,
      "verse_key": "26:47",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا آمَنَّا بِرَبِّ الْعَالَمِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2980,
      "verse_number": 48,
      "verse_key": "26:48",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "رَبِّ مُوسَىٰ وَهَارُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2981,
      "verse_number": 49,
      "verse_key": "26:49",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ آمَنتُمْ لَهُ قَبْلَ أَنْ آذَنَ لَكُمْ ۖ إِنَّهُ لَكَبِيرُكُمُ الَّذِي عَلَّمَكُمُ السِّحْرَ فَلَسَوْفَ تَعْلَمُونَ ۚ لَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَافٍ وَلَأُصَلِّبَنَّكُمْ أَجْمَعِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2982,
      "verse_number": 50,
      "verse_key": "26:50",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا لَا ضَيْرَ ۖ إِنَّا إِلَىٰ رَبِّنَا مُنقَلِبُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2983,
      "verse_number": 51,
      "verse_key": "26:51",
      "hizb_number": 37,
      "rub_el_hizb_number": 147,
      "ruku_number": 318,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّا نَطْمَعُ أَن يَغْفِرَ لَنَا رَبُّنَا خَطَايَانَا أَن كُنَّا أَوَّلَ الْمُؤْمِنِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2984,
      "verse_number": 52,
      "verse_key": "26:52",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنْ أَسْرِ بِعِبَادِي إِنَّكُم مُّتَّبَعُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2985,
      "verse_number": 53,
      "verse_key": "26:53",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَرْسَلَ فِرْعَوْنُ فِي الْمَدَائِنِ حَاشِرِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2986,
      "verse_number": 54,
      "verse_key": "26:54",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ هَٰؤُلَاءِ لَشِرْذِمَةٌ قَلِيلُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2987,
      "verse_number": 55,
      "verse_key": "26:55",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّهُمْ لَنَا لَغَائِظُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2988,
      "verse_number": 56,
      "verse_key": "26:56",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّا لَجَمِيعٌ حَاذِرُونَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2989,
      "verse_number": 57,
      "verse_key": "26:57",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَخْرَجْنَاهُم مِّن جَنَّاتٍ وَعُيُونٍ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2990,
      "verse_number": 58,
      "verse_key": "26:58",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَكُنُوزٍ وَمَقَامٍ كَرِيمٍ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2991,
      "verse_number": 59,
      "verse_key": "26:59",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "كَذَٰلِكَ وَأَوْرَثْنَاهَا بَنِي إِسْرَائِيلَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2992,
      "verse_number": 60,
      "verse_key": "26:60",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَتْبَعُوهُم مُّشْرِقِينَ",
      "page_number": 369,
      "juz_number": 19
    },
    {
      "id": 2993,
      "verse_number": 61,
      "verse_key": "26:61",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَلَمَّا تَرَاءَى الْجَمْعَانِ قَالَ أَصْحَابُ مُوسَىٰ إِنَّا لَمُدْرَكُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2994,
      "verse_number": 62,
      "verse_key": "26:62",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ كَلَّا ۖ إِنَّ مَعِيَ رَبِّي سَيَهْدِينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2995,
      "verse_number": 63,
      "verse_key": "26:63",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَأَوْحَيْنَا إِلَىٰ مُوسَىٰ أَنِ اضْرِب بِّعَصَاكَ الْبَحْرَ ۖ فَانفَلَقَ فَكَانَ كُلُّ فِرْقٍ كَالطَّوْدِ الْعَظِيمِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2996,
      "verse_number": 64,
      "verse_key": "26:64",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَأَزْلَفْنَا ثَمَّ الْآخَرِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2997,
      "verse_number": 65,
      "verse_key": "26:65",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَأَنجَيْنَا مُوسَىٰ وَمَن مَّعَهُ أَجْمَعِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2998,
      "verse_number": 66,
      "verse_key": "26:66",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "ثُمَّ أَغْرَقْنَا الْآخَرِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 2999,
      "verse_number": 67,
      "verse_key": "26:67",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِنَّ فِي ذَٰلِكَ لَآيَةً ۖ وَمَا كَانَ أَكْثَرُهُم مُّؤْمِنِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3000,
      "verse_number": 68,
      "verse_key": "26:68",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 319,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِنَّ رَبَّكَ لَهُوَ الْعَزِيزُ الرَّحِيمُ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3001,
      "verse_number": 69,
      "verse_key": "26:69",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَاتْلُ عَلَيْهِمْ نَبَأَ إِبْرَاهِيمَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3002,
      "verse_number": 70,
      "verse_key": "26:70",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِ مَا تَعْبُدُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3003,
      "verse_number": 71,
      "verse_key": "26:71",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا نَعْبُدُ أَصْنَامًا فَنَظَلُّ لَهَا عَاكِفِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3004,
      "verse_number": 72,
      "verse_key": "26:72",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ هَلْ يَسْمَعُونَكُمْ إِذْ تَدْعُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3005,
      "verse_number": 73,
      "verse_key": "26:73",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَوْ يَنفَعُونَكُمْ أَوْ يَضُرُّونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3006,
      "verse_number": 74,
      "verse_key": "26:74",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالُوا بَلْ وَجَدْنَا آبَاءَنَا كَذَٰلِكَ يَفْعَلُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3007,
      "verse_number": 75,
      "verse_key": "26:75",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "قَالَ أَفَرَأَيْتُم مَّا كُنتُمْ تَعْبُدُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3008,
      "verse_number": 76,
      "verse_key": "26:76",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "أَنتُمْ وَآبَاؤُكُمُ الْأَقْدَمُونَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3009,
      "verse_number": 77,
      "verse_key": "26:77",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "فَإِنَّهُمْ عَدُوٌّ لِّي إِلَّا رَبَّ الْعَالَمِينَ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3010,
      "verse_number": 78,
      "verse_key": "26:78",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "الَّذِي خَلَقَنِي فَهُوَ يَهْدِينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3011,
      "verse_number": 79,
      "verse_key": "26:79",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِي هُوَ يُطْعِمُنِي وَيَسْقِينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3012,
      "verse_number": 80,
      "verse_key": "26:80",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَإِذَا مَرِضْتُ فَهُوَ يَشْفِينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3013,
      "verse_number": 81,
      "verse_key": "26:81",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِي يُمِيتُنِي ثُمَّ يُحْيِينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3014,
      "verse_number": 82,
      "verse_key": "26:82",
      "hizb_number": 37,
      "rub_el_hizb_number": 148,
      "ruku_number": 320,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_imlaei": "وَالَّذِي أَطْمَعُ أَن يَغْفِرَ لِي خَطِيئَتِي يَوْمَ الدِّينِ",
      "page_number": 370,
      "juz_number": 19
    },
    {
      "id": 3015,
      "verse_number": 83,
      "verse_key": "26:83",
      "hizb_number": 37,
