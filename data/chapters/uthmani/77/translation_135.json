{
  "id": 77,
  "name": "The Emissaries",
  "verses": [
    {
      "id": 5623,
      "verse_number": 1,
      "verse_key": "77:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928288,
          "resource_id": 135,
          "text": "سوگند به فرشتگانی که پیاپی فرستاده می‌شوند،"
        }
      ]
    },
    {
      "id": 5624,
      "verse_number": 2,
      "verse_key": "77:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928289,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که همچون تندباد می‌روند،"
        }
      ]
    },
    {
      "id": 5625,
      "verse_number": 3,
      "verse_key": "77:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928290,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که [ابرها را] پراکنده می‌کنند،"
        }
      ]
    },
    {
      "id": 5626,
      "verse_number": 4,
      "verse_key": "77:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928291,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که جداکننده[ی حق از باطل] هستند"
        }
      ]
    },
    {
      "id": 5627,
      "verse_number": 5,
      "verse_key": "77:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928292,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که وحی [الهی] را [به پیامبران] القا می‌کنند،"
        }
      ]
    },
    {
      "id": 5628,
      "verse_number": 6,
      "verse_key": "77:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928293,
          "resource_id": 135,
          "text": "برای اتمامِ‌حجت یا برای بیم و هشدار."
        }
      ]
    },
    {
      "id": 5629,
      "verse_number": 7,
      "verse_key": "77:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928294,
          "resource_id": 135,
          "text": "بی‌گمان، آنچه به شما وعده داده می‌شود، واقع خواهد شد؛"
        }
      ]
    },
    {
      "id": 5630,
      "verse_number": 8,
      "verse_key": "77:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928295,
          "resource_id": 135,
          "text": "آنگاه ‌که ستارگان تیره [و محو] شود"
        }
      ]
    },
    {
      "id": 5631,
      "verse_number": 9,
      "verse_key": "77:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928296,
          "resource_id": 135,
          "text": "و آنگاه‌ که آسمان شکافته شود،"
        }
      ]
    },
    {
      "id": 5632,
      "verse_number": 10,
      "verse_key": "77:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928297,
          "resource_id": 135,
          "text": "و آنگاه ‌که کوه‌ها [از جا کنده و] پراکنده شود،"
        }
      ]
    },
    {
      "id": 5633,
      "verse_number": 11,
      "verse_key": "77:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928298,
          "resource_id": 135,
          "text": "و آنگاه ‌که برای پیامبران [جهت گواهی دادن در مورد امت‌ها]، وقت تعیین گردد،"
        }
      ]
    },
    {
      "id": 5634,
      "verse_number": 12,
      "verse_key": "77:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928299,
          "resource_id": 135,
          "text": "این [امر،] برای چه روزی به تأخیر افتاده است؟"
        }
      ]
    },
    {
      "id": 5635,
      "verse_number": 13,
      "verse_key": "77:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928300,
          "resource_id": 135,
          "text": "برای روز جدایی [و داوری]."
        }
      ]
    },
    {
      "id": 5636,
      "verse_number": 14,
      "verse_key": "77:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928301,
          "resource_id": 135,
          "text": "و تو چه دانی روز جدایی [و داوری] چیست؟"
        }
      ]
    },
    {
      "id": 5637,
      "verse_number": 15,
      "verse_key": "77:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928302,
          "resource_id": 135,
          "text": "در آن روز، [= قیامت] وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5638,
      "verse_number": 16,
      "verse_key": "77:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928303,
          "resource_id": 135,
          "text": "آیا ما پیشینیان [مجرم] را نابود نکردیم؟"
        }
      ]
    },
    {
      "id": 5639,
      "verse_number": 17,
      "verse_key": "77:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928304,
          "resource_id": 135,
          "text": "سپس دیگران را در پی آنها خواهیم آورد."
        }
      ]
    },
    {
      "id": 5640,
      "verse_number": 18,
      "verse_key": "77:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928305,
          "resource_id": 135,
          "text": "اینچنین با گنهکاران رفتار می‌کنیم."
        }
      ]
    },
    {
      "id": 5641,
      "verse_number": 19,
      "verse_key": "77:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 580,
      "juz_number": 29,
      "translations": [
        {
          "id": 928306,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5642,
      "verse_number": 20,
      "verse_key": "77:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928307,
          "resource_id": 135,
          "text": "آیا شما را از آبی پست [و ناچیز] نیافریدیم؟"
        }
      ]
    },
    {
      "id": 5643,
      "verse_number": 21,
      "verse_key": "77:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928308,
          "resource_id": 135,
          "text": "سپس آن را در قرارگاهی محفوظ [= رحِم] قرار دادیم"
        }
      ]
    },
    {
      "id": 5644,
      "verse_number": 22,
      "verse_key": "77:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928309,
          "resource_id": 135,
          "text": "تا زمانی معین."
        }
      ]
    },
    {
      "id": 5645,
      "verse_number": 23,
      "verse_key": "77:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928310,
          "resource_id": 135,
          "text": "ما [بر این کار] توانا بودیم و چه نیک توانا [و قدرتمند] هستیم."
        }
      ]
    },
    {
      "id": 5646,
      "verse_number": 24,
      "verse_key": "77:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928311,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5647,
      "verse_number": 25,
      "verse_key": "77:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928312,
          "resource_id": 135,
          "text": "آیا زمین را جایگاه [تجمع مردم] قرار ندادیم؟"
        }
      ]
    },
    {
      "id": 5648,
      "verse_number": 26,
      "verse_key": "77:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928313,
          "resource_id": 135,
          "text": "هم در حال حیات و هم مرگشان؟"
        }
      ]
    },
    {
      "id": 5649,
      "verse_number": 27,
      "verse_key": "77:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928314,
          "resource_id": 135,
          "text": "و کوه‌های بسیار بلند [و استوار] در آن قرار دادیم و آبی شیرین [و گوارا] به شما نوشاندیم."
        }
      ]
    },
    {
      "id": 5650,
      "verse_number": 28,
      "verse_key": "77:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928315,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5651,
      "verse_number": 29,
      "verse_key": "77:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928316,
          "resource_id": 135,
          "text": "[به آنها گفته می‌شود:] «به سوی همان چیزی بروید که پیوسته آن را تکذیب می‌کردید."
        }
      ]
    },
    {
      "id": 5652,
      "verse_number": 30,
      "verse_key": "77:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928317,
          "resource_id": 135,
          "text": "بروید به سوی سایۀ [دودهای آتش] سه‌شاخه،"
        }
      ]
    },
    {
      "id": 5653,
      "verse_number": 31,
      "verse_key": "77:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928318,
          "resource_id": 135,
          "text": "نه سایه‌افکن [و خنک] است و نه از [گرمیِ] شعله‌های آتش جلوگیری می‌کند»."
        }
      ]
    },
    {
      "id": 5654,
      "verse_number": 32,
      "verse_key": "77:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928319,
          "resource_id": 135,
          "text": "همانا [دوزخ،‌] شراره‌هایی چون كاخى [بلند] مى‌افكنَد."
        }
      ]
    },
    {
      "id": 5655,
      "verse_number": 33,
      "verse_key": "77:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928320,
          "resource_id": 135,
          "text": "گویی که آن [شراره‌ها] شتران زردرنگ هستند."
        }
      ]
    },
    {
      "id": 5656,
      "verse_number": 34,
      "verse_key": "77:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928321,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5657,
      "verse_number": 35,
      "verse_key": "77:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928322,
          "resource_id": 135,
          "text": "این [همان] روزی است که سخن نمی‌گویند."
        }
      ]
    },
    {
      "id": 5658,
      "verse_number": 36,
      "verse_key": "77:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928323,
          "resource_id": 135,
          "text": "و به آنها اجازه داده نمی‌شود تا عذرخواهی کنند."
        }
      ]
    },
    {
      "id": 5659,
      "verse_number": 37,
      "verse_key": "77:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928324,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5660,
      "verse_number": 38,
      "verse_key": "77:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928325,
          "resource_id": 135,
          "text": "این [همان] روز جدایی [و داوری] است که شما و گذشتگان را گرد آورده‌ایم."
        }
      ]
    },
    {
      "id": 5661,
      "verse_number": 39,
      "verse_key": "77:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928326,
          "resource_id": 135,
          "text": "پس اگر [حیله و] نیرنگی دارید، آن را در حق من به کار گیرید."
        }
      ]
    },
    {
      "id": 5662,
      "verse_number": 40,
      "verse_key": "77:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 518,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928327,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5663,
      "verse_number": 41,
      "verse_key": "77:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928328,
          "resource_id": 135,
          "text": "به راستی که [در آن روز،] پرهیزگاران در سایه‌ها و [کنار] چشمه‌ها قرار دارند،"
        }
      ]
    },
    {
      "id": 5664,
      "verse_number": 42,
      "verse_key": "77:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928329,
          "resource_id": 135,
          "text": "و میوه‌هایی که میل داشته باشند."
        }
      ]
    },
    {
      "id": 5665,
      "verse_number": 43,
      "verse_key": "77:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928330,
          "resource_id": 135,
          "text": "[به آنها گفته می‌شود:] «به پاداش آنچه می‌کردید، گوارا بخورید و بیاشامید."
        }
      ]
    },
    {
      "id": 5666,
      "verse_number": 44,
      "verse_key": "77:44",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928331,
          "resource_id": 135,
          "text": "ما این گونه نیکوکاران را پاداش می‌دهیم»."
        }
      ]
    },
    {
      "id": 5667,
      "verse_number": 45,
      "verse_key": "77:45",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928332,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5668,
      "verse_number": 46,
      "verse_key": "77:46",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928333,
          "resource_id": 135,
          "text": "«[ای کافران، در این دنیا] اندکی بخورید و بهره گیرید؛ زیرا شما گناهکارید»."
        }
      ]
    },
    {
      "id": 5669,
      "verse_number": 47,
      "verse_key": "77:47",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928334,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5670,
      "verse_number": 48,
      "verse_key": "77:48",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928335,
          "resource_id": 135,
          "text": "و هنگامی‌ که به آنها گفته شود: [نماز بگزارید و] رکوع کنید، [نماز نمی‌گزارند و] رکوع نمی‌کنند."
        }
      ]
    },
    {
      "id": 5671,
      "verse_number": 49,
      "verse_key": "77:49",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928336,
          "resource_id": 135,
          "text": "در آن روز، وای بر تکذیب‌کنندگان!"
        }
      ]
    },
    {
      "id": 5672,
      "verse_number": 50,
      "verse_key": "77:50",
      "hizb_number": 58,
      "rub_el_hizb_number": 232,
      "ruku_number": 519,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 581,
      "juz_number": 29,
      "translations": [
        {
          "id": 928337,
          "resource_id": 135,
          "text": "[وقتی مردم به این قرآن که از جانب پروردگارشان نازل شده است ایمان نمی‌آورند،] پس به کدام سخن دیگری غیر از آن ایمان خواهند آورد؟"
        }
      ]
    }
  ]
}