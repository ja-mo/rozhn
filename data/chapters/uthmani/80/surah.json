{
  "id": 80,
  "name": "He Frowned",
  "verses": [
    {
      "id": 5759,
      "verse_number": 1,
      "verse_key": "80:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " عَبَسَ وَتَوَلَّىٰٓ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5760,
      "verse_number": 2,
      "verse_key": "80:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَن جَآءَهُ ٱلْأَعْمَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5761,
      "verse_number": 3,
      "verse_key": "80:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5762,
      "verse_number": 4,
      "verse_key": "80:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5763,
      "verse_number": 5,
      "verse_key": "80:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمَّا مَنِ ٱسْتَغْنَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5764,
      "verse_number": 6,
      "verse_key": "80:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ لَهُۥ تَصَدَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5765,
      "verse_number": 7,
      "verse_key": "80:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5766,
      "verse_number": 8,
      "verse_key": "80:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5767,
      "verse_number": 9,
      "verse_key": "80:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ يَخْشَىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5768,
      "verse_number": 10,
      "verse_key": "80:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنتَ عَنْهُ تَلَهَّىٰ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5769,
      "verse_number": 11,
      "verse_key": "80:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّهَا تَذْكِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5770,
      "verse_number": 12,
      "verse_key": "80:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَن شَآءَ ذَكَرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5771,
      "verse_number": 13,
      "verse_key": "80:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى صُحُفٍ مُّكَرَّمَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5772,
      "verse_number": 14,
      "verse_key": "80:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5773,
      "verse_number": 15,
      "verse_key": "80:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بِأَيْدِى سَفَرَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5774,
      "verse_number": 16,
      "verse_key": "80:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كِرَامٍۭ بَرَرَةٍ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5775,
      "verse_number": 17,
      "verse_key": "80:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُتِلَ ٱلْإِنسَـٰنُ مَآ أَكْفَرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5776,
      "verse_number": 18,
      "verse_key": "80:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5777,
      "verse_number": 19,
      "verse_key": "80:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5778,
      "verse_number": 20,
      "verse_key": "80:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5779,
      "verse_number": 21,
      "verse_key": "80:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5780,
      "verse_number": 22,
      "verse_key": "80:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5781,
      "verse_number": 23,
      "verse_key": "80:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5782,
      "verse_number": 24,
      "verse_key": "80:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَنظُرِ ٱلْإِنسَـٰنُ إِلَىٰ طَعَامِهِۦٓ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5783,
      "verse_number": 25,
      "verse_key": "80:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5784,
      "verse_number": 26,
      "verse_key": "80:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5785,
      "verse_number": 27,
      "verse_key": "80:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَنۢبَتْنَا فِيهَا حَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5786,
      "verse_number": 28,
      "verse_key": "80:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَعِنَبًا وَقَضْبًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5787,
      "verse_number": 29,
      "verse_key": "80:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَزَيْتُونًا وَنَخْلًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5788,
      "verse_number": 30,
      "verse_key": "80:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحَدَآئِقَ غُلْبًا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5789,
      "verse_number": 31,
      "verse_key": "80:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَـٰكِهَةً وَأَبًّا",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5790,
      "verse_number": 32,
      "verse_key": "80:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5791,
      "verse_number": 33,
      "verse_key": "80:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5792,
      "verse_number": 34,
      "verse_key": "80:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5793,
      "verse_number": 35,
      "verse_key": "80:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأُمِّهِۦ وَأَبِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5794,
      "verse_number": 36,
      "verse_key": "80:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَصَـٰحِبَتِهِۦ وَبَنِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5795,
      "verse_number": 37,
      "verse_key": "80:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5796,
      "verse_number": 38,
      "verse_key": "80:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5797,
      "verse_number": 39,
      "verse_key": "80:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5798,
      "verse_number": 40,
      "verse_key": "80:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5799,
      "verse_number": 41,
      "verse_key": "80:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَرْهَقُهَا قَتَرَةٌ",
      "page_number": 585,
      "juz_number": 30
    },
    {
      "id": 5800,
      "verse_number": 42,
      "verse_key": "80:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 524,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أُو۟لَـٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
      "page_number": 585,
      "juz_number": 30
    }
  ]
}