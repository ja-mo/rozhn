{
  "id": 100,
  "name": "The Courser",
  "verses": [
    {
      "id": 6147,
      "verse_number": 1,
      "verse_key": "100:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلْعَـٰدِيَـٰتِ ضَبْحًا",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928812,
          "resource_id": 135,
          "text": "سوگند به اسب‌های دونده که نفس‌زنان [به سوی میدان جهاد] پیش می‌روند."
        }
      ]
    },
    {
      "id": 6148,
      "verse_number": 2,
      "verse_key": "100:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُورِيَـٰتِ قَدْحًا",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928813,
          "resource_id": 135,
          "text": "و سوگند به اسب‌هایی که [با برخورد سُم‌هایشان به سنگ‌ها] جرقۀ [آتش] ایجاد می‌کنند."
        }
      ]
    },
    {
      "id": 6149,
      "verse_number": 3,
      "verse_key": "100:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُغِيرَٰتِ صُبْحًا",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928814,
          "resource_id": 135,
          "text": "باز سوگند به اسب‌هایی که در صبحگاهان [بر دشمن] یورش می‌برند."
        }
      ]
    },
    {
      "id": 6150,
      "verse_number": 4,
      "verse_key": "100:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَثَرْنَ بِهِۦ نَقْعًا",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928815,
          "resource_id": 135,
          "text": "و در آن هنگام گرد و غبار برمی‌انگیزند."
        }
      ]
    },
    {
      "id": 6151,
      "verse_number": 5,
      "verse_key": "100:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَوَسَطْنَ بِهِۦ جَمْعًا",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928816,
          "resource_id": 135,
          "text": "آنگاه در دل [سپاه دشمن] می‌تازند."
        }
      ]
    },
    {
      "id": 6152,
      "verse_number": 6,
      "verse_key": "100:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْإِنسَـٰنَ لِرَبِّهِۦ لَكَنُودٌ",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928817,
          "resource_id": 135,
          "text": "یقیناً انسان در برابر [نعمت‌های] پروردگارش بسیار ناسپاس است،"
        }
      ]
    },
    {
      "id": 6153,
      "verse_number": 7,
      "verse_key": "100:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928818,
          "resource_id": 135,
          "text": "و بی‌گمان، او [خود] بر این [ناسپاسی] گواه است،"
        }
      ]
    },
    {
      "id": 6154,
      "verse_number": 8,
      "verse_key": "100:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928819,
          "resource_id": 135,
          "text": "و همانا او علاقۀ فراوانی به مال [دنیا] دارد."
        }
      ]
    },
    {
      "id": 6155,
      "verse_number": 9,
      "verse_key": "100:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "۞ أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ",
      "page_number": 599,
      "juz_number": 30,
      "translations": [
        {
          "id": 928820,
          "resource_id": 135,
          "text": "آیا او نمی‌داند که در آن روز، آنچه در گور‌هاست، [همه زنده و] برانگیخته می‌شوند؟"
        }
      ]
    },
    {
      "id": 6156,
      "verse_number": 10,
      "verse_key": "100:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحُصِّلَ مَا فِى ٱلصُّدُورِ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928821,
          "resource_id": 135,
          "text": "و آنچه در سینه‌هاست [همه] آشکار می‌گردد."
        }
      ]
    },
    {
      "id": 6157,
      "verse_number": 11,
      "verse_key": "100:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 544,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍ لَّخَبِيرٌۢ",
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928822,
          "resource_id": 135,
          "text": "یقیناً در آن روز، پروردگارشان کاملاً به [وضع و حال] آنان آگاه است."
        }
      ]
    }
  ]
}