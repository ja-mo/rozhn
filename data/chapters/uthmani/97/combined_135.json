{
  "id": 97,
  "name": "The Power",
  "verses": [
    {
      "id": 6126,
      "verse_number": 1,
      "verse_key": "97:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَنزَلْنَـٰهُ فِى لَيْلَةِ ٱلْقَدْرِ",
      "page_number": 598,
      "juz_number": 30,
      "translations": [
        {
          "id": 928791,
          "resource_id": 135,
          "text": "همانا ما آن [قرآن] را در شب قدر نازل کردیم."
        }
      ]
    },
    {
      "id": 6127,
      "verse_number": 2,
      "verse_key": "97:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا لَيْلَةُ ٱلْقَدْرِ",
      "page_number": 598,
      "juz_number": 30,
      "translations": [
        {
          "id": 928792,
          "resource_id": 135,
          "text": "و تو چه دانی شب قدر چیست؟"
        }
      ]
    },
    {
      "id": 6128,
      "verse_number": 3,
      "verse_key": "97:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَيْلَةُ ٱلْقَدْرِ خَيْرٌ مِّنْ أَلْفِ شَهْرٍ",
      "page_number": 598,
      "juz_number": 30,
      "translations": [
        {
          "id": 928793,
          "resource_id": 135,
          "text": "شب قدر، بهتر از هزار ماه است."
        }
      ]
    },
    {
      "id": 6129,
      "verse_number": 4,
      "verse_key": "97:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَنَزَّلُ ٱلْمَلَـٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٍ",
      "page_number": 598,
      "juz_number": 30,
      "translations": [
        {
          "id": 928794,
          "resource_id": 135,
          "text": "فرشتگان و روح [= جبرئیل] در آن [شب،] به فرمان پروردگارشان برای [انجام] هر کاری نازل می‌شوند."
        }
      ]
    },
    {
      "id": 6130,
      "verse_number": 5,
      "verse_key": "97:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 541,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَلَـٰمٌ هِىَ حَتَّىٰ مَطْلَعِ ٱلْفَجْرِ",
      "page_number": 598,
      "juz_number": 30,
      "translations": [
        {
          "id": 928795,
          "resource_id": 135,
          "text": "[آن شب] تا طلوع فجر، سلامت [و رحمت] است."
        }
      ]
    }
  ]
}