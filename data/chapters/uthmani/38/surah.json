{
  "id": 38,
  "name": "The Letter \"Saad\"",
  "verses": [
    {
      "id": 3971,
      "verse_number": 1,
      "verse_key": "38:1",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": " صٓ ۚ وَٱلْقُرْءَانِ ذِى ٱلذِّكْرِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3972,
      "verse_number": 2,
      "verse_key": "38:2",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى عِزَّةٍ وَشِقَاقٍ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3973,
      "verse_number": 3,
      "verse_key": "38:3",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "كَمْ أَهْلَكْنَا مِن قَبْلِهِم مِّن قَرْنٍ فَنَادَوا۟ وَّلَاتَ حِينَ مَنَاصٍ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3974,
      "verse_number": 4,
      "verse_key": "38:4",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَعَجِبُوٓا۟ أَن جَآءَهُم مُّنذِرٌ مِّنْهُمْ ۖ وَقَالَ ٱلْكَـٰفِرُونَ هَـٰذَا سَـٰحِرٌ كَذَّابٌ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3975,
      "verse_number": 5,
      "verse_key": "38:5",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَجَعَلَ ٱلْـَٔالِهَةَ إِلَـٰهًا وَٰحِدًا ۖ إِنَّ هَـٰذَا لَشَىْءٌ عُجَابٌ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3976,
      "verse_number": 6,
      "verse_key": "38:6",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱنطَلَقَ ٱلْمَلَأُ مِنْهُمْ أَنِ ٱمْشُوا۟ وَٱصْبِرُوا۟ عَلَىٰٓ ءَالِهَتِكُمْ ۖ إِنَّ هَـٰذَا لَشَىْءٌ يُرَادُ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3977,
      "verse_number": 7,
      "verse_key": "38:7",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "مَا سَمِعْنَا بِهَـٰذَا فِى ٱلْمِلَّةِ ٱلْـَٔاخِرَةِ إِنْ هَـٰذَآ إِلَّا ٱخْتِلَـٰقٌ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3978,
      "verse_number": 8,
      "verse_key": "38:8",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَءُنزِلَ عَلَيْهِ ٱلذِّكْرُ مِنۢ بَيْنِنَا ۚ بَلْ هُمْ فِى شَكٍّ مِّن ذِكْرِى ۖ بَل لَّمَّا يَذُوقُوا۟ عَذَابِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3979,
      "verse_number": 9,
      "verse_key": "38:9",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ عِندَهُمْ خَزَآئِنُ رَحْمَةِ رَبِّكَ ٱلْعَزِيزِ ٱلْوَهَّابِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3980,
      "verse_number": 10,
      "verse_key": "38:10",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ لَهُم مُّلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ۖ فَلْيَرْتَقُوا۟ فِى ٱلْأَسْبَـٰبِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3981,
      "verse_number": 11,
      "verse_key": "38:11",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "جُندٌ مَّا هُنَالِكَ مَهْزُومٌ مِّنَ ٱلْأَحْزَابِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3982,
      "verse_number": 12,
      "verse_key": "38:12",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَعَادٌ وَفِرْعَوْنُ ذُو ٱلْأَوْتَادِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3983,
      "verse_number": 13,
      "verse_key": "38:13",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَثَمُودُ وَقَوْمُ لُوطٍ وَأَصْحَـٰبُ لْـَٔيْكَةِ ۚ أُو۟لَـٰٓئِكَ ٱلْأَحْزَابُ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3984,
      "verse_number": 14,
      "verse_key": "38:14",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 393,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِن كُلٌّ إِلَّا كَذَّبَ ٱلرُّسُلَ فَحَقَّ عِقَابِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3985,
      "verse_number": 15,
      "verse_key": "38:15",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يَنظُرُ هَـٰٓؤُلَآءِ إِلَّا صَيْحَةً وَٰحِدَةً مَّا لَهَا مِن فَوَاقٍ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3986,
      "verse_number": 16,
      "verse_key": "38:16",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ رَبَّنَا عَجِّل لَّنَا قِطَّنَا قَبْلَ يَوْمِ ٱلْحِسَابِ",
      "page_number": 453,
      "juz_number": 23
    },
    {
      "id": 3987,
      "verse_number": 17,
      "verse_key": "38:17",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱصْبِرْ عَلَىٰ مَا يَقُولُونَ وَٱذْكُرْ عَبْدَنَا دَاوُۥدَ ذَا ٱلْأَيْدِ ۖ إِنَّهُۥٓ أَوَّابٌ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3988,
      "verse_number": 18,
      "verse_key": "38:18",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا سَخَّرْنَا ٱلْجِبَالَ مَعَهُۥ يُسَبِّحْنَ بِٱلْعَشِىِّ وَٱلْإِشْرَاقِ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3989,
      "verse_number": 19,
      "verse_key": "38:19",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱلطَّيْرَ مَحْشُورَةً ۖ كُلٌّ لَّهُۥٓ أَوَّابٌ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3990,
      "verse_number": 20,
      "verse_key": "38:20",
      "hizb_number": 46,
      "rub_el_hizb_number": 181,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَشَدَدْنَا مُلْكَهُۥ وَءَاتَيْنَـٰهُ ٱلْحِكْمَةَ وَفَصْلَ ٱلْخِطَابِ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3991,
      "verse_number": 21,
      "verse_key": "38:21",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "۞ وَهَلْ أَتَىٰكَ نَبَؤُا۟ ٱلْخَصْمِ إِذْ تَسَوَّرُوا۟ ٱلْمِحْرَابَ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3992,
      "verse_number": 22,
      "verse_key": "38:22",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِذْ دَخَلُوا۟ عَلَىٰ دَاوُۥدَ فَفَزِعَ مِنْهُمْ ۖ قَالُوا۟ لَا تَخَفْ ۖ خَصْمَانِ بَغَىٰ بَعْضُنَا عَلَىٰ بَعْضٍ فَٱحْكُم بَيْنَنَا بِٱلْحَقِّ وَلَا تُشْطِطْ وَٱهْدِنَآ إِلَىٰ سَوَآءِ ٱلصِّرَٰطِ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3993,
      "verse_number": 23,
      "verse_key": "38:23",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ هَـٰذَآ أَخِى لَهُۥ تِسْعٌ وَتِسْعُونَ نَعْجَةً وَلِىَ نَعْجَةٌ وَٰحِدَةٌ فَقَالَ أَكْفِلْنِيهَا وَعَزَّنِى فِى ٱلْخِطَابِ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3994,
      "verse_number": 24,
      "verse_key": "38:24",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": 10,
      "text_uthmani": "قَالَ لَقَدْ ظَلَمَكَ بِسُؤَالِ نَعْجَتِكَ إِلَىٰ نِعَاجِهِۦ ۖ وَإِنَّ كَثِيرًا مِّنَ ٱلْخُلَطَآءِ لَيَبْغِى بَعْضُهُمْ عَلَىٰ بَعْضٍ إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ وَقَلِيلٌ مَّا هُمْ ۗ وَظَنَّ دَاوُۥدُ أَنَّمَا فَتَنَّـٰهُ فَٱسْتَغْفَرَ رَبَّهُۥ وَخَرَّ رَاكِعًا وَأَنَابَ ۩",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3995,
      "verse_number": 25,
      "verse_key": "38:25",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَغَفَرْنَا لَهُۥ ذَٰلِكَ ۖ وَإِنَّ لَهُۥ عِندَنَا لَزُلْفَىٰ وَحُسْنَ مَـَٔابٍ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3996,
      "verse_number": 26,
      "verse_key": "38:26",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 394,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "يَـٰدَاوُۥدُ إِنَّا جَعَلْنَـٰكَ خَلِيفَةً فِى ٱلْأَرْضِ فَٱحْكُم بَيْنَ ٱلنَّاسِ بِٱلْحَقِّ وَلَا تَتَّبِعِ ٱلْهَوَىٰ فَيُضِلَّكَ عَن سَبِيلِ ٱللَّهِ ۚ إِنَّ ٱلَّذِينَ يَضِلُّونَ عَن سَبِيلِ ٱللَّهِ لَهُمْ عَذَابٌ شَدِيدٌۢ بِمَا نَسُوا۟ يَوْمَ ٱلْحِسَابِ",
      "page_number": 454,
      "juz_number": 23
    },
    {
      "id": 3997,
      "verse_number": 27,
      "verse_key": "38:27",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَا خَلَقْنَا ٱلسَّمَآءَ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا بَـٰطِلًا ۚ ذَٰلِكَ ظَنُّ ٱلَّذِينَ كَفَرُوا۟ ۚ فَوَيْلٌ لِّلَّذِينَ كَفَرُوا۟ مِنَ ٱلنَّارِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 3998,
      "verse_number": 28,
      "verse_key": "38:28",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ نَجْعَلُ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ كَٱلْمُفْسِدِينَ فِى ٱلْأَرْضِ أَمْ نَجْعَلُ ٱلْمُتَّقِينَ كَٱلْفُجَّارِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 3999,
      "verse_number": 29,
      "verse_key": "38:29",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "كِتَـٰبٌ أَنزَلْنَـٰهُ إِلَيْكَ مُبَـٰرَكٌ لِّيَدَّبَّرُوٓا۟ ءَايَـٰتِهِۦ وَلِيَتَذَكَّرَ أُو۟لُوا۟ ٱلْأَلْبَـٰبِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4000,
      "verse_number": 30,
      "verse_key": "38:30",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَوَهَبْنَا لِدَاوُۥدَ سُلَيْمَـٰنَ ۚ نِعْمَ ٱلْعَبْدُ ۖ إِنَّهُۥٓ أَوَّابٌ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4001,
      "verse_number": 31,
      "verse_key": "38:31",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِذْ عُرِضَ عَلَيْهِ بِٱلْعَشِىِّ ٱلصَّـٰفِنَـٰتُ ٱلْجِيَادُ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4002,
      "verse_number": 32,
      "verse_key": "38:32",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ إِنِّىٓ أَحْبَبْتُ حُبَّ ٱلْخَيْرِ عَن ذِكْرِ رَبِّى حَتَّىٰ تَوَارَتْ بِٱلْحِجَابِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4003,
      "verse_number": 33,
      "verse_key": "38:33",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "رُدُّوهَا عَلَىَّ ۖ فَطَفِقَ مَسْحًۢا بِٱلسُّوقِ وَٱلْأَعْنَاقِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4004,
      "verse_number": 34,
      "verse_key": "38:34",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ فَتَنَّا سُلَيْمَـٰنَ وَأَلْقَيْنَا عَلَىٰ كُرْسِيِّهِۦ جَسَدًا ثُمَّ أَنَابَ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4005,
      "verse_number": 35,
      "verse_key": "38:35",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّ ٱغْفِرْ لِى وَهَبْ لِى مُلْكًا لَّا يَنۢبَغِى لِأَحَدٍ مِّنۢ بَعْدِىٓ ۖ إِنَّكَ أَنتَ ٱلْوَهَّابُ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4006,
      "verse_number": 36,
      "verse_key": "38:36",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَسَخَّرْنَا لَهُ ٱلرِّيحَ تَجْرِى بِأَمْرِهِۦ رُخَآءً حَيْثُ أَصَابَ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4007,
      "verse_number": 37,
      "verse_key": "38:37",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱلشَّيَـٰطِينَ كُلَّ بَنَّآءٍ وَغَوَّاصٍ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4008,
      "verse_number": 38,
      "verse_key": "38:38",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَءَاخَرِينَ مُقَرَّنِينَ فِى ٱلْأَصْفَادِ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4009,
      "verse_number": 39,
      "verse_key": "38:39",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا عَطَآؤُنَا فَٱمْنُنْ أَوْ أَمْسِكْ بِغَيْرِ حِسَابٍ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4010,
      "verse_number": 40,
      "verse_key": "38:40",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 395,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ لَهُۥ عِندَنَا لَزُلْفَىٰ وَحُسْنَ مَـَٔابٍ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4011,
      "verse_number": 41,
      "verse_key": "38:41",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱذْكُرْ عَبْدَنَآ أَيُّوبَ إِذْ نَادَىٰ رَبَّهُۥٓ أَنِّى مَسَّنِىَ ٱلشَّيْطَـٰنُ بِنُصْبٍ وَعَذَابٍ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4012,
      "verse_number": 42,
      "verse_key": "38:42",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱرْكُضْ بِرِجْلِكَ ۖ هَـٰذَا مُغْتَسَلٌۢ بَارِدٌ وَشَرَابٌ",
      "page_number": 455,
      "juz_number": 23
    },
    {
      "id": 4013,
      "verse_number": 43,
      "verse_key": "38:43",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَوَهَبْنَا لَهُۥٓ أَهْلَهُۥ وَمِثْلَهُم مَّعَهُمْ رَحْمَةً مِّنَّا وَذِكْرَىٰ لِأُو۟لِى ٱلْأَلْبَـٰبِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4014,
      "verse_number": 44,
      "verse_key": "38:44",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَخُذْ بِيَدِكَ ضِغْثًا فَٱضْرِب بِّهِۦ وَلَا تَحْنَثْ ۗ إِنَّا وَجَدْنَـٰهُ صَابِرًا ۚ نِّعْمَ ٱلْعَبْدُ ۖ إِنَّهُۥٓ أَوَّابٌ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4015,
      "verse_number": 45,
      "verse_key": "38:45",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱذْكُرْ عِبَـٰدَنَآ إِبْرَٰهِيمَ وَإِسْحَـٰقَ وَيَعْقُوبَ أُو۟لِى ٱلْأَيْدِى وَٱلْأَبْصَـٰرِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4016,
      "verse_number": 46,
      "verse_key": "38:46",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَخْلَصْنَـٰهُم بِخَالِصَةٍ ذِكْرَى ٱلدَّارِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4017,
      "verse_number": 47,
      "verse_key": "38:47",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُمْ عِندَنَا لَمِنَ ٱلْمُصْطَفَيْنَ ٱلْأَخْيَارِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4018,
      "verse_number": 48,
      "verse_key": "38:48",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱذْكُرْ إِسْمَـٰعِيلَ وَٱلْيَسَعَ وَذَا ٱلْكِفْلِ ۖ وَكُلٌّ مِّنَ ٱلْأَخْيَارِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4019,
      "verse_number": 49,
      "verse_key": "38:49",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا ذِكْرٌ ۚ وَإِنَّ لِلْمُتَّقِينَ لَحُسْنَ مَـَٔابٍ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4020,
      "verse_number": 50,
      "verse_key": "38:50",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "جَنَّـٰتِ عَدْنٍ مُّفَتَّحَةً لَّهُمُ ٱلْأَبْوَٰبُ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4021,
      "verse_number": 51,
      "verse_key": "38:51",
      "hizb_number": 46,
      "rub_el_hizb_number": 182,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "مُتَّكِـِٔينَ فِيهَا يَدْعُونَ فِيهَا بِفَـٰكِهَةٍ كَثِيرَةٍ وَشَرَابٍ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4022,
      "verse_number": 52,
      "verse_key": "38:52",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "۞ وَعِندَهُمْ قَـٰصِرَٰتُ ٱلطَّرْفِ أَتْرَابٌ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4023,
      "verse_number": 53,
      "verse_key": "38:53",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا مَا تُوعَدُونَ لِيَوْمِ ٱلْحِسَابِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4024,
      "verse_number": 54,
      "verse_key": "38:54",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ هَـٰذَا لَرِزْقُنَا مَا لَهُۥ مِن نَّفَادٍ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4025,
      "verse_number": 55,
      "verse_key": "38:55",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا ۚ وَإِنَّ لِلطَّـٰغِينَ لَشَرَّ مَـَٔابٍ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4026,
      "verse_number": 56,
      "verse_key": "38:56",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "جَهَنَّمَ يَصْلَوْنَهَا فَبِئْسَ ٱلْمِهَادُ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4027,
      "verse_number": 57,
      "verse_key": "38:57",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا فَلْيَذُوقُوهُ حَمِيمٌ وَغَسَّاقٌ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4028,
      "verse_number": 58,
      "verse_key": "38:58",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَءَاخَرُ مِن شَكْلِهِۦٓ أَزْوَٰجٌ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4029,
      "verse_number": 59,
      "verse_key": "38:59",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا فَوْجٌ مُّقْتَحِمٌ مَّعَكُمْ ۖ لَا مَرْحَبًۢا بِهِمْ ۚ إِنَّهُمْ صَالُوا۟ ٱلنَّارِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4030,
      "verse_number": 60,
      "verse_key": "38:60",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ بَلْ أَنتُمْ لَا مَرْحَبًۢا بِكُمْ ۖ أَنتُمْ قَدَّمْتُمُوهُ لَنَا ۖ فَبِئْسَ ٱلْقَرَارُ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4031,
      "verse_number": 61,
      "verse_key": "38:61",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ رَبَّنَا مَن قَدَّمَ لَنَا هَـٰذَا فَزِدْهُ عَذَابًا ضِعْفًا فِى ٱلنَّارِ",
      "page_number": 456,
      "juz_number": 23
    },
    {
      "id": 4032,
      "verse_number": 62,
      "verse_key": "38:62",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ مَا لَنَا لَا نَرَىٰ رِجَالًا كُنَّا نَعُدُّهُم مِّنَ ٱلْأَشْرَارِ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4033,
      "verse_number": 63,
      "verse_key": "38:63",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَتَّخَذْنَـٰهُمْ سِخْرِيًّا أَمْ زَاغَتْ عَنْهُمُ ٱلْأَبْصَـٰرُ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4034,
      "verse_number": 64,
      "verse_key": "38:64",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 396,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ذَٰلِكَ لَحَقٌّ تَخَاصُمُ أَهْلِ ٱلنَّارِ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4035,
      "verse_number": 65,
      "verse_key": "38:65",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قُلْ إِنَّمَآ أَنَا۠ مُنذِرٌ ۖ وَمَا مِنْ إِلَـٰهٍ إِلَّا ٱللَّهُ ٱلْوَٰحِدُ ٱلْقَهَّارُ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4036,
      "verse_number": 66,
      "verse_key": "38:66",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "رَبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلْعَزِيزُ ٱلْغَفَّـٰرُ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4037,
      "verse_number": 67,
      "verse_key": "38:67",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قُلْ هُوَ نَبَؤٌا۟ عَظِيمٌ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4038,
      "verse_number": 68,
      "verse_key": "38:68",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَنتُمْ عَنْهُ مُعْرِضُونَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4039,
      "verse_number": 69,
      "verse_key": "38:69",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "مَا كَانَ لِىَ مِنْ عِلْمٍۭ بِٱلْمَلَإِ ٱلْأَعْلَىٰٓ إِذْ يَخْتَصِمُونَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4040,
      "verse_number": 70,
      "verse_key": "38:70",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِن يُوحَىٰٓ إِلَىَّ إِلَّآ أَنَّمَآ أَنَا۠ نَذِيرٌ مُّبِينٌ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4041,
      "verse_number": 71,
      "verse_key": "38:71",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِذْ قَالَ رَبُّكَ لِلْمَلَـٰٓئِكَةِ إِنِّى خَـٰلِقٌۢ بَشَرًا مِّن طِينٍ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4042,
      "verse_number": 72,
      "verse_key": "38:72",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا سَوَّيْتُهُۥ وَنَفَخْتُ فِيهِ مِن رُّوحِى فَقَعُوا۟ لَهُۥ سَـٰجِدِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4043,
      "verse_number": 73,
      "verse_key": "38:73",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَسَجَدَ ٱلْمَلَـٰٓئِكَةُ كُلُّهُمْ أَجْمَعُونَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4044,
      "verse_number": 74,
      "verse_key": "38:74",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِلَّآ إِبْلِيسَ ٱسْتَكْبَرَ وَكَانَ مِنَ ٱلْكَـٰفِرِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4045,
      "verse_number": 75,
      "verse_key": "38:75",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ يَـٰٓإِبْلِيسُ مَا مَنَعَكَ أَن تَسْجُدَ لِمَا خَلَقْتُ بِيَدَىَّ ۖ أَسْتَكْبَرْتَ أَمْ كُنتَ مِنَ ٱلْعَالِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4046,
      "verse_number": 76,
      "verse_key": "38:76",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ أَنَا۠ خَيْرٌ مِّنْهُ ۖ خَلَقْتَنِى مِن نَّارٍ وَخَلَقْتَهُۥ مِن طِينٍ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4047,
      "verse_number": 77,
      "verse_key": "38:77",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَٱخْرُجْ مِنْهَا فَإِنَّكَ رَجِيمٌ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4048,
      "verse_number": 78,
      "verse_key": "38:78",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ عَلَيْكَ لَعْنَتِىٓ إِلَىٰ يَوْمِ ٱلدِّينِ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4049,
      "verse_number": 79,
      "verse_key": "38:79",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّ فَأَنظِرْنِىٓ إِلَىٰ يَوْمِ يُبْعَثُونَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4050,
      "verse_number": 80,
      "verse_key": "38:80",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَإِنَّكَ مِنَ ٱلْمُنظَرِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4051,
      "verse_number": 81,
      "verse_key": "38:81",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ يَوْمِ ٱلْوَقْتِ ٱلْمَعْلُومِ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4052,
      "verse_number": 82,
      "verse_key": "38:82",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَبِعِزَّتِكَ لَأُغْوِيَنَّهُمْ أَجْمَعِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4053,
      "verse_number": 83,
      "verse_key": "38:83",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا عِبَادَكَ مِنْهُمُ ٱلْمُخْلَصِينَ",
      "page_number": 457,
      "juz_number": 23
    },
    {
      "id": 4054,
      "verse_number": 84,
      "verse_key": "38:84",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَٱلْحَقُّ وَٱلْحَقَّ أَقُولُ",
      "page_number": 458,
      "juz_number": 23
    },
    {
      "id": 4055,
      "verse_number": 85,
      "verse_key": "38:85",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "لَأَمْلَأَنَّ جَهَنَّمَ مِنكَ وَمِمَّن تَبِعَكَ مِنْهُمْ أَجْمَعِينَ",
      "page_number": 458,
      "juz_number": 23
    },
    {
      "id": 4056,
      "verse_number": 86,
      "verse_key": "38:86",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قُلْ مَآ أَسْـَٔلُكُمْ عَلَيْهِ مِنْ أَجْرٍ وَمَآ أَنَا۠ مِنَ ٱلْمُتَكَلِّفِينَ",
      "page_number": 458,
      "juz_number": 23
    },
    {
      "id": 4057,
      "verse_number": 87,
      "verse_key": "38:87",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
      "page_number": 458,
      "juz_number": 23
    },
    {
      "id": 4058,
      "verse_number": 88,
      "verse_key": "38:88",
      "hizb_number": 46,
      "rub_el_hizb_number": 183,
      "ruku_number": 397,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَتَعْلَمُنَّ نَبَأَهُۥ بَعْدَ حِينٍۭ",
      "page_number": 458,
      "juz_number": 23
    }
  ]
}