{
  "id": 96,
  "name": "The Clot",
  "verses": [
    {
      "id": 6107,
      "verse_number": 1,
      "verse_key": "96:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928772,
          "resource_id": 135,
          "text": "بخوان به نام پروردگارت که [هستی را] آفرید؛"
        }
      ]
    },
    {
      "id": 6108,
      "verse_number": 2,
      "verse_key": "96:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928773,
          "resource_id": 135,
          "text": "[همان پروردگاری که] انسان را از خونِ بسته آفرید."
        }
      ]
    },
    {
      "id": 6109,
      "verse_number": 3,
      "verse_key": "96:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928774,
          "resource_id": 135,
          "text": "بخوان و پروردگارت [از همه] بزرگوار‌تر است."
        }
      ]
    },
    {
      "id": 6110,
      "verse_number": 4,
      "verse_key": "96:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928775,
          "resource_id": 135,
          "text": "[همان] ذاتی ‌که به وسیلۀ قلم [نوشتن] آموخت."
        }
      ]
    },
    {
      "id": 6111,
      "verse_number": 5,
      "verse_key": "96:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928776,
          "resource_id": 135,
          "text": "به انسان آنچه را که نمی‌دانست [به تدریج] آموخت."
        }
      ]
    },
    {
      "id": 6112,
      "verse_number": 6,
      "verse_key": "96:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928777,
          "resource_id": 135,
          "text": "چنین نیست [که شما می‌پندارید]؛ حقا که انسان طغیان [و سرکشی] می‌کند"
        }
      ]
    },
    {
      "id": 6113,
      "verse_number": 7,
      "verse_key": "96:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928778,
          "resource_id": 135,
          "text": "[بعد] از اینکه خود را بی‌نیاز [و توانگر] ببیند."
        }
      ]
    },
    {
      "id": 6114,
      "verse_number": 8,
      "verse_key": "96:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928779,
          "resource_id": 135,
          "text": "یقیناً بازگشت [همه] به سوی پروردگار توست."
        }
      ]
    },
    {
      "id": 6115,
      "verse_number": 9,
      "verse_key": "96:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928780,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا دیده‌ای آن کسی [= ابوجهل] را که [از عبادت الله] باز می‌دارد"
        }
      ]
    },
    {
      "id": 6116,
      "verse_number": 10,
      "verse_key": "96:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928781,
          "resource_id": 135,
          "text": "بنده‌ای [= محمد] را هنگامی که [کنار کعبه] نماز می‌خواند."
        }
      ]
    },
    {
      "id": 6117,
      "verse_number": 11,
      "verse_key": "96:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928782,
          "resource_id": 135,
          "text": "به من خبر بده اگر این [بنده بازداشته‌شده] بر [راه] هدایت باشد"
        }
      ]
    },
    {
      "id": 6118,
      "verse_number": 12,
      "verse_key": "96:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928783,
          "resource_id": 135,
          "text": "یا [مردم را] به پرهیزگاری فرمان دهد، [سزای کسی‌ که چنین بنده‌ای را بیازارد چیست؟]"
        }
      ]
    },
    {
      "id": 6119,
      "verse_number": 13,
      "verse_key": "96:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928784,
          "resource_id": 135,
          "text": "به من خبر بده اگر این [شخص سرکش، حق را] تکذیب کند و رویگردان شود [آیا سزاوار عذاب نیست؟]."
        }
      ]
    },
    {
      "id": 6120,
      "verse_number": 14,
      "verse_key": "96:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928785,
          "resource_id": 135,
          "text": "آیا نمی‌داند که بی‌تردید، الله [همه اعمالش را] می‌بیند؟"
        }
      ]
    },
    {
      "id": 6121,
      "verse_number": 15,
      "verse_key": "96:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928786,
          "resource_id": 135,
          "text": "چنین نیست [که او گمان می‌کند]؛ اگر باز نیاید [و دست از شرارت برندارد،] قطعاً موی پیشانی [او] را به شدّت خواهیم گرفت [و به سوی دوزخ خواهیم کشاند]؛"
        }
      ]
    },
    {
      "id": 6122,
      "verse_number": 16,
      "verse_key": "96:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928787,
          "resource_id": 135,
          "text": "موی پیشانیِ [همان] دروغگوی خطاکار را."
        }
      ]
    },
    {
      "id": 6123,
      "verse_number": 17,
      "verse_key": "96:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928788,
          "resource_id": 135,
          "text": "پس باید هم‌مجلسانش را [به کمک] بخواند."
        }
      ]
    },
    {
      "id": 6124,
      "verse_number": 18,
      "verse_key": "96:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928789,
          "resource_id": 135,
          "text": "ما [نیز] آتشبانان [دوزخ] را فرا خواهیم ‌خواند."
        }
      ]
    },
    {
      "id": 6125,
      "verse_number": 19,
      "verse_key": "96:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 540,
      "manzil_number": 7,
      "sajdah_number": 14,
      "page_number": 597,
      "juz_number": 30,
      "translations": [
        {
          "id": 928790,
          "resource_id": 135,
          "text": "چنین نیست [که او می‌پندارد]؛ هرگز او را اطاعت مکن و سجده کن، و [به الله] تقرّب جوی."
        }
      ]
    }
  ]
}