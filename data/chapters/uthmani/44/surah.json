      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ذُقْ إِنَّكَ أَنتَ ٱلْعَزِيزُ ٱلْكَرِيمُ",
      "page_number": 498,
      "juz_number": 25
