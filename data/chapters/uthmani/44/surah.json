      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "كَٱلْمُهْلِ يَغْلِى فِى ٱلْبُطُونِ",
      "page_number": 498,
      "juz_number": 25
    },
    {
      "id": 4460,
      "verse_number": 46,
