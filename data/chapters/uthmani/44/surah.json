      "verse_key": "44:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "كَذَٰلِكَ وَزَوَّجْنَـٰهُم بِحُورٍ عِينٍ",
