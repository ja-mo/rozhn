      "hizb_number": 50,
      "rub_el_hizb_number": 199,
      "ruku_number": 435,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "طَعَامُ ٱلْأَثِيمِ",
      "page_number": 498,
