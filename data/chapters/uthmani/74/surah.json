      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ",
      "page_number": 575,
      "juz_number": 29
    },
    {
      "id": 5511,
