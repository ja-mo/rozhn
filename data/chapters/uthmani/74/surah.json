      "verse_key": "74:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ فَكَّرَ وَقَدَّرَ",
