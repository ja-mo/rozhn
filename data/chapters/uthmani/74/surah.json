      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ نَظَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5517,
      "verse_number": 22,
      "verse_key": "74:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ عَبَسَ وَبَسَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5518,
      "verse_number": 23,
      "verse_key": "74:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَدْبَرَ وَٱسْتَكْبَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5519,
      "verse_number": 24,
      "verse_key": "74:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ إِنْ هَـٰذَآ إِلَّا سِحْرٌ يُؤْثَرُ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5520,
      "verse_number": 25,
      "verse_key": "74:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هَـٰذَآ إِلَّا قَوْلُ ٱلْبَشَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5521,
      "verse_number": 26,
      "verse_key": "74:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَأُصْلِيهِ سَقَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5522,
      "verse_number": 27,
      "verse_key": "74:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا سَقَرُ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5523,
      "verse_number": 28,
      "verse_key": "74:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَا تُبْقِى وَلَا تَذَرُ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5524,
      "verse_number": 29,
      "verse_key": "74:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَوَّاحَةٌ لِّلْبَشَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5525,
      "verse_number": 30,
      "verse_key": "74:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلَيْهَا تِسْعَةَ عَشَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5526,
      "verse_number": 31,
      "verse_key": "74:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا جَعَلْنَآ أَصْحَـٰبَ ٱلنَّارِ إِلَّا مَلَـٰٓئِكَةً ۙ وَمَا جَعَلْنَا عِدَّتَهُمْ إِلَّا فِتْنَةً لِّلَّذِينَ كَفَرُوا۟ لِيَسْتَيْقِنَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَـٰبَ وَيَزْدَادَ ٱلَّذِينَ ءَامَنُوٓا۟ إِيمَـٰنًا ۙ وَلَا يَرْتَابَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَـٰبَ وَٱلْمُؤْمِنُونَ ۙ وَلِيَقُولَ ٱلَّذِينَ فِى قُلُوبِهِم مَّرَضٌ وَٱلْكَـٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَـٰذَا مَثَلًا ۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهْدِى مَن يَشَآءُ ۚ وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَ ۚ وَمَا هِىَ إِلَّا ذِكْرَىٰ لِلْبَشَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5527,
      "verse_number": 32,
      "verse_key": "74:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا وَٱلْقَمَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5528,
      "verse_number": 33,
      "verse_key": "74:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّيْلِ إِذْ أَدْبَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5529,
      "verse_number": 34,
      "verse_key": "74:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلصُّبْحِ إِذَآ أَسْفَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5530,
      "verse_number": 35,
      "verse_key": "74:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهَا لَإِحْدَى ٱلْكُبَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5531,
      "verse_number": 36,
      "verse_key": "74:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "نَذِيرًا لِّلْبَشَرِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5532,
      "verse_number": 37,
      "verse_key": "74:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِمَن شَآءَ مِنكُمْ أَن يَتَقَدَّمَ أَوْ يَتَأَخَّرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5533,
      "verse_number": 38,
      "verse_key": "74:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كُلُّ نَفْسٍۭ بِمَا كَسَبَتْ رَهِينَةٌ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5534,
      "verse_number": 39,
      "verse_key": "74:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَّآ أَصْحَـٰبَ ٱلْيَمِينِ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5535,
      "verse_number": 40,
      "verse_key": "74:40",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى جَنَّـٰتٍ يَتَسَآءَلُونَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5536,
      "verse_number": 41,
      "verse_key": "74:41",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَنِ ٱلْمُجْرِمِينَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5537,
      "verse_number": 42,
      "verse_key": "74:42",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا سَلَكَكُمْ فِى سَقَرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5538,
      "verse_number": 43,
      "verse_key": "74:43",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ لَمْ نَكُ مِنَ ٱلْمُصَلِّينَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5539,
      "verse_number": 44,
      "verse_key": "74:44",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَمْ نَكُ نُطْعِمُ ٱلْمِسْكِينَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5540,
      "verse_number": 45,
      "verse_key": "74:45",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 513,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكُنَّا نَخُوضُ مَعَ ٱلْخَآئِضِينَ",
      "page_number": 576,
      "juz_number": 29
    },
