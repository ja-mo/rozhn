    },
    {
      "id": 5514,
      "verse_number": 19,
      "verse_key": "74:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقُتِلَ كَيْفَ قَدَّرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5515,
      "verse_number": 20,
      "verse_key": "74:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 230,
      "ruku_number": 512,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ",
      "page_number": 576,
      "juz_number": 29
    },
    {
      "id": 5516,
