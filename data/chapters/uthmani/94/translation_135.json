{
  "id": 94,
  "name": "The Relief",
  "verses": [
    {
      "id": 6091,
      "verse_number": 1,
      "verse_key": "94:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928756,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا ما سینه‌ات را [برای نزول وحی] برایت نگشودیم؟"
        }
      ]
    },
    {
      "id": 6092,
      "verse_number": 2,
      "verse_key": "94:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928757,
          "resource_id": 135,
          "text": "و سنگینی [بارِ گناه و دشواریِ رسالت] را از [دوشِ] تو برنداشتیم؟"
        }
      ]
    },
    {
      "id": 6093,
      "verse_number": 3,
      "verse_key": "94:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928758,
          "resource_id": 135,
          "text": "همان [باری] که بر پشتت سنگینی می‌کرد."
        }
      ]
    },
    {
      "id": 6094,
      "verse_number": 4,
      "verse_key": "94:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928759,
          "resource_id": 135,
          "text": "و [نام و] آوازۀ تو را بلند ساختیم."
        }
      ]
    },
    {
      "id": 6095,
      "verse_number": 5,
      "verse_key": "94:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928760,
          "resource_id": 135,
          "text": "مسلماً با [هر] دشواری، آسانی است."
        }
      ]
    },
    {
      "id": 6096,
      "verse_number": 6,
      "verse_key": "94:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928761,
          "resource_id": 135,
          "text": "[آری؛] مسلماً با [هر] دشواری، آسانی است."
        }
      ]
    },
    {
      "id": 6097,
      "verse_number": 7,
      "verse_key": "94:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928762,
          "resource_id": 135,
          "text": "پس هنگامی‌ که [از کار و امور دنیا] فارغ شدی، [به عبادت پروردگارت] بکوش"
        }
      ]
    },
    {
      "id": 6098,
      "verse_number": 8,
      "verse_key": "94:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 538,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928763,
          "resource_id": 135,
          "text": "و به سوی پروردگارت راغب [و مشتاق] شو."
        }
      ]
    }
  ]
}