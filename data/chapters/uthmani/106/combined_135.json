{
  "id": 106,
  "name": "Quraysh",
  "verses": [
    {
      "id": 6194,
      "verse_number": 1,
      "verse_key": "106:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " لِإِيلَـٰفِ قُرَيْشٍ",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928859,
          "resource_id": 135,
          "text": "برای [سپاسگزاری از] همبستگی و اُنس قریش،"
        }
      ]
    },
    {
      "id": 6195,
      "verse_number": 2,
      "verse_key": "106:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِۦلَـٰفِهِمْ رِحْلَةَ ٱلشِّتَآءِ وَٱلصَّيْفِ",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928860,
          "resource_id": 135,
          "text": "[همان] همبستگی و اُنسِ آنان در سفر‌های زمستانی [به یَمَن] و تابستانی [به شام]؛"
        }
      ]
    },
    {
      "id": 6196,
      "verse_number": 3,
      "verse_key": "106:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلْيَعْبُدُوا۟ رَبَّ هَـٰذَا ٱلْبَيْتِ",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928861,
          "resource_id": 135,
          "text": "باید پروردگارِ خانۀ کعبه را عبادت کنند؛"
        }
      ]
    },
    {
      "id": 6197,
      "verse_number": 4,
      "verse_key": "106:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 550,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِىٓ أَطْعَمَهُم مِّن جُوعٍ وَءَامَنَهُم مِّنْ خَوْفٍۭ",
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928862,
          "resource_id": 135,
          "text": "همان [پروردگاری] که آنان را از گرسنگی [رهانید، به آنان] خوراک داد و از وحشت و ترس ایمن نمود."
        }
      ]
    }
  ]
}