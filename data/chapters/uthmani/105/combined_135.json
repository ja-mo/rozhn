{
  "id": 105,
  "name": "The Elephant",
  "verses": [
    {
      "id": 6189,
      "verse_number": 1,
      "verse_key": "105:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَـٰبِ ٱلْفِيلِ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 928854,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا ندیدی پروردگارت با اصحاب فیل چه کرد؟"
        }
      ]
    },
    {
      "id": 6190,
      "verse_number": 2,
      "verse_key": "105:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 928855,
          "resource_id": 135,
          "text": "آیا [فریب و] نقشۀ آنان را در تباهی قرار نداد؟"
        }
      ]
    },
    {
      "id": 6191,
      "verse_number": 3,
      "verse_key": "105:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 928856,
          "resource_id": 135,
          "text": "و پرندگانی گروه‌گروه بر [سر] آنان فرستاد"
        }
      ]
    },
    {
      "id": 6192,
      "verse_number": 4,
      "verse_key": "105:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 928857,
          "resource_id": 135,
          "text": "[که] با سنگ‌گِل [که در منقار و چنگال داشتند] آنان را سنگباران می‌کردند؛"
        }
      ]
    },
    {
      "id": 6193,
      "verse_number": 5,
      "verse_key": "105:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 549,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍۭ",
      "page_number": 601,
      "juz_number": 30,
      "translations": [
        {
          "id": 928858,
          "resource_id": 135,
          "text": "و آنها را همچون کاهِ جویده‌شده ساخت."
        }
      ]
    }
  ]
}