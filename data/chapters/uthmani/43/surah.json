{
  "id": 43,
  "name": "The Ornaments of Gold",
  "verses": [
    {
      "id": 4326,
      "verse_number": 1,
      "verse_key": "43:1",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": " حمٓ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4327,
      "verse_number": 2,
      "verse_key": "43:2",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْكِتَـٰبِ ٱلْمُبِينِ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4328,
      "verse_number": 3,
      "verse_key": "43:3",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا جَعَلْنَـٰهُ قُرْءَٰنًا عَرَبِيًّا لَّعَلَّكُمْ تَعْقِلُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4329,
      "verse_number": 4,
      "verse_key": "43:4",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُۥ فِىٓ أُمِّ ٱلْكِتَـٰبِ لَدَيْنَا لَعَلِىٌّ حَكِيمٌ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4330,
      "verse_number": 5,
      "verse_key": "43:5",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَفَنَضْرِبُ عَنكُمُ ٱلذِّكْرَ صَفْحًا أَن كُنتُمْ قَوْمًا مُّسْرِفِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4331,
      "verse_number": 6,
      "verse_key": "43:6",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَكَمْ أَرْسَلْنَا مِن نَّبِىٍّ فِى ٱلْأَوَّلِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4332,
      "verse_number": 7,
      "verse_key": "43:7",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يَأْتِيهِم مِّن نَّبِىٍّ إِلَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4333,
      "verse_number": 8,
      "verse_key": "43:8",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَأَهْلَكْنَآ أَشَدَّ مِنْهُم بَطْشًا وَمَضَىٰ مَثَلُ ٱلْأَوَّلِينَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4334,
      "verse_number": 9,
      "verse_key": "43:9",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَئِن سَأَلْتَهُم مَّنْ خَلَقَ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضَ لَيَقُولُنَّ خَلَقَهُنَّ ٱلْعَزِيزُ ٱلْعَلِيمُ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4335,
      "verse_number": 10,
      "verse_key": "43:10",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى جَعَلَ لَكُمُ ٱلْأَرْضَ مَهْدًا وَجَعَلَ لَكُمْ فِيهَا سُبُلًا لَّعَلَّكُمْ تَهْتَدُونَ",
      "page_number": 489,
      "juz_number": 25
    },
    {
      "id": 4336,
      "verse_number": 11,
      "verse_key": "43:11",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِى نَزَّلَ مِنَ ٱلسَّمَآءِ مَآءًۢ بِقَدَرٍ فَأَنشَرْنَا بِهِۦ بَلْدَةً مَّيْتًا ۚ كَذَٰلِكَ تُخْرَجُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4337,
      "verse_number": 12,
      "verse_key": "43:12",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِى خَلَقَ ٱلْأَزْوَٰجَ كُلَّهَا وَجَعَلَ لَكُم مِّنَ ٱلْفُلْكِ وَٱلْأَنْعَـٰمِ مَا تَرْكَبُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4338,
      "verse_number": 13,
      "verse_key": "43:13",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "لِتَسْتَوُۥا۟ عَلَىٰ ظُهُورِهِۦ ثُمَّ تَذْكُرُوا۟ نِعْمَةَ رَبِّكُمْ إِذَا ٱسْتَوَيْتُمْ عَلَيْهِ وَتَقُولُوا۟ سُبْحَـٰنَ ٱلَّذِى سَخَّرَ لَنَا هَـٰذَا وَمَا كُنَّا لَهُۥ مُقْرِنِينَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4339,
      "verse_number": 14,
      "verse_key": "43:14",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّآ إِلَىٰ رَبِّنَا لَمُنقَلِبُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4340,
      "verse_number": 15,
      "verse_key": "43:15",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلُوا۟ لَهُۥ مِنْ عِبَادِهِۦ جُزْءًا ۚ إِنَّ ٱلْإِنسَـٰنَ لَكَفُورٌ مُّبِينٌ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4341,
      "verse_number": 16,
      "verse_key": "43:16",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمِ ٱتَّخَذَ مِمَّا يَخْلُقُ بَنَاتٍ وَأَصْفَىٰكُم بِٱلْبَنِينَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4342,
      "verse_number": 17,
      "verse_key": "43:17",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا بُشِّرَ أَحَدُهُم بِمَا ضَرَبَ لِلرَّحْمَـٰنِ مَثَلًا ظَلَّ وَجْهُهُۥ مُسْوَدًّا وَهُوَ كَظِيمٌ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4343,
      "verse_number": 18,
      "verse_key": "43:18",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَوَمَن يُنَشَّؤُا۟ فِى ٱلْحِلْيَةِ وَهُوَ فِى ٱلْخِصَامِ غَيْرُ مُبِينٍ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4344,
      "verse_number": 19,
      "verse_key": "43:19",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلُوا۟ ٱلْمَلَـٰٓئِكَةَ ٱلَّذِينَ هُمْ عِبَـٰدُ ٱلرَّحْمَـٰنِ إِنَـٰثًا ۚ أَشَهِدُوا۟ خَلْقَهُمْ ۚ سَتُكْتَبُ شَهَـٰدَتُهُمْ وَيُسْـَٔلُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4345,
      "verse_number": 20,
      "verse_key": "43:20",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ لَوْ شَآءَ ٱلرَّحْمَـٰنُ مَا عَبَدْنَـٰهُم ۗ مَّا لَهُم بِذَٰلِكَ مِنْ عِلْمٍ ۖ إِنْ هُمْ إِلَّا يَخْرُصُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4346,
      "verse_number": 21,
      "verse_key": "43:21",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ ءَاتَيْنَـٰهُمْ كِتَـٰبًا مِّن قَبْلِهِۦ فَهُم بِهِۦ مُسْتَمْسِكُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4347,
      "verse_number": 22,
      "verse_key": "43:22",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "بَلْ قَالُوٓا۟ إِنَّا وَجَدْنَآ ءَابَآءَنَا عَلَىٰٓ أُمَّةٍ وَإِنَّا عَلَىٰٓ ءَاثَـٰرِهِم مُّهْتَدُونَ",
      "page_number": 490,
      "juz_number": 25
    },
    {
      "id": 4348,
      "verse_number": 23,
      "verse_key": "43:23",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَكَذَٰلِكَ مَآ أَرْسَلْنَا مِن قَبْلِكَ فِى قَرْيَةٍ مِّن نَّذِيرٍ إِلَّا قَالَ مُتْرَفُوهَآ إِنَّا وَجَدْنَآ ءَابَآءَنَا عَلَىٰٓ أُمَّةٍ وَإِنَّا عَلَىٰٓ ءَاثَـٰرِهِم مُّقْتَدُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4349,
      "verse_number": 24,
      "verse_key": "43:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "۞ قَـٰلَ أَوَلَوْ جِئْتُكُم بِأَهْدَىٰ مِمَّا وَجَدتُّمْ عَلَيْهِ ءَابَآءَكُمْ ۖ قَالُوٓا۟ إِنَّا بِمَآ أُرْسِلْتُم بِهِۦ كَـٰفِرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4350,
      "verse_number": 25,
      "verse_key": "43:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَٱنتَقَمْنَا مِنْهُمْ ۖ فَٱنظُرْ كَيْفَ كَانَ عَـٰقِبَةُ ٱلْمُكَذِّبِينَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4351,
      "verse_number": 26,
      "verse_key": "43:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِذْ قَالَ إِبْرَٰهِيمُ لِأَبِيهِ وَقَوْمِهِۦٓ إِنَّنِى بَرَآءٌ مِّمَّا تَعْبُدُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4352,
      "verse_number": 27,
      "verse_key": "43:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا ٱلَّذِى فَطَرَنِى فَإِنَّهُۥ سَيَهْدِينِ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4353,
      "verse_number": 28,
      "verse_key": "43:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلَهَا كَلِمَةًۢ بَاقِيَةً فِى عَقِبِهِۦ لَعَلَّهُمْ يَرْجِعُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4354,
      "verse_number": 29,
      "verse_key": "43:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "بَلْ مَتَّعْتُ هَـٰٓؤُلَآءِ وَءَابَآءَهُمْ حَتَّىٰ جَآءَهُمُ ٱلْحَقُّ وَرَسُولٌ مُّبِينٌ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4355,
      "verse_number": 30,
      "verse_key": "43:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَمَّا جَآءَهُمُ ٱلْحَقُّ قَالُوا۟ هَـٰذَا سِحْرٌ وَإِنَّا بِهِۦ كَـٰفِرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4356,
      "verse_number": 31,
      "verse_key": "43:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ لَوْلَا نُزِّلَ هَـٰذَا ٱلْقُرْءَانُ عَلَىٰ رَجُلٍ مِّنَ ٱلْقَرْيَتَيْنِ عَظِيمٍ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4357,
      "verse_number": 32,
      "verse_key": "43:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَهُمْ يَقْسِمُونَ رَحْمَتَ رَبِّكَ ۚ نَحْنُ قَسَمْنَا بَيْنَهُم مَّعِيشَتَهُمْ فِى ٱلْحَيَوٰةِ ٱلدُّنْيَا ۚ وَرَفَعْنَا بَعْضَهُمْ فَوْقَ بَعْضٍ دَرَجَـٰتٍ لِّيَتَّخِذَ بَعْضُهُم بَعْضًا سُخْرِيًّا ۗ وَرَحْمَتُ رَبِّكَ خَيْرٌ مِّمَّا يَجْمَعُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4358,
      "verse_number": 33,
      "verse_key": "43:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَوْلَآ أَن يَكُونَ ٱلنَّاسُ أُمَّةً وَٰحِدَةً لَّجَعَلْنَا لِمَن يَكْفُرُ بِٱلرَّحْمَـٰنِ لِبُيُوتِهِمْ سُقُفًا مِّن فِضَّةٍ وَمَعَارِجَ عَلَيْهَا يَظْهَرُونَ",
      "page_number": 491,
      "juz_number": 25
    },
    {
      "id": 4359,
      "verse_number": 34,
      "verse_key": "43:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلِبُيُوتِهِمْ أَبْوَٰبًا وَسُرُرًا عَلَيْهَا يَتَّكِـُٔونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4360,
      "verse_number": 35,
      "verse_key": "43:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَزُخْرُفًا ۚ وَإِن كُلُّ ذَٰلِكَ لَمَّا مَتَـٰعُ ٱلْحَيَوٰةِ ٱلدُّنْيَا ۚ وَٱلْـَٔاخِرَةُ عِندَ رَبِّكَ لِلْمُتَّقِينَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4361,
      "verse_number": 36,
      "verse_key": "43:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَن يَعْشُ عَن ذِكْرِ ٱلرَّحْمَـٰنِ نُقَيِّضْ لَهُۥ شَيْطَـٰنًا فَهُوَ لَهُۥ قَرِينٌ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4362,
      "verse_number": 37,
      "verse_key": "43:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُمْ لَيَصُدُّونَهُمْ عَنِ ٱلسَّبِيلِ وَيَحْسَبُونَ أَنَّهُم مُّهْتَدُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4363,
      "verse_number": 38,
      "verse_key": "43:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰٓ إِذَا جَآءَنَا قَالَ يَـٰلَيْتَ بَيْنِى وَبَيْنَكَ بُعْدَ ٱلْمَشْرِقَيْنِ فَبِئْسَ ٱلْقَرِينُ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4364,
      "verse_number": 39,
      "verse_key": "43:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَن يَنفَعَكُمُ ٱلْيَوْمَ إِذ ظَّلَمْتُمْ أَنَّكُمْ فِى ٱلْعَذَابِ مُشْتَرِكُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4365,
      "verse_number": 40,
      "verse_key": "43:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَفَأَنتَ تُسْمِعُ ٱلصُّمَّ أَوْ تَهْدِى ٱلْعُمْىَ وَمَن كَانَ فِى ضَلَـٰلٍ مُّبِينٍ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4366,
      "verse_number": 41,
      "verse_key": "43:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَإِمَّا نَذْهَبَنَّ بِكَ فَإِنَّا مِنْهُم مُّنتَقِمُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4367,
      "verse_number": 42,
      "verse_key": "43:42",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَوْ نُرِيَنَّكَ ٱلَّذِى وَعَدْنَـٰهُمْ فَإِنَّا عَلَيْهِم مُّقْتَدِرُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4368,
      "verse_number": 43,
      "verse_key": "43:43",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَٱسْتَمْسِكْ بِٱلَّذِىٓ أُوحِىَ إِلَيْكَ ۖ إِنَّكَ عَلَىٰ صِرَٰطٍ مُّسْتَقِيمٍ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4369,
      "verse_number": 44,
      "verse_key": "43:44",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُۥ لَذِكْرٌ لَّكَ وَلِقَوْمِكَ ۖ وَسَوْفَ تُسْـَٔلُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4370,
      "verse_number": 45,
      "verse_key": "43:45",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَسْـَٔلْ مَنْ أَرْسَلْنَا مِن قَبْلِكَ مِن رُّسُلِنَآ أَجَعَلْنَا مِن دُونِ ٱلرَّحْمَـٰنِ ءَالِهَةً يُعْبَدُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4371,
      "verse_number": 46,
      "verse_key": "43:46",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَرْسَلْنَا مُوسَىٰ بِـَٔايَـٰتِنَآ إِلَىٰ فِرْعَوْنَ وَمَلَإِي۟هِۦ فَقَالَ إِنِّى رَسُولُ رَبِّ ٱلْعَـٰلَمِينَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4372,
      "verse_number": 47,
      "verse_key": "43:47",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَلَمَّا جَآءَهُم بِـَٔايَـٰتِنَآ إِذَا هُم مِّنْهَا يَضْحَكُونَ",
      "page_number": 492,
      "juz_number": 25
    },
    {
      "id": 4373,
      "verse_number": 48,
      "verse_key": "43:48",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَا نُرِيهِم مِّنْ ءَايَةٍ إِلَّا هِىَ أَكْبَرُ مِنْ أُخْتِهَا ۖ وَأَخَذْنَـٰهُم بِٱلْعَذَابِ لَعَلَّهُمْ يَرْجِعُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4374,
      "verse_number": 49,
      "verse_key": "43:49",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ يَـٰٓأَيُّهَ ٱلسَّاحِرُ ٱدْعُ لَنَا رَبَّكَ بِمَا عَهِدَ عِندَكَ إِنَّنَا لَمُهْتَدُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4375,
      "verse_number": 50,
      "verse_key": "43:50",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَلَمَّا كَشَفْنَا عَنْهُمُ ٱلْعَذَابَ إِذَا هُمْ يَنكُثُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4376,
      "verse_number": 51,
      "verse_key": "43:51",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَنَادَىٰ فِرْعَوْنُ فِى قَوْمِهِۦ قَالَ يَـٰقَوْمِ أَلَيْسَ لِى مُلْكُ مِصْرَ وَهَـٰذِهِ ٱلْأَنْهَـٰرُ تَجْرِى مِن تَحْتِىٓ ۖ أَفَلَا تُبْصِرُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4377,
      "verse_number": 52,
      "verse_key": "43:52",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ أَنَا۠ خَيْرٌ مِّنْ هَـٰذَا ٱلَّذِى هُوَ مَهِينٌ وَلَا يَكَادُ يُبِينُ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4378,
      "verse_number": 53,
      "verse_key": "43:53",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَلَوْلَآ أُلْقِىَ عَلَيْهِ أَسْوِرَةٌ مِّن ذَهَبٍ أَوْ جَآءَ مَعَهُ ٱلْمَلَـٰٓئِكَةُ مُقْتَرِنِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4379,
      "verse_number": 54,
      "verse_key": "43:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَٱسْتَخَفَّ قَوْمَهُۥ فَأَطَاعُوهُ ۚ إِنَّهُمْ كَانُوا۟ قَوْمًا فَـٰسِقِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4380,
      "verse_number": 55,
      "verse_key": "43:55",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَلَمَّآ ءَاسَفُونَا ٱنتَقَمْنَا مِنْهُمْ فَأَغْرَقْنَـٰهُمْ أَجْمَعِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4381,
      "verse_number": 56,
      "verse_key": "43:56",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلْنَـٰهُمْ سَلَفًا وَمَثَلًا لِّلْـَٔاخِرِينَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4382,
      "verse_number": 57,
      "verse_key": "43:57",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "۞ وَلَمَّا ضُرِبَ ٱبْنُ مَرْيَمَ مَثَلًا إِذَا قَوْمُكَ مِنْهُ يَصِدُّونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4383,
      "verse_number": 58,
      "verse_key": "43:58",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوٓا۟ ءَأَـٰلِهَتُنَا خَيْرٌ أَمْ هُوَ ۚ مَا ضَرَبُوهُ لَكَ إِلَّا جَدَلًۢا ۚ بَلْ هُمْ قَوْمٌ خَصِمُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4384,
      "verse_number": 59,
      "verse_key": "43:59",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا عَبْدٌ أَنْعَمْنَا عَلَيْهِ وَجَعَلْنَـٰهُ مَثَلًا لِّبَنِىٓ إِسْرَٰٓءِيلَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4385,
      "verse_number": 60,
      "verse_key": "43:60",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَوْ نَشَآءُ لَجَعَلْنَا مِنكُم مَّلَـٰٓئِكَةً فِى ٱلْأَرْضِ يَخْلُفُونَ",
      "page_number": 493,
      "juz_number": 25
    },
    {
      "id": 4386,
      "verse_number": 61,
      "verse_key": "43:61",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّهُۥ لَعِلْمٌ لِّلسَّاعَةِ فَلَا تَمْتَرُنَّ بِهَا وَٱتَّبِعُونِ ۚ هَـٰذَا صِرَٰطٌ مُّسْتَقِيمٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4387,
      "verse_number": 62,
      "verse_key": "43:62",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يَصُدَّنَّكُمُ ٱلشَّيْطَـٰنُ ۖ إِنَّهُۥ لَكُمْ عَدُوٌّ مُّبِينٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4388,
      "verse_number": 63,
      "verse_key": "43:63",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَمَّا جَآءَ عِيسَىٰ بِٱلْبَيِّنَـٰتِ قَالَ قَدْ جِئْتُكُم بِٱلْحِكْمَةِ وَلِأُبَيِّنَ لَكُم بَعْضَ ٱلَّذِى تَخْتَلِفُونَ فِيهِ ۖ فَٱتَّقُوا۟ ٱللَّهَ وَأَطِيعُونِ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4389,
      "verse_number": 64,
      "verse_key": "43:64",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱللَّهَ هُوَ رَبِّى وَرَبُّكُمْ فَٱعْبُدُوهُ ۚ هَـٰذَا صِرَٰطٌ مُّسْتَقِيمٌ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4390,
      "verse_number": 65,
      "verse_key": "43:65",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَٱخْتَلَفَ ٱلْأَحْزَابُ مِنۢ بَيْنِهِمْ ۖ فَوَيْلٌ لِّلَّذِينَ ظَلَمُوا۟ مِنْ عَذَابِ يَوْمٍ أَلِيمٍ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4391,
      "verse_number": 66,
      "verse_key": "43:66",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "هَلْ يَنظُرُونَ إِلَّا ٱلسَّاعَةَ أَن تَأْتِيَهُم بَغْتَةً وَهُمْ لَا يَشْعُرُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4392,
      "verse_number": 67,
      "verse_key": "43:67",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱلْأَخِلَّآءُ يَوْمَئِذٍۭ بَعْضُهُمْ لِبَعْضٍ عَدُوٌّ إِلَّا ٱلْمُتَّقِينَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4393,
      "verse_number": 68,
      "verse_key": "43:68",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "يَـٰعِبَادِ لَا خَوْفٌ عَلَيْكُمُ ٱلْيَوْمَ وَلَآ أَنتُمْ تَحْزَنُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4394,
      "verse_number": 69,
      "verse_key": "43:69",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ ءَامَنُوا۟ بِـَٔايَـٰتِنَا وَكَانُوا۟ مُسْلِمِينَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4395,
      "verse_number": 70,
      "verse_key": "43:70",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "ٱدْخُلُوا۟ ٱلْجَنَّةَ أَنتُمْ وَأَزْوَٰجُكُمْ تُحْبَرُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4396,
      "verse_number": 71,
      "verse_key": "43:71",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "يُطَافُ عَلَيْهِم بِصِحَافٍ مِّن ذَهَبٍ وَأَكْوَابٍ ۖ وَفِيهَا مَا تَشْتَهِيهِ ٱلْأَنفُسُ وَتَلَذُّ ٱلْأَعْيُنُ ۖ وَأَنتُمْ فِيهَا خَـٰلِدُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4397,
      "verse_number": 72,
      "verse_key": "43:72",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَتِلْكَ ٱلْجَنَّةُ ٱلَّتِىٓ أُورِثْتُمُوهَا بِمَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4398,
      "verse_number": 73,
      "verse_key": "43:73",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "لَكُمْ فِيهَا فَـٰكِهَةٌ كَثِيرَةٌ مِّنْهَا تَأْكُلُونَ",
      "page_number": 494,
      "juz_number": 25
    },
    {
      "id": 4399,
      "verse_number": 74,
      "verse_key": "43:74",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُجْرِمِينَ فِى عَذَابِ جَهَنَّمَ خَـٰلِدُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4400,
      "verse_number": 75,
      "verse_key": "43:75",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "لَا يُفَتَّرُ عَنْهُمْ وَهُمْ فِيهِ مُبْلِسُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4401,
      "verse_number": 76,
      "verse_key": "43:76",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَمَا ظَلَمْنَـٰهُمْ وَلَـٰكِن كَانُوا۟ هُمُ ٱلظَّـٰلِمِينَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4402,
      "verse_number": 77,
      "verse_key": "43:77",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَنَادَوْا۟ يَـٰمَـٰلِكُ لِيَقْضِ عَلَيْنَا رَبُّكَ ۖ قَالَ إِنَّكُم مَّـٰكِثُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4403,
      "verse_number": 78,
      "verse_key": "43:78",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ جِئْنَـٰكُم بِٱلْحَقِّ وَلَـٰكِنَّ أَكْثَرَكُمْ لِلْحَقِّ كَـٰرِهُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4404,
      "verse_number": 79,
      "verse_key": "43:79",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ أَبْرَمُوٓا۟ أَمْرًا فَإِنَّا مُبْرِمُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4405,
      "verse_number": 80,
      "verse_key": "43:80",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "أَمْ يَحْسَبُونَ أَنَّا لَا نَسْمَعُ سِرَّهُمْ وَنَجْوَىٰهُم ۚ بَلَىٰ وَرُسُلُنَا لَدَيْهِمْ يَكْتُبُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4406,
      "verse_number": 81,
      "verse_key": "43:81",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "قُلْ إِن كَانَ لِلرَّحْمَـٰنِ وَلَدٌ فَأَنَا۠ أَوَّلُ ٱلْعَـٰبِدِينَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4407,
      "verse_number": 82,
      "verse_key": "43:82",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "سُبْحَـٰنَ رَبِّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ رَبِّ ٱلْعَرْشِ عَمَّا يَصِفُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4408,
      "verse_number": 83,
      "verse_key": "43:83",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَذَرْهُمْ يَخُوضُوا۟ وَيَلْعَبُوا۟ حَتَّىٰ يُلَـٰقُوا۟ يَوْمَهُمُ ٱلَّذِى يُوعَدُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4409,
      "verse_number": 84,
      "verse_key": "43:84",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلَّذِى فِى ٱلسَّمَآءِ إِلَـٰهٌ وَفِى ٱلْأَرْضِ إِلَـٰهٌ ۚ وَهُوَ ٱلْحَكِيمُ ٱلْعَلِيمُ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4410,
      "verse_number": 85,
      "verse_key": "43:85",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَتَبَارَكَ ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا وَعِندَهُۥ عِلْمُ ٱلسَّاعَةِ وَإِلَيْهِ تُرْجَعُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4411,
      "verse_number": 86,
      "verse_key": "43:86",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يَمْلِكُ ٱلَّذِينَ يَدْعُونَ مِن دُونِهِ ٱلشَّفَـٰعَةَ إِلَّا مَن شَهِدَ بِٱلْحَقِّ وَهُمْ يَعْلَمُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4412,
      "verse_number": 87,
      "verse_key": "43:87",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَلَئِن سَأَلْتَهُم مَّنْ خَلَقَهُمْ لَيَقُولُنَّ ٱللَّهُ ۖ فَأَنَّىٰ يُؤْفَكُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4413,
      "verse_number": 88,
      "verse_key": "43:88",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "وَقِيلِهِۦ يَـٰرَبِّ إِنَّ هَـٰٓؤُلَآءِ قَوْمٌ لَّا يُؤْمِنُونَ",
      "page_number": 495,
      "juz_number": 25
    },
    {
      "id": 4414,
      "verse_number": 89,
      "verse_key": "43:89",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "text_uthmani": "فَٱصْفَحْ عَنْهُمْ وَقُلْ سَلَـٰمٌ ۚ فَسَوْفَ يَعْلَمُونَ",
      "page_number": 495,
      "juz_number": 25
    }
  ]
}