{
  "id": 43,
  "name": "The Ornaments of Gold",
  "verses": [
    {
      "id": 4326,
      "verse_number": 1,
      "verse_key": "43:1",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926991,
          "resource_id": 135,
          "text": "حم [حا. میم]."
        }
      ]
    },
    {
      "id": 4327,
      "verse_number": 2,
      "verse_key": "43:2",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926992,
          "resource_id": 135,
          "text": "سوگند به کتاب روشنگر."
        }
      ]
    },
    {
      "id": 4328,
      "verse_number": 3,
      "verse_key": "43:3",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926993,
          "resource_id": 135,
          "text": "ما آن را قرآنى به زبان عربی قرار دادیم، باشد که خردورزى كنید."
        }
      ]
    },
    {
      "id": 4329,
      "verse_number": 4,
      "verse_key": "43:4",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926994,
          "resource_id": 135,
          "text": "این كتاب، كه بلندمرتبه و حكمت‌آموز است، در لوح محفوظ نزد ما ثبت شده است [و دستخوش تغییر نخواهد شد]."
        }
      ]
    },
    {
      "id": 4330,
      "verse_number": 5,
      "verse_key": "43:5",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926995,
          "resource_id": 135,
          "text": "آیا به صِرف اینكه شما [مشرکان،] گروهی اسرافکارید [و پیام الهی را انكار مى‌كنید، نزول] قرآن را از شما بازداریم؟"
        }
      ]
    },
    {
      "id": 4331,
      "verse_number": 6,
      "verse_key": "43:6",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926996,
          "resource_id": 135,
          "text": "چه بسیار پیامبرانى كه در میان پیشینیان فرستادیم."
        }
      ]
    },
    {
      "id": 4332,
      "verse_number": 7,
      "verse_key": "43:7",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926997,
          "resource_id": 135,
          "text": "و[لی] هر پیامبری به سوی آنان می‌آمد، مسخره‌اش می‌کردند."
        }
      ]
    },
    {
      "id": 4333,
      "verse_number": 8,
      "verse_key": "43:8",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926998,
          "resource_id": 135,
          "text": "ما افرادی نیرومند‌تر از مشرکان مکه را هلاک کرده‌ایم؛ و داستان [عبرت‌انگیز] پیشینیان گذشت."
        }
      ]
    },
    {
      "id": 4334,
      "verse_number": 9,
      "verse_key": "43:9",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 926999,
          "resource_id": 135,
          "text": "[ای پیامبر] اگر از مشرکان بپرسی: «آسمان‌ها و زمین را چه کسی آفریده است؟»، قطعاً می‌گویند: «[الله] شکست‌ناپذیر دانا»."
        }
      ]
    },
    {
      "id": 4335,
      "verse_number": 10,
      "verse_key": "43:10",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 489,
      "juz_number": 25,
      "translations": [
        {
          "id": 927000,
          "resource_id": 135,
          "text": "همان [پروردگاری] كه زمین را محل آسایش شما قرار داد و راه‌هایی در آن برایتان پدید آورد تا راه یابید."
        }
      ]
    },
    {
      "id": 4336,
      "verse_number": 11,
      "verse_key": "43:11",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927001,
          "resource_id": 135,
          "text": "و همان ذاتی كه باران را از آسمان به مقدار معینى فرستاد؛ آنگاه سرزمین خزان‌زده و خشک را به وسیلۀ آن حیات بخشیدیم؛ شما نیز همین گونه [از گورها] بیرون آورده مى‌شوید."
        }
      ]
    },
    {
      "id": 4337,
      "verse_number": 12,
      "verse_key": "43:12",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927002,
          "resource_id": 135,
          "text": "پروردگاری كه تمام مخلوقات را آفرید و از کشتی‌ها و چهارپایان، وسیلۀ سوارى برایتان پدید آورد؛"
        }
      ]
    },
    {
      "id": 4338,
      "verse_number": 13,
      "verse_key": "43:13",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927003,
          "resource_id": 135,
          "text": "تا بر پشت آنها قرار گیرید و آنگاه كه بر آنها قرار گرفتید، نعمت پروردگارتان را یاد كنید و بگویید: «پاک و منزه است ذاتی كه این [وسیله] را در خدمت ما گماشت و[گرنه] ما خود توان این کار را نداشتیم؛"
        }
      ]
    },
    {
      "id": 4339,
      "verse_number": 14,
      "verse_key": "43:14",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927004,
          "resource_id": 135,
          "text": "و ما به پیشگاهِ پروردگار‌مان بازمی‌گردیم»."
        }
      ]
    },
    {
      "id": 4340,
      "verse_number": 15,
      "verse_key": "43:15",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 426,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927005,
          "resource_id": 135,
          "text": "مشرکان از میان بندگان الله، سهمى [از فرزند] برایش قرار دادند. به راستى كه انسان آشكارا ناسپاس است."
        }
      ]
    },
    {
      "id": 4341,
      "verse_number": 16,
      "verse_key": "43:16",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927006,
          "resource_id": 135,
          "text": "آیا [می‌پندارید] الله از میان آفریدگانش، دختران را انتخاب كرده و پسران را به شما اختصاص داده است؟"
        }
      ]
    },
    {
      "id": 4342,
      "verse_number": 17,
      "verse_key": "43:17",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927007,
          "resource_id": 135,
          "text": "اما هر گاه به یكى از آنان، همان چیزی را بشارت دهند که براى [الله] رحمان قائل هستند [= تولد دختر]، در حالى كه خشم خود را فرومى‌خورد، چهره‌اش [از اندوه] تیره مى‌گردد."
        }
      ]
    },
    {
      "id": 4343,
      "verse_number": 18,
      "verse_key": "43:18",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927008,
          "resource_id": 135,
          "text": "آیا کسی را که در زیور [و راحتی] پرورش می‌یابد و در [هنگام] جدال [قادر به] تبیین مقصودش نیست [به الله نسبت می‌دهید؟]"
        }
      ]
    },
    {
      "id": 4344,
      "verse_number": 19,
      "verse_key": "43:19",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927009,
          "resource_id": 135,
          "text": "[مشرکان]، فرشتگان را كه بندگانِ [الله] رحمانند، مؤنث پنداشتند. آیا به هنگام آفرینش آنان حضور داشتند؟ سخنانشان ثبت می‌گردد و [در روز قیامت،] بازخواست خواهند شد."
        }
      ]
    },
    {
      "id": 4345,
      "verse_number": 20,
      "verse_key": "43:20",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927010,
          "resource_id": 135,
          "text": "[آنان برای توجیهِ شرکِ خود] می‌گویند: «اگر [الله] رحمان می‌خواست، ما فرشتگان را عبادت نمی‌کردیم». آنان در مورد این [سخن] بى‌اطلاعند [و] جز سخن دروغ نمى‌گویند."
        }
      ]
    },
    {
      "id": 4346,
      "verse_number": 21,
      "verse_key": "43:21",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927011,
          "resource_id": 135,
          "text": "آیا پیش از قرآن كتابى به آنان داده‌ایم كه به آن استدلال كنند؟"
        }
      ]
    },
    {
      "id": 4347,
      "verse_number": 22,
      "verse_key": "43:22",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 490,
      "juz_number": 25,
      "translations": [
        {
          "id": 927012,
          "resource_id": 135,
          "text": "بلکه می‌گویند: «نیاکانمان نیز همین آیین را داشتند و ما [نیز] راه آنان را دنبال می‌کنیم»."
        }
      ]
    },
    {
      "id": 4348,
      "verse_number": 23,
      "verse_key": "43:23",
      "hizb_number": 49,
      "rub_el_hizb_number": 196,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927013,
          "resource_id": 135,
          "text": "پیش از تو نیز هر [پیامبرِ] هشداردهنده‌ای را به هر شهری فرستادیم، ثروتمندانِ سرکشِ آن شهر می‌گفتند: «نیاکانمان نیز همین آیین را داشتند و ما [نیز] راه آنان را دنبال می‌کنیم»."
        }
      ]
    },
    {
      "id": 4349,
      "verse_number": 24,
      "verse_key": "43:24",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927014,
          "resource_id": 135,
          "text": "[پیامبر‌شان] می‌گفت: «اگر آیینى هدایت‌بخش‌تر از آنچه نیاکانتان داشتند آورده باشم [باز هم از آنان پیروی می‌کنید]؟» آنان [در پاسخ] می‌گفتند: «[آری،] پیامى را كه مأمور [ابلاغ] آن شده‌اید، انكار مى‌كنیم»."
        }
      ]
    },
    {
      "id": 4350,
      "verse_number": 25,
      "verse_key": "43:25",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 427,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927015,
          "resource_id": 135,
          "text": "ما نیز از آنان انتقام گرفتیم؛ پس بنگر که سرانجامِ تكذیب‌كنندگان چگونه بود."
        }
      ]
    },
    {
      "id": 4351,
      "verse_number": 26,
      "verse_key": "43:26",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927016,
          "resource_id": 135,
          "text": "[یاد کن] هنگامی را ‌که ابراهیم به پدر و قومش گفت: «از آنچه شما عبادت می‌کنید، بیزارم؛"
        }
      ]
    },
    {
      "id": 4352,
      "verse_number": 27,
      "verse_key": "43:27",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927017,
          "resource_id": 135,
          "text": "مگر از ذاتی كه مرا آفریده است كه او هدایتم خواهد كرد»."
        }
      ]
    },
    {
      "id": 4353,
      "verse_number": 28,
      "verse_key": "43:28",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927018,
          "resource_id": 135,
          "text": "ابراهیم، یكتاپرستى را در میانِ نسل‌های پس از خود، سخنى ماندگار قرار داد؛ باشد که [به پیشگاه الهی] بازگردند."
        }
      ]
    },
    {
      "id": 4354,
      "verse_number": 29,
      "verse_key": "43:29",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927019,
          "resource_id": 135,
          "text": "بلکه اینان [که در مکه آلوده به شرک هستند] و پدرانشان را [از نعمت‌های دنیا] برخوردار كردم تا آنكه حق [= قرآن] و پیامبری روشنگر به سراغشان آمد؛"
        }
      ]
    },
    {
      "id": 4355,
      "verse_number": 30,
      "verse_key": "43:30",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927020,
          "resource_id": 135,
          "text": "و آنگاه كه حق [= قرآن] به سویشان آمد، گفتند: «این جادوست و آن را انكار مى‌كنیم»؛"
        }
      ]
    },
    {
      "id": 4356,
      "verse_number": 31,
      "verse_key": "43:31",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927021,
          "resource_id": 135,
          "text": "و گفتند: «چرا این قرآن بر مردى بزرگ از دو شهرِ [مکه و طائف] نازل نشده است؟»"
        }
      ]
    },
    {
      "id": 4357,
      "verse_number": 32,
      "verse_key": "43:32",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927022,
          "resource_id": 135,
          "text": "آیا آنان رحمت پروردگارت را تقسیم مى‌كنند؟ ماییم كه روزی [و معیشت] آنان را در زندگى دنیا میانشان تقسیم كرده‌ایم و آنان را نسبت به هم، به درجاتى برتری بخشیده‌ایم تا از خدماتِ [متقابلِ] یكدیگر استفاده كنند؛ و رحمت پروردگارت از اموالى كه جمع مى‌كنند بهتر است."
        }
      ]
    },
    {
      "id": 4358,
      "verse_number": 33,
      "verse_key": "43:33",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 491,
      "juz_number": 25,
      "translations": [
        {
          "id": 927023,
          "resource_id": 135,
          "text": "اگر [نگرانى از آن] نبود كه همۀ مردم [در دنیاطلبى و انكار الله] یك گروه ‌شوند، سقف خانۀ كسانى را كه به الله رحمان ایمان ندارند، از نقره قرار مى‌دادیم و نیز پله‌هایى [از نقره در آن می‌نهادیم] كه بتوانند از [طبقاتِ] آن [خانه] بالا روند؛"
        }
      ]
    },
    {
      "id": 4359,
      "verse_number": 34,
      "verse_key": "43:34",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927024,
          "resource_id": 135,
          "text": "و برای خانه‌هایشان در‌هایی [از طلا] می‌گذاشتیم و تخت‌هایی [قرار می‌دادیم] که بر آن تکیه زنند؛"
        }
      ]
    },
    {
      "id": 4360,
      "verse_number": 35,
      "verse_key": "43:35",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 428,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927025,
          "resource_id": 135,
          "text": "و تجمل [و زینت‌های] فراوان [برایشان فراهم می‌کردیم] و تمام اینها بهرۀ [ناچیز] زندگی دنیاست؛ و آخرت نزد پروردگارت برای پرهیزگاران است."
        }
      ]
    },
    {
      "id": 4361,
      "verse_number": 36,
      "verse_key": "43:36",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927026,
          "resource_id": 135,
          "text": "هر کس از یاد [الله] رحمان دل بگرداند، شیطانى بر او مى‌گماریم تا [یار و] همنشینش باشد [و همواره او را فریب دهد]."
        }
      ]
    },
    {
      "id": 4362,
      "verse_number": 37,
      "verse_key": "43:37",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927027,
          "resource_id": 135,
          "text": "شیاطین، این گروه را از راه [الله] بازمی‌دارند؛ و[لی] آنان گمان می‌کنند که هدایت‌یافته‌اند."
        }
      ]
    },
    {
      "id": 4363,
      "verse_number": 38,
      "verse_key": "43:38",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927028,
          "resource_id": 135,
          "text": "تا آنگاه كه [در قیامت] نزد ما بیاید، [به شیطانِ همنشینش] می‌گوید: «ای كاش میان من و تو، فاصله‌اى به دورى شرق و غرب بود! چه همنشین بدی بودى!»."
        }
      ]
    },
    {
      "id": 4364,
      "verse_number": 39,
      "verse_key": "43:39",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927029,
          "resource_id": 135,
          "text": "[الله می‌فرماید:] «امروز [پشیمانى] سودى به حالتان نخواهد داشت؛ زیرا ستم كرده‌اید و همگی در عذاب شریكید»."
        }
      ]
    },
    {
      "id": 4365,
      "verse_number": 40,
      "verse_key": "43:40",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927030,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا می‌توانی [پیام خود را] به گوش ناشنوایان برسانى یا نابینایان و گمراهانِ آشكار را [به راه راست] هدایت كنى؟"
        }
      ]
    },
    {
      "id": 4366,
      "verse_number": 41,
      "verse_key": "43:41",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927031,
          "resource_id": 135,
          "text": "پس اگر تو را [پیش از عذاب آنان از دنیا] ببریم، [در هر حال،] از آنان انتقام خواهیم گرفت."
        }
      ]
    },
    {
      "id": 4367,
      "verse_number": 42,
      "verse_key": "43:42",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927032,
          "resource_id": 135,
          "text": "یا عذابى را كه به آنان وعده داده‌ایم، [در دوران زندگی‌ات] به تو نشان خواهیم داد؛ زیرا ما بر آنان اقتدار [کامل] داریم."
        }
      ]
    },
    {
      "id": 4368,
      "verse_number": 43,
      "verse_key": "43:43",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927033,
          "resource_id": 135,
          "text": "به آنچه بر تو وحی می‌شود تمسّک بجوی که بی‌گمان، بر راه راست قرار داری."
        }
      ]
    },
    {
      "id": 4369,
      "verse_number": 44,
      "verse_key": "43:44",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927034,
          "resource_id": 135,
          "text": "تردیدی نیست که قرآن برای تو و قومت مایۀ اندرز [و سربلندی] است و [روز قیامت، دربارۀ ایمان و عمل به آن] بازخواست خواهید شد."
        }
      ]
    },
    {
      "id": 4370,
      "verse_number": 45,
      "verse_key": "43:45",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 429,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927035,
          "resource_id": 135,
          "text": "از پیامبرانی که پیش از تو فرستادیم بپرس که آیا غیر از [الله] رحمان، معبودانی برای عبادت قرار داده‌ایم [مسلّماً چنین نیست]."
        }
      ]
    },
    {
      "id": 4371,
      "verse_number": 46,
      "verse_key": "43:46",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927036,
          "resource_id": 135,
          "text": "ما موسى را با معجزات خویش به سوى فرعون و سران قومش فرستادیم و به آنان گفت: «من فرستادۀ پروردگار جهانیانم»؛"
        }
      ]
    },
    {
      "id": 4372,
      "verse_number": 47,
      "verse_key": "43:47",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 492,
      "juz_number": 25,
      "translations": [
        {
          "id": 927037,
          "resource_id": 135,
          "text": "و هنگامی که آیات [و معجزات] ما را به آنان ارائه كرد، همگی به آنها خندیدند."
        }
      ]
    },
    {
      "id": 4373,
      "verse_number": 48,
      "verse_key": "43:48",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927038,
          "resource_id": 135,
          "text": "هر معجزه‌اى به آنان ارائه دادیم، یكى از دیگرى بزرگ‌تر بود و [چون همه را تكذیب كردند،] آنان را به عذاب گرفتار كردیم؛ باشد که [از کفر] بازگردند."
        }
      ]
    },
    {
      "id": 4374,
      "verse_number": 49,
      "verse_key": "43:49",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927039,
          "resource_id": 135,
          "text": "[پس هنگامی که گوشه‌ای از عذاب الهی را چشیدند،] گفتند: «اى جادوگر! از پروردگارت ـ بنا به عهدى كه با تو دارد ـ بخواه [تا عذاب را از ما دور كند] كه ما [دیگر] راه‌یافته‌ایم»."
        }
      ]
    },
    {
      "id": 4375,
      "verse_number": 50,
      "verse_key": "43:50",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927040,
          "resource_id": 135,
          "text": "اما هنگامی که عذاب را از آنان برداشتیم، پیمان شكستند [و بر کفر اصرار ورزیدند]."
        }
      ]
    },
    {
      "id": 4376,
      "verse_number": 51,
      "verse_key": "43:51",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927041,
          "resource_id": 135,
          "text": "فرعون در میان قوم خود ندا سر داد و گفت: «اى قوم من، آیا فرمانروایىِ مصر در اختیار من نیست و این نهرها از دامنۀ قصرِ من جریان ندارد؟ آیا [قدرت و عظمتم را] نمى‌بینید؟"
        }
      ]
    },
    {
      "id": 4377,
      "verse_number": 52,
      "verse_key": "43:52",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927042,
          "resource_id": 135,
          "text": "من از این مردِ بى‌مقدار که [حتی] نمى‌تواند به روشنى سخن بگوید، شایسته‌ترم."
        }
      ]
    },
    {
      "id": 4378,
      "verse_number": 53,
      "verse_key": "43:53",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927043,
          "resource_id": 135,
          "text": "چرا [به نشانۀ سروَرى،] دستبند‌هاى زرین [از آسمان] بر او افكنده نشده است [تا ثابت شود که پیامبر است]؟ یا چرا فرشتگان همراه او نیامده‌اند؟»"
        }
      ]
    },
    {
      "id": 4379,
      "verse_number": 54,
      "verse_key": "43:54",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927044,
          "resource_id": 135,
          "text": "او [= فرعون] قوم خود را به [بی‌خردی و] سبُكسَرى كشاند و همگی مُطیع او شدند. حقا که آنان [نسبت به اوامر الهی] نافرمان بودند."
        }
      ]
    },
    {
      "id": 4380,
      "verse_number": 55,
      "verse_key": "43:55",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927045,
          "resource_id": 135,
          "text": "پس چون ما را به خشم آوردند، از آنان انتقام گرفتیم و همگی را [در دریا] غرق کردیم."
        }
      ]
    },
    {
      "id": 4381,
      "verse_number": 56,
      "verse_key": "43:56",
      "hizb_number": 50,
      "rub_el_hizb_number": 197,
      "ruku_number": 430,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927046,
          "resource_id": 135,
          "text": "آنگاه آنان را در شمارِ پیشگامان [کفر و نافرمانی] و عبرتی [برای آیندگان] قرار دادیم."
        }
      ]
    },
    {
      "id": 4382,
      "verse_number": 57,
      "verse_key": "43:57",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927047,
          "resource_id": 135,
          "text": "و هنگامی که [داستانِ تولد] پسر مریم مطرح شد، قومِ تو [که گمان ‌کردند می‌توانند مانند نصاری چند معبود داشته باشند] به ناگاه، شادی و هیاهو به راه‌ انداختند."
        }
      ]
    },
    {
      "id": 4383,
      "verse_number": 58,
      "verse_key": "43:58",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927048,
          "resource_id": 135,
          "text": "و گفتند: «معبودان ما بهترند یا او [= عیسی]؟» آنان این سخن را تنها براى جدال با تو عنوان كردند؛ به راستی که گروهى ستیزه‌جو هستند."
        }
      ]
    },
    {
      "id": 4384,
      "verse_number": 59,
      "verse_key": "43:59",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927049,
          "resource_id": 135,
          "text": "عیسى فقط بنده‌اى است كه نعمت [رسالت] بر او ارزانى داشتیم و او را نمونه‌ای [از قدرت الهی] براى بنی‌اسرائیل قرار دادیم."
        }
      ]
    },
    {
      "id": 4385,
      "verse_number": 60,
      "verse_key": "43:60",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 493,
      "juz_number": 25,
      "translations": [
        {
          "id": 927050,
          "resource_id": 135,
          "text": "اگر مى‌خواستیم، [همۀ شما انسان‌ها را نابود می‌کردیم و] به جای شما، فرشتگانى پدید مى‌آوردیم كه در زمین جانشین [شما] شوند."
        }
      ]
    },
    {
      "id": 4386,
      "verse_number": 61,
      "verse_key": "43:61",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927051,
          "resource_id": 135,
          "text": "قطعاً [نزول] او [= عیسی] نشانه [نزدیکی وقوع] قیامت است؛ پس هرگز در مورد قیامت شک نكنید و پیرو من باشید كه راه راست [به سوی بهشت،] همین است."
        }
      ]
    },
    {
      "id": 4387,
      "verse_number": 62,
      "verse_key": "43:62",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927052,
          "resource_id": 135,
          "text": "[آگاه باشید تا] شیطان شما را [از راه الله] بازندارد؛ چرا كه او دشمن آشكار شماست."
        }
      ]
    },
    {
      "id": 4388,
      "verse_number": 63,
      "verse_key": "43:63",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927053,
          "resource_id": 135,
          "text": "و چون عیسى [معجزات و] نشانه‌هاى روشن به آنان ارائه كرد، گفت: «من حکمت برایتان آورده‌ام تا [به آن راه یابید] و برخى از موارد اختلافتان را روشن كنم؛ پس از الله پروا کنید و مطیع [دستورهای] من باشید."
        }
      ]
    },
    {
      "id": 4389,
      "verse_number": 64,
      "verse_key": "43:64",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927054,
          "resource_id": 135,
          "text": "الله پروردگار من و شماست؛ پس او را عبادت کنید که راهِ راست همین است»."
        }
      ]
    },
    {
      "id": 4390,
      "verse_number": 65,
      "verse_key": "43:65",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927055,
          "resource_id": 135,
          "text": "آنگاه گروه‌های [مختلف] بنی‌اسرائیل میان خود [دربارۀ عیسی] دچار اختلاف شدند؛ پس وای بر ستمکاران از عذاب روز دردناک!"
        }
      ]
    },
    {
      "id": 4391,
      "verse_number": 66,
      "verse_key": "43:66",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927056,
          "resource_id": 135,
          "text": "آیا جز این انتظار دارند كه قیامت ـ در حالی که غافلند ـ ناگهان بر آنان فرا رسد؟"
        }
      ]
    },
    {
      "id": 4392,
      "verse_number": 67,
      "verse_key": "43:67",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 431,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927057,
          "resource_id": 135,
          "text": "دوستان [دنیایى،] در آن روز، دشمن یكدیگرند؛ مگر پرهیزگاران."
        }
      ]
    },
    {
      "id": 4393,
      "verse_number": 68,
      "verse_key": "43:68",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927058,
          "resource_id": 135,
          "text": "[الله به آنان می‌فرماید:] «ای بندگانِ [پرهیزكارِ] من، امروز ترس و ‌اندوهى نخواهید داشت»."
        }
      ]
    },
    {
      "id": 4394,
      "verse_number": 69,
      "verse_key": "43:69",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927059,
          "resource_id": 135,
          "text": "همان بندگانى كه به آیات ما ایمان آوردند و تسلیم [فرمانِ ما] بودند."
        }
      ]
    },
    {
      "id": 4395,
      "verse_number": 70,
      "verse_key": "43:70",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927060,
          "resource_id": 135,
          "text": "شما و دیگر مؤمنانی که همچون شما هستند، به بهشت درآیید تا [از نعمت‌های جاودانش بهره‌مند گردید و] شادمان شوید."
        }
      ]
    },
    {
      "id": 4396,
      "verse_number": 71,
      "verse_key": "43:71",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927061,
          "resource_id": 135,
          "text": "ظرف‌ها و جام‌های زرین را پیرامونشان مى‌گردانند؛ و هر چه دل‌ها بخواهد و دیده‌ها از آن لذت بَرَد، در آنجا موجود است؛ و شما در بهشت جاودانه‌اید."
        }
      ]
    },
    {
      "id": 4397,
      "verse_number": 72,
      "verse_key": "43:72",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927062,
          "resource_id": 135,
          "text": "این همان بهشتى است كه در برابر اعمالتان به شما داده‌اند."
        }
      ]
    },
    {
      "id": 4398,
      "verse_number": 73,
      "verse_key": "43:73",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 494,
      "juz_number": 25,
      "translations": [
        {
          "id": 927063,
          "resource_id": 135,
          "text": "میوه‌هاى فراوانی آنجا در اختیار دارید كه از آنها مى‌خورید."
        }
      ]
    },
    {
      "id": 4399,
      "verse_number": 74,
      "verse_key": "43:74",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927064,
          "resource_id": 135,
          "text": "بی‌تردید، مجرمان در عذاب دوزخ جاودانند."
        }
      ]
    },
    {
      "id": 4400,
      "verse_number": 75,
      "verse_key": "43:75",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927065,
          "resource_id": 135,
          "text": "[در حالى كه عذاب] از آنان كاسته نمی‌شود و در یأس و نومیدی فرورفته‌اند."
        }
      ]
    },
    {
      "id": 4401,
      "verse_number": 76,
      "verse_key": "43:76",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927066,
          "resource_id": 135,
          "text": "ما به آنان ستم نكردیم؛ بلکه آنها خود به خویشتن ستم می‌کردند."
        }
      ]
    },
    {
      "id": 4402,
      "verse_number": 77,
      "verse_key": "43:77",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927067,
          "resource_id": 135,
          "text": "[دوزخیان، نگهبان آتش را صدا می‌زنند و] فریاد برمی‌آرند: «اى مالک [كاش] پروردگارت كار ما را یكسره كند [و ما را بمیراند]». او می‌گوید: «نه، شما [در این عذاب] ماندگارید»."
        }
      ]
    },
    {
      "id": 4403,
      "verse_number": 78,
      "verse_key": "43:78",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927068,
          "resource_id": 135,
          "text": "به راستی ما حق را برای شما آوردیم؛ ولی بیشتر شما از آن کراهت داشتید."
        }
      ]
    },
    {
      "id": 4404,
      "verse_number": 79,
      "verse_key": "43:79",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927069,
          "resource_id": 135,
          "text": "بلکه آنان عزمِ [خود را برای توطئه علیه تو] جزم كردند؛ ما نیز ارادۀ محکمی [بر ناکام گذاشتنِ آنان] داریم."
        }
      ]
    },
    {
      "id": 4405,
      "verse_number": 80,
      "verse_key": "43:80",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927070,
          "resource_id": 135,
          "text": "آیا مى‌پندارند كه ما سخنان سِرّى و نجوایشان را نمى‌شنویم؟ آرى، فرشتگان ما در كنارشان [هستند و همه چیز را] ثبت مى‌كنند."
        }
      ]
    },
    {
      "id": 4406,
      "verse_number": 81,
      "verse_key": "43:81",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927071,
          "resource_id": 135,
          "text": "[ای پیامبر، به مشرکان] بگو: «اگر [به فرض محال، الله] رحمان فرزندى داشت، من نخستین عبادت‌کننده [او] بودم»."
        }
      ]
    },
    {
      "id": 4407,
      "verse_number": 82,
      "verse_key": "43:82",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927072,
          "resource_id": 135,
          "text": "[معبود یگانه‌ای كه] پروردگار آسمان‌ها و زمین و عرش است، از توصیفِ [جاهلانه] آنان منزه است."
        }
      ]
    },
    {
      "id": 4408,
      "verse_number": 83,
      "verse_key": "43:83",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927073,
          "resource_id": 135,
          "text": "پس [ای پیامبر،] آنان را [به حال خود] بگذار تا یاوه‌گویی کنند و به بازى سرگرم باشند تا سرانجام، با روز موعود [= قیامت] مواجه شوند [و سزای اعمالشان را ببینند]."
        }
      ]
    },
    {
      "id": 4409,
      "verse_number": 84,
      "verse_key": "43:84",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927074,
          "resource_id": 135,
          "text": "او تعالی است که در آسمان و زمین معبود است؛ و او حکیم داناست."
        }
      ]
    },
    {
      "id": 4410,
      "verse_number": 85,
      "verse_key": "43:85",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927075,
          "resource_id": 135,
          "text": "پربرکت و بزرگوار است ذاتی که فرمانروایی آسمان‌ها و زمین و آنچه میان آنهاست، از آنِ اوست؛ و آگاهى از [لحظۀ برپایىِ] رستاخیز تنها در اختیار اوست و [همگی] به پیشگاه او بازگردانده مى‌شوید."
        }
      ]
    },
    {
      "id": 4411,
      "verse_number": 86,
      "verse_key": "43:86",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927076,
          "resource_id": 135,
          "text": "كسانی كه [مشركان] به جاى الله [به نیایش] مى‌خوانند، قادر به شفاعت نیستند؛ مگر افرادى كه به [کلمۀ توحید و] حق گواهى مى‌دهند، در حالی‌که [نسبت به حقیقت آنچه بدان گواهی داده‌اند] دانش و آگاهی دارند."
        }
      ]
    },
    {
      "id": 4412,
      "verse_number": 87,
      "verse_key": "43:87",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927077,
          "resource_id": 135,
          "text": "و اگر از مشرکان بپرسی چه کسی آنان را آفریده است، مسلّماً می‌گویند: «الله»؛ پس چگونه [از عبادت الله] منحرف می‌شوند و [به او شرک می‌ورزند]؟!"
        }
      ]
    },
    {
      "id": 4413,
      "verse_number": 88,
      "verse_key": "43:88",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927078,
          "resource_id": 135,
          "text": "[شکایت و] سخن پیامبر [این بود:] «پروردگارا، اینان قومی هستند که ایمان نمی‌آورند»."
        }
      ]
    },
    {
      "id": 4414,
      "verse_number": 89,
      "verse_key": "43:89",
      "hizb_number": 50,
      "rub_el_hizb_number": 198,
      "ruku_number": 432,
      "manzil_number": 6,
      "sajdah_number": null,
      "page_number": 495,
      "juz_number": 25,
      "translations": [
        {
          "id": 927079,
          "resource_id": 135,
          "text": "[ای پیامبر،] از [آزار و اذیت] آنان درگذر و [برای رهایی از شرّشان] بگو: «سلام بر شما»؛ به زودى خواهند دانست [که عذابشان چگونه است]."
        }
      ]
    }
  ]
}