{
  "id": 51,
  "name": "The Winnowing Winds",
  "verses": [
    {
      "id": 4676,
      "verse_number": 1,
      "verse_key": "51:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927341,
          "resource_id": 135,
          "text": "سوگند به بادهاى خاک [و باران] ‌افشان"
        }
      ]
    },
    {
      "id": 4677,
      "verse_number": 2,
      "verse_key": "51:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927342,
          "resource_id": 135,
          "text": "و سوگند به ابرهای سنگین‌بار [از باران]"
        }
      ]
    },
    {
      "id": 4678,
      "verse_number": 3,
      "verse_key": "51:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927343,
          "resource_id": 135,
          "text": "و سوگند به کشتی‌های سبک‌سیر"
        }
      ]
    },
    {
      "id": 4679,
      "verse_number": 4,
      "verse_key": "51:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927344,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که کارها را [به امر الله] تقسیم می‌کنند"
        }
      ]
    },
    {
      "id": 4680,
      "verse_number": 5,
      "verse_key": "51:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927345,
          "resource_id": 135,
          "text": "که آنچه [در مورد پاداش و عذاب] به شما وعده داده شده حقیقت دارد؛"
        }
      ]
    },
    {
      "id": 4681,
      "verse_number": 6,
      "verse_key": "51:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927346,
          "resource_id": 135,
          "text": "و کیفر و جزا[ی اعمال] قطعاً به وقوع مى‌پیوندد."
        }
      ]
    },
    {
      "id": 4682,
      "verse_number": 7,
      "verse_key": "51:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927347,
          "resource_id": 135,
          "text": "سوگند به آسمان که [با ستارگان آراسته شده، و گویی] دارای راه‌هاست،"
        }
      ]
    },
    {
      "id": 4683,
      "verse_number": 8,
      "verse_key": "51:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927348,
          "resource_id": 135,
          "text": "كه شما [در مورد پیامبر و قرآن] اختلاف‌نظر دارید."
        }
      ]
    },
    {
      "id": 4684,
      "verse_number": 9,
      "verse_key": "51:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927349,
          "resource_id": 135,
          "text": "هر کس [از راه حق] بازگردانده شود، از [ایمان به] پیامبر و قرآن نیز بازگردانده خواهد شد."
        }
      ]
    },
    {
      "id": 4685,
      "verse_number": 10,
      "verse_key": "51:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927350,
          "resource_id": 135,
          "text": "مرگ [و لعنت] بر دروغگویان!"
        }
      ]
    },
    {
      "id": 4686,
      "verse_number": 11,
      "verse_key": "51:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927351,
          "resource_id": 135,
          "text": "همان كسانى كه در ورطۀ جهل، غافلند."
        }
      ]
    },
    {
      "id": 4687,
      "verse_number": 12,
      "verse_key": "51:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927352,
          "resource_id": 135,
          "text": "آنان [پیوسته و از روى انكار] مى‌پرسند: «روز جزا چه زمانى است؟»"
        }
      ]
    },
    {
      "id": 4688,
      "verse_number": 13,
      "verse_key": "51:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927353,
          "resource_id": 135,
          "text": "همان روزى است كه به آتش مجازات مى‌شوند؛"
        }
      ]
    },
    {
      "id": 4689,
      "verse_number": 14,
      "verse_key": "51:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927354,
          "resource_id": 135,
          "text": "[و به آنان گفته می‌شود:] «عذاب خود را بچشید؛ این همان آتشى است كه به شتاب مى‌خواستید»."
        }
      ]
    },
    {
      "id": 4690,
      "verse_number": 15,
      "verse_key": "51:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927355,
          "resource_id": 135,
          "text": "[در روز قیامت،] مسلماً پرهیزگاران در باغ‌هایی [از بهشت] و [كنار] چشمه‌سارها به سر مى‌برند؛"
        }
      ]
    },
    {
      "id": 4691,
      "verse_number": 16,
      "verse_key": "51:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927356,
          "resource_id": 135,
          "text": "آنچه پروردگارشان به آنان عطا كرده است دریافت مى‌دارند؛ زیرا در [زندگیِ] گذشته نیكوكار بوده‌اند."
        }
      ]
    },
    {
      "id": 4692,
      "verse_number": 17,
      "verse_key": "51:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927357,
          "resource_id": 135,
          "text": "آنان ‌اندكى از شب را مى‌خفتند [و بقیه را به نماز و نیایش مشغول بودند]"
        }
      ]
    },
    {
      "id": 4693,
      "verse_number": 18,
      "verse_key": "51:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927358,
          "resource_id": 135,
          "text": "و سحرگاهان [از الله] آمرزش مى‌خواستند"
        }
      ]
    },
    {
      "id": 4694,
      "verse_number": 19,
      "verse_key": "51:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927359,
          "resource_id": 135,
          "text": "و در اموالشان براى سائل و محروم، حقى [معیّن از صدقه و زکات] بود."
        }
      ]
    },
    {
      "id": 4695,
      "verse_number": 20,
      "verse_key": "51:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927360,
          "resource_id": 135,
          "text": "و در زمین نشانه‌ها[ى قدرت الهى] براى اهل یقین پدیدار است؛"
        }
      ]
    },
    {
      "id": 4696,
      "verse_number": 21,
      "verse_key": "51:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927361,
          "resource_id": 135,
          "text": "و [نیز] در وجود خودتان، آیا نمى‌بینید؟"
        }
      ]
    },
    {
      "id": 4697,
      "verse_number": 22,
      "verse_key": "51:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927362,
          "resource_id": 135,
          "text": "و رِزق و روزی‌تان و آنچه [از کیفر و پاداش و خیر و شر که] به شما وعده داده می‌شود، در آسمان است."
        }
      ]
    },
    {
      "id": 4698,
      "verse_number": 23,
      "verse_key": "51:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927363,
          "resource_id": 135,
          "text": "سوگند به پروردگار آسمان و زمین كه وعده[ی قیامت و پاداش و عذاب] همچون سخن گفتنتان، حقیقی و قطعى است."
        }
      ]
    },
    {
      "id": 4699,
      "verse_number": 24,
      "verse_key": "51:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927364,
          "resource_id": 135,
          "text": "[ای پیامبر] آیا داستان میهمانان گرامى ابراهیم به تو رسیده است؟"
        }
      ]
    },
    {
      "id": 4700,
      "verse_number": 25,
      "verse_key": "51:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927365,
          "resource_id": 135,
          "text": "آنگاه که بر او وارد شدند و سلام گفتند، او گفت: «سلام» [و زیر لب گفت:] «گروهى ناشناسند»."
        }
      ]
    },
    {
      "id": 4701,
      "verse_number": 26,
      "verse_key": "51:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927366,
          "resource_id": 135,
          "text": "آنگاه آهسته نزد خانوادۀ خود رفت و گوساله‌اى فربه [و بریان براى پذیرایى] آورد."
        }
      ]
    },
    {
      "id": 4702,
      "verse_number": 27,
      "verse_key": "51:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927367,
          "resource_id": 135,
          "text": "[او غذا را] نزد آنان گذاشت [و چند لحظه بعد] گفت: «آیا نمی‌خورید؟»"
        }
      ]
    },
    {
      "id": 4703,
      "verse_number": 28,
      "verse_key": "51:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927368,
          "resource_id": 135,
          "text": "[چون دست به غذا نزدند] از آنان احساس ترس كرد؛ گفتند: نترس [ما فرشته‌ایم]؛ و او را به [تولد] پسرى دانا بشارت دادند."
        }
      ]
    },
    {
      "id": 4704,
      "verse_number": 29,
      "verse_key": "51:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927369,
          "resource_id": 135,
          "text": "همسرش با فریاد [و شگفتى] پیش آمد و در حالى كه به صورت خود مى‌زد گفت: «پیرزنى نازا [فرزند می‌زاید]؟!»"
        }
      ]
    },
    {
      "id": 4705,
      "verse_number": 30,
      "verse_key": "51:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 521,
      "juz_number": 26,
      "translations": [
        {
          "id": 927370,
          "resource_id": 135,
          "text": "[فرشتگان] گفتند: «پروردگارت اینچنین فرموده است؛ و بی‌گمان، او حکیمِ داناست»."
        }
      ]
    },
    {
      "id": 4706,
      "verse_number": 31,
      "verse_key": "51:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927371,
          "resource_id": 135,
          "text": "[ابراهیم] گفت: «ای فرستادگان [الله،] کار و مأموریت شما چیست؟»"
        }
      ]
    },
    {
      "id": 4707,
      "verse_number": 32,
      "verse_key": "51:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927372,
          "resource_id": 135,
          "text": "[فرشتگان] گفتند: «ما برای [مجازات] قومی مجرم [و گناهکار] فرستاده شده‌ایم"
        }
      ]
    },
    {
      "id": 4708,
      "verse_number": 33,
      "verse_key": "51:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927373,
          "resource_id": 135,
          "text": "تا سنگ‌هایی از گلِ [سخت‌شده] بر سرشان فروباریم."
        }
      ]
    },
    {
      "id": 4709,
      "verse_number": 34,
      "verse_key": "51:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927374,
          "resource_id": 135,
          "text": "[سنگ‌هایی] که نزد پروردگارت، برای اسرافکاران، نشان گذاشته شده است»."
        }
      ]
    },
    {
      "id": 4710,
      "verse_number": 35,
      "verse_key": "51:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927375,
          "resource_id": 135,
          "text": "مؤمنانی را که در آن [شهرهای قوم لوط] بودند، [پیش از نزول عذاب] بیرون آوردیم؛"
        }
      ]
    },
    {
      "id": 4711,
      "verse_number": 36,
      "verse_key": "51:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927376,
          "resource_id": 135,
          "text": "ولى در آنجا فقط یک خانواده را یافتیم كه [نسبت به اوامر الهی] تسلیم بودند."
        }
      ]
    },
    {
      "id": 4712,
      "verse_number": 37,
      "verse_key": "51:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927377,
          "resource_id": 135,
          "text": "و در آن [شهرها] براى مردمى كه از عذاب دردناک [الهی] ترس دارند، نشانه‌اى روشن بر جاى گذاشتیم."
        }
      ]
    },
    {
      "id": 4713,
      "verse_number": 38,
      "verse_key": "51:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927378,
          "resource_id": 135,
          "text": "در [داستان] موسی [نیز نشانه و عبرتی است]؛ آنگاه که او را با دلیلی روشن نزد فرعون فرستادیم."
        }
      ]
    },
    {
      "id": 4714,
      "verse_number": 39,
      "verse_key": "51:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927379,
          "resource_id": 135,
          "text": "ولى [فرعون] با تكیه بر قدرتش، روی برگرداند [و حق را نپذیرفت] و گفت: «[این مرد، یا] جادوگر است یا دیوانه»."
        }
      ]
    },
    {
      "id": 4715,
      "verse_number": 40,
      "verse_key": "51:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927380,
          "resource_id": 135,
          "text": "او را همراه سپاهیانش گرفتیم و به دریا افكندیم [و هلاک کردیم]؛ در حالى كه سزاوار سرزنش بود."
        }
      ]
    },
    {
      "id": 4716,
      "verse_number": 41,
      "verse_key": "51:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927381,
          "resource_id": 135,
          "text": "در [داستان قوم] عاد [نیز درس عبرتی است]؛ آنگاه که تندبادى بی‌خیر و برکت بر آنان فرستادیم."
        }
      ]
    },
    {
      "id": 4717,
      "verse_number": 42,
      "verse_key": "51:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927382,
          "resource_id": 135,
          "text": "[تندبادی] که بر هر چه وزید، آن را خاک و خاشاک كرد."
        }
      ]
    },
    {
      "id": 4718,
      "verse_number": 43,
      "verse_key": "51:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927383,
          "resource_id": 135,
          "text": "در [داستان قوم] ثمود [نیز درس عبرتی است] آنگاه که [توسط پیامبرشان صالح] به آنان گفته شد: «اندکی [از لذت‌های زندگی] بهره‌مند شوید [که تنها سه روز فرصت دارید]»."
        }
      ]
    },
    {
      "id": 4719,
      "verse_number": 44,
      "verse_key": "51:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927384,
          "resource_id": 135,
          "text": "آنان از فرمان پروردگارشان سرپیچی کردند؛ پس در حالی که چشم به راه بودند، بانگ عذاب آنان را فراگرفت."
        }
      ]
    },
    {
      "id": 4720,
      "verse_number": 45,
      "verse_key": "51:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927385,
          "resource_id": 135,
          "text": "[چنان بر زمین افتادند که] نه توان برخاستن [و گریز] داشتند و نه یاراى دفاع."
        }
      ]
    },
    {
      "id": 4721,
      "verse_number": 46,
      "verse_key": "51:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927386,
          "resource_id": 135,
          "text": "پیش از آن، قوم نوح را كه گروهى نافرمان بودند [هلاک كردیم]."
        }
      ]
    },
    {
      "id": 4722,
      "verse_number": 47,
      "verse_key": "51:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927387,
          "resource_id": 135,
          "text": "ما آسمان را به توان [خود] بنا نهادیم و همواره آن را وسعت می‌بخشیم؛"
        }
      ]
    },
    {
      "id": 4723,
      "verse_number": 48,
      "verse_key": "51:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927388,
          "resource_id": 135,
          "text": "و زمین را [برای زندگی] گستردیم و چه نیک گستراننده‌ای هستیم!"
        }
      ]
    },
    {
      "id": 4724,
      "verse_number": 49,
      "verse_key": "51:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927389,
          "resource_id": 135,
          "text": "و همه چیز را به صورت زوج [= نر و ماده] آفریدیم؛ باشد که [توجه كنید و] پند گیرید."
        }
      ]
    },
    {
      "id": 4725,
      "verse_number": 50,
      "verse_key": "51:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927390,
          "resource_id": 135,
          "text": "[ای پیامبر، به مردم بگو:] «به سوى الله بشتابید كه من هشداردهنده‌ای آشکار از جانب او برای شما هستم؛"
        }
      ]
    },
    {
      "id": 4726,
      "verse_number": 51,
      "verse_key": "51:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 522,
      "juz_number": 27,
      "translations": [
        {
          "id": 927391,
          "resource_id": 135,
          "text": "و در كنار الله معبود دیگرى قرار ندهید كه من از [جانب] او هشداردهنده‌اى آشکار برایتان هستم»."
        }
      ]
    },
    {
      "id": 4727,
      "verse_number": 52,
      "verse_key": "51:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927392,
          "resource_id": 135,
          "text": "هر پیامبری که بر پیشینیانشان مبعوث شد، همین گونه گفتند: «[او یا] جادوگر است یا دیوانه»."
        }
      ]
    },
    {
      "id": 4728,
      "verse_number": 53,
      "verse_key": "51:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927393,
          "resource_id": 135,
          "text": "آیا یكدیگر را به این [کفر و تکذیب] سفارش كرده بودند؟ نه، بلكه مردمی سرکش بودند."
        }
      ]
    },
    {
      "id": 4729,
      "verse_number": 54,
      "verse_key": "51:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927394,
          "resource_id": 135,
          "text": "پس [ای پیامبر،] از آنان روی بگردان که تو [به خاطر کفر آنان] هرگز سزاوار سرزنش نیستی."
        }
      ]
    },
    {
      "id": 4730,
      "verse_number": 55,
      "verse_key": "51:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927395,
          "resource_id": 135,
          "text": "و [پیوسته به آنان] تذكر بده كه قطعاً تذكر براى مؤمنان سودبخش است."
        }
      ]
    },
    {
      "id": 4731,
      "verse_number": 56,
      "verse_key": "51:56",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927396,
          "resource_id": 135,
          "text": "و جن و انس را فقط برای این آفریده‌ام که مرا عبادت كنند [و از بندگىِ دیگران سر باز زنند]."
        }
      ]
    },
    {
      "id": 4732,
      "verse_number": 57,
      "verse_key": "51:57",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927397,
          "resource_id": 135,
          "text": "نه از آنان روزى مى‌خواهم، نه اینكه مرا خوراک دهند؛"
        }
      ]
    },
    {
      "id": 4733,
      "verse_number": 58,
      "verse_key": "51:58",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927398,
          "resource_id": 135,
          "text": "[زیرا] الله است که روزی‌رسانِ نیرومند و برقرار است."
        }
      ]
    },
    {
      "id": 4734,
      "verse_number": 59,
      "verse_key": "51:59",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927399,
          "resource_id": 135,
          "text": "ستمکاران [مشرک نیز] همچون یاران [گذشتۀ] خویش، سهمى از عذاب دارند؛ پس به شتاب [آن را] از من نخواهند."
        }
      ]
    },
    {
      "id": 4735,
      "verse_number": 60,
      "verse_key": "51:60",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927400,
          "resource_id": 135,
          "text": "واى بر کافران از [عذاب] روزی که به آن وعده داده می‌شوند!"
        }
      ]
    }
  ]
}