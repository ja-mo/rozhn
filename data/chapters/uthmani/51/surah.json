{
  "id": 51,
  "name": "The Winnowing Winds",
  "verses": [
    {
      "id": 4676,
      "verse_number": 1,
      "verse_key": "51:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلذَّٰرِيَـٰتِ ذَرْوًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4677,
      "verse_number": 2,
      "verse_key": "51:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْحَـٰمِلَـٰتِ وِقْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4678,
      "verse_number": 3,
      "verse_key": "51:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْجَـٰرِيَـٰتِ يُسْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4679,
      "verse_number": 4,
      "verse_key": "51:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُقَسِّمَـٰتِ أَمْرًا",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4680,
      "verse_number": 5,
      "verse_key": "51:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَا تُوعَدُونَ لَصَادِقٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4681,
      "verse_number": 6,
      "verse_key": "51:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ ٱلدِّينَ لَوَٰقِعٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4682,
      "verse_number": 7,
      "verse_key": "51:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلسَّمَآءِ ذَاتِ ٱلْحُبُكِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4683,
      "verse_number": 8,
      "verse_key": "51:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّكُمْ لَفِى قَوْلٍ مُّخْتَلِفٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4684,
      "verse_number": 9,
      "verse_key": "51:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يُؤْفَكُ عَنْهُ مَنْ أُفِكَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4685,
      "verse_number": 10,
      "verse_key": "51:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُتِلَ ٱلْخَرَّٰصُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4686,
      "verse_number": 11,
      "verse_key": "51:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ هُمْ فِى غَمْرَةٍ سَاهُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4687,
      "verse_number": 12,
      "verse_key": "51:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَسْـَٔلُونَ أَيَّانَ يَوْمُ ٱلدِّينِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4688,
      "verse_number": 13,
      "verse_key": "51:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ هُمْ عَلَى ٱلنَّارِ يُفْتَنُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4689,
      "verse_number": 14,
      "verse_key": "51:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذُوقُوا۟ فِتْنَتَكُمْ هَـٰذَا ٱلَّذِى كُنتُم بِهِۦ تَسْتَعْجِلُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4690,
      "verse_number": 15,
      "verse_key": "51:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّـٰتٍ وَعُيُونٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4691,
      "verse_number": 16,
      "verse_key": "51:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ءَاخِذِينَ مَآ ءَاتَىٰهُمْ رَبُّهُمْ ۚ إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُحْسِنِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4692,
      "verse_number": 17,
      "verse_key": "51:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَانُوا۟ قَلِيلًا مِّنَ ٱلَّيْلِ مَا يَهْجَعُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4693,
      "verse_number": 18,
      "verse_key": "51:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَبِٱلْأَسْحَارِ هُمْ يَسْتَغْفِرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4694,
      "verse_number": 19,
      "verse_key": "51:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِىٓ أَمْوَٰلِهِمْ حَقٌّ لِّلسَّآئِلِ وَٱلْمَحْرُومِ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4695,
      "verse_number": 20,
      "verse_key": "51:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِى ٱلْأَرْضِ ءَايَـٰتٌ لِّلْمُوقِنِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4696,
      "verse_number": 21,
      "verse_key": "51:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِىٓ أَنفُسِكُمْ ۚ أَفَلَا تُبْصِرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4697,
      "verse_number": 22,
      "verse_key": "51:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِى ٱلسَّمَآءِ رِزْقُكُمْ وَمَا تُوعَدُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4698,
      "verse_number": 23,
      "verse_key": "51:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 457,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَوَرَبِّ ٱلسَّمَآءِ وَٱلْأَرْضِ إِنَّهُۥ لَحَقٌّ مِّثْلَ مَآ أَنَّكُمْ تَنطِقُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4699,
      "verse_number": 24,
      "verse_key": "51:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلْ أَتَىٰكَ حَدِيثُ ضَيْفِ إِبْرَٰهِيمَ ٱلْمُكْرَمِينَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4700,
      "verse_number": 25,
      "verse_key": "51:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ دَخَلُوا۟ عَلَيْهِ فَقَالُوا۟ سَلَـٰمًا ۖ قَالَ سَلَـٰمٌ قَوْمٌ مُّنكَرُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4701,
      "verse_number": 26,
      "verse_key": "51:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَرَاغَ إِلَىٰٓ أَهْلِهِۦ فَجَآءَ بِعِجْلٍ سَمِينٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4702,
      "verse_number": 27,
      "verse_key": "51:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَرَّبَهُۥٓ إِلَيْهِمْ قَالَ أَلَا تَأْكُلُونَ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4703,
      "verse_number": 28,
      "verse_key": "51:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَوْجَسَ مِنْهُمْ خِيفَةً ۖ قَالُوا۟ لَا تَخَفْ ۖ وَبَشَّرُوهُ بِغُلَـٰمٍ عَلِيمٍ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4704,
      "verse_number": 29,
      "verse_key": "51:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَقْبَلَتِ ٱمْرَأَتُهُۥ فِى صَرَّةٍ فَصَكَّتْ وَجْهَهَا وَقَالَتْ عَجُوزٌ عَقِيمٌ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4705,
      "verse_number": 30,
      "verse_key": "51:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ كَذَٰلِكِ قَالَ رَبُّكِ ۖ إِنَّهُۥ هُوَ ٱلْحَكِيمُ ٱلْعَلِيمُ",
      "page_number": 521,
      "juz_number": 26
    },
    {
      "id": 4706,
      "verse_number": 31,
      "verse_key": "51:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "۞ قَالَ فَمَا خَطْبُكُمْ أَيُّهَا ٱلْمُرْسَلُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4707,
      "verse_number": 32,
      "verse_key": "51:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالُوٓا۟ إِنَّآ أُرْسِلْنَآ إِلَىٰ قَوْمٍ مُّجْرِمِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4708,
      "verse_number": 33,
      "verse_key": "51:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِنُرْسِلَ عَلَيْهِمْ حِجَارَةً مِّن طِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4709,
      "verse_number": 34,
      "verse_key": "51:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مُّسَوَّمَةً عِندَ رَبِّكَ لِلْمُسْرِفِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4710,
      "verse_number": 35,
      "verse_key": "51:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَخْرَجْنَا مَن كَانَ فِيهَا مِنَ ٱلْمُؤْمِنِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4711,
      "verse_number": 36,
      "verse_key": "51:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَا وَجَدْنَا فِيهَا غَيْرَ بَيْتٍ مِّنَ ٱلْمُسْلِمِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4712,
      "verse_number": 37,
      "verse_key": "51:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَتَرَكْنَا فِيهَآ ءَايَةً لِّلَّذِينَ يَخَافُونَ ٱلْعَذَابَ ٱلْأَلِيمَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4713,
      "verse_number": 38,
      "verse_key": "51:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِى مُوسَىٰٓ إِذْ أَرْسَلْنَـٰهُ إِلَىٰ فِرْعَوْنَ بِسُلْطَـٰنٍ مُّبِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4714,
      "verse_number": 39,
      "verse_key": "51:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَتَوَلَّىٰ بِرُكْنِهِۦ وَقَالَ سَـٰحِرٌ أَوْ مَجْنُونٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4715,
      "verse_number": 40,
      "verse_key": "51:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَخَذْنَـٰهُ وَجُنُودَهُۥ فَنَبَذْنَـٰهُمْ فِى ٱلْيَمِّ وَهُوَ مُلِيمٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4716,
      "verse_number": 41,
      "verse_key": "51:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِى عَادٍ إِذْ أَرْسَلْنَا عَلَيْهِمُ ٱلرِّيحَ ٱلْعَقِيمَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4717,
      "verse_number": 42,
      "verse_key": "51:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا تَذَرُ مِن شَىْءٍ أَتَتْ عَلَيْهِ إِلَّا جَعَلَتْهُ كَٱلرَّمِيمِ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4718,
      "verse_number": 43,
      "verse_key": "51:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفِى ثَمُودَ إِذْ قِيلَ لَهُمْ تَمَتَّعُوا۟ حَتَّىٰ حِينٍ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4719,
      "verse_number": 44,
      "verse_key": "51:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَعَتَوْا۟ عَنْ أَمْرِ رَبِّهِمْ فَأَخَذَتْهُمُ ٱلصَّـٰعِقَةُ وَهُمْ يَنظُرُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4720,
      "verse_number": 45,
      "verse_key": "51:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَمَا ٱسْتَطَـٰعُوا۟ مِن قِيَامٍ وَمَا كَانُوا۟ مُنتَصِرِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4721,
      "verse_number": 46,
      "verse_key": "51:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 458,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَقَوْمَ نُوحٍ مِّن قَبْلُ ۖ إِنَّهُمْ كَانُوا۟ قَوْمًا فَـٰسِقِينَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4722,
      "verse_number": 47,
      "verse_key": "51:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلسَّمَآءَ بَنَيْنَـٰهَا بِأَيْي۟دٍ وَإِنَّا لَمُوسِعُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4723,
      "verse_number": 48,
      "verse_key": "51:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْأَرْضَ فَرَشْنَـٰهَا فَنِعْمَ ٱلْمَـٰهِدُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4724,
      "verse_number": 49,
      "verse_key": "51:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمِن كُلِّ شَىْءٍ خَلَقْنَا زَوْجَيْنِ لَعَلَّكُمْ تَذَكَّرُونَ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4725,
      "verse_number": 50,
      "verse_key": "51:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَفِرُّوٓا۟ إِلَى ٱللَّهِ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌ مُّبِينٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4726,
      "verse_number": 51,
      "verse_key": "51:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا تَجْعَلُوا۟ مَعَ ٱللَّهِ إِلَـٰهًا ءَاخَرَ ۖ إِنِّى لَكُم مِّنْهُ نَذِيرٌ مُّبِينٌ",
      "page_number": 522,
      "juz_number": 27
    },
    {
      "id": 4727,
      "verse_number": 52,
      "verse_key": "51:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَٰلِكَ مَآ أَتَى ٱلَّذِينَ مِن قَبْلِهِم مِّن رَّسُولٍ إِلَّا قَالُوا۟ سَاحِرٌ أَوْ مَجْنُونٌ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4728,
      "verse_number": 53,
      "verse_key": "51:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَتَوَاصَوْا۟ بِهِۦ ۚ بَلْ هُمْ قَوْمٌ طَاغُونَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4729,
      "verse_number": 54,
      "verse_key": "51:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَتَوَلَّ عَنْهُمْ فَمَآ أَنتَ بِمَلُومٍ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4730,
      "verse_number": 55,
      "verse_key": "51:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَذَكِّرْ فَإِنَّ ٱلذِّكْرَىٰ تَنفَعُ ٱلْمُؤْمِنِينَ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4731,
      "verse_number": 56,
      "verse_key": "51:56",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا خَلَقْتُ ٱلْجِنَّ وَٱلْإِنسَ إِلَّا لِيَعْبُدُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4732,
      "verse_number": 57,
      "verse_key": "51:57",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَآ أُرِيدُ مِنْهُم مِّن رِّزْقٍ وَمَآ أُرِيدُ أَن يُطْعِمُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4733,
      "verse_number": 58,
      "verse_key": "51:58",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱللَّهَ هُوَ ٱلرَّزَّاقُ ذُو ٱلْقُوَّةِ ٱلْمَتِينُ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4734,
      "verse_number": 59,
      "verse_key": "51:59",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّ لِلَّذِينَ ظَلَمُوا۟ ذَنُوبًا مِّثْلَ ذَنُوبِ أَصْحَـٰبِهِمْ فَلَا يَسْتَعْجِلُونِ",
      "page_number": 523,
      "juz_number": 27
    },
    {
      "id": 4735,
      "verse_number": 60,
      "verse_key": "51:60",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 459,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَوَيْلٌ لِّلَّذِينَ كَفَرُوا۟ مِن يَوْمِهِمُ ٱلَّذِى يُوعَدُونَ",
      "page_number": 523,
      "juz_number": 27
    }
  ]
}