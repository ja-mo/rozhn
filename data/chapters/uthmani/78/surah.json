    },
    {
      "id": 5674,
      "verse_number": 2,
      "verse_key": "78:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5675,
      "verse_number": 3,
      "verse_key": "78:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5676,
      "verse_number": 4,
      "verse_key": "78:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا سَيَعْلَمُونَ",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5677,
      "verse_number": 5,
      "verse_key": "78:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ كَلَّا سَيَعْلَمُونَ",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5678,
      "verse_number": 6,
      "verse_key": "78:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَـٰدًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5679,
      "verse_number": 7,
      "verse_key": "78:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْجِبَالَ أَوْتَادًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5680,
      "verse_number": 8,
      "verse_key": "78:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَخَلَقْنَـٰكُمْ أَزْوَٰجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5681,
      "verse_number": 9,
      "verse_key": "78:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5682,
      "verse_number": 10,
      "verse_key": "78:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5683,
      "verse_number": 11,
      "verse_key": "78:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5684,
      "verse_number": 12,
      "verse_key": "78:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5685,
      "verse_number": 13,
      "verse_key": "78:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا سِرَاجًا وَهَّاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5686,
      "verse_number": 14,
      "verse_key": "78:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءً ثَجَّاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5687,
      "verse_number": 15,
      "verse_key": "78:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِّنُخْرِجَ بِهِۦ حَبًّا وَنَبَاتًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5688,
      "verse_number": 16,
      "verse_key": "78:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَنَّـٰتٍ أَلْفَافًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5689,
      "verse_number": 17,
      "verse_key": "78:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَـٰتًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5690,
      "verse_number": 18,
      "verse_key": "78:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5691,
      "verse_number": 19,
      "verse_key": "78:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5692,
      "verse_number": 20,
      "verse_key": "78:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5693,
      "verse_number": 21,
      "verse_key": "78:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5694,
      "verse_number": 22,
      "verse_key": "78:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِّلطَّـٰغِينَ مَـَٔابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5695,
      "verse_number": 23,
      "verse_key": "78:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّـٰبِثِينَ فِيهَآ أَحْقَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5696,
      "verse_number": 24,
      "verse_key": "78:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5697,
      "verse_number": 25,
      "verse_key": "78:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا حَمِيمًا وَغَسَّاقًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5698,
      "verse_number": 26,
      "verse_key": "78:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "جَزَآءً وِفَاقًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5699,
      "verse_number": 27,
      "verse_key": "78:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5700,
      "verse_number": 28,
      "verse_key": "78:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكَذَّبُوا۟ بِـَٔايَـٰتِنَا كِذَّابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5701,
      "verse_number": 29,
      "verse_key": "78:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكُلَّ شَىْءٍ أَحْصَيْنَـٰهُ كِتَـٰبًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5702,
      "verse_number": 30,
      "verse_key": "78:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 520,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا",
      "page_number": 582,
      "juz_number": 30
    },
    {
      "id": 5703,
      "verse_number": 31,
      "verse_key": "78:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ لِلْمُتَّقِينَ مَفَازًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5704,
      "verse_number": 32,
      "verse_key": "78:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "حَدَآئِقَ وَأَعْنَـٰبًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5705,
      "verse_number": 33,
      "verse_key": "78:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكَوَاعِبَ أَتْرَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5706,
      "verse_number": 34,
      "verse_key": "78:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكَأْسًا دِهَاقًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5707,
      "verse_number": 35,
      "verse_key": "78:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّٰبًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5708,
      "verse_number": 36,
      "verse_key": "78:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "جَزَآءً مِّن رَّبِّكَ عَطَآءً حِسَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5709,
      "verse_number": 37,
      "verse_key": "78:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "رَّبِّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَـٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5710,
      "verse_number": 38,
      "verse_key": "78:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَـٰٓئِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَـٰنُ وَقَالَ صَوَابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5711,
      "verse_number": 39,
      "verse_key": "78:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5712,
      "verse_number": 40,
      "verse_key": "78:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 521,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَنذَرْنَـٰكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَـٰلَيْتَنِى كُنتُ تُرَٰبًۢا",
      "page_number": 583,
      "juz_number": 30
    }
  ]
}