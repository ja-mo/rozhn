      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَّا تَطْغَوْا۟ فِى ٱلْمِيزَانِ",
      "page_number": 531,
      "juz_number": 27
