      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِيهَا فَـٰكِهَةٌ وَٱلنَّخْلُ ذَاتُ ٱلْأَكْمَامِ",
      "page_number": 531,
