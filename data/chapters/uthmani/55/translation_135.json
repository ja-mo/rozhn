{
  "id": 55,
  "name": "The Beneficent",
  "verses": [
    {
      "id": 4902,
      "verse_number": 1,
      "verse_key": "55:1",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927567,
          "resource_id": 135,
          "text": "[الله] رحمان"
        }
      ]
    },
    {
      "id": 4903,
      "verse_number": 2,
      "verse_key": "55:2",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927568,
          "resource_id": 135,
          "text": "قرآن را آموزش داد؛"
        }
      ]
    },
    {
      "id": 4904,
      "verse_number": 3,
      "verse_key": "55:3",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927569,
          "resource_id": 135,
          "text": "انسان را آفرید؛"
        }
      ]
    },
    {
      "id": 4905,
      "verse_number": 4,
      "verse_key": "55:4",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927570,
          "resource_id": 135,
          "text": "[و] به او سخن‌گفتن آموخت."
        }
      ]
    },
    {
      "id": 4906,
      "verse_number": 5,
      "verse_key": "55:5",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927571,
          "resource_id": 135,
          "text": "خورشید و ماه، با حسابی منظم [و دقیق] در گردِشَند؛"
        }
      ]
    },
    {
      "id": 4907,
      "verse_number": 6,
      "verse_key": "55:6",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927572,
          "resource_id": 135,
          "text": "و گیاه و درخت [برایش] سجده می‌کنند."
        }
      ]
    },
    {
      "id": 4908,
      "verse_number": 7,
      "verse_key": "55:7",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927573,
          "resource_id": 135,
          "text": "و آسمان را برافراشت و میزان [عدالت را در زمین] برقرار نمود."
        }
      ]
    },
    {
      "id": 4909,
      "verse_number": 8,
      "verse_key": "55:8",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927574,
          "resource_id": 135,
          "text": "[ای مردم، چنین کرد] تا در وزن و پیمانه، [به یکدیگر] ستم نکنید؛"
        }
      ]
    },
    {
      "id": 4910,
      "verse_number": 9,
      "verse_key": "55:9",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927575,
          "resource_id": 135,
          "text": "و سنجش [حقوق دیگران] را به عدالت برآورد كنید و در سنجش [چیزی] مکاهید [و کم‌فروشی نکنید]."
        }
      ]
    },
    {
      "id": 4911,
      "verse_number": 10,
      "verse_key": "55:10",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927576,
          "resource_id": 135,
          "text": "و زمین را براى [زندگى] انسان‌ها مقرر داشت،"
        }
      ]
    },
    {
      "id": 4912,
      "verse_number": 11,
      "verse_key": "55:11",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927577,
          "resource_id": 135,
          "text": "که [انواع] میوه‌ها و نخل‌های خوشه‌دار در آن است."
        }
      ]
    },
    {
      "id": 4913,
      "verse_number": 12,
      "verse_key": "55:12",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927578,
          "resource_id": 135,
          "text": "و [همچنین] حبوباتِ برگ‌دار و گیاهانِ خوشبو؛"
        }
      ]
    },
    {
      "id": 4914,
      "verse_number": 13,
      "verse_key": "55:13",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927579,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4915,
      "verse_number": 14,
      "verse_key": "55:14",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927580,
          "resource_id": 135,
          "text": "او انسان را از گِل خشکی همچون سفال آفرید؛"
        }
      ]
    },
    {
      "id": 4916,
      "verse_number": 15,
      "verse_key": "55:15",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927581,
          "resource_id": 135,
          "text": "و جن را از شعله‌ای از آتش خلق کرد؛"
        }
      ]
    },
    {
      "id": 4917,
      "verse_number": 16,
      "verse_key": "55:16",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 531,
      "juz_number": 27,
      "translations": [
        {
          "id": 927582,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4918,
      "verse_number": 17,
      "verse_key": "55:17",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927583,
          "resource_id": 135,
          "text": "[او] پروردگار مشرق‌ها و مغرب‌هاست؛"
        }
      ]
    },
    {
      "id": 4919,
      "verse_number": 18,
      "verse_key": "55:18",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927584,
          "resource_id": 135,
          "text": "پس [ای گروه انس و جن] کدام یک از نعمت‌های پروردگارتان را تکذیب می‌کنید؟"
        }
      ]
    },
    {
      "id": 4920,
      "verse_number": 19,
      "verse_key": "55:19",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927585,
          "resource_id": 135,
          "text": "او دو دریا[ی شور و شیرین] را روان کرد تا [به یکدیگر] برسند؛"
        }
      ]
    },
    {
      "id": 4921,
      "verse_number": 20,
      "verse_key": "55:20",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927586,
          "resource_id": 135,
          "text": "[در عین حال،] مانعی میان آن دو است که با هم نمی‌آمیزند؛"
        }
      ]
    },
    {
      "id": 4922,
      "verse_number": 21,
      "verse_key": "55:21",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927587,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4923,
      "verse_number": 22,
      "verse_key": "55:22",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927588,
          "resource_id": 135,
          "text": "از آن دو [دریا] مروارید و مرجان به دست می‌آید؛"
        }
      ]
    },
    {
      "id": 4924,
      "verse_number": 23,
      "verse_key": "55:23",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927589,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4925,
      "verse_number": 24,
      "verse_key": "55:24",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927590,
          "resource_id": 135,
          "text": "کشتی‌های كوه‌پیكرِ بادبان‌برافراشته [و جاری] در دریا برای اوست؛"
        }
      ]
    },
    {
      "id": 4926,
      "verse_number": 25,
      "verse_key": "55:25",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 468,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927591,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4927,
      "verse_number": 26,
      "verse_key": "55:26",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927592,
          "resource_id": 135,
          "text": "هر چه روی زمین وجود دارد، فناپذیر است"
        }
      ]
    },
    {
      "id": 4928,
      "verse_number": 27,
      "verse_key": "55:27",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927593,
          "resource_id": 135,
          "text": "و [تنها ذات جاوید و] روی پروردگار باشکوه و ارجمندت باقی خواهد ماند؛"
        }
      ]
    },
    {
      "id": 4929,
      "verse_number": 28,
      "verse_key": "55:28",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927594,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4930,
      "verse_number": 29,
      "verse_key": "55:29",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927595,
          "resource_id": 135,
          "text": "[تمام] کسانی که در آسمان‌ها و زمین هستند از او درخواستِ نیاز می‌کنند و او هر روز [و هر لحظه] در کاری است؛"
        }
      ]
    },
    {
      "id": 4931,
      "verse_number": 30,
      "verse_key": "55:30",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927596,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4932,
      "verse_number": 31,
      "verse_key": "55:31",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927597,
          "resource_id": 135,
          "text": "ای گروه جن و انس، به زودی به حساب شما می‌پردازیم؛"
        }
      ]
    },
    {
      "id": 4933,
      "verse_number": 32,
      "verse_key": "55:32",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927598,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4934,
      "verse_number": 33,
      "verse_key": "55:33",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927599,
          "resource_id": 135,
          "text": "ای گروه جن و انس، اگر مى‌توانید از كرانه‌هاى آسمان و زمین گذر كنید [تا از مجازات الهی برهید]، پس بگذرید؛ قادر [به این کار] نخواهید بود، مگر با قدرت [و بَیّنه فراوان که شما آن را ندارید]؛"
        }
      ]
    },
    {
      "id": 4935,
      "verse_number": 34,
      "verse_key": "55:34",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927600,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4936,
      "verse_number": 35,
      "verse_key": "55:35",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927601,
          "resource_id": 135,
          "text": "شراره‌اى از آتش و مِس گداخته بر شما باریده مى‌شود كه [در برابر آن] قادر به دفاع [از خود] نیستید؛"
        }
      ]
    },
    {
      "id": 4937,
      "verse_number": 36,
      "verse_key": "55:36",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927602,
          "resource_id": 135,
          "text": "پس [ای جن و انس] كدام یک از نعمت‌های پروردگارتان را انكار مى‌كنید؟"
        }
      ]
    },
    {
      "id": 4938,
      "verse_number": 37,
      "verse_key": "55:37",
      "hizb_number": 54,
      "rub_el_hizb_number": 213,
      "ruku_number": 469,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 532,
      "juz_number": 27,
      "translations": [
        {
          "id": 927603,
          "resource_id": 135,
          "text": "آنگاه كه آسمان بشكافد و همچون روغن گداخته، سرخ‌فام گردد [قیامت آغاز خواهد شد]."
        }
      ]
    },
    {
      "id": 4939,
      "verse_number": 38,
      "verse_key": "55:38",
      "hizb_number": 54,
