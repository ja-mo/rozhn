{
  "id": 52,
  "name": "The Mount",
  "verses": [
    {
      "id": 4736,
      "verse_number": 1,
      "verse_key": "52:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927401,
          "resource_id": 135,
          "text": "سوگند به کوه طور"
        }
      ]
    },
    {
      "id": 4737,
      "verse_number": 2,
      "verse_key": "52:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927402,
          "resource_id": 135,
          "text": "و سوگند به کتاب نوشته‌شده"
        }
      ]
    },
    {
      "id": 4738,
      "verse_number": 3,
      "verse_key": "52:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927403,
          "resource_id": 135,
          "text": "در صحیفه‌ای گشوده"
        }
      ]
    },
    {
      "id": 4739,
      "verse_number": 4,
      "verse_key": "52:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927404,
          "resource_id": 135,
          "text": "و سوگند به «بیت المَعمور» [که فرشتگان در آن، به عبادت پروردگار مشغولند]"
        }
      ]
    },
    {
      "id": 4740,
      "verse_number": 5,
      "verse_key": "52:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927405,
          "resource_id": 135,
          "text": "و سوگند به [آسمان بلند، همان] سقف برافراشته [بر زمین]"
        }
      ]
    },
    {
      "id": 4741,
      "verse_number": 6,
      "verse_key": "52:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927406,
          "resource_id": 135,
          "text": "و سوگند به دریای آکنده [از آب]"
        }
      ]
    },
    {
      "id": 4742,
      "verse_number": 7,
      "verse_key": "52:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927407,
          "resource_id": 135,
          "text": "که بی‌تردید، عذاب پروردگارت به وقوع خواهد پیوست؛"
        }
      ]
    },
    {
      "id": 4743,
      "verse_number": 8,
      "verse_key": "52:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927408,
          "resource_id": 135,
          "text": "و هیچ بازدارنده‌اى ندارد."
        }
      ]
    },
    {
      "id": 4744,
      "verse_number": 9,
      "verse_key": "52:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927409,
          "resource_id": 135,
          "text": "روزى كه آسمان به سختى در جنبش و اضطراب افتد"
        }
      ]
    },
    {
      "id": 4745,
      "verse_number": 10,
      "verse_key": "52:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927410,
          "resource_id": 135,
          "text": "و کوه‌ها با حركتى شدید روان شوند."
        }
      ]
    },
    {
      "id": 4746,
      "verse_number": 11,
      "verse_key": "52:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927411,
          "resource_id": 135,
          "text": "در آن روز، واى به حال تكذیب‌كنندگان!"
        }
      ]
    },
    {
      "id": 4747,
      "verse_number": 12,
      "verse_key": "52:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927412,
          "resource_id": 135,
          "text": "همان کسانی كه در بیهوده‌گویى، سرگرم بازى‌اند."
        }
      ]
    },
    {
      "id": 4748,
      "verse_number": 13,
      "verse_key": "52:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927413,
          "resource_id": 135,
          "text": "روزى كه با خشونت به سوى آتش دوزخ رانده مى‌شوند."
        }
      ]
    },
    {
      "id": 4749,
      "verse_number": 14,
      "verse_key": "52:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 523,
      "juz_number": 27,
      "translations": [
        {
          "id": 927414,
          "resource_id": 135,
          "text": "[به آنان گفته می‌شود:] «این [همان] آتشی است که آن را دروغ می‌پنداشتید."
        }
      ]
    },
    {
      "id": 4750,
      "verse_number": 15,
      "verse_key": "52:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927415,
          "resource_id": 135,
          "text": "آیا این [آتش و عذاب] جادوست یا [هنوز هم آن را] نمی‌بینید؟"
        }
      ]
    },
    {
      "id": 4751,
      "verse_number": 16,
      "verse_key": "52:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927416,
          "resource_id": 135,
          "text": "به آتش درآیید [و بسوزید] كه شکیبایی و ناشکیباییِ شما [در برابر شعله‌های سوزانش] برایتان یكسان است؛ تنها در برابر آنچه كرده‌اید، مجازات مى‌شوید»."
        }
      ]
    },
    {
      "id": 4752,
      "verse_number": 17,
      "verse_key": "52:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927417,
          "resource_id": 135,
          "text": "به راستی که پرهیزگاران در باغ‌های بهشت در ناز و نعمتند؛"
        }
      ]
    },
    {
      "id": 4753,
      "verse_number": 18,
      "verse_key": "52:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927418,
          "resource_id": 135,
          "text": "و از بخشش‌های [بی‌دریغِ] پروردگارشان و از اینكه از عذاب دوزخ نگاهشان داشته است شادمانند."
        }
      ]
    },
    {
      "id": 4754,
      "verse_number": 19,
      "verse_key": "52:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927419,
          "resource_id": 135,
          "text": "[به آنان گفته می‌شود:] «به پاداشِ کردارتان [در دنیا، اینک] گوارا بخورید و بیاشامید»."
        }
      ]
    },
    {
      "id": 4755,
      "verse_number": 20,
      "verse_key": "52:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927420,
          "resource_id": 135,
          "text": "در حالى كه بر تخت‌هایى ردیف‌شده تكیه زده‌اند و حور العین را به همسریِ آنان درمی‌آوریم."
        }
      ]
    },
    {
      "id": 4756,
      "verse_number": 21,
      "verse_key": "52:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927421,
          "resource_id": 135,
          "text": "مؤمنانى كه فرزندانشان در ایمان پیرو آنان بودند، فرزندانشان را [نیز در بهشت] به آنان ملحق خواهیم كرد و از [پاداش] عملشان ذره‌اى نمى‌كاهیم؛ چرا که [سرنوشتِ] هر كس در گرو دستاورد خودِ اوست [و گناه دیگری را به دوش نمی‌کشد]."
        }
      ]
    },
    {
      "id": 4757,
      "verse_number": 22,
      "verse_key": "52:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927422,
          "resource_id": 135,
          "text": "[در بهشت، از] هر نوع میوه و گوشتى كه بخواهند، پى‌درپى در دسترسشان قرار مى‌دهیم."
        }
      ]
    },
    {
      "id": 4758,
      "verse_number": 23,
      "verse_key": "52:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 209,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927423,
          "resource_id": 135,
          "text": "در آنجا جام‌های شرابى از دست یکدیگر مى‌گیرند كه نه موجب بیهوده‌گویى است و نه گناه؛"
        }
      ]
    },
    {
      "id": 4759,
      "verse_number": 24,
      "verse_key": "52:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927424,
          "resource_id": 135,
          "text": "و نوجوانانى [که براى خدمت] پیرامونشان در رفت و آمدند، گویی [از زیبایی و سپیدرویی] همچون مرواریدى پنهان [در صدف] هستند."
        }
      ]
    },
    {
      "id": 4760,
      "verse_number": 25,
      "verse_key": "52:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927425,
          "resource_id": 135,
          "text": "[بهشتیان] به یکدیگر روی می‌آورند [و] از [دنیا و احوال گذشتۀ] خود سؤال می‌کنند."
        }
      ]
    },
    {
      "id": 4761,
      "verse_number": 26,
      "verse_key": "52:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927426,
          "resource_id": 135,
          "text": "می‌گویند: «ما در دنیا، در خانوادۀ خود [از عذاب الهی] می‌ترسیدیم."
        }
      ]
    },
    {
      "id": 4762,
      "verse_number": 27,
      "verse_key": "52:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927427,
          "resource_id": 135,
          "text": "الله [با نعمت اسلام] بر ما منت نهاد و ما را از عذاب سوزان [دوزخ] محافظت کرد."
        }
      ]
    },
    {
      "id": 4763,
      "verse_number": 28,
      "verse_key": "52:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 460,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927428,
          "resource_id": 135,
          "text": "ما در زندگی دنیا، همواره او را [عبادت می‌کردیم و به ربوبیت] می‌خواندیم. بی‌تردید، او نیکوکار مهربان است»."
        }
      ]
    },
    {
      "id": 4764,
      "verse_number": 29,
      "verse_key": "52:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927429,
          "resource_id": 135,
          "text": "[ای پیامبر، با آیات قرآن به مردم] اندرز بده که به لطف پروردگارت [تو برخلاف ادعای کافران،] نه كاهنى و نه مجنون."
        }
      ]
    },
    {
      "id": 4765,
      "verse_number": 30,
      "verse_key": "52:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927430,
          "resource_id": 135,
          "text": "آیا می‌گویند: «محمد شاعری است که منتظریم تا مرگش فرارسد [و از سخنانش رهایی یابیم]؟»"
        }
      ]
    },
    {
      "id": 4766,
      "verse_number": 31,
      "verse_key": "52:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 524,
      "juz_number": 27,
      "translations": [
        {
          "id": 927431,
          "resource_id": 135,
          "text": "بگو: «منتظر [مرگِ من] باشید؛ من [نیز] با شما منتظر خواهم ماند [تا عذابتان را به چشم ببینم]»."
        }
      ]
    },
    {
      "id": 4767,
      "verse_number": 32,
      "verse_key": "52:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927432,
          "resource_id": 135,
          "text": "آیا عقل‌هایشان به [بیان] این [سخنان باطل] حُكم مى‌كند یا [اساساً] مردمی سرکش هستند؟"
        }
      ]
    },
    {
      "id": 4768,
      "verse_number": 33,
      "verse_key": "52:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927433,
          "resource_id": 135,
          "text": "آیا می‌گویند: «[محمد] قرآن را جعل کرده است؟» [هرگز چنین نیست؛] بلکه [غرق در تکبرند که] ایمان نمی‌آورند."
        }
      ]
    },
    {
      "id": 4769,
      "verse_number": 34,
      "verse_key": "52:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927434,
          "resource_id": 135,
          "text": "اگر راست مى‌گویند، سخنى مانند آن بیاورند."
        }
      ]
    },
    {
      "id": 4770,
      "verse_number": 35,
      "verse_key": "52:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927435,
          "resource_id": 135,
          "text": "آیا آنان از هیچ آفریده شده‌اند یا خود آفرینندۀ خویشند؟"
        }
      ]
    },
    {
      "id": 4771,
      "verse_number": 36,
      "verse_key": "52:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927436,
          "resource_id": 135,
          "text": "آیا آنان آسمان‌ها و زمین را آفریده‌اند؟ [نه،] بلکه آنان یقین ندارند [که الله آفرینندۀ مطلق است]."
        }
      ]
    },
    {
      "id": 4772,
      "verse_number": 37,
      "verse_key": "52:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927437,
          "resource_id": 135,
          "text": "آیا گنجینه‌های [نعمت و هدایت] پروردگارت نزد آنان است؟ آیا [بر همه چیز] تسلط دارند؟"
        }
      ]
    },
    {
      "id": 4773,
      "verse_number": 38,
      "verse_key": "52:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927438,
          "resource_id": 135,
          "text": "آیا نردبانى دارند كه [گفتار فرشتگان وحى را] به وسیلۀ آن مى‌شنوند؟ هر کس شنیده است، باید دلیلى روشن بیاورد."
        }
      ]
    },
    {
      "id": 4774,
      "verse_number": 39,
      "verse_key": "52:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927439,
          "resource_id": 135,
          "text": "آیا الله فرزندان دختر دارد و شما پسر؟"
        }
      ]
    },
    {
      "id": 4775,
      "verse_number": 40,
      "verse_key": "52:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927440,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا پاداشی از آنان درخواست می‌کنی [که از پرداخت آن در رنجند و هزینه‌اش] برایشان [گران و] سنگین است؟"
        }
      ]
    },
    {
      "id": 4776,
      "verse_number": 41,
      "verse_key": "52:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927441,
          "resource_id": 135,
          "text": "آیا از عالم غیب آگاهند و [خبرهاى نهان را] می‌نویسند؟"
        }
      ]
    },
    {
      "id": 4777,
      "verse_number": 42,
      "verse_key": "52:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927442,
          "resource_id": 135,
          "text": "آیا قصد نیرنگ [و نقشۀ پلیدی برایت] دارند؟ ولى کافران خود گرفتار نیرنگ [و تدبیر الهی] هستند."
        }
      ]
    },
    {
      "id": 4778,
      "verse_number": 43,
      "verse_key": "52:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927443,
          "resource_id": 135,
          "text": "آیا آنان معبودی غیر از الله دارند؟ الله از چیزهایی که [با او] شریک قرار می‌دهند پاک و منزه است."
        }
      ]
    },
    {
      "id": 4779,
      "verse_number": 44,
      "verse_key": "52:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927444,
          "resource_id": 135,
          "text": "[این کافران، حتی] اگر پاره‌هایی از آسمان را نیز در حال سقوط ببینند، [باز هم ایمان نمى‌آورند و] مى‌گویند: «ابرى متراكم است»."
        }
      ]
    },
    {
      "id": 4780,
      "verse_number": 45,
      "verse_key": "52:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927445,
          "resource_id": 135,
          "text": "پس آنان را [به حال خود] رها كن تا با روزِ موعودشان كه [با نخستین دمیدنِ صور] مدهوش مرگ می‌گردند، مواجه شوند."
        }
      ]
    },
    {
      "id": 4781,
      "verse_number": 46,
      "verse_key": "52:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927446,
          "resource_id": 135,
          "text": "روزى كه نیرنگشان سودمند نخواهد بود و [از هیچ سو] یارى نمی‌شوند."
        }
      ]
    },
    {
      "id": 4782,
      "verse_number": 47,
      "verse_key": "52:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927447,
          "resource_id": 135,
          "text": "ستمکاران [در دنیا نیز به دست مؤمنان] به عذاب دیگرى گرفتار خواهند شد؛ ولى بیشتر آنان نمى‌دانند."
        }
      ]
    },
    {
      "id": 4783,
      "verse_number": 48,
      "verse_key": "52:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927448,
          "resource_id": 135,
          "text": "[ای پیامبر،] در برابر حکم پروردگارت شکیبا باش؛ زیرا تو تحت نظر و حفاظت مایی؛ و هنگامی که [از خواب یا به قصد عبادت] برمی‌خیزی، پروردگارت را به پاکی ستایش کن."
        }
      ]
    },
    {
      "id": 4784,
      "verse_number": 49,
      "verse_key": "52:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 461,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 525,
      "juz_number": 27,
      "translations": [
        {
          "id": 927449,
          "resource_id": 135,
          "text": "و [نیز در پاسى] از شب او را تسبیح بگوی [و نماز بگزار] و به هنگام افول ستارگان [در سپیده‌دم نیز نماز صبح بگزار]."
        }
      ]
    }
  ]
}