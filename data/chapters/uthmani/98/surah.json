{
  "id": 98,
  "name": "The Clear Proof",
  "verses": [
    {
      "id": 6131,
      "verse_number": 1,
      "verse_key": "98:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " لَمْ يَكُنِ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَـٰبِ وَٱلْمُشْرِكِينَ مُنفَكِّينَ حَتَّىٰ تَأْتِيَهُمُ ٱلْبَيِّنَةُ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6132,
      "verse_number": 2,
      "verse_key": "98:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "رَسُولٌ مِّنَ ٱللَّهِ يَتْلُوا۟ صُحُفًا مُّطَهَّرَةً",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6133,
      "verse_number": 3,
      "verse_key": "98:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِيهَا كُتُبٌ قَيِّمَةٌ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6134,
      "verse_number": 4,
      "verse_key": "98:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا تَفَرَّقَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَـٰبَ إِلَّا مِنۢ بَعْدِ مَا جَآءَتْهُمُ ٱلْبَيِّنَةُ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6135,
      "verse_number": 5,
      "verse_key": "98:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أُمِرُوٓا۟ إِلَّا لِيَعْبُدُوا۟ ٱللَّهَ مُخْلِصِينَ لَهُ ٱلدِّينَ حُنَفَآءَ وَيُقِيمُوا۟ ٱلصَّلَوٰةَ وَيُؤْتُوا۟ ٱلزَّكَوٰةَ ۚ وَذَٰلِكَ دِينُ ٱلْقَيِّمَةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6136,
      "verse_number": 6,
      "verse_key": "98:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَـٰبِ وَٱلْمُشْرِكِينَ فِى نَارِ جَهَنَّمَ خَـٰلِدِينَ فِيهَآ ۚ أُو۟لَـٰٓئِكَ هُمْ شَرُّ ٱلْبَرِيَّةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6137,
      "verse_number": 7,
      "verse_key": "98:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ أُو۟لَـٰٓئِكَ هُمْ خَيْرُ ٱلْبَرِيَّةِ",
      "page_number": 598,
      "juz_number": 30
    },
    {
      "id": 6138,
      "verse_number": 8,
      "verse_key": "98:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 239,
      "ruku_number": 542,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "جَزَآؤُهُمْ عِندَ رَبِّهِمْ جَنَّـٰتُ عَدْنٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ خَـٰلِدِينَ فِيهَآ أَبَدًا ۖ رَّضِىَ ٱللَّهُ عَنْهُمْ وَرَضُوا۟ عَنْهُ ۚ ذَٰلِكَ لِمَنْ خَشِىَ رَبَّهُۥ",
      "page_number": 599,
      "juz_number": 30
    }
  ]
}