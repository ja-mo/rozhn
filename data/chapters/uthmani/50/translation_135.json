{
  "id": 50,
  "name": "The Letter \"Qaf\"",
  "verses": [
    {
      "id": 4631,
      "verse_number": 1,
      "verse_key": "50:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927296,
          "resource_id": 135,
          "text": "ق [قاف]؛ سوگند به قرآن ارجمند [كه رستاخیز آمدنى است]."
        }
      ]
    },
    {
      "id": 4632,
      "verse_number": 2,
      "verse_key": "50:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927297,
          "resource_id": 135,
          "text": "مشرکان از اینكه هشداردهنده‌اى از میان خودشان به سراغشان آمده است، ابراز شگفتى می‌کنند و می‌گویند: «ماجرای عجیبى است."
        }
      ]
    },
    {
      "id": 4633,
      "verse_number": 3,
      "verse_key": "50:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927298,
          "resource_id": 135,
          "text": "آیا هنگامی که مُردیم و خاک شدیم، [دوباره برانگیخته خواهیم شد؟] این بازگشتی بعید است»."
        }
      ]
    },
    {
      "id": 4634,
      "verse_number": 4,
      "verse_key": "50:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927299,
          "resource_id": 135,
          "text": "بی‌تردید، ذراتى كه زمین از بدن آنان مى‌كاهد [همه را] مى‌دانیم؛ و كتابى نگاهبان در اختیار داریم [که هر چه بر آنان مقدر گشته، در آن ثبت است]."
        }
      ]
    },
    {
      "id": 4635,
      "verse_number": 5,
      "verse_key": "50:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927300,
          "resource_id": 135,
          "text": "وقتى كه [قرآن، یعنی كلام] حق برایشان آمد، آن را تکذیب كردند؛ پس در حالت سرگردانی [و پریشانی] هستند [و دربارۀ قرآن حیرانند]."
        }
      ]
    },
    {
      "id": 4636,
      "verse_number": 6,
      "verse_key": "50:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927301,
          "resource_id": 135,
          "text": "آیا کافران، آسمان را برفرازشان نمى‌نگرند كه چگونه آن را برافراشتیم و [به زیور ستارگان] آراستیم و عارى از [هرگونه خلل و] شکافی است؟"
        }
      ]
    },
    {
      "id": 4637,
      "verse_number": 7,
      "verse_key": "50:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927302,
          "resource_id": 135,
          "text": "و زمین را گستردیم و کوه‌های ثابت در آن پى افكندیم و از هر نوع گیاه زیبا [و شادی‌انگیزی] در آن رویاندیم."
        }
      ]
    },
    {
      "id": 4638,
      "verse_number": 8,
      "verse_key": "50:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927303,
          "resource_id": 135,
          "text": "[آنها را آفریدیم] تا برای هر بندۀ توبه‌کاری، موجب بینش و پند باشد؛"
        }
      ]
    },
    {
      "id": 4639,
      "verse_number": 9,
      "verse_key": "50:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927304,
          "resource_id": 135,
          "text": "و بارانى پربركت از آسمان فرستادیم و با آن، باغ‌ها[ى میوه] و [مزارعِ] دانه[هاى] دروكردنى رویاندیم،"
        }
      ]
    },
    {
      "id": 4640,
      "verse_number": 10,
      "verse_key": "50:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927305,
          "resource_id": 135,
          "text": "و درختان سركشیدۀ خرما با خوشه‌هاى برهم‌نشسته."
        }
      ]
    },
    {
      "id": 4641,
      "verse_number": 11,
      "verse_key": "50:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927306,
          "resource_id": 135,
          "text": "براى روزى بندگان؛ به وسیلۀ آن [باران،] سرزمین خزان‌زده و خشک را حیات بخشیدیم؛ خروج از قبرها [در قیامت] نیز همین گونه است."
        }
      ]
    },
    {
      "id": 4642,
      "verse_number": 12,
      "verse_key": "50:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927307,
          "resource_id": 135,
          "text": "پیش از آنان نیز قوم نوح و اهالى «رَسّ» [= چاه] و قوم ثمود [پیامبرانشان را] دروغگو شمردند؛"
        }
      ]
    },
    {
      "id": 4643,
      "verse_number": 13,
      "verse_key": "50:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927308,
          "resource_id": 135,
          "text": "و [همچنین قوم] عاد و فرعون و قوم لوط؛"
        }
      ]
    },
    {
      "id": 4644,
      "verse_number": 14,
      "verse_key": "50:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927309,
          "resource_id": 135,
          "text": "و اهالی «اَیکه» [= قوم شعیب] و قوم «تُبَّع»؛ هر یک [از آنان] پیامبران [الهی] را دروغگو شمردند؛ پس وعدۀ عذاب من [بر آنان] تحقّق یافت."
        }
      ]
    },
    {
      "id": 4645,
      "verse_number": 15,
      "verse_key": "50:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 518,
      "juz_number": 26,
      "translations": [
        {
          "id": 927310,
          "resource_id": 135,
          "text": "آیا ما در آفرینش نخستین فروماندیم [تا از بازآفرینیِ آن عاجز باشیم]؟ [هرگز،] بلكه آنان از آفرینش جدید در [حیرت و] تردیدند."
        }
      ]
    },
    {
      "id": 4646,
      "verse_number": 16,
      "verse_key": "50:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927311,
          "resource_id": 135,
          "text": "ما انسان را آفریدیم و از وسوسه‌هاى ضمیرش آگاهیم؛ و از شاهرگ [وی] به او نزدیكتریم."
        }
      ]
    },
    {
      "id": 4647,
      "verse_number": 17,
      "verse_key": "50:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927312,
          "resource_id": 135,
          "text": "وقتی دو فرشته که بر سمت راست و چپ مراقب نشسته‌اند [رفتارش را] ثبت مى‌كنند؛"
        }
      ]
    },
    {
      "id": 4648,
      "verse_number": 18,
      "verse_key": "50:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927313,
          "resource_id": 135,
          "text": "هر سخنى که بر زبان مى‌آورد، [فرشتۀ] مراقبى در کنار او آماده است [و آن سخن را می‌نویسد]."
        }
      ]
    },
    {
      "id": 4649,
      "verse_number": 19,
      "verse_key": "50:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927314,
          "resource_id": 135,
          "text": "سختی‌های مرگ، به راستى دررسید؛ [ای انسان غافل،] این همان چیزى است كه از آن مى‌گریختى؛"
        }
      ]
    },
    {
      "id": 4650,
      "verse_number": 20,
      "verse_key": "50:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927315,
          "resource_id": 135,
          "text": "و [برای دومین بار] در «صور» دمیده می‌شود [و قیامت برپا می‌گردد]. آن روز، [روز تحققِ] وعدۀ [عذاب] است."
        }
      ]
    },
    {
      "id": 4651,
      "verse_number": 21,
      "verse_key": "50:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927316,
          "resource_id": 135,
          "text": "و هر کس در حالی [که دو فرشته او را همراهی می‌کنند به عرصۀ محشر] می‌آید؛ یکی [او را] سوق می‌دهد و دیگری [بر اعمالش] گواه است."
        }
      ]
    },
    {
      "id": 4652,
      "verse_number": 22,
      "verse_key": "50:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927317,
          "resource_id": 135,
          "text": "[به او گفته می‌شود:] «تو از این [روز] در غفلت بودى و ما [با عذاب دوزخ،] پرده[ی غفلت] را از مقابل [چشم‌های] تو برداشتیم؛ اكنون دیده‌ات تیزبین شده است."
        }
      ]
    },
    {
      "id": 4653,
      "verse_number": 23,
      "verse_key": "50:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927318,
          "resource_id": 135,
          "text": "[فرشتۀ] همنشین وى مى‌گوید: «این همان [نامۀ اعمال او] است كه [آن را ثبت کرده‌ام و] نزدم آماده است»."
        }
      ]
    },
    {
      "id": 4654,
      "verse_number": 24,
      "verse_key": "50:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927319,
          "resource_id": 135,
          "text": "[الله خطاب به آن دو فرشته مى‌فرماید:] «هر ستیزه‌جوی حق‌ستیزی را در دوزخ بیفكنید؛"
        }
      ]
    },
    {
      "id": 4655,
      "verse_number": 25,
      "verse_key": "50:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927320,
          "resource_id": 135,
          "text": "[هر] مانع خیر و متجاوز و بدگمان را؛"
        }
      ]
    },
    {
      "id": 4656,
      "verse_number": 26,
      "verse_key": "50:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927321,
          "resource_id": 135,
          "text": "همان كسى كه در كنار الله، معبود دیگرى قرار مى‌داد. او را در عذابى سخت بیفكنید»."
        }
      ]
    },
    {
      "id": 4657,
      "verse_number": 27,
      "verse_key": "50:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927322,
          "resource_id": 135,
          "text": "[شیطانِ] همنشینش مى‌گوید: «پروردگارا، من او را به طغیان نکشیدم؛ بلکه او خود در گمراهیِ بى‌پایانی بود»."
        }
      ]
    },
    {
      "id": 4658,
      "verse_number": 28,
      "verse_key": "50:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927323,
          "resource_id": 135,
          "text": "[الله] می‌فرماید: «در پیشگاه من ستیز نکنید كه پیش از این به شما هشدار داده بودم."
        }
      ]
    },
    {
      "id": 4659,
      "verse_number": 29,
      "verse_key": "50:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927324,
          "resource_id": 135,
          "text": "فرمانِ مجازات، نزد من تغییرپذیر نیست؛ و الله [در کیفر و پاداش] به بندگانش ستم نمی‌کند»."
        }
      ]
    },
    {
      "id": 4660,
      "verse_number": 30,
      "verse_key": "50:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927325,
          "resource_id": 135,
          "text": "روزى كه به دوزخ مى‌گوییم: «آیا پُر شده‌ای؟» [و او] مى‌گوید: «آیا بیش از این هم هست؟»."
        }
      ]
    },
    {
      "id": 4661,
      "verse_number": 31,
      "verse_key": "50:31",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927326,
          "resource_id": 135,
          "text": "[در آن روز] بهشت را برای پرهیزگاران نزدیک می‌کنند، و فاصله‌ای از آنان ندارد [تا نعمت‌هایش را از نزدیک ببینند]."
        }
      ]
    },
    {
      "id": 4662,
      "verse_number": 32,
      "verse_key": "50:32",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927327,
          "resource_id": 135,
          "text": "[به آنان گفته می‌شود:] «این همان پاداشى است كه به شما وعده داده مى‌شد که برای هر توبه‌کاری است که [به سوی پروردگارش بازمی‌گردد و به پیمان و احکام الهی] پایبند است."
        }
      ]
    },
    {
      "id": 4663,
      "verse_number": 33,
      "verse_key": "50:33",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927328,
          "resource_id": 135,
          "text": "همان كسی كه در نهان از [خشمِ الله] رحمان می‌ترسد و با قلبی توبه‌کار [به پیشگاهش] بازمی‌گردد."
        }
      ]
    },
    {
      "id": 4664,
      "verse_number": 34,
      "verse_key": "50:34",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927329,
          "resource_id": 135,
          "text": "با سلامت به بهشت درآیید؛ این است روز جاودانگى»."
        }
      ]
    },
    {
      "id": 4665,
      "verse_number": 35,
      "verse_key": "50:35",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 519,
      "juz_number": 26,
      "translations": [
        {
          "id": 927330,
          "resource_id": 135,
          "text": "در آنجا هرچه بخواهند، در اختیار دارند و حتی بیشتر از آن [نیز] در اختیارِ ماست."
        }
      ]
    },
    {
      "id": 4666,
      "verse_number": 36,
      "verse_key": "50:36",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927331,
          "resource_id": 135,
          "text": "و پیش از اینان [= مشرکان مکه]، امت‌های زیادی را نابود کردیم که آنان [به مراتب] از اینها قوی‌تر بودند؛ و در گوشه و کنار شهرها به جستجو پرداختند [که ببینند آیا] هیچ پناه و گریزگاهی [از عذاب و هلاکت، برایشان] وجود دارد."
        }
      ]
    },
    {
      "id": 4667,
      "verse_number": 37,
      "verse_key": "50:37",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927332,
          "resource_id": 135,
          "text": "بی‌گمان در این [سخن] برای صاحبدلان یا کسانی که با حضور [قلب به ندای توحید] گوش فرامی‌دهند و هوشیارند، اندرز است."
        }
      ]
    },
    {
      "id": 4668,
      "verse_number": 38,
      "verse_key": "50:38",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927333,
          "resource_id": 135,
          "text": "ما آسمان‌ها و زمین و آنچه را که میان آنهاست، در شش روز آفریدیم و [هرگز] رنج و خستگى به ما نرسید."
        }
      ]
    },
    {
      "id": 4669,
      "verse_number": 39,
      "verse_key": "50:39",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927334,
          "resource_id": 135,
          "text": "[ای پیامبر،] در برابر سخنان مشرکان شکیبا باش؛ و پیش از طلوع آفتاب و قبل از غروب آن، پروردگارت را به پاکی ستایش كن [و نماز بگزار]."
        }
      ]
    },
    {
      "id": 4670,
      "verse_number": 40,
      "verse_key": "50:40",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927335,
          "resource_id": 135,
          "text": "و نیز [در پاسى] از شب و پس از هر نماز، او را به پاکی ستایش کن."
        }
      ]
    },
    {
      "id": 4671,
      "verse_number": 41,
      "verse_key": "50:41",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927336,
          "resource_id": 135,
          "text": "و [ای پیامبر،] آنگاه که منادی [قیامت] از مکانی نزدیک ندا می‌دهد، گوش فرادار."
        }
      ]
    },
    {
      "id": 4672,
      "verse_number": 42,
      "verse_key": "50:42",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927337,
          "resource_id": 135,
          "text": "روزی که همگان بانگِ هولناک [رستاخیز] را به‌حق می‌شنوند، [آن روز،] روزِ خروج [از گورها] است."
        }
      ]
    },
    {
      "id": 4673,
      "verse_number": 43,
      "verse_key": "50:43",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927338,
          "resource_id": 135,
          "text": "ماییم كه حیات مى‌بخشیم و مرگ مى‌دهیم؛ و بازگشت [همگان] به سوی ماست."
        }
      ]
    },
    {
      "id": 4674,
      "verse_number": 44,
      "verse_key": "50:44",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927339,
          "resource_id": 135,
          "text": "روزى كه زمین از [فراز] آنان شكافته مى‌شود، انسان‌ها شتابان [بیرون می‌آیند]. این احضارى است كه براى ما آسان است."
        }
      ]
    },
    {
      "id": 4675,
      "verse_number": 45,
      "verse_key": "50:45",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 520,
      "juz_number": 26,
      "translations": [
        {
          "id": 927340,
          "resource_id": 135,
          "text": "ما به آنچه [کافران] می‌گویند داناتریم؛ و تو [ای پیامبر،] بر آنان مسلط نیستی [که بخواهی آنان را به ایمان آوردن وادار کنی]؛ کسی را که از عذاب [و هشدار] من می‌ترسد، به [وسیلۀ] قرآن اندرز بده."
        }
      ]
    }
  ]
}