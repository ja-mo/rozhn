{
  "id": 50,
  "name": "The Letter \"Qaf\"",
  "verses": [
    {
      "id": 4631,
      "verse_number": 1,
      "verse_key": "50:1",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " قٓ ۚ وَٱلْقُرْءَانِ ٱلْمَجِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4632,
      "verse_number": 2,
      "verse_key": "50:2",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلْ عَجِبُوٓا۟ أَن جَآءَهُم مُّنذِرٌ مِّنْهُمْ فَقَالَ ٱلْكَـٰفِرُونَ هَـٰذَا شَىْءٌ عَجِيبٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4633,
      "verse_number": 3,
      "verse_key": "50:3",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَءِذَا مِتْنَا وَكُنَّا تُرَابًا ۖ ذَٰلِكَ رَجْعٌۢ بَعِيدٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4634,
      "verse_number": 4,
      "verse_key": "50:4",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَدْ عَلِمْنَا مَا تَنقُصُ ٱلْأَرْضُ مِنْهُمْ ۖ وَعِندَنَا كِتَـٰبٌ حَفِيظٌۢ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4635,
      "verse_number": 5,
      "verse_key": "50:5",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلْ كَذَّبُوا۟ بِٱلْحَقِّ لَمَّا جَآءَهُمْ فَهُمْ فِىٓ أَمْرٍ مَّرِيجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4636,
      "verse_number": 6,
      "verse_key": "50:6",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَفَلَمْ يَنظُرُوٓا۟ إِلَى ٱلسَّمَآءِ فَوْقَهُمْ كَيْفَ بَنَيْنَـٰهَا وَزَيَّنَّـٰهَا وَمَا لَهَا مِن فُرُوجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4637,
      "verse_number": 7,
      "verse_key": "50:7",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْأَرْضَ مَدَدْنَـٰهَا وَأَلْقَيْنَا فِيهَا رَوَٰسِىَ وَأَنۢبَتْنَا فِيهَا مِن كُلِّ زَوْجٍۭ بَهِيجٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4638,
      "verse_number": 8,
      "verse_key": "50:8",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَبْصِرَةً وَذِكْرَىٰ لِكُلِّ عَبْدٍ مُّنِيبٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4639,
      "verse_number": 9,
      "verse_key": "50:9",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَنَزَّلْنَا مِنَ ٱلسَّمَآءِ مَآءً مُّبَـٰرَكًا فَأَنۢبَتْنَا بِهِۦ جَنَّـٰتٍ وَحَبَّ ٱلْحَصِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4640,
      "verse_number": 10,
      "verse_key": "50:10",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلنَّخْلَ بَاسِقَـٰتٍ لَّهَا طَلْعٌ نَّضِيدٌ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4641,
      "verse_number": 11,
      "verse_key": "50:11",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "رِّزْقًا لِّلْعِبَادِ ۖ وَأَحْيَيْنَا بِهِۦ بَلْدَةً مَّيْتًا ۚ كَذَٰلِكَ ٱلْخُرُوجُ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4642,
      "verse_number": 12,
      "verse_key": "50:12",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ وَأَصْحَـٰبُ ٱلرَّسِّ وَثَمُودُ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4643,
      "verse_number": 13,
      "verse_key": "50:13",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَعَادٌ وَفِرْعَوْنُ وَإِخْوَٰنُ لُوطٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4644,
      "verse_number": 14,
      "verse_key": "50:14",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَصْحَـٰبُ ٱلْأَيْكَةِ وَقَوْمُ تُبَّعٍ ۚ كُلٌّ كَذَّبَ ٱلرُّسُلَ فَحَقَّ وَعِيدِ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4645,
      "verse_number": 15,
      "verse_key": "50:15",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 454,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَفَعَيِينَا بِٱلْخَلْقِ ٱلْأَوَّلِ ۚ بَلْ هُمْ فِى لَبْسٍ مِّنْ خَلْقٍ جَدِيدٍ",
      "page_number": 518,
      "juz_number": 26
    },
    {
      "id": 4646,
      "verse_number": 16,
      "verse_key": "50:16",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ وَنَعْلَمُ مَا تُوَسْوِسُ بِهِۦ نَفْسُهُۥ ۖ وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنْ حَبْلِ ٱلْوَرِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4647,
      "verse_number": 17,
      "verse_key": "50:17",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ يَتَلَقَّى ٱلْمُتَلَقِّيَانِ عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ قَعِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4648,
      "verse_number": 18,
      "verse_key": "50:18",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّا يَلْفِظُ مِن قَوْلٍ إِلَّا لَدَيْهِ رَقِيبٌ عَتِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4649,
      "verse_number": 19,
      "verse_key": "50:19",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَآءَتْ سَكْرَةُ ٱلْمَوْتِ بِٱلْحَقِّ ۖ ذَٰلِكَ مَا كُنتَ مِنْهُ تَحِيدُ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4650,
      "verse_number": 20,
      "verse_key": "50:20",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَنُفِخَ فِى ٱلصُّورِ ۚ ذَٰلِكَ يَوْمُ ٱلْوَعِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4651,
      "verse_number": 21,
      "verse_key": "50:21",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَآءَتْ كُلُّ نَفْسٍ مَّعَهَا سَآئِقٌ وَشَهِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4652,
      "verse_number": 22,
      "verse_key": "50:22",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّقَدْ كُنتَ فِى غَفْلَةٍ مِّنْ هَـٰذَا فَكَشَفْنَا عَنكَ غِطَآءَكَ فَبَصَرُكَ ٱلْيَوْمَ حَدِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4653,
      "verse_number": 23,
      "verse_key": "50:23",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَقَالَ قَرِينُهُۥ هَـٰذَا مَا لَدَىَّ عَتِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4654,
      "verse_number": 24,
      "verse_key": "50:24",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلْقِيَا فِى جَهَنَّمَ كُلَّ كَفَّارٍ عَنِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4655,
      "verse_number": 25,
      "verse_key": "50:25",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ مُّرِيبٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4656,
      "verse_number": 26,
      "verse_key": "50:26",
      "hizb_number": 52,
      "rub_el_hizb_number": 207,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى جَعَلَ مَعَ ٱللَّهِ إِلَـٰهًا ءَاخَرَ فَأَلْقِيَاهُ فِى ٱلْعَذَابِ ٱلشَّدِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4657,
      "verse_number": 27,
      "verse_key": "50:27",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "۞ قَالَ قَرِينُهُۥ رَبَّنَا مَآ أَطْغَيْتُهُۥ وَلَـٰكِن كَانَ فِى ضَلَـٰلٍۭ بَعِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4658,
      "verse_number": 28,
      "verse_key": "50:28",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالَ لَا تَخْتَصِمُوا۟ لَدَىَّ وَقَدْ قَدَّمْتُ إِلَيْكُم بِٱلْوَعِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4659,
      "verse_number": 29,
      "verse_key": "50:29",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 455,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا يُبَدَّلُ ٱلْقَوْلُ لَدَىَّ وَمَآ أَنَا۠ بِظَلَّـٰمٍ لِّلْعَبِيدِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4660,
      "verse_number": 30,
      "verse_key": "50:30",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ نَقُولُ لِجَهَنَّمَ هَلِ ٱمْتَلَأْتِ وَتَقُولُ هَلْ مِن مَّزِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4661,
      "verse_number": 31,
      "verse_key": "50:31",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأُزْلِفَتِ ٱلْجَنَّةُ لِلْمُتَّقِينَ غَيْرَ بَعِيدٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4662,
      "verse_number": 32,
      "verse_key": "50:32",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَـٰذَا مَا تُوعَدُونَ لِكُلِّ أَوَّابٍ حَفِيظٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4663,
      "verse_number": 33,
      "verse_key": "50:33",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَّنْ خَشِىَ ٱلرَّحْمَـٰنَ بِٱلْغَيْبِ وَجَآءَ بِقَلْبٍ مُّنِيبٍ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4664,
      "verse_number": 34,
      "verse_key": "50:34",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱدْخُلُوهَا بِسَلَـٰمٍ ۖ ذَٰلِكَ يَوْمُ ٱلْخُلُودِ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4665,
      "verse_number": 35,
      "verse_key": "50:35",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَهُم مَّا يَشَآءُونَ فِيهَا وَلَدَيْنَا مَزِيدٌ",
      "page_number": 519,
      "juz_number": 26
    },
    {
      "id": 4666,
      "verse_number": 36,
      "verse_key": "50:36",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكَمْ أَهْلَكْنَا قَبْلَهُم مِّن قَرْنٍ هُمْ أَشَدُّ مِنْهُم بَطْشًا فَنَقَّبُوا۟ فِى ٱلْبِلَـٰدِ هَلْ مِن مَّحِيصٍ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4667,
      "verse_number": 37,
      "verse_key": "50:37",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ فِى ذَٰلِكَ لَذِكْرَىٰ لِمَن كَانَ لَهُۥ قَلْبٌ أَوْ أَلْقَى ٱلسَّمْعَ وَهُوَ شَهِيدٌ",
      "page_number": 520,
      "juz_number": 26
    },
    {
      "id": 4668,
      "verse_number": 38,
      "verse_key": "50:38",
      "hizb_number": 52,
      "rub_el_hizb_number": 208,
      "ruku_number": 456,
