{
  "id": 79,
  "name": "Those who drag forth",
  "verses": [
    {
      "id": 5713,
      "verse_number": 1,
      "verse_key": "79:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928378,
          "resource_id": 135,
          "text": "سوگند به فرشتگانی که جان [کافران] را به شدت بیرون می‌کشند."
        }
      ]
    },
    {
      "id": 5714,
      "verse_number": 2,
      "verse_key": "79:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928379,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که جان [مؤمنان] را به نرمی و آسانی می‌گیرند."
        }
      ]
    },
    {
      "id": 5715,
      "verse_number": 3,
      "verse_key": "79:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928380,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که [به امرِ الله، از آسمان به سوی زمین و بالعکس] شناورند."
        }
      ]
    },
    {
      "id": 5716,
      "verse_number": 4,
      "verse_key": "79:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928381,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی که [در اجرای اوامر الهی] بر یکدیگر سبقت می‌گیرند."
        }
      ]
    },
    {
      "id": 5717,
      "verse_number": 5,
      "verse_key": "79:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928382,
          "resource_id": 135,
          "text": "و سوگند به فرشتگانی [که به امر الهی] کار‌‌ها را تدبیر می‌کنند."
        }
      ]
    },
    {
      "id": 5718,
      "verse_number": 6,
      "verse_key": "79:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928383,
          "resource_id": 135,
          "text": "روزی‌ که [پس از اولین دمیدن در صور] زمین [و کوه‌ها و همه چیز] به لرزه درآید"
        }
      ]
    },
    {
      "id": 5719,
      "verse_number": 7,
      "verse_key": "79:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928384,
          "resource_id": 135,
          "text": "[و] در پی آن، [دمیدن دوم حشر] بیاید،"
        }
      ]
    },
    {
      "id": 5720,
      "verse_number": 8,
      "verse_key": "79:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928385,
          "resource_id": 135,
          "text": "در آن روز، دل‌هایی سخت مضطرب [و ترسان] هستند."
        }
      ]
    },
    {
      "id": 5721,
      "verse_number": 9,
      "verse_key": "79:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928386,
          "resource_id": 135,
          "text": "و چشم‌های آنان [از ترس و شرمساری] فرو افتاده است."
        }
      ]
    },
    {
      "id": 5722,
      "verse_number": 10,
      "verse_key": "79:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928387,
          "resource_id": 135,
          "text": "[کافران در دنیا] می‌گفتند: «آیا ما [پس از مرگ، دوباره] به حال اول خود باز‌گردانده می‌شویم؟"
        }
      ]
    },
    {
      "id": 5723,
      "verse_number": 11,
      "verse_key": "79:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928388,
          "resource_id": 135,
          "text": "آیا هنگامی‌ که استخوان‌هایی پوسیده شدیم [و به خاک تبدیل گشتیم، باز هم زنده می‌شویم]»؟"
        }
      ]
    },
    {
      "id": 5724,
      "verse_number": 12,
      "verse_key": "79:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928389,
          "resource_id": 135,
          "text": "می‌گفتند: «[اگر چنین وعده‌ای درست باشد،] آنگاه آن بازگشتی زیانبار است»."
        }
      ]
    },
    {
      "id": 5725,
      "verse_number": 13,
      "verse_key": "79:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928390,
          "resource_id": 135,
          "text": "آن [بازگشت،] تنها [به] یک بانگِ مهیب است [و بس]."
        }
      ]
    },
    {
      "id": 5726,
      "verse_number": 14,
      "verse_key": "79:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928391,
          "resource_id": 135,
          "text": "و ناگهان همگی بر عرصۀ زمین [محشر] ظاهر می‌شوند."
        }
      ]
    },
    {
      "id": 5727,
      "verse_number": 15,
      "verse_key": "79:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 583,
      "juz_number": 30,
      "translations": [
        {
          "id": 928392,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا داستان موسی به تو رسیده است؟"
        }
      ]
    },
    {
      "id": 5728,
      "verse_number": 16,
      "verse_key": "79:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928393,
          "resource_id": 135,
          "text": "آنگاه که پروردگارش او را در سرزمین مقدس «طُوی» ندا داد [و فرمود:]"
        }
      ]
    },
    {
      "id": 5729,
      "verse_number": 17,
      "verse_key": "79:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928394,
          "resource_id": 135,
          "text": "«به سوی فرعون برو که او طغیان کرده است،"
        }
      ]
    },
    {
      "id": 5730,
      "verse_number": 18,
      "verse_key": "79:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928395,
          "resource_id": 135,
          "text": "و به او بگو: «آیا می‌خواهی [از کفر و گناه] پاک شوی؟"
        }
      ]
    },
    {
      "id": 5731,
      "verse_number": 19,
      "verse_key": "79:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928396,
          "resource_id": 135,
          "text": "و من تو را به سوی پروردگارت هدایت کنم تا [از او] بترسی [و فرمانبردار شوی]؟"
        }
      ]
    },
    {
      "id": 5732,
      "verse_number": 20,
      "verse_key": "79:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928397,
          "resource_id": 135,
          "text": "پس [موسی] معجزۀ بزرگ را به او نشان داد."
        }
      ]
    },
    {
      "id": 5733,
      "verse_number": 21,
      "verse_key": "79:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928398,
          "resource_id": 135,
          "text": "اما او تکذیب و سرکشی کرد."
        }
      ]
    },
    {
      "id": 5734,
      "verse_number": 22,
      "verse_key": "79:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928399,
          "resource_id": 135,
          "text": "سپس [به حق] پشت کرد و به کوشش و تلاش [علیه موسی] پرداخت."
        }
      ]
    },
    {
      "id": 5735,
      "verse_number": 23,
      "verse_key": "79:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928400,
          "resource_id": 135,
          "text": "آنگاه [قومش را] جمع کرد و ندا داد"
        }
      ]
    },
    {
      "id": 5736,
      "verse_number": 24,
      "verse_key": "79:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928401,
          "resource_id": 135,
          "text": "و گفت: «من پروردگارِ برتر شما هستم»."
        }
      ]
    },
    {
      "id": 5737,
      "verse_number": 25,
      "verse_key": "79:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928402,
          "resource_id": 135,
          "text": "الله او را به عذاب آخرت و دنیا گرفتار کرد."
        }
      ]
    },
    {
      "id": 5738,
      "verse_number": 26,
      "verse_key": "79:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928403,
          "resource_id": 135,
          "text": "بی‌گمان، در این [واقعه،] برای هر کس ‌که [از الله] می‌ترسد، عبرتی است."
        }
      ]
    },
    {
      "id": 5739,
      "verse_number": 27,
      "verse_key": "79:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928404,
          "resource_id": 135,
          "text": "آیا آفرینش شما [بعد از مرگ] سخت‌تر است یا آسمانی که [الله] آن را بنا کرد؟"
        }
      ]
    },
    {
      "id": 5740,
      "verse_number": 28,
      "verse_key": "79:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928405,
          "resource_id": 135,
          "text": "سقف آن را بر افراشت و به آن شکل و نظم داد،"
        }
      ]
    },
    {
      "id": 5741,
      "verse_number": 29,
      "verse_key": "79:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928406,
          "resource_id": 135,
          "text": "و شبش را تاریک و روزش را روشن گردانید،"
        }
      ]
    },
    {
      "id": 5742,
      "verse_number": 30,
      "verse_key": "79:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928407,
          "resource_id": 135,
          "text": "و زمین را بعد از آن گسترانید،"
        }
      ]
    },
    {
      "id": 5743,
      "verse_number": 31,
      "verse_key": "79:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928408,
          "resource_id": 135,
          "text": "و از آن، آب [چشمه‌ها و چاه‌ها و] چراگاهش را بیرون آورد،"
        }
      ]
    },
    {
      "id": 5744,
      "verse_number": 32,
      "verse_key": "79:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928409,
          "resource_id": 135,
          "text": "و کوه‌ها را محکم و استوار [بر روی زمین] قرار داد."
        }
      ]
    },
    {
      "id": 5745,
      "verse_number": 33,
      "verse_key": "79:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928410,
          "resource_id": 135,
          "text": "[همۀ اینها] برای بهره‌گیری شما و چهارپایانتان است."
        }
      ]
    },
    {
      "id": 5746,
      "verse_number": 34,
      "verse_key": "79:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928411,
          "resource_id": 135,
          "text": "هنگامی‌ که [آن] حادثۀ بزرگ [قیامت] فرا رسد،"
        }
      ]
    },
    {
      "id": 5747,
      "verse_number": 35,
      "verse_key": "79:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928412,
          "resource_id": 135,
          "text": "در آن روز، انسان [تمام اعمال و] تلاش‌های خود را به یاد می‌آورد."
        }
      ]
    },
    {
      "id": 5748,
      "verse_number": 36,
      "verse_key": "79:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928413,
          "resource_id": 135,
          "text": "و جهنم برای هر بیننده‌ای آشکار می‌شود."
        }
      ]
    },
    {
      "id": 5749,
      "verse_number": 37,
      "verse_key": "79:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928414,
          "resource_id": 135,
          "text": "اما آن کس که طغیان [و سرکشی] کرده باشد"
        }
      ]
    },
    {
      "id": 5750,
      "verse_number": 38,
      "verse_key": "79:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928415,
          "resource_id": 135,
          "text": "و زندگی دنیا را [بر آخرت] ترجیح داده باشد،"
        }
      ]
    },
    {
      "id": 5751,
      "verse_number": 39,
      "verse_key": "79:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928416,
          "resource_id": 135,
          "text": "بی‌تردید، جهنم جایگاه اوست."
        }
      ]
    },
    {
      "id": 5752,
      "verse_number": 40,
      "verse_key": "79:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928417,
          "resource_id": 135,
          "text": "و اما کسی‌ که از ایستادن در حضور پروردگارش بیمناک بوده و نفس را از هوی [و هوس] بازداشته باشد،"
        }
      ]
    },
    {
      "id": 5753,
      "verse_number": 41,
      "verse_key": "79:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928418,
          "resource_id": 135,
          "text": "قطعاً بهشت جایگاه اوست."
        }
      ]
    },
    {
      "id": 5754,
      "verse_number": 42,
      "verse_key": "79:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928419,
          "resource_id": 135,
          "text": "[ای پیامبر،] دربارۀ قیامت از تو می‌پرسند که در چه زمانی واقع می‌شود؟"
        }
      ]
    },
    {
      "id": 5755,
      "verse_number": 43,
      "verse_key": "79:43",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928420,
          "resource_id": 135,
          "text": "تو را با یادآوری این سخن چه کار است؟"
        }
      ]
    },
    {
      "id": 5756,
      "verse_number": 44,
      "verse_key": "79:44",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928421,
          "resource_id": 135,
          "text": "[سرانجام و] منتهای [علمِ] آن، نزد پروردگار توست."
        }
      ]
    },
    {
      "id": 5757,
      "verse_number": 45,
      "verse_key": "79:45",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928422,
          "resource_id": 135,
          "text": "تو فقط بیم‌دهندۀ کسانی هستی که از آن [= قیامت] می‌ترسند."
        }
      ]
    },
    {
      "id": 5758,
      "verse_number": 46,
      "verse_key": "79:46",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 584,
      "juz_number": 30,
      "translations": [
        {
          "id": 928423,
          "resource_id": 135,
          "text": "روزی‌ که [کافران قیامت را] ببینند، [چنین احساس می‌کنند که] گویی [در دنیا] جز یک شامگاه یا بامداد به سر نبرده‌اند."
        }
      ]
    }
  ]
}