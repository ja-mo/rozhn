    },
    {
      "id": 5717,
      "verse_number": 5,
      "verse_key": "79:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْمُدَبِّرَٰتِ أَمْرًا",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5718,
      "verse_number": 6,
      "verse_key": "79:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5719,
      "verse_number": 7,
      "verse_key": "79:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَتْبَعُهَا ٱلرَّادِفَةُ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5720,
      "verse_number": 8,
      "verse_key": "79:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5721,
      "verse_number": 9,
      "verse_key": "79:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَبْصَـٰرُهَا خَـٰشِعَةٌ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5722,
      "verse_number": 10,
      "verse_key": "79:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5723,
      "verse_number": 11,
      "verse_key": "79:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَءِذَا كُنَّا عِظَـٰمًا نَّخِرَةً",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5724,
      "verse_number": 12,
      "verse_key": "79:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5725,
      "verse_number": 13,
      "verse_key": "79:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5726,
      "verse_number": 14,
      "verse_key": "79:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا هُم بِٱلسَّاهِرَةِ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5727,
      "verse_number": 15,
      "verse_key": "79:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "page_number": 583,
      "juz_number": 30
    },
    {
      "id": 5728,
      "verse_number": 16,
      "verse_key": "79:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5729,
      "verse_number": 17,
      "verse_key": "79:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5730,
      "verse_number": 18,
      "verse_key": "79:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5731,
      "verse_number": 19,
      "verse_key": "79:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5732,
      "verse_number": 20,
      "verse_key": "79:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَرَىٰهُ ٱلْـَٔايَةَ ٱلْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5733,
      "verse_number": 21,
      "verse_key": "79:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَذَّبَ وَعَصَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5734,
      "verse_number": 22,
      "verse_key": "79:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَدْبَرَ يَسْعَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5735,
      "verse_number": 23,
      "verse_key": "79:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَحَشَرَ فَنَادَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5736,
      "verse_number": 24,
      "verse_key": "79:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5737,
      "verse_number": 25,
      "verse_key": "79:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْـَٔاخِرَةِ وَٱلْأُولَىٰٓ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5738,
      "verse_number": 26,
      "verse_key": "79:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 522,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5739,
      "verse_number": 27,
      "verse_key": "79:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5740,
      "verse_number": 28,
      "verse_key": "79:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5741,
      "verse_number": 29,
      "verse_key": "79:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5742,
      "verse_number": 30,
      "verse_key": "79:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5743,
      "verse_number": 31,
      "verse_key": "79:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5744,
      "verse_number": 32,
      "verse_key": "79:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْجِبَالَ أَرْسَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5745,
      "verse_number": 33,
      "verse_key": "79:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَتَـٰعًا لَّكُمْ وَلِأَنْعَـٰمِكُمْ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5746,
      "verse_number": 34,
      "verse_key": "79:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5747,
      "verse_number": 35,
      "verse_key": "79:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَـٰنُ مَا سَعَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5748,
      "verse_number": 36,
      "verse_key": "79:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5749,
      "verse_number": 37,
      "verse_key": "79:37",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَمَّا مَن طَغَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5750,
      "verse_number": 38,
      "verse_key": "79:38",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5751,
      "verse_number": 39,
      "verse_key": "79:39",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5752,
      "verse_number": 40,
      "verse_key": "79:40",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5753,
      "verse_number": 41,
      "verse_key": "79:41",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5754,
      "verse_number": 42,
      "verse_key": "79:42",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5755,
      "verse_number": 43,
      "verse_key": "79:43",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5756,
      "verse_number": 44,
      "verse_key": "79:44",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5757,
      "verse_number": 45,
      "verse_key": "79:45",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا",
      "page_number": 584,
      "juz_number": 30
    },
    {
      "id": 5758,
      "verse_number": 46,
      "verse_key": "79:46",
      "hizb_number": 59,
      "rub_el_hizb_number": 233,
      "ruku_number": 523,
