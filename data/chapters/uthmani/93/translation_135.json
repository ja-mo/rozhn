{
  "id": 93,
  "name": "The Morning Hours",
  "verses": [
    {
      "id": 6080,
      "verse_number": 1,
      "verse_key": "93:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928745,
          "resource_id": 135,
          "text": "سوگند به آغاز روز،"
        }
      ]
    },
    {
      "id": 6081,
      "verse_number": 2,
      "verse_key": "93:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928746,
          "resource_id": 135,
          "text": "و سوگند به شب، هنگامی‌ که آرام گیرد"
        }
      ]
    },
    {
      "id": 6082,
      "verse_number": 3,
      "verse_key": "93:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928747,
          "resource_id": 135,
          "text": "[که] پروردگارت تو را وانگذاشته و [بر تو] خشم نگرفته است."
        }
      ]
    },
    {
      "id": 6083,
      "verse_number": 4,
      "verse_key": "93:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928748,
          "resource_id": 135,
          "text": "و مسلماً آخرت برای تو از دنیا بهتر است."
        }
      ]
    },
    {
      "id": 6084,
      "verse_number": 5,
      "verse_key": "93:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928749,
          "resource_id": 135,
          "text": "و به زودی پروردگارت به تو [و امت تو آنقدر] عطا خواهد کرد که راضی گردی."
        }
      ]
    },
    {
      "id": 6085,
      "verse_number": 6,
      "verse_key": "93:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928750,
          "resource_id": 135,
          "text": "آیا تو را یتیم نیافت، و پناه داد؟"
        }
      ]
    },
    {
      "id": 6086,
      "verse_number": 7,
      "verse_key": "93:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928751,
          "resource_id": 135,
          "text": "و تو را [راه‌نایافته و] سرگشته یافت، و هدایت کرد."
        }
      ]
    },
    {
      "id": 6087,
      "verse_number": 8,
      "verse_key": "93:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928752,
          "resource_id": 135,
          "text": "و تو را فقیر یافت، و بی‌نیاز نمود."
        }
      ]
    },
    {
      "id": 6088,
      "verse_number": 9,
      "verse_key": "93:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928753,
          "resource_id": 135,
          "text": "پس [تو نیز] یتیم را میازار،"
        }
      ]
    },
    {
      "id": 6089,
      "verse_number": 10,
      "verse_key": "93:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928754,
          "resource_id": 135,
          "text": "و [مستمندِ] نیازخواه را مران [و با او خشونت مکن]،"
        }
      ]
    },
    {
      "id": 6090,
      "verse_number": 11,
      "verse_key": "93:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 238,
      "ruku_number": 537,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 596,
      "juz_number": 30,
      "translations": [
        {
          "id": 928755,
          "resource_id": 135,
          "text": "و نعمت‌های پروردگارت را بازگو کن [و سپاس گزار]."
        }
      ]
    }
  ]
}