{
  "id": 107,
  "name": "The Small kindnesses",
  "verses": [
    {
      "id": 6198,
      "verse_number": 1,
      "verse_key": "107:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928863,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا کسی را که [روز] جزا را تکذیب می‌کند، دیده‌ای؟"
        }
      ]
    },
    {
      "id": 6199,
      "verse_number": 2,
      "verse_key": "107:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928864,
          "resource_id": 135,
          "text": "او [همان] کسی است ‌که یتیم را [با خشونت از خود] می‌رانَد"
        }
      ]
    },
    {
      "id": 6200,
      "verse_number": 3,
      "verse_key": "107:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928865,
          "resource_id": 135,
          "text": "و [دیگران را] به اطعامِ مسکین تشویق نمی‌کند."
        }
      ]
    },
    {
      "id": 6201,
      "verse_number": 4,
      "verse_key": "107:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928866,
          "resource_id": 135,
          "text": "پس وای بر نمازگزاران!"
        }
      ]
    },
    {
      "id": 6202,
      "verse_number": 5,
      "verse_key": "107:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928867,
          "resource_id": 135,
          "text": "همان کسانی ‌که از نمازشان غافلند"
        }
      ]
    },
    {
      "id": 6203,
      "verse_number": 6,
      "verse_key": "107:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928868,
          "resource_id": 135,
          "text": "کسانی ‌که خودنمایی می‌کنند"
        }
      ]
    },
    {
      "id": 6204,
      "verse_number": 7,
      "verse_key": "107:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 551,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928869,
          "resource_id": 135,
          "text": "و [از امانت دادنِ] وسایل ضروری زندگی دریغ می‌ورزند."
        }
      ]
    }
  ]
}