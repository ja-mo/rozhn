{
  "id": 54,
  "name": "The Moon",
  "verses": [
    {
      "id": 4847,
      "verse_number": 1,
      "verse_key": "54:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " ٱقْتَرَبَتِ ٱلسَّاعَةُ وَٱنشَقَّ ٱلْقَمَرُ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4848,
      "verse_number": 2,
      "verse_key": "54:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِن يَرَوْا۟ ءَايَةً يُعْرِضُوا۟ وَيَقُولُوا۟ سِحْرٌ مُّسْتَمِرٌّ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4849,
      "verse_number": 3,
      "verse_key": "54:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكَذَّبُوا۟ وَٱتَّبَعُوٓا۟ أَهْوَآءَهُمْ ۚ وَكُلُّ أَمْرٍ مُّسْتَقِرٌّ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4850,
      "verse_number": 4,
      "verse_key": "54:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ جَآءَهُم مِّنَ ٱلْأَنۢبَآءِ مَا فِيهِ مُزْدَجَرٌ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4851,
      "verse_number": 5,
      "verse_key": "54:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "حِكْمَةٌۢ بَـٰلِغَةٌ ۖ فَمَا تُغْنِ ٱلنُّذُرُ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4852,
      "verse_number": 6,
      "verse_key": "54:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَتَوَلَّ عَنْهُمْ ۘ يَوْمَ يَدْعُ ٱلدَّاعِ إِلَىٰ شَىْءٍ نُّكُرٍ",
      "page_number": 528,
      "juz_number": 27
    },
    {
      "id": 4853,
      "verse_number": 7,
      "verse_key": "54:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "خُشَّعًا أَبْصَـٰرُهُمْ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ كَأَنَّهُمْ جَرَادٌ مُّنتَشِرٌ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4854,
      "verse_number": 8,
      "verse_key": "54:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مُّهْطِعِينَ إِلَى ٱلدَّاعِ ۖ يَقُولُ ٱلْكَـٰفِرُونَ هَـٰذَا يَوْمٌ عَسِرٌ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4855,
      "verse_number": 9,
      "verse_key": "54:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "۞ كَذَّبَتْ قَبْلَهُمْ قَوْمُ نُوحٍ فَكَذَّبُوا۟ عَبْدَنَا وَقَالُوا۟ مَجْنُونٌ وَٱزْدُجِرَ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4856,
      "verse_number": 10,
      "verse_key": "54:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَدَعَا رَبَّهُۥٓ أَنِّى مَغْلُوبٌ فَٱنتَصِرْ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4857,
      "verse_number": 11,
      "verse_key": "54:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَفَتَحْنَآ أَبْوَٰبَ ٱلسَّمَآءِ بِمَآءٍ مُّنْهَمِرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4858,
      "verse_number": 12,
      "verse_key": "54:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَفَجَّرْنَا ٱلْأَرْضَ عُيُونًا فَٱلْتَقَى ٱلْمَآءُ عَلَىٰٓ أَمْرٍ قَدْ قُدِرَ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4859,
      "verse_number": 13,
      "verse_key": "54:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحَمَلْنَـٰهُ عَلَىٰ ذَاتِ أَلْوَٰحٍ وَدُسُرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4860,
      "verse_number": 14,
      "verse_key": "54:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَجْرِى بِأَعْيُنِنَا جَزَآءً لِّمَن كَانَ كُفِرَ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4861,
      "verse_number": 15,
      "verse_key": "54:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَد تَّرَكْنَـٰهَآ ءَايَةً فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4862,
      "verse_number": 16,
      "verse_key": "54:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4863,
      "verse_number": 17,
      "verse_key": "54:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4864,
      "verse_number": 18,
      "verse_key": "54:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ عَادٌ فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4865,
      "verse_number": 19,
      "verse_key": "54:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ رِيحًا صَرْصَرًا فِى يَوْمِ نَحْسٍ مُّسْتَمِرٍّ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4866,
      "verse_number": 20,
      "verse_key": "54:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَنزِعُ ٱلنَّاسَ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ مُّنقَعِرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4867,
      "verse_number": 21,
      "verse_key": "54:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4868,
      "verse_number": 22,
      "verse_key": "54:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 465,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4869,
      "verse_number": 23,
      "verse_key": "54:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ ثَمُودُ بِٱلنُّذُرِ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4870,
      "verse_number": 24,
      "verse_key": "54:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَقَالُوٓا۟ أَبَشَرًا مِّنَّا وَٰحِدًا نَّتَّبِعُهُۥٓ إِنَّآ إِذًا لَّفِى ضَلَـٰلٍ وَسُعُرٍ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4871,
      "verse_number": 25,
      "verse_key": "54:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَءُلْقِىَ ٱلذِّكْرُ عَلَيْهِ مِنۢ بَيْنِنَا بَلْ هُوَ كَذَّابٌ أَشِرٌ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4872,
      "verse_number": 26,
      "verse_key": "54:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَيَعْلَمُونَ غَدًا مَّنِ ٱلْكَذَّابُ ٱلْأَشِرُ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4873,
      "verse_number": 27,
      "verse_key": "54:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا مُرْسِلُوا۟ ٱلنَّاقَةِ فِتْنَةً لَّهُمْ فَٱرْتَقِبْهُمْ وَٱصْطَبِرْ",
      "page_number": 529,
      "juz_number": 27
    },
    {
      "id": 4874,
      "verse_number": 28,
      "verse_key": "54:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَنَبِّئْهُمْ أَنَّ ٱلْمَآءَ قِسْمَةٌۢ بَيْنَهُمْ ۖ كُلُّ شِرْبٍ مُّحْتَضَرٌ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4875,
      "verse_number": 29,
      "verse_key": "54:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَنَادَوْا۟ صَاحِبَهُمْ فَتَعَاطَىٰ فَعَقَرَ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4876,
      "verse_number": 30,
      "verse_key": "54:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَيْفَ كَانَ عَذَابِى وَنُذُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4877,
      "verse_number": 31,
      "verse_key": "54:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ صَيْحَةً وَٰحِدَةً فَكَانُوا۟ كَهَشِيمِ ٱلْمُحْتَظِرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4878,
      "verse_number": 32,
      "verse_key": "54:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4879,
      "verse_number": 33,
      "verse_key": "54:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ قَوْمُ لُوطٍۭ بِٱلنُّذُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4880,
      "verse_number": 34,
      "verse_key": "54:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ أَرْسَلْنَا عَلَيْهِمْ حَاصِبًا إِلَّآ ءَالَ لُوطٍ ۖ نَّجَّيْنَـٰهُم بِسَحَرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4881,
      "verse_number": 35,
      "verse_key": "54:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "نِّعْمَةً مِّنْ عِندِنَا ۚ كَذَٰلِكَ نَجْزِى مَن شَكَرَ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4882,
      "verse_number": 36,
      "verse_key": "54:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَنذَرَهُم بَطْشَتَنَا فَتَمَارَوْا۟ بِٱلنُّذُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4883,
      "verse_number": 37,
      "verse_key": "54:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ رَٰوَدُوهُ عَن ضَيْفِهِۦ فَطَمَسْنَآ أَعْيُنَهُمْ فَذُوقُوا۟ عَذَابِى وَنُذُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4884,
      "verse_number": 38,
      "verse_key": "54:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ صَبَّحَهُم بُكْرَةً عَذَابٌ مُّسْتَقِرٌّ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4885,
      "verse_number": 39,
      "verse_key": "54:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَذُوقُوا۟ عَذَابِى وَنُذُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4886,
      "verse_number": 40,
      "verse_key": "54:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 466,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ يَسَّرْنَا ٱلْقُرْءَانَ لِلذِّكْرِ فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4887,
      "verse_number": 41,
      "verse_key": "54:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ جَآءَ ءَالَ فِرْعَوْنَ ٱلنُّذُرُ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4888,
      "verse_number": 42,
      "verse_key": "54:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبُوا۟ بِـَٔايَـٰتِنَا كُلِّهَا فَأَخَذْنَـٰهُمْ أَخْذَ عَزِيزٍ مُّقْتَدِرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4889,
      "verse_number": 43,
      "verse_key": "54:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَكُفَّارُكُمْ خَيْرٌ مِّنْ أُو۟لَـٰٓئِكُمْ أَمْ لَكُم بَرَآءَةٌ فِى ٱلزُّبُرِ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4890,
      "verse_number": 44,
      "verse_key": "54:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمْ يَقُولُونَ نَحْنُ جَمِيعٌ مُّنتَصِرٌ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4891,
      "verse_number": 45,
      "verse_key": "54:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَيُهْزَمُ ٱلْجَمْعُ وَيُوَلُّونَ ٱلدُّبُرَ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4892,
      "verse_number": 46,
      "verse_key": "54:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بَلِ ٱلسَّاعَةُ مَوْعِدُهُمْ وَٱلسَّاعَةُ أَدْهَىٰ وَأَمَرُّ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4893,
      "verse_number": 47,
      "verse_key": "54:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُجْرِمِينَ فِى ضَلَـٰلٍ وَسُعُرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4894,
      "verse_number": 48,
      "verse_key": "54:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يُسْحَبُونَ فِى ٱلنَّارِ عَلَىٰ وُجُوهِهِمْ ذُوقُوا۟ مَسَّ سَقَرَ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4895,
      "verse_number": 49,
      "verse_key": "54:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا كُلَّ شَىْءٍ خَلَقْنَـٰهُ بِقَدَرٍ",
      "page_number": 530,
      "juz_number": 27
    },
    {
      "id": 4896,
      "verse_number": 50,
      "verse_key": "54:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَمْرُنَآ إِلَّا وَٰحِدَةٌ كَلَمْحٍۭ بِٱلْبَصَرِ",
      "page_number": 531,
      "juz_number": 27
    },
    {
      "id": 4897,
      "verse_number": 51,
      "verse_key": "54:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَهْلَكْنَآ أَشْيَاعَكُمْ فَهَلْ مِن مُّدَّكِرٍ",
      "page_number": 531,
      "juz_number": 27
    },
    {
      "id": 4898,
      "verse_number": 52,
      "verse_key": "54:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكُلُّ شَىْءٍ فَعَلُوهُ فِى ٱلزُّبُرِ",
      "page_number": 531,
      "juz_number": 27
    },
    {
      "id": 4899,
      "verse_number": 53,
      "verse_key": "54:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَكُلُّ صَغِيرٍ وَكَبِيرٍ مُّسْتَطَرٌ",
      "page_number": 531,
      "juz_number": 27
    },
    {
      "id": 4900,
      "verse_number": 54,
      "verse_key": "54:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْمُتَّقِينَ فِى جَنَّـٰتٍ وَنَهَرٍ",
      "page_number": 531,
      "juz_number": 27
    },
    {
      "id": 4901,
      "verse_number": 55,
      "verse_key": "54:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 212,
      "ruku_number": 467,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى مَقْعَدِ صِدْقٍ عِندَ مَلِيكٍ مُّقْتَدِرٍۭ",
      "page_number": 531,
      "juz_number": 27
    }
  ]
