{
  "id": 83,
  "name": "The Defrauding",
  "verses": [
    {
      "id": 5849,
      "verse_number": 1,
      "verse_key": "83:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَيْلٌ لِّلْمُطَفِّفِينَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5850,
      "verse_number": 2,
      "verse_key": "83:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ إِذَا ٱكْتَالُوا۟ عَلَى ٱلنَّاسِ يَسْتَوْفُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5851,
      "verse_number": 3,
      "verse_key": "83:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5852,
      "verse_number": 4,
      "verse_key": "83:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَا يَظُنُّ أُو۟لَـٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5853,
      "verse_number": 5,
      "verse_key": "83:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِيَوْمٍ عَظِيمٍ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5854,
      "verse_number": 6,
      "verse_key": "83:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلْعَـٰلَمِينَ",
      "page_number": 587,
      "juz_number": 30
    },
    {
      "id": 5855,
      "verse_number": 7,
      "verse_key": "83:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّ كِتَـٰبَ ٱلْفُجَّارِ لَفِى سِجِّينٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5856,
      "verse_number": 8,
      "verse_key": "83:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا سِجِّينٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5857,
      "verse_number": 9,
      "verse_key": "83:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كِتَـٰبٌ مَّرْقُومٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5858,
      "verse_number": 10,
      "verse_key": "83:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5859,
      "verse_number": 11,
      "verse_key": "83:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ يُكَذِّبُونَ بِيَوْمِ ٱلدِّينِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5860,
      "verse_number": 12,
      "verse_key": "83:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5861,
      "verse_number": 13,
      "verse_key": "83:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَـٰتُنَا قَالَ أَسَـٰطِيرُ ٱلْأَوَّلِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5862,
      "verse_number": 14,
      "verse_key": "83:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا۟ يَكْسِبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5863,
      "verse_number": 15,
      "verse_key": "83:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍ لَّمَحْجُوبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5864,
      "verse_number": 16,
      "verse_key": "83:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِنَّهُمْ لَصَالُوا۟ ٱلْجَحِيمِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5865,
      "verse_number": 17,
      "verse_key": "83:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ يُقَالُ هَـٰذَا ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5866,
      "verse_number": 18,
      "verse_key": "83:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَلَّآ إِنَّ كِتَـٰبَ ٱلْأَبْرَارِ لَفِى عِلِّيِّينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5867,
      "verse_number": 19,
      "verse_key": "83:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا عِلِّيُّونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5868,
      "verse_number": 20,
      "verse_key": "83:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كِتَـٰبٌ مَّرْقُومٌ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5869,
      "verse_number": 21,
      "verse_key": "83:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَشْهَدُهُ ٱلْمُقَرَّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5870,
      "verse_number": 22,
      "verse_key": "83:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5871,
      "verse_number": 23,
      "verse_key": "83:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5872,
      "verse_number": 24,
      "verse_key": "83:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تَعْرِفُ فِى وُجُوهِهِمْ نَضْرَةَ ٱلنَّعِيمِ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5873,
      "verse_number": 25,
      "verse_key": "83:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يُسْقَوْنَ مِن رَّحِيقٍ مَّخْتُومٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5874,
      "verse_number": 26,
      "verse_key": "83:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "خِتَـٰمُهُۥ مِسْكٌ ۚ وَفِى ذَٰلِكَ فَلْيَتَنَافَسِ ٱلْمُتَنَـٰفِسُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5875,
      "verse_number": 27,
      "verse_key": "83:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمِزَاجُهُۥ مِن تَسْنِيمٍ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5876,
      "verse_number": 28,
      "verse_key": "83:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَيْنًا يَشْرَبُ بِهَا ٱلْمُقَرَّبُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5877,
      "verse_number": 29,
      "verse_key": "83:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ أَجْرَمُوا۟ كَانُوا۟ مِنَ ٱلَّذِينَ ءَامَنُوا۟ يَضْحَكُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5878,
      "verse_number": 30,
      "verse_key": "83:30",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا مَرُّوا۟ بِهِمْ يَتَغَامَزُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5879,
      "verse_number": 31,
      "verse_key": "83:31",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱنقَلَبُوٓا۟ إِلَىٰٓ أَهْلِهِمُ ٱنقَلَبُوا۟ فَكِهِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5880,
      "verse_number": 32,
      "verse_key": "83:32",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا رَأَوْهُمْ قَالُوٓا۟ إِنَّ هَـٰٓؤُلَآءِ لَضَآلُّونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5881,
      "verse_number": 33,
      "verse_key": "83:33",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أُرْسِلُوا۟ عَلَيْهِمْ حَـٰفِظِينَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5882,
      "verse_number": 34,
      "verse_key": "83:34",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَٱلْيَوْمَ ٱلَّذِينَ ءَامَنُوا۟ مِنَ ٱلْكُفَّارِ يَضْحَكُونَ",
      "page_number": 588,
      "juz_number": 30
    },
    {
      "id": 5883,
      "verse_number": 35,
      "verse_key": "83:35",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ",
      "page_number": 589,
      "juz_number": 30
    },
    {
      "id": 5884,
      "verse_number": 36,
      "verse_key": "83:36",
      "hizb_number": 59,
      "rub_el_hizb_number": 235,
      "ruku_number": 527,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلْ ثُوِّبَ ٱلْكُفَّارُ مَا كَانُوا۟ يَفْعَلُونَ",
      "page_number": 589,
      "juz_number": 30
    }
  ]
}