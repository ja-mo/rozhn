{
  "id": 32,
  "name": "The Prostration",
  "verses": [
    {
      "id": 3504,
      "verse_number": 1,
      "verse_key": "32:1",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": " الٓمٓ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3505,
      "verse_number": 2,
      "verse_key": "32:2",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "تَنزِيلُ ٱلْكِتَـٰبِ لَا رَيْبَ فِيهِ مِن رَّبِّ ٱلْعَـٰلَمِينَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3506,
      "verse_number": 3,
      "verse_key": "32:3",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "أَمْ يَقُولُونَ ٱفْتَرَىٰهُ ۚ بَلْ هُوَ ٱلْحَقُّ مِن رَّبِّكَ لِتُنذِرَ قَوْمًا مَّآ أَتَىٰهُم مِّن نَّذِيرٍ مِّن قَبْلِكَ لَعَلَّهُمْ يَهْتَدُونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3507,
      "verse_number": 4,
      "verse_key": "32:4",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "ٱللَّهُ ٱلَّذِى خَلَقَ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا فِى سِتَّةِ أَيَّامٍ ثُمَّ ٱسْتَوَىٰ عَلَى ٱلْعَرْشِ ۖ مَا لَكُم مِّن دُونِهِۦ مِن وَلِىٍّ وَلَا شَفِيعٍ ۚ أَفَلَا تَتَذَكَّرُونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3508,
      "verse_number": 5,
      "verse_key": "32:5",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "يُدَبِّرُ ٱلْأَمْرَ مِنَ ٱلسَّمَآءِ إِلَى ٱلْأَرْضِ ثُمَّ يَعْرُجُ إِلَيْهِ فِى يَوْمٍ كَانَ مِقْدَارُهُۥٓ أَلْفَ سَنَةٍ مِّمَّا تَعُدُّونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3509,
      "verse_number": 6,
      "verse_key": "32:6",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "ذَٰلِكَ عَـٰلِمُ ٱلْغَيْبِ وَٱلشَّهَـٰدَةِ ٱلْعَزِيزُ ٱلرَّحِيمُ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3510,
      "verse_number": 7,
      "verse_key": "32:7",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِىٓ أَحْسَنَ كُلَّ شَىْءٍ خَلَقَهُۥ ۖ وَبَدَأَ خَلْقَ ٱلْإِنسَـٰنِ مِن طِينٍ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3511,
      "verse_number": 8,
      "verse_key": "32:8",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ جَعَلَ نَسْلَهُۥ مِن سُلَـٰلَةٍ مِّن مَّآءٍ مَّهِينٍ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3512,
      "verse_number": 9,
      "verse_key": "32:9",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ سَوَّىٰهُ وَنَفَخَ فِيهِ مِن رُّوحِهِۦ ۖ وَجَعَلَ لَكُمُ ٱلسَّمْعَ وَٱلْأَبْصَـٰرَ وَٱلْأَفْـِٔدَةَ ۚ قَلِيلًا مَّا تَشْكُرُونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3513,
      "verse_number": 10,
      "verse_key": "32:10",
      "hizb_number": 42,
      "rub_el_hizb_number": 165,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوٓا۟ أَءِذَا ضَلَلْنَا فِى ٱلْأَرْضِ أَءِنَّا لَفِى خَلْقٍ جَدِيدٍۭ ۚ بَلْ هُم بِلِقَآءِ رَبِّهِمْ كَـٰفِرُونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3514,
      "verse_number": 11,
      "verse_key": "32:11",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 360,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "۞ قُلْ يَتَوَفَّىٰكُم مَّلَكُ ٱلْمَوْتِ ٱلَّذِى وُكِّلَ بِكُمْ ثُمَّ إِلَىٰ رَبِّكُمْ تُرْجَعُونَ",
      "page_number": 415,
      "juz_number": 21
    },
    {
      "id": 3515,
      "verse_number": 12,
      "verse_key": "32:12",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَلَوْ تَرَىٰٓ إِذِ ٱلْمُجْرِمُونَ نَاكِسُوا۟ رُءُوسِهِمْ عِندَ رَبِّهِمْ رَبَّنَآ أَبْصَرْنَا وَسَمِعْنَا فَٱرْجِعْنَا نَعْمَلْ صَـٰلِحًا إِنَّا مُوقِنُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3516,
      "verse_number": 13,
      "verse_key": "32:13",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَلَوْ شِئْنَا لَـَٔاتَيْنَا كُلَّ نَفْسٍ هُدَىٰهَا وَلَـٰكِنْ حَقَّ ٱلْقَوْلُ مِنِّى لَأَمْلَأَنَّ جَهَنَّمَ مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ أَجْمَعِينَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3517,
      "verse_number": 14,
      "verse_key": "32:14",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "فَذُوقُوا۟ بِمَا نَسِيتُمْ لِقَآءَ يَوْمِكُمْ هَـٰذَآ إِنَّا نَسِينَـٰكُمْ ۖ وَذُوقُوا۟ عَذَابَ ٱلْخُلْدِ بِمَا كُنتُمْ تَعْمَلُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3518,
      "verse_number": 15,
      "verse_key": "32:15",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": 9,
      "text_uthmani": "إِنَّمَا يُؤْمِنُ بِـَٔايَـٰتِنَا ٱلَّذِينَ إِذَا ذُكِّرُوا۟ بِهَا خَرُّوا۟ سُجَّدًا وَسَبَّحُوا۟ بِحَمْدِ رَبِّهِمْ وَهُمْ لَا يَسْتَكْبِرُونَ ۩",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3519,
      "verse_number": 16,
      "verse_key": "32:16",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "تَتَجَافَىٰ جُنُوبُهُمْ عَنِ ٱلْمَضَاجِعِ يَدْعُونَ رَبَّهُمْ خَوْفًا وَطَمَعًا وَمِمَّا رَزَقْنَـٰهُمْ يُنفِقُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3520,
      "verse_number": 17,
      "verse_key": "32:17",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "فَلَا تَعْلَمُ نَفْسٌ مَّآ أُخْفِىَ لَهُم مِّن قُرَّةِ أَعْيُنٍ جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3521,
      "verse_number": 18,
      "verse_key": "32:18",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "أَفَمَن كَانَ مُؤْمِنًا كَمَن كَانَ فَاسِقًا ۚ لَّا يَسْتَوُۥنَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3522,
      "verse_number": 19,
      "verse_key": "32:19",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "أَمَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّـٰلِحَـٰتِ فَلَهُمْ جَنَّـٰتُ ٱلْمَأْوَىٰ نُزُلًۢا بِمَا كَانُوا۟ يَعْمَلُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3523,
      "verse_number": 20,
      "verse_key": "32:20",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا ٱلَّذِينَ فَسَقُوا۟ فَمَأْوَىٰهُمُ ٱلنَّارُ ۖ كُلَّمَآ أَرَادُوٓا۟ أَن يَخْرُجُوا۟ مِنْهَآ أُعِيدُوا۟ فِيهَا وَقِيلَ لَهُمْ ذُوقُوا۟ عَذَابَ ٱلنَّارِ ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ",
      "page_number": 416,
      "juz_number": 21
    },
    {
      "id": 3524,
      "verse_number": 21,
      "verse_key": "32:21",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَلَنُذِيقَنَّهُم مِّنَ ٱلْعَذَابِ ٱلْأَدْنَىٰ دُونَ ٱلْعَذَابِ ٱلْأَكْبَرِ لَعَلَّهُمْ يَرْجِعُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3525,
      "verse_number": 22,
      "verse_key": "32:22",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 361,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَمَنْ أَظْلَمُ مِمَّن ذُكِّرَ بِـَٔايَـٰتِ رَبِّهِۦ ثُمَّ أَعْرَضَ عَنْهَآ ۚ إِنَّا مِنَ ٱلْمُجْرِمِينَ مُنتَقِمُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3526,
      "verse_number": 23,
      "verse_key": "32:23",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ ءَاتَيْنَا مُوسَى ٱلْكِتَـٰبَ فَلَا تَكُن فِى مِرْيَةٍ مِّن لِّقَآئِهِۦ ۖ وَجَعَلْنَـٰهُ هُدًى لِّبَنِىٓ إِسْرَٰٓءِيلَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3527,
      "verse_number": 24,
      "verse_key": "32:24",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا مِنْهُمْ أَئِمَّةً يَهْدُونَ بِأَمْرِنَا لَمَّا صَبَرُوا۟ ۖ وَكَانُوا۟ بِـَٔايَـٰتِنَا يُوقِنُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3528,
      "verse_number": 25,
      "verse_key": "32:25",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ رَبَّكَ هُوَ يَفْصِلُ بَيْنَهُمْ يَوْمَ ٱلْقِيَـٰمَةِ فِيمَا كَانُوا۟ فِيهِ يَخْتَلِفُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3529,
      "verse_number": 26,
      "verse_key": "32:26",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "أَوَلَمْ يَهْدِ لَهُمْ كَمْ أَهْلَكْنَا مِن قَبْلِهِم مِّنَ ٱلْقُرُونِ يَمْشُونَ فِى مَسَـٰكِنِهِمْ ۚ إِنَّ فِى ذَٰلِكَ لَـَٔايَـٰتٍ ۖ أَفَلَا يَسْمَعُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3530,
      "verse_number": 27,
      "verse_key": "32:27",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "أَوَلَمْ يَرَوْا۟ أَنَّا نَسُوقُ ٱلْمَآءَ إِلَى ٱلْأَرْضِ ٱلْجُرُزِ فَنُخْرِجُ بِهِۦ زَرْعًا تَأْكُلُ مِنْهُ أَنْعَـٰمُهُمْ وَأَنفُسُهُمْ ۖ أَفَلَا يُبْصِرُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3531,
      "verse_number": 28,
      "verse_key": "32:28",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "وَيَقُولُونَ مَتَىٰ هَـٰذَا ٱلْفَتْحُ إِن كُنتُمْ صَـٰدِقِينَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3532,
      "verse_number": 29,
      "verse_key": "32:29",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "قُلْ يَوْمَ ٱلْفَتْحِ لَا يَنفَعُ ٱلَّذِينَ كَفَرُوٓا۟ إِيمَـٰنُهُمْ وَلَا هُمْ يُنظَرُونَ",
      "page_number": 417,
      "juz_number": 21
    },
    {
      "id": 3533,
      "verse_number": 30,
      "verse_key": "32:30",
      "hizb_number": 42,
      "rub_el_hizb_number": 166,
      "ruku_number": 362,
      "manzil_number": 5,
      "sajdah_number": null,
      "text_uthmani": "فَأَعْرِضْ عَنْهُمْ وَٱنتَظِرْ إِنَّهُم مُّنتَظِرُونَ",
      "page_number": 417,
      "juz_number": 21
    }
  ]
}