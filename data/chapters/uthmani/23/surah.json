{
  "id": 23,
  "name": "The Believers",
  "verses": [
    {
      "id": 2674,
      "verse_number": 1,
      "verse_key": "23:1",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": " قَدْ أَفْلَحَ ٱلْمُؤْمِنُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2675,
      "verse_number": 2,
      "verse_key": "23:2",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ هُمْ فِى صَلَاتِهِمْ خَـٰشِعُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2676,
      "verse_number": 3,
      "verse_key": "23:3",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُمْ عَنِ ٱللَّغْوِ مُعْرِضُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2677,
      "verse_number": 4,
      "verse_key": "23:4",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُمْ لِلزَّكَوٰةِ فَـٰعِلُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2678,
      "verse_number": 5,
      "verse_key": "23:5",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُمْ لِفُرُوجِهِمْ حَـٰفِظُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2679,
      "verse_number": 6,
      "verse_key": "23:6",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَـٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2680,
      "verse_number": 7,
      "verse_key": "23:7",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَمَنِ ٱبْتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُو۟لَـٰٓئِكَ هُمُ ٱلْعَادُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2681,
      "verse_number": 8,
      "verse_key": "23:8",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُمْ لِأَمَـٰنَـٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2682,
      "verse_number": 9,
      "verse_key": "23:9",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُمْ عَلَىٰ صَلَوَٰتِهِمْ يُحَافِظُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2683,
      "verse_number": 10,
      "verse_key": "23:10",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أُو۟لَـٰٓئِكَ هُمُ ٱلْوَٰرِثُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2684,
      "verse_number": 11,
      "verse_key": "23:11",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ يَرِثُونَ ٱلْفِرْدَوْسَ هُمْ فِيهَا خَـٰلِدُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2685,
      "verse_number": 12,
      "verse_key": "23:12",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ خَلَقْنَا ٱلْإِنسَـٰنَ مِن سُلَـٰلَةٍ مِّن طِينٍ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2686,
      "verse_number": 13,
      "verse_key": "23:13",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ جَعَلْنَـٰهُ نُطْفَةً فِى قَرَارٍ مَّكِينٍ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2687,
      "verse_number": 14,
      "verse_key": "23:14",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ خَلَقْنَا ٱلنُّطْفَةَ عَلَقَةً فَخَلَقْنَا ٱلْعَلَقَةَ مُضْغَةً فَخَلَقْنَا ٱلْمُضْغَةَ عِظَـٰمًا فَكَسَوْنَا ٱلْعِظَـٰمَ لَحْمًا ثُمَّ أَنشَأْنَـٰهُ خَلْقًا ءَاخَرَ ۚ فَتَبَارَكَ ٱللَّهُ أَحْسَنُ ٱلْخَـٰلِقِينَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2688,
      "verse_number": 15,
      "verse_key": "23:15",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِنَّكُم بَعْدَ ذَٰلِكَ لَمَيِّتُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2689,
      "verse_number": 16,
      "verse_key": "23:16",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ إِنَّكُمْ يَوْمَ ٱلْقِيَـٰمَةِ تُبْعَثُونَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2690,
      "verse_number": 17,
      "verse_key": "23:17",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ خَلَقْنَا فَوْقَكُمْ سَبْعَ طَرَآئِقَ وَمَا كُنَّا عَنِ ٱلْخَلْقِ غَـٰفِلِينَ",
      "page_number": 342,
      "juz_number": 18
    },
    {
      "id": 2691,
      "verse_number": 18,
      "verse_key": "23:18",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَنزَلْنَا مِنَ ٱلسَّمَآءِ مَآءًۢ بِقَدَرٍ فَأَسْكَنَّـٰهُ فِى ٱلْأَرْضِ ۖ وَإِنَّا عَلَىٰ ذَهَابٍۭ بِهِۦ لَقَـٰدِرُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2692,
      "verse_number": 19,
      "verse_key": "23:19",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَنشَأْنَا لَكُم بِهِۦ جَنَّـٰتٍ مِّن نَّخِيلٍ وَأَعْنَـٰبٍ لَّكُمْ فِيهَا فَوَٰكِهُ كَثِيرَةٌ وَمِنْهَا تَأْكُلُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2693,
      "verse_number": 20,
      "verse_key": "23:20",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَشَجَرَةً تَخْرُجُ مِن طُورِ سَيْنَآءَ تَنۢبُتُ بِٱلدُّهْنِ وَصِبْغٍ لِّلْـَٔاكِلِينَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2694,
      "verse_number": 21,
      "verse_key": "23:21",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ لَكُمْ فِى ٱلْأَنْعَـٰمِ لَعِبْرَةً ۖ نُّسْقِيكُم مِّمَّا فِى بُطُونِهَا وَلَكُمْ فِيهَا مَنَـٰفِعُ كَثِيرَةٌ وَمِنْهَا تَأْكُلُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2695,
      "verse_number": 22,
      "verse_key": "23:22",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 295,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَعَلَيْهَا وَعَلَى ٱلْفُلْكِ تُحْمَلُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2696,
      "verse_number": 23,
      "verse_key": "23:23",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِۦ فَقَالَ يَـٰقَوْمِ ٱعْبُدُوا۟ ٱللَّهَ مَا لَكُم مِّنْ إِلَـٰهٍ غَيْرُهُۥٓ ۖ أَفَلَا تَتَّقُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2697,
      "verse_number": 24,
      "verse_key": "23:24",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَقَالَ ٱلْمَلَؤُا۟ ٱلَّذِينَ كَفَرُوا۟ مِن قَوْمِهِۦ مَا هَـٰذَآ إِلَّا بَشَرٌ مِّثْلُكُمْ يُرِيدُ أَن يَتَفَضَّلَ عَلَيْكُمْ وَلَوْ شَآءَ ٱللَّهُ لَأَنزَلَ مَلَـٰٓئِكَةً مَّا سَمِعْنَا بِهَـٰذَا فِىٓ ءَابَآئِنَا ٱلْأَوَّلِينَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2698,
      "verse_number": 25,
      "verse_key": "23:25",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا رَجُلٌۢ بِهِۦ جِنَّةٌ فَتَرَبَّصُوا۟ بِهِۦ حَتَّىٰ حِينٍ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2699,
      "verse_number": 26,
      "verse_key": "23:26",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّ ٱنصُرْنِى بِمَا كَذَّبُونِ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2700,
      "verse_number": 27,
      "verse_key": "23:27",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَوْحَيْنَآ إِلَيْهِ أَنِ ٱصْنَعِ ٱلْفُلْكَ بِأَعْيُنِنَا وَوَحْيِنَا فَإِذَا جَآءَ أَمْرُنَا وَفَارَ ٱلتَّنُّورُ ۙ فَٱسْلُكْ فِيهَا مِن كُلٍّ زَوْجَيْنِ ٱثْنَيْنِ وَأَهْلَكَ إِلَّا مَن سَبَقَ عَلَيْهِ ٱلْقَوْلُ مِنْهُمْ ۖ وَلَا تُخَـٰطِبْنِى فِى ٱلَّذِينَ ظَلَمُوٓا۟ ۖ إِنَّهُم مُّغْرَقُونَ",
      "page_number": 343,
      "juz_number": 18
    },
    {
      "id": 2701,
      "verse_number": 28,
      "verse_key": "23:28",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا ٱسْتَوَيْتَ أَنتَ وَمَن مَّعَكَ عَلَى ٱلْفُلْكِ فَقُلِ ٱلْحَمْدُ لِلَّهِ ٱلَّذِى نَجَّىٰنَا مِنَ ٱلْقَوْمِ ٱلظَّـٰلِمِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2702,
      "verse_number": 29,
      "verse_key": "23:29",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَقُل رَّبِّ أَنزِلْنِى مُنزَلًا مُّبَارَكًا وَأَنتَ خَيْرُ ٱلْمُنزِلِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2703,
      "verse_number": 30,
      "verse_key": "23:30",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ فِى ذَٰلِكَ لَـَٔايَـٰتٍ وَإِن كُنَّا لَمُبْتَلِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2704,
      "verse_number": 31,
      "verse_key": "23:31",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَنشَأْنَا مِنۢ بَعْدِهِمْ قَرْنًا ءَاخَرِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2705,
      "verse_number": 32,
      "verse_key": "23:32",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 296,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَرْسَلْنَا فِيهِمْ رَسُولًا مِّنْهُمْ أَنِ ٱعْبُدُوا۟ ٱللَّهَ مَا لَكُم مِّنْ إِلَـٰهٍ غَيْرُهُۥٓ ۖ أَفَلَا تَتَّقُونَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2706,
      "verse_number": 33,
      "verse_key": "23:33",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَقَالَ ٱلْمَلَأُ مِن قَوْمِهِ ٱلَّذِينَ كَفَرُوا۟ وَكَذَّبُوا۟ بِلِقَآءِ ٱلْـَٔاخِرَةِ وَأَتْرَفْنَـٰهُمْ فِى ٱلْحَيَوٰةِ ٱلدُّنْيَا مَا هَـٰذَآ إِلَّا بَشَرٌ مِّثْلُكُمْ يَأْكُلُ مِمَّا تَأْكُلُونَ مِنْهُ وَيَشْرَبُ مِمَّا تَشْرَبُونَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2707,
      "verse_number": 34,
      "verse_key": "23:34",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَئِنْ أَطَعْتُم بَشَرًا مِّثْلَكُمْ إِنَّكُمْ إِذًا لَّخَـٰسِرُونَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2708,
      "verse_number": 35,
      "verse_key": "23:35",
      "hizb_number": 35,
      "rub_el_hizb_number": 137,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَيَعِدُكُمْ أَنَّكُمْ إِذَا مِتُّمْ وَكُنتُمْ تُرَابًا وَعِظَـٰمًا أَنَّكُم مُّخْرَجُونَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2709,
      "verse_number": 36,
      "verse_key": "23:36",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ هَيْهَاتَ هَيْهَاتَ لِمَا تُوعَدُونَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2710,
      "verse_number": 37,
      "verse_key": "23:37",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هِىَ إِلَّا حَيَاتُنَا ٱلدُّنْيَا نَمُوتُ وَنَحْيَا وَمَا نَحْنُ بِمَبْعُوثِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2711,
      "verse_number": 38,
      "verse_key": "23:38",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا رَجُلٌ ٱفْتَرَىٰ عَلَى ٱللَّهِ كَذِبًا وَمَا نَحْنُ لَهُۥ بِمُؤْمِنِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2712,
      "verse_number": 39,
      "verse_key": "23:39",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّ ٱنصُرْنِى بِمَا كَذَّبُونِ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2713,
      "verse_number": 40,
      "verse_key": "23:40",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ عَمَّا قَلِيلٍ لَّيُصْبِحُنَّ نَـٰدِمِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2714,
      "verse_number": 41,
      "verse_key": "23:41",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَخَذَتْهُمُ ٱلصَّيْحَةُ بِٱلْحَقِّ فَجَعَلْنَـٰهُمْ غُثَآءً ۚ فَبُعْدًا لِّلْقَوْمِ ٱلظَّـٰلِمِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2715,
      "verse_number": 42,
      "verse_key": "23:42",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَنشَأْنَا مِنۢ بَعْدِهِمْ قُرُونًا ءَاخَرِينَ",
      "page_number": 344,
      "juz_number": 18
    },
    {
      "id": 2716,
      "verse_number": 43,
      "verse_key": "23:43",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَا تَسْبِقُ مِنْ أُمَّةٍ أَجَلَهَا وَمَا يَسْتَـْٔخِرُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2717,
      "verse_number": 44,
      "verse_key": "23:44",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَرْسَلْنَا رُسُلَنَا تَتْرَا ۖ كُلَّ مَا جَآءَ أُمَّةً رَّسُولُهَا كَذَّبُوهُ ۚ فَأَتْبَعْنَا بَعْضَهُم بَعْضًا وَجَعَلْنَـٰهُمْ أَحَادِيثَ ۚ فَبُعْدًا لِّقَوْمٍ لَّا يُؤْمِنُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2718,
      "verse_number": 45,
      "verse_key": "23:45",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ أَرْسَلْنَا مُوسَىٰ وَأَخَاهُ هَـٰرُونَ بِـَٔايَـٰتِنَا وَسُلْطَـٰنٍ مُّبِينٍ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2719,
      "verse_number": 46,
      "verse_key": "23:46",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِلَىٰ فِرْعَوْنَ وَمَلَإِي۟هِۦ فَٱسْتَكْبَرُوا۟ وَكَانُوا۟ قَوْمًا عَالِينَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2720,
      "verse_number": 47,
      "verse_key": "23:47",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَقَالُوٓا۟ أَنُؤْمِنُ لِبَشَرَيْنِ مِثْلِنَا وَقَوْمُهُمَا لَنَا عَـٰبِدُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2721,
      "verse_number": 48,
      "verse_key": "23:48",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَكَذَّبُوهُمَا فَكَانُوا۟ مِنَ ٱلْمُهْلَكِينَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2722,
      "verse_number": 49,
      "verse_key": "23:49",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ ءَاتَيْنَا مُوسَى ٱلْكِتَـٰبَ لَعَلَّهُمْ يَهْتَدُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2723,
      "verse_number": 50,
      "verse_key": "23:50",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 297,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا ٱبْنَ مَرْيَمَ وَأُمَّهُۥٓ ءَايَةً وَءَاوَيْنَـٰهُمَآ إِلَىٰ رَبْوَةٍ ذَاتِ قَرَارٍ وَمَعِينٍ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2724,
      "verse_number": 51,
      "verse_key": "23:51",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "يَـٰٓأَيُّهَا ٱلرُّسُلُ كُلُوا۟ مِنَ ٱلطَّيِّبَـٰتِ وَٱعْمَلُوا۟ صَـٰلِحًا ۖ إِنِّى بِمَا تَعْمَلُونَ عَلِيمٌ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2725,
      "verse_number": 52,
      "verse_key": "23:52",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ هَـٰذِهِۦٓ أُمَّتُكُمْ أُمَّةً وَٰحِدَةً وَأَنَا۠ رَبُّكُمْ فَٱتَّقُونِ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2726,
      "verse_number": 53,
      "verse_key": "23:53",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَتَقَطَّعُوٓا۟ أَمْرَهُم بَيْنَهُمْ زُبُرًا ۖ كُلُّ حِزْبٍۭ بِمَا لَدَيْهِمْ فَرِحُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2727,
      "verse_number": 54,
      "verse_key": "23:54",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَذَرْهُمْ فِى غَمْرَتِهِمْ حَتَّىٰ حِينٍ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2728,
      "verse_number": 55,
      "verse_key": "23:55",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَيَحْسَبُونَ أَنَّمَا نُمِدُّهُم بِهِۦ مِن مَّالٍ وَبَنِينَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2729,
      "verse_number": 56,
      "verse_key": "23:56",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "نُسَارِعُ لَهُمْ فِى ٱلْخَيْرَٰتِ ۚ بَل لَّا يَشْعُرُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2730,
      "verse_number": 57,
      "verse_key": "23:57",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ هُم مِّنْ خَشْيَةِ رَبِّهِم مُّشْفِقُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2731,
      "verse_number": 58,
      "verse_key": "23:58",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُم بِـَٔايَـٰتِ رَبِّهِمْ يُؤْمِنُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2732,
      "verse_number": 59,
      "verse_key": "23:59",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ هُم بِرَبِّهِمْ لَا يُشْرِكُونَ",
      "page_number": 345,
      "juz_number": 18
    },
    {
      "id": 2733,
      "verse_number": 60,
      "verse_key": "23:60",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّذِينَ يُؤْتُونَ مَآ ءَاتَوا۟ وَّقُلُوبُهُمْ وَجِلَةٌ أَنَّهُمْ إِلَىٰ رَبِّهِمْ رَٰجِعُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2734,
      "verse_number": 61,
      "verse_key": "23:61",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أُو۟لَـٰٓئِكَ يُسَـٰرِعُونَ فِى ٱلْخَيْرَٰتِ وَهُمْ لَهَا سَـٰبِقُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2735,
      "verse_number": 62,
      "verse_key": "23:62",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَا نُكَلِّفُ نَفْسًا إِلَّا وُسْعَهَا ۖ وَلَدَيْنَا كِتَـٰبٌ يَنطِقُ بِٱلْحَقِّ ۚ وَهُمْ لَا يُظْلَمُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2736,
      "verse_number": 63,
      "verse_key": "23:63",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ قُلُوبُهُمْ فِى غَمْرَةٍ مِّنْ هَـٰذَا وَلَهُمْ أَعْمَـٰلٌ مِّن دُونِ ذَٰلِكَ هُمْ لَهَا عَـٰمِلُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2737,
      "verse_number": 64,
      "verse_key": "23:64",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰٓ إِذَآ أَخَذْنَا مُتْرَفِيهِم بِٱلْعَذَابِ إِذَا هُمْ يَجْـَٔرُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2738,
      "verse_number": 65,
      "verse_key": "23:65",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَا تَجْـَٔرُوا۟ ٱلْيَوْمَ ۖ إِنَّكُم مِّنَّا لَا تُنصَرُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2739,
      "verse_number": 66,
      "verse_key": "23:66",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَدْ كَانَتْ ءَايَـٰتِى تُتْلَىٰ عَلَيْكُمْ فَكُنتُمْ عَلَىٰٓ أَعْقَـٰبِكُمْ تَنكِصُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2740,
      "verse_number": 67,
      "verse_key": "23:67",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مُسْتَكْبِرِينَ بِهِۦ سَـٰمِرًا تَهْجُرُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2741,
      "verse_number": 68,
      "verse_key": "23:68",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَفَلَمْ يَدَّبَّرُوا۟ ٱلْقَوْلَ أَمْ جَآءَهُم مَّا لَمْ يَأْتِ ءَابَآءَهُمُ ٱلْأَوَّلِينَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2742,
      "verse_number": 69,
      "verse_key": "23:69",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمْ لَمْ يَعْرِفُوا۟ رَسُولَهُمْ فَهُمْ لَهُۥ مُنكِرُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2743,
      "verse_number": 70,
      "verse_key": "23:70",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمْ يَقُولُونَ بِهِۦ جِنَّةٌۢ ۚ بَلْ جَآءَهُم بِٱلْحَقِّ وَأَكْثَرُهُمْ لِلْحَقِّ كَـٰرِهُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2744,
      "verse_number": 71,
      "verse_key": "23:71",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَوِ ٱتَّبَعَ ٱلْحَقُّ أَهْوَآءَهُمْ لَفَسَدَتِ ٱلسَّمَـٰوَٰتُ وَٱلْأَرْضُ وَمَن فِيهِنَّ ۚ بَلْ أَتَيْنَـٰهُم بِذِكْرِهِمْ فَهُمْ عَن ذِكْرِهِم مُّعْرِضُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2745,
      "verse_number": 72,
      "verse_key": "23:72",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمْ تَسْـَٔلُهُمْ خَرْجًا فَخَرَاجُ رَبِّكَ خَيْرٌ ۖ وَهُوَ خَيْرُ ٱلرَّٰزِقِينَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2746,
      "verse_number": 73,
      "verse_key": "23:73",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّكَ لَتَدْعُوهُمْ إِلَىٰ صِرَٰطٍ مُّسْتَقِيمٍ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2747,
      "verse_number": 74,
      "verse_key": "23:74",
      "hizb_number": 35,
      "rub_el_hizb_number": 138,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّ ٱلَّذِينَ لَا يُؤْمِنُونَ بِٱلْـَٔاخِرَةِ عَنِ ٱلصِّرَٰطِ لَنَـٰكِبُونَ",
      "page_number": 346,
      "juz_number": 18
    },
    {
      "id": 2748,
      "verse_number": 75,
      "verse_key": "23:75",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ وَلَوْ رَحِمْنَـٰهُمْ وَكَشَفْنَا مَا بِهِم مِّن ضُرٍّ لَّلَجُّوا۟ فِى طُغْيَـٰنِهِمْ يَعْمَهُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2749,
      "verse_number": 76,
      "verse_key": "23:76",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَخَذْنَـٰهُم بِٱلْعَذَابِ فَمَا ٱسْتَكَانُوا۟ لِرَبِّهِمْ وَمَا يَتَضَرَّعُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2750,
      "verse_number": 77,
      "verse_key": "23:77",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 298,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰٓ إِذَا فَتَحْنَا عَلَيْهِم بَابًا ذَا عَذَابٍ شَدِيدٍ إِذَا هُمْ فِيهِ مُبْلِسُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2751,
      "verse_number": 78,
      "verse_key": "23:78",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلَّذِىٓ أَنشَأَ لَكُمُ ٱلسَّمْعَ وَٱلْأَبْصَـٰرَ وَٱلْأَفْـِٔدَةَ ۚ قَلِيلًا مَّا تَشْكُرُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2752,
      "verse_number": 79,
      "verse_key": "23:79",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلَّذِى ذَرَأَكُمْ فِى ٱلْأَرْضِ وَإِلَيْهِ تُحْشَرُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2753,
      "verse_number": 80,
      "verse_key": "23:80",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلَّذِى يُحْىِۦ وَيُمِيتُ وَلَهُ ٱخْتِلَـٰفُ ٱلَّيْلِ وَٱلنَّهَارِ ۚ أَفَلَا تَعْقِلُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2754,
      "verse_number": 81,
      "verse_key": "23:81",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ قَالُوا۟ مِثْلَ مَا قَالَ ٱلْأَوَّلُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2755,
      "verse_number": 82,
      "verse_key": "23:82",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوٓا۟ أَءِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَـٰمًا أَءِنَّا لَمَبْعُوثُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2756,
      "verse_number": 83,
      "verse_key": "23:83",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ وُعِدْنَا نَحْنُ وَءَابَآؤُنَا هَـٰذَا مِن قَبْلُ إِنْ هَـٰذَآ إِلَّآ أَسَـٰطِيرُ ٱلْأَوَّلِينَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2757,
      "verse_number": 84,
      "verse_key": "23:84",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُل لِّمَنِ ٱلْأَرْضُ وَمَن فِيهَآ إِن كُنتُمْ تَعْلَمُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2758,
      "verse_number": 85,
      "verse_key": "23:85",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "سَيَقُولُونَ لِلَّهِ ۚ قُلْ أَفَلَا تَذَكَّرُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2759,
      "verse_number": 86,
      "verse_key": "23:86",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْ مَن رَّبُّ ٱلسَّمَـٰوَٰتِ ٱلسَّبْعِ وَرَبُّ ٱلْعَرْشِ ٱلْعَظِيمِ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2760,
      "verse_number": 87,
      "verse_key": "23:87",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "سَيَقُولُونَ لِلَّهِ ۚ قُلْ أَفَلَا تَتَّقُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2761,
      "verse_number": 88,
      "verse_key": "23:88",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْ مَنۢ بِيَدِهِۦ مَلَكُوتُ كُلِّ شَىْءٍ وَهُوَ يُجِيرُ وَلَا يُجَارُ عَلَيْهِ إِن كُنتُمْ تَعْلَمُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2762,
      "verse_number": 89,
      "verse_key": "23:89",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "سَيَقُولُونَ لِلَّهِ ۚ قُلْ فَأَنَّىٰ تُسْحَرُونَ",
      "page_number": 347,
      "juz_number": 18
    },
    {
      "id": 2763,
      "verse_number": 90,
      "verse_key": "23:90",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ أَتَيْنَـٰهُم بِٱلْحَقِّ وَإِنَّهُمْ لَكَـٰذِبُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2764,
      "verse_number": 91,
      "verse_key": "23:91",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَا ٱتَّخَذَ ٱللَّهُ مِن وَلَدٍ وَمَا كَانَ مَعَهُۥ مِنْ إِلَـٰهٍ ۚ إِذًا لَّذَهَبَ كُلُّ إِلَـٰهٍۭ بِمَا خَلَقَ وَلَعَلَا بَعْضُهُمْ عَلَىٰ بَعْضٍ ۚ سُبْحَـٰنَ ٱللَّهِ عَمَّا يَصِفُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2765,
      "verse_number": 92,
      "verse_key": "23:92",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 299,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "عَـٰلِمِ ٱلْغَيْبِ وَٱلشَّهَـٰدَةِ فَتَعَـٰلَىٰ عَمَّا يُشْرِكُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2766,
      "verse_number": 93,
      "verse_key": "23:93",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُل رَّبِّ إِمَّا تُرِيَنِّى مَا يُوعَدُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2767,
      "verse_number": 94,
      "verse_key": "23:94",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "رَبِّ فَلَا تَجْعَلْنِى فِى ٱلْقَوْمِ ٱلظَّـٰلِمِينَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2768,
      "verse_number": 95,
      "verse_key": "23:95",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنَّا عَلَىٰٓ أَن نُّرِيَكَ مَا نَعِدُهُمْ لَقَـٰدِرُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2769,
      "verse_number": 96,
      "verse_key": "23:96",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱدْفَعْ بِٱلَّتِى هِىَ أَحْسَنُ ٱلسَّيِّئَةَ ۚ نَحْنُ أَعْلَمُ بِمَا يَصِفُونَ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2770,
      "verse_number": 97,
      "verse_key": "23:97",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَقُل رَّبِّ أَعُوذُ بِكَ مِنْ هَمَزَٰتِ ٱلشَّيَـٰطِينِ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2771,
      "verse_number": 98,
      "verse_key": "23:98",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَعُوذُ بِكَ رَبِّ أَن يَحْضُرُونِ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2772,
      "verse_number": 99,
      "verse_key": "23:99",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰٓ إِذَا جَآءَ أَحَدَهُمُ ٱلْمَوْتُ قَالَ رَبِّ ٱرْجِعُونِ",
      "page_number": 348,
      "juz_number": 18
    },
    {
      "id": 2773,
      "verse_number": 100,
      "verse_key": "23:100",
      "hizb_number": 35,
      "rub_el_hizb_number": 139,
      "ruku_number": 300,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَعَلِّىٓ أَعْمَلُ صَـٰلِحًا فِيمَا تَرَكْتُ ۚ كَلَّآ ۚ إِنَّهَا كَلِمَةٌ هُوَ قَآئِلُهَا ۖ وَمِن وَرَآئِهِم بَرْزَخٌ إِلَىٰ يَوْمِ يُبْعَثُونَ",
      "page_number": 348,
      "juz_number": 18
    }
  ]
}