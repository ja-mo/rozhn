{
  "id": 21,
  "name": "The Prophets",
  "verses": [
    {
      "id": 2484,
      "verse_number": 1,
      "verse_key": "21:1",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": " ٱقْتَرَبَ لِلنَّاسِ حِسَابُهُمْ وَهُمْ فِى غَفْلَةٍ مُّعْرِضُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2485,
      "verse_number": 2,
      "verse_key": "21:2",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَا يَأْتِيهِم مِّن ذِكْرٍ مِّن رَّبِّهِم مُّحْدَثٍ إِلَّا ٱسْتَمَعُوهُ وَهُمْ يَلْعَبُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2486,
      "verse_number": 3,
      "verse_key": "21:3",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَاهِيَةً قُلُوبُهُمْ ۗ وَأَسَرُّوا۟ ٱلنَّجْوَى ٱلَّذِينَ ظَلَمُوا۟ هَلْ هَـٰذَآ إِلَّا بَشَرٌ مِّثْلُكُمْ ۖ أَفَتَأْتُونَ ٱلسِّحْرَ وَأَنتُمْ تُبْصِرُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2487,
      "verse_number": 4,
      "verse_key": "21:4",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّى يَعْلَمُ ٱلْقَوْلَ فِى ٱلسَّمَآءِ وَٱلْأَرْضِ ۖ وَهُوَ ٱلسَّمِيعُ ٱلْعَلِيمُ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2488,
      "verse_number": 5,
      "verse_key": "21:5",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ قَالُوٓا۟ أَضْغَـٰثُ أَحْلَـٰمٍۭ بَلِ ٱفْتَرَىٰهُ بَلْ هُوَ شَاعِرٌ فَلْيَأْتِنَا بِـَٔايَةٍ كَمَآ أُرْسِلَ ٱلْأَوَّلُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2489,
      "verse_number": 6,
      "verse_key": "21:6",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَآ ءَامَنَتْ قَبْلَهُم مِّن قَرْيَةٍ أَهْلَكْنَـٰهَآ ۖ أَفَهُمْ يُؤْمِنُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2490,
      "verse_number": 7,
      "verse_key": "21:7",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَرْسَلْنَا قَبْلَكَ إِلَّا رِجَالًا نُّوحِىٓ إِلَيْهِمْ ۖ فَسْـَٔلُوٓا۟ أَهْلَ ٱلذِّكْرِ إِن كُنتُمْ لَا تَعْلَمُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2491,
      "verse_number": 8,
      "verse_key": "21:8",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَا جَعَلْنَـٰهُمْ جَسَدًا لَّا يَأْكُلُونَ ٱلطَّعَامَ وَمَا كَانُوا۟ خَـٰلِدِينَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2492,
      "verse_number": 9,
      "verse_key": "21:9",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ صَدَقْنَـٰهُمُ ٱلْوَعْدَ فَأَنجَيْنَـٰهُمْ وَمَن نَّشَآءُ وَأَهْلَكْنَا ٱلْمُسْرِفِينَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2493,
      "verse_number": 10,
      "verse_key": "21:10",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 278,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ أَنزَلْنَآ إِلَيْكُمْ كِتَـٰبًا فِيهِ ذِكْرُكُمْ ۖ أَفَلَا تَعْقِلُونَ",
      "page_number": 322,
      "juz_number": 17
    },
    {
      "id": 2494,
      "verse_number": 11,
      "verse_key": "21:11",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَكَمْ قَصَمْنَا مِن قَرْيَةٍ كَانَتْ ظَالِمَةً وَأَنشَأْنَا بَعْدَهَا قَوْمًا ءَاخَرِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2495,
      "verse_number": 12,
      "verse_key": "21:12",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَلَمَّآ أَحَسُّوا۟ بَأْسَنَآ إِذَا هُم مِّنْهَا يَرْكُضُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2496,
      "verse_number": 13,
      "verse_key": "21:13",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَا تَرْكُضُوا۟ وَٱرْجِعُوٓا۟ إِلَىٰ مَآ أُتْرِفْتُمْ فِيهِ وَمَسَـٰكِنِكُمْ لَعَلَّكُمْ تُسْـَٔلُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2497,
      "verse_number": 14,
      "verse_key": "21:14",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ يَـٰوَيْلَنَآ إِنَّا كُنَّا ظَـٰلِمِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2498,
      "verse_number": 15,
      "verse_key": "21:15",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَمَا زَالَت تِّلْكَ دَعْوَىٰهُمْ حَتَّىٰ جَعَلْنَـٰهُمْ حَصِيدًا خَـٰمِدِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2499,
      "verse_number": 16,
      "verse_key": "21:16",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَا خَلَقْنَا ٱلسَّمَآءَ وَٱلْأَرْضَ وَمَا بَيْنَهُمَا لَـٰعِبِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2500,
      "verse_number": 17,
      "verse_key": "21:17",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَوْ أَرَدْنَآ أَن نَّتَّخِذَ لَهْوًا لَّٱتَّخَذْنَـٰهُ مِن لَّدُنَّآ إِن كُنَّا فَـٰعِلِينَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2501,
      "verse_number": 18,
      "verse_key": "21:18",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ نَقْذِفُ بِٱلْحَقِّ عَلَى ٱلْبَـٰطِلِ فَيَدْمَغُهُۥ فَإِذَا هُوَ زَاهِقٌ ۚ وَلَكُمُ ٱلْوَيْلُ مِمَّا تَصِفُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2502,
      "verse_number": 19,
      "verse_key": "21:19",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَهُۥ مَن فِى ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ۚ وَمَنْ عِندَهُۥ لَا يَسْتَكْبِرُونَ عَنْ عِبَادَتِهِۦ وَلَا يَسْتَحْسِرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2503,
      "verse_number": 20,
      "verse_key": "21:20",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "يُسَبِّحُونَ ٱلَّيْلَ وَٱلنَّهَارَ لَا يَفْتُرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2504,
      "verse_number": 21,
      "verse_key": "21:21",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمِ ٱتَّخَذُوٓا۟ ءَالِهَةً مِّنَ ٱلْأَرْضِ هُمْ يُنشِرُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2505,
      "verse_number": 22,
      "verse_key": "21:22",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَوْ كَانَ فِيهِمَآ ءَالِهَةٌ إِلَّا ٱللَّهُ لَفَسَدَتَا ۚ فَسُبْحَـٰنَ ٱللَّهِ رَبِّ ٱلْعَرْشِ عَمَّا يَصِفُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2506,
      "verse_number": 23,
      "verse_key": "21:23",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَا يُسْـَٔلُ عَمَّا يَفْعَلُ وَهُمْ يُسْـَٔلُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2507,
      "verse_number": 24,
      "verse_key": "21:24",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمِ ٱتَّخَذُوا۟ مِن دُونِهِۦٓ ءَالِهَةً ۖ قُلْ هَاتُوا۟ بُرْهَـٰنَكُمْ ۖ هَـٰذَا ذِكْرُ مَن مَّعِىَ وَذِكْرُ مَن قَبْلِى ۗ بَلْ أَكْثَرُهُمْ لَا يَعْلَمُونَ ٱلْحَقَّ ۖ فَهُم مُّعْرِضُونَ",
      "page_number": 323,
      "juz_number": 17
    },
    {
      "id": 2508,
      "verse_number": 25,
      "verse_key": "21:25",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَرْسَلْنَا مِن قَبْلِكَ مِن رَّسُولٍ إِلَّا نُوحِىٓ إِلَيْهِ أَنَّهُۥ لَآ إِلَـٰهَ إِلَّآ أَنَا۠ فَٱعْبُدُونِ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2509,
      "verse_number": 26,
      "verse_key": "21:26",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَقَالُوا۟ ٱتَّخَذَ ٱلرَّحْمَـٰنُ وَلَدًا ۗ سُبْحَـٰنَهُۥ ۚ بَلْ عِبَادٌ مُّكْرَمُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2510,
      "verse_number": 27,
      "verse_key": "21:27",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَا يَسْبِقُونَهُۥ بِٱلْقَوْلِ وَهُم بِأَمْرِهِۦ يَعْمَلُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2511,
      "verse_number": 28,
      "verse_key": "21:28",
      "hizb_number": 33,
      "rub_el_hizb_number": 129,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "يَعْلَمُ مَا بَيْنَ أَيْدِيهِمْ وَمَا خَلْفَهُمْ وَلَا يَشْفَعُونَ إِلَّا لِمَنِ ٱرْتَضَىٰ وَهُم مِّنْ خَشْيَتِهِۦ مُشْفِقُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2512,
      "verse_number": 29,
      "verse_key": "21:29",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 279,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ وَمَن يَقُلْ مِنْهُمْ إِنِّىٓ إِلَـٰهٌ مِّن دُونِهِۦ فَذَٰلِكَ نَجْزِيهِ جَهَنَّمَ ۚ كَذَٰلِكَ نَجْزِى ٱلظَّـٰلِمِينَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2513,
      "verse_number": 30,
      "verse_key": "21:30",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَوَلَمْ يَرَ ٱلَّذِينَ كَفَرُوٓا۟ أَنَّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضَ كَانَتَا رَتْقًا فَفَتَقْنَـٰهُمَا ۖ وَجَعَلْنَا مِنَ ٱلْمَآءِ كُلَّ شَىْءٍ حَىٍّ ۖ أَفَلَا يُؤْمِنُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2514,
      "verse_number": 31,
      "verse_key": "21:31",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا فِى ٱلْأَرْضِ رَوَٰسِىَ أَن تَمِيدَ بِهِمْ وَجَعَلْنَا فِيهَا فِجَاجًا سُبُلًا لَّعَلَّهُمْ يَهْتَدُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2515,
      "verse_number": 32,
      "verse_key": "21:32",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَا ٱلسَّمَآءَ سَقْفًا مَّحْفُوظًا ۖ وَهُمْ عَنْ ءَايَـٰتِهَا مُعْرِضُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2516,
      "verse_number": 33,
      "verse_key": "21:33",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ ٱلَّذِى خَلَقَ ٱلَّيْلَ وَٱلنَّهَارَ وَٱلشَّمْسَ وَٱلْقَمَرَ ۖ كُلٌّ فِى فَلَكٍ يَسْبَحُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2517,
      "verse_number": 34,
      "verse_key": "21:34",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَا جَعَلْنَا لِبَشَرٍ مِّن قَبْلِكَ ٱلْخُلْدَ ۖ أَفَإِي۟ن مِّتَّ فَهُمُ ٱلْخَـٰلِدُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2518,
      "verse_number": 35,
      "verse_key": "21:35",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "كُلُّ نَفْسٍ ذَآئِقَةُ ٱلْمَوْتِ ۗ وَنَبْلُوكُم بِٱلشَّرِّ وَٱلْخَيْرِ فِتْنَةً ۖ وَإِلَيْنَا تُرْجَعُونَ",
      "page_number": 324,
      "juz_number": 17
    },
    {
      "id": 2519,
      "verse_number": 36,
      "verse_key": "21:36",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا رَءَاكَ ٱلَّذِينَ كَفَرُوٓا۟ إِن يَتَّخِذُونَكَ إِلَّا هُزُوًا أَهَـٰذَا ٱلَّذِى يَذْكُرُ ءَالِهَتَكُمْ وَهُم بِذِكْرِ ٱلرَّحْمَـٰنِ هُمْ كَـٰفِرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2520,
      "verse_number": 37,
      "verse_key": "21:37",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "خُلِقَ ٱلْإِنسَـٰنُ مِنْ عَجَلٍ ۚ سَأُو۟رِيكُمْ ءَايَـٰتِى فَلَا تَسْتَعْجِلُونِ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2521,
      "verse_number": 38,
      "verse_key": "21:38",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَيَقُولُونَ مَتَىٰ هَـٰذَا ٱلْوَعْدُ إِن كُنتُمْ صَـٰدِقِينَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2522,
      "verse_number": 39,
      "verse_key": "21:39",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَوْ يَعْلَمُ ٱلَّذِينَ كَفَرُوا۟ حِينَ لَا يَكُفُّونَ عَن وُجُوهِهِمُ ٱلنَّارَ وَلَا عَن ظُهُورِهِمْ وَلَا هُمْ يُنصَرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2523,
      "verse_number": 40,
      "verse_key": "21:40",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ تَأْتِيهِم بَغْتَةً فَتَبْهَتُهُمْ فَلَا يَسْتَطِيعُونَ رَدَّهَا وَلَا هُمْ يُنظَرُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2524,
      "verse_number": 41,
      "verse_key": "21:41",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 280,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدِ ٱسْتُهْزِئَ بِرُسُلٍ مِّن قَبْلِكَ فَحَاقَ بِٱلَّذِينَ سَخِرُوا۟ مِنْهُم مَّا كَانُوا۟ بِهِۦ يَسْتَهْزِءُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2525,
      "verse_number": 42,
      "verse_key": "21:42",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْ مَن يَكْلَؤُكُم بِٱلَّيْلِ وَٱلنَّهَارِ مِنَ ٱلرَّحْمَـٰنِ ۗ بَلْ هُمْ عَن ذِكْرِ رَبِّهِم مُّعْرِضُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2526,
      "verse_number": 43,
      "verse_key": "21:43",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَمْ لَهُمْ ءَالِهَةٌ تَمْنَعُهُم مِّن دُونِنَا ۚ لَا يَسْتَطِيعُونَ نَصْرَ أَنفُسِهِمْ وَلَا هُم مِّنَّا يُصْحَبُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2527,
      "verse_number": 44,
      "verse_key": "21:44",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "بَلْ مَتَّعْنَا هَـٰٓؤُلَآءِ وَءَابَآءَهُمْ حَتَّىٰ طَالَ عَلَيْهِمُ ٱلْعُمُرُ ۗ أَفَلَا يَرَوْنَ أَنَّا نَأْتِى ٱلْأَرْضَ نَنقُصُهَا مِنْ أَطْرَافِهَآ ۚ أَفَهُمُ ٱلْغَـٰلِبُونَ",
      "page_number": 325,
      "juz_number": 17
    },
    {
      "id": 2528,
      "verse_number": 45,
      "verse_key": "21:45",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْ إِنَّمَآ أُنذِرُكُم بِٱلْوَحْىِ ۚ وَلَا يَسْمَعُ ٱلصُّمُّ ٱلدُّعَآءَ إِذَا مَا يُنذَرُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2529,
      "verse_number": 46,
      "verse_key": "21:46",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَئِن مَّسَّتْهُمْ نَفْحَةٌ مِّنْ عَذَابِ رَبِّكَ لَيَقُولُنَّ يَـٰوَيْلَنَآ إِنَّا كُنَّا ظَـٰلِمِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2530,
      "verse_number": 47,
      "verse_key": "21:47",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَنَضَعُ ٱلْمَوَٰزِينَ ٱلْقِسْطَ لِيَوْمِ ٱلْقِيَـٰمَةِ فَلَا تُظْلَمُ نَفْسٌ شَيْـًٔا ۖ وَإِن كَانَ مِثْقَالَ حَبَّةٍ مِّنْ خَرْدَلٍ أَتَيْنَا بِهَا ۗ وَكَفَىٰ بِنَا حَـٰسِبِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2531,
      "verse_number": 48,
      "verse_key": "21:48",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ ءَاتَيْنَا مُوسَىٰ وَهَـٰرُونَ ٱلْفُرْقَانَ وَضِيَآءً وَذِكْرًا لِّلْمُتَّقِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2532,
      "verse_number": 49,
      "verse_key": "21:49",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ يَخْشَوْنَ رَبَّهُم بِٱلْغَيْبِ وَهُم مِّنَ ٱلسَّاعَةِ مُشْفِقُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2533,
      "verse_number": 50,
      "verse_key": "21:50",
      "hizb_number": 33,
      "rub_el_hizb_number": 130,
      "ruku_number": 281,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهَـٰذَا ذِكْرٌ مُّبَارَكٌ أَنزَلْنَـٰهُ ۚ أَفَأَنتُمْ لَهُۥ مُنكِرُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2534,
      "verse_number": 51,
      "verse_key": "21:51",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ وَلَقَدْ ءَاتَيْنَآ إِبْرَٰهِيمَ رُشْدَهُۥ مِن قَبْلُ وَكُنَّا بِهِۦ عَـٰلِمِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2535,
      "verse_number": 52,
      "verse_key": "21:52",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِذْ قَالَ لِأَبِيهِ وَقَوْمِهِۦ مَا هَـٰذِهِ ٱلتَّمَاثِيلُ ٱلَّتِىٓ أَنتُمْ لَهَا عَـٰكِفُونَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2536,
      "verse_number": 53,
      "verse_key": "21:53",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ وَجَدْنَآ ءَابَآءَنَا لَهَا عَـٰبِدِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2537,
      "verse_number": 54,
      "verse_key": "21:54",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ لَقَدْ كُنتُمْ أَنتُمْ وَءَابَآؤُكُمْ فِى ضَلَـٰلٍ مُّبِينٍ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2538,
      "verse_number": 55,
      "verse_key": "21:55",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوٓا۟ أَجِئْتَنَا بِٱلْحَقِّ أَمْ أَنتَ مِنَ ٱللَّـٰعِبِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2539,
      "verse_number": 56,
      "verse_key": "21:56",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ بَل رَّبُّكُمْ رَبُّ ٱلسَّمَـٰوَٰتِ وَٱلْأَرْضِ ٱلَّذِى فَطَرَهُنَّ وَأَنَا۠ عَلَىٰ ذَٰلِكُم مِّنَ ٱلشَّـٰهِدِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2540,
      "verse_number": 57,
      "verse_key": "21:57",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَتَٱللَّهِ لَأَكِيدَنَّ أَصْنَـٰمَكُم بَعْدَ أَن تُوَلُّوا۟ مُدْبِرِينَ",
      "page_number": 326,
      "juz_number": 17
    },
    {
      "id": 2541,
      "verse_number": 58,
      "verse_key": "21:58",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَجَعَلَهُمْ جُذَٰذًا إِلَّا كَبِيرًا لَّهُمْ لَعَلَّهُمْ إِلَيْهِ يَرْجِعُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2542,
      "verse_number": 59,
      "verse_key": "21:59",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ مَن فَعَلَ هَـٰذَا بِـَٔالِهَتِنَآ إِنَّهُۥ لَمِنَ ٱلظَّـٰلِمِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2543,
      "verse_number": 60,
      "verse_key": "21:60",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ سَمِعْنَا فَتًى يَذْكُرُهُمْ يُقَالُ لَهُۥٓ إِبْرَٰهِيمُ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2544,
      "verse_number": 61,
      "verse_key": "21:61",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ فَأْتُوا۟ بِهِۦ عَلَىٰٓ أَعْيُنِ ٱلنَّاسِ لَعَلَّهُمْ يَشْهَدُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2545,
      "verse_number": 62,
      "verse_key": "21:62",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوٓا۟ ءَأَنتَ فَعَلْتَ هَـٰذَا بِـَٔالِهَتِنَا يَـٰٓإِبْرَٰهِيمُ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2546,
      "verse_number": 63,
      "verse_key": "21:63",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ بَلْ فَعَلَهُۥ كَبِيرُهُمْ هَـٰذَا فَسْـَٔلُوهُمْ إِن كَانُوا۟ يَنطِقُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2547,
      "verse_number": 64,
      "verse_key": "21:64",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَرَجَعُوٓا۟ إِلَىٰٓ أَنفُسِهِمْ فَقَالُوٓا۟ إِنَّكُمْ أَنتُمُ ٱلظَّـٰلِمُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2548,
      "verse_number": 65,
      "verse_key": "21:65",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ نُكِسُوا۟ عَلَىٰ رُءُوسِهِمْ لَقَدْ عَلِمْتَ مَا هَـٰٓؤُلَآءِ يَنطِقُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2549,
      "verse_number": 66,
      "verse_key": "21:66",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ أَفَتَعْبُدُونَ مِن دُونِ ٱللَّهِ مَا لَا يَنفَعُكُمْ شَيْـًٔا وَلَا يَضُرُّكُمْ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2550,
      "verse_number": 67,
      "verse_key": "21:67",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أُفٍّ لَّكُمْ وَلِمَا تَعْبُدُونَ مِن دُونِ ٱللَّهِ ۖ أَفَلَا تَعْقِلُونَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2551,
      "verse_number": 68,
      "verse_key": "21:68",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ حَرِّقُوهُ وَٱنصُرُوٓا۟ ءَالِهَتَكُمْ إِن كُنتُمْ فَـٰعِلِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2552,
      "verse_number": 69,
      "verse_key": "21:69",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْنَا يَـٰنَارُ كُونِى بَرْدًا وَسَلَـٰمًا عَلَىٰٓ إِبْرَٰهِيمَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2553,
      "verse_number": 70,
      "verse_key": "21:70",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَرَادُوا۟ بِهِۦ كَيْدًا فَجَعَلْنَـٰهُمُ ٱلْأَخْسَرِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2554,
      "verse_number": 71,
      "verse_key": "21:71",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَنَجَّيْنَـٰهُ وَلُوطًا إِلَى ٱلْأَرْضِ ٱلَّتِى بَـٰرَكْنَا فِيهَا لِلْعَـٰلَمِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2555,
      "verse_number": 72,
      "verse_key": "21:72",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَوَهَبْنَا لَهُۥٓ إِسْحَـٰقَ وَيَعْقُوبَ نَافِلَةً ۖ وَكُلًّا جَعَلْنَا صَـٰلِحِينَ",
      "page_number": 327,
      "juz_number": 17
    },
    {
      "id": 2556,
      "verse_number": 73,
      "verse_key": "21:73",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَجَعَلْنَـٰهُمْ أَئِمَّةً يَهْدُونَ بِأَمْرِنَا وَأَوْحَيْنَآ إِلَيْهِمْ فِعْلَ ٱلْخَيْرَٰتِ وَإِقَامَ ٱلصَّلَوٰةِ وَإِيتَآءَ ٱلزَّكَوٰةِ ۖ وَكَانُوا۟ لَنَا عَـٰبِدِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2557,
      "verse_number": 74,
      "verse_key": "21:74",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلُوطًا ءَاتَيْنَـٰهُ حُكْمًا وَعِلْمًا وَنَجَّيْنَـٰهُ مِنَ ٱلْقَرْيَةِ ٱلَّتِى كَانَت تَّعْمَلُ ٱلْخَبَـٰٓئِثَ ۗ إِنَّهُمْ كَانُوا۟ قَوْمَ سَوْءٍ فَـٰسِقِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2558,
      "verse_number": 75,
      "verse_key": "21:75",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 282,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَدْخَلْنَـٰهُ فِى رَحْمَتِنَآ ۖ إِنَّهُۥ مِنَ ٱلصَّـٰلِحِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2559,
      "verse_number": 76,
      "verse_key": "21:76",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَنُوحًا إِذْ نَادَىٰ مِن قَبْلُ فَٱسْتَجَبْنَا لَهُۥ فَنَجَّيْنَـٰهُ وَأَهْلَهُۥ مِنَ ٱلْكَرْبِ ٱلْعَظِيمِ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2560,
      "verse_number": 77,
      "verse_key": "21:77",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَنَصَرْنَـٰهُ مِنَ ٱلْقَوْمِ ٱلَّذِينَ كَذَّبُوا۟ بِـَٔايَـٰتِنَآ ۚ إِنَّهُمْ كَانُوا۟ قَوْمَ سَوْءٍ فَأَغْرَقْنَـٰهُمْ أَجْمَعِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2561,
      "verse_number": 78,
      "verse_key": "21:78",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَدَاوُۥدَ وَسُلَيْمَـٰنَ إِذْ يَحْكُمَانِ فِى ٱلْحَرْثِ إِذْ نَفَشَتْ فِيهِ غَنَمُ ٱلْقَوْمِ وَكُنَّا لِحُكْمِهِمْ شَـٰهِدِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2562,
      "verse_number": 79,
      "verse_key": "21:79",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَفَهَّمْنَـٰهَا سُلَيْمَـٰنَ ۚ وَكُلًّا ءَاتَيْنَا حُكْمًا وَعِلْمًا ۚ وَسَخَّرْنَا مَعَ دَاوُۥدَ ٱلْجِبَالَ يُسَبِّحْنَ وَٱلطَّيْرَ ۚ وَكُنَّا فَـٰعِلِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2563,
      "verse_number": 80,
      "verse_key": "21:80",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَعَلَّمْنَـٰهُ صَنْعَةَ لَبُوسٍ لَّكُمْ لِتُحْصِنَكُم مِّنۢ بَأْسِكُمْ ۖ فَهَلْ أَنتُمْ شَـٰكِرُونَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2564,
      "verse_number": 81,
      "verse_key": "21:81",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلِسُلَيْمَـٰنَ ٱلرِّيحَ عَاصِفَةً تَجْرِى بِأَمْرِهِۦٓ إِلَى ٱلْأَرْضِ ٱلَّتِى بَـٰرَكْنَا فِيهَا ۚ وَكُنَّا بِكُلِّ شَىْءٍ عَـٰلِمِينَ",
      "page_number": 328,
      "juz_number": 17
    },
    {
      "id": 2565,
      "verse_number": 82,
      "verse_key": "21:82",
      "hizb_number": 33,
      "rub_el_hizb_number": 131,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمِنَ ٱلشَّيَـٰطِينِ مَن يَغُوصُونَ لَهُۥ وَيَعْمَلُونَ عَمَلًا دُونَ ذَٰلِكَ ۖ وَكُنَّا لَهُمْ حَـٰفِظِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2566,
      "verse_number": 83,
      "verse_key": "21:83",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ وَأَيُّوبَ إِذْ نَادَىٰ رَبَّهُۥٓ أَنِّى مَسَّنِىَ ٱلضُّرُّ وَأَنتَ أَرْحَمُ ٱلرَّٰحِمِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2567,
      "verse_number": 84,
      "verse_key": "21:84",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَٱسْتَجَبْنَا لَهُۥ فَكَشَفْنَا مَا بِهِۦ مِن ضُرٍّ ۖ وَءَاتَيْنَـٰهُ أَهْلَهُۥ وَمِثْلَهُم مَّعَهُمْ رَحْمَةً مِّنْ عِندِنَا وَذِكْرَىٰ لِلْعَـٰبِدِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2568,
      "verse_number": 85,
      "verse_key": "21:85",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِسْمَـٰعِيلَ وَإِدْرِيسَ وَذَا ٱلْكِفْلِ ۖ كُلٌّ مِّنَ ٱلصَّـٰبِرِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2569,
      "verse_number": 86,
      "verse_key": "21:86",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَدْخَلْنَـٰهُمْ فِى رَحْمَتِنَآ ۖ إِنَّهُم مِّنَ ٱلصَّـٰلِحِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2570,
      "verse_number": 87,
      "verse_key": "21:87",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَذَا ٱلنُّونِ إِذ ذَّهَبَ مُغَـٰضِبًا فَظَنَّ أَن لَّن نَّقْدِرَ عَلَيْهِ فَنَادَىٰ فِى ٱلظُّلُمَـٰتِ أَن لَّآ إِلَـٰهَ إِلَّآ أَنتَ سُبْحَـٰنَكَ إِنِّى كُنتُ مِنَ ٱلظَّـٰلِمِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2571,
      "verse_number": 88,
      "verse_key": "21:88",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَٱسْتَجَبْنَا لَهُۥ وَنَجَّيْنَـٰهُ مِنَ ٱلْغَمِّ ۚ وَكَذَٰلِكَ نُـۨجِى ٱلْمُؤْمِنِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2572,
      "verse_number": 89,
      "verse_key": "21:89",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَزَكَرِيَّآ إِذْ نَادَىٰ رَبَّهُۥ رَبِّ لَا تَذَرْنِى فَرْدًا وَأَنتَ خَيْرُ ٱلْوَٰرِثِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2573,
      "verse_number": 90,
      "verse_key": "21:90",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَٱسْتَجَبْنَا لَهُۥ وَوَهَبْنَا لَهُۥ يَحْيَىٰ وَأَصْلَحْنَا لَهُۥ زَوْجَهُۥٓ ۚ إِنَّهُمْ كَانُوا۟ يُسَـٰرِعُونَ فِى ٱلْخَيْرَٰتِ وَيَدْعُونَنَا رَغَبًا وَرَهَبًا ۖ وَكَانُوا۟ لَنَا خَـٰشِعِينَ",
      "page_number": 329,
      "juz_number": 17
    },
    {
      "id": 2574,
      "verse_number": 91,
      "verse_key": "21:91",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّتِىٓ أَحْصَنَتْ فَرْجَهَا فَنَفَخْنَا فِيهَا مِن رُّوحِنَا وَجَعَلْنَـٰهَا وَٱبْنَهَآ ءَايَةً لِّلْعَـٰلَمِينَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2575,
      "verse_number": 92,
      "verse_key": "21:92",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ هَـٰذِهِۦٓ أُمَّتُكُمْ أُمَّةً وَٰحِدَةً وَأَنَا۠ رَبُّكُمْ فَٱعْبُدُونِ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2576,
      "verse_number": 93,
      "verse_key": "21:93",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 283,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَتَقَطَّعُوٓا۟ أَمْرَهُم بَيْنَهُمْ ۖ كُلٌّ إِلَيْنَا رَٰجِعُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2577,
      "verse_number": 94,
      "verse_key": "21:94",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَمَن يَعْمَلْ مِنَ ٱلصَّـٰلِحَـٰتِ وَهُوَ مُؤْمِنٌ فَلَا كُفْرَانَ لِسَعْيِهِۦ وَإِنَّا لَهُۥ كَـٰتِبُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2578,
      "verse_number": 95,
      "verse_key": "21:95",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَحَرَٰمٌ عَلَىٰ قَرْيَةٍ أَهْلَكْنَـٰهَآ أَنَّهُمْ لَا يَرْجِعُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2579,
      "verse_number": 96,
      "verse_key": "21:96",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "حَتَّىٰٓ إِذَا فُتِحَتْ يَأْجُوجُ وَمَأْجُوجُ وَهُم مِّن كُلِّ حَدَبٍ يَنسِلُونَ",
      "page_number": 330,
      "juz_number": 17
    },
    {
      "id": 2580,
      "verse_number": 97,
      "verse_key": "21:97",
      "hizb_number": 33,
      "rub_el_hizb_number": 132,
      "ruku_number": 284,
      "manzil_number": 4,
      "sajdah_number": null,
