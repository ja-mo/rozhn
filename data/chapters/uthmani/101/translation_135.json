{
  "id": 101,
  "name": "The Calamity",
  "verses": [
    {
      "id": 6158,
      "verse_number": 1,
      "verse_key": "101:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928823,
          "resource_id": 135,
          "text": "[آن] فروکوبنده."
        }
      ]
    },
    {
      "id": 6159,
      "verse_number": 2,
      "verse_key": "101:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928824,
          "resource_id": 135,
          "text": "[آن] فروکوبنده چیست؟"
        }
      ]
    },
    {
      "id": 6160,
      "verse_number": 3,
      "verse_key": "101:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928825,
          "resource_id": 135,
          "text": "و تو چه ‌دانی که [آن] فروکوبنده چیست؟"
        }
      ]
    },
    {
      "id": 6161,
      "verse_number": 4,
      "verse_key": "101:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928826,
          "resource_id": 135,
          "text": "روزی‌ که مردم مانند پروانه‌های پراکنده [حیران و سرگردان] خواهند بود."
        }
      ]
    },
    {
      "id": 6162,
      "verse_number": 5,
      "verse_key": "101:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928827,
          "resource_id": 135,
          "text": "و کوه‌ها مانند پشم رنگینِ حلاجی‌شده خواهند شد."
        }
      ]
    },
    {
      "id": 6163,
      "verse_number": 6,
      "verse_key": "101:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928828,
          "resource_id": 135,
          "text": "و اما هر کس که [در آن روز،] کفّۀ میزانش سنگین باشد،"
        }
      ]
    },
    {
      "id": 6164,
      "verse_number": 7,
      "verse_key": "101:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928829,
          "resource_id": 135,
          "text": "[در بهشت،] در زندگی رضایت‌بخشی خواهد بود."
        }
      ]
    },
    {
      "id": 6165,
      "verse_number": 8,
      "verse_key": "101:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928830,
          "resource_id": 135,
          "text": "و اما هر کس که کفّۀ میزانش سبک باشد،"
        }
      ]
    },
    {
      "id": 6166,
      "verse_number": 9,
      "verse_key": "101:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928831,
          "resource_id": 135,
          "text": "[مسکن و] پناهگاهش «هاویه» است."
        }
      ]
    },
    {
      "id": 6167,
      "verse_number": 10,
      "verse_key": "101:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928832,
          "resource_id": 135,
          "text": "و تو چه دانی که آن [هاویه] چیست؟"
        }
      ]
    },
    {
      "id": 6168,
      "verse_number": 11,
      "verse_key": "101:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 545,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 600,
      "juz_number": 30,
      "translations": [
        {
          "id": 928833,
          "resource_id": 135,
          "text": "آتشی است سوزان [و شعله‌ور]."
        }
      ]
    }
  ]
}