{
  "id": 88,
  "name": "The Overwhelming",
  "verses": [
    {
      "id": 5968,
      "verse_number": 1,
      "verse_key": "88:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928633,
          "resource_id": 135,
          "text": "[ای پیامبر،] آیا خبرِ قیامت هولناکِ فراگیر به تو رسیده است؟"
        }
      ]
    },
    {
      "id": 5969,
      "verse_number": 2,
      "verse_key": "88:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928634,
          "resource_id": 135,
          "text": "در آن روز، چهره‌هایی خوار و ذلیل خواهند بود."
        }
      ]
    },
    {
      "id": 5970,
      "verse_number": 3,
      "verse_key": "88:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928635,
          "resource_id": 135,
          "text": "[در دنیا] پیوسته تلاش کرده [و] خسته شده [و چون در راه حق نبوده، نتیجه‌ای ندیده‌اند]."
        }
      ]
    },
    {
      "id": 5971,
      "verse_number": 4,
      "verse_key": "88:4",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928636,
          "resource_id": 135,
          "text": "در آتش سوزان درمی‌آیند،"
        }
      ]
    },
    {
      "id": 5972,
      "verse_number": 5,
      "verse_key": "88:5",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928637,
          "resource_id": 135,
          "text": "از چشمۀ بسیار داغ به آنها نوشانیده می‌شود."
        }
      ]
    },
    {
      "id": 5973,
      "verse_number": 6,
      "verse_key": "88:6",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928638,
          "resource_id": 135,
          "text": "غذایی جز خارِ خشک و تلخ ندارند"
        }
      ]
    },
    {
      "id": 5974,
      "verse_number": 7,
      "verse_key": "88:7",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928639,
          "resource_id": 135,
          "text": "که نه [آنان را] فربه کند و نه گرسنگی را دفع نماید."
        }
      ]
    },
    {
      "id": 5975,
      "verse_number": 8,
      "verse_key": "88:8",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928640,
          "resource_id": 135,
          "text": "در آن روز، چهره‌هایی شاداب و تازه هستند"
        }
      ]
    },
    {
      "id": 5976,
      "verse_number": 9,
      "verse_key": "88:9",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928641,
          "resource_id": 135,
          "text": "و از سعی [و تلاش] خود راضیند؛"
        }
      ]
    },
    {
      "id": 5977,
      "verse_number": 10,
      "verse_key": "88:10",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928642,
          "resource_id": 135,
          "text": "در بهشت برین هستند."
        }
      ]
    },
    {
      "id": 5978,
      "verse_number": 11,
      "verse_key": "88:11",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928643,
          "resource_id": 135,
          "text": "در آنجا هیچ سخن بیهوده‌ای نمی‌شنوند."
        }
      ]
    },
    {
      "id": 5979,
      "verse_number": 12,
      "verse_key": "88:12",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928644,
          "resource_id": 135,
          "text": "در آن، چشمه[های آب] روان است."
        }
      ]
    },
    {
      "id": 5980,
      "verse_number": 13,
      "verse_key": "88:13",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928645,
          "resource_id": 135,
          "text": "تخت‌های [زیبای] بلند [پایه] در آن است."
        }
      ]
    },
    {
      "id": 5981,
      "verse_number": 14,
      "verse_key": "88:14",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928646,
          "resource_id": 135,
          "text": "و جام‌هایی [که در کنار چشمه‌ها] نهاده شده."
        }
      ]
    },
    {
      "id": 5982,
      "verse_number": 15,
      "verse_key": "88:15",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928647,
          "resource_id": 135,
          "text": "و بالشت‌های [منظم] چیده‌شده،"
        }
      ]
    },
    {
      "id": 5983,
      "verse_number": 16,
      "verse_key": "88:16",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928648,
          "resource_id": 135,
          "text": "و [نیز] فرش‌های گرانبها گسترده شده است."
        }
      ]
    },
    {
      "id": 5984,
      "verse_number": 17,
      "verse_key": "88:17",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928649,
          "resource_id": 135,
          "text": "آیا [کافران] به شتر نمی‌نگرند که چگونه آفریده شده است؟"
        }
      ]
    },
    {
      "id": 5985,
      "verse_number": 18,
      "verse_key": "88:18",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928650,
          "resource_id": 135,
          "text": "و به آسمان [نمی‌نگرند] که چگونه برافراشته شده است؟"
        }
      ]
    },
    {
      "id": 5986,
      "verse_number": 19,
      "verse_key": "88:19",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928651,
          "resource_id": 135,
          "text": "و به کوه‌ها [نمی‌نگرند] که چگونه [محکم بر زمین] نصب شده است؟"
        }
      ]
    },
    {
      "id": 5987,
      "verse_number": 20,
      "verse_key": "88:20",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928652,
          "resource_id": 135,
          "text": "و به زمین [نمی‌نگرند] که چگونه گسترده شده است؟"
        }
      ]
    },
    {
      "id": 5988,
      "verse_number": 21,
      "verse_key": "88:21",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928653,
          "resource_id": 135,
          "text": "[ای پیامبر،] پند بده، که تو فقط پنددهنده‌ای."
        }
      ]
    },
    {
      "id": 5989,
      "verse_number": 22,
      "verse_key": "88:22",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928654,
          "resource_id": 135,
          "text": "تو بر آنان مسلط [و چیره] نیستی [که آنان را بر ایمان‌آوردن وادار کنی]."
        }
      ]
    },
    {
      "id": 5990,
      "verse_number": 23,
      "verse_key": "88:23",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928655,
          "resource_id": 135,
          "text": "اما کسی‌ که روی بگردانَد و کافر شود"
        }
      ]
    },
    {
      "id": 5991,
      "verse_number": 24,
      "verse_key": "88:24",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928656,
          "resource_id": 135,
          "text": "الله او را با بزرگ‌ترین عذاب، مجازات می‌کند."
        }
      ]
    },
    {
      "id": 5992,
      "verse_number": 25,
      "verse_key": "88:25",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928657,
          "resource_id": 135,
          "text": "همانا بازگشت آنها به سوی ماست."
        }
      ]
    },
    {
      "id": 5993,
      "verse_number": 26,
      "verse_key": "88:26",
      "hizb_number": 60,
      "rub_el_hizb_number": 237,
      "ruku_number": 532,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 592,
      "juz_number": 30,
      "translations": [
        {
          "id": 928658,
          "resource_id": 135,
          "text": "سپس بی‌گمان، حسابشان [نیز] با ماست."
        }
      ]
    }
  ]
}