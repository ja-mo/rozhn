{
  "id": 20,
  "name": "Ta-Ha",
  "verses": [
    {
      "id": 2349,
      "verse_number": 1,
      "verse_key": "20:1",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": " طه",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2350,
      "verse_number": 2,
      "verse_key": "20:2",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَآ أَنزَلْنَا عَلَيْكَ ٱلْقُرْءَانَ لِتَشْقَىٰٓ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2351,
      "verse_number": 3,
      "verse_key": "20:3",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِلَّا تَذْكِرَةً لِّمَن يَخْشَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2352,
      "verse_number": 4,
      "verse_key": "20:4",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "تَنزِيلًا مِّمَّنْ خَلَقَ ٱلْأَرْضَ وَٱلسَّمَـٰوَٰتِ ٱلْعُلَى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2353,
      "verse_number": 5,
      "verse_key": "20:5",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱلرَّحْمَـٰنُ عَلَى ٱلْعَرْشِ ٱسْتَوَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2354,
      "verse_number": 6,
      "verse_key": "20:6",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لَهُۥ مَا فِى ٱلسَّمَـٰوَٰتِ وَمَا فِى ٱلْأَرْضِ وَمَا بَيْنَهُمَا وَمَا تَحْتَ ٱلثَّرَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2355,
      "verse_number": 7,
      "verse_key": "20:7",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِن تَجْهَرْ بِٱلْقَوْلِ فَإِنَّهُۥ يَعْلَمُ ٱلسِّرَّ وَأَخْفَى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2356,
      "verse_number": 8,
      "verse_key": "20:8",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱللَّهُ لَآ إِلَـٰهَ إِلَّا هُوَ ۖ لَهُ ٱلْأَسْمَآءُ ٱلْحُسْنَىٰ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2357,
      "verse_number": 9,
      "verse_key": "20:9",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَهَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2358,
      "verse_number": 10,
      "verse_key": "20:10",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِذْ رَءَا نَارًا فَقَالَ لِأَهْلِهِ ٱمْكُثُوٓا۟ إِنِّىٓ ءَانَسْتُ نَارًا لَّعَلِّىٓ ءَاتِيكُم مِّنْهَا بِقَبَسٍ أَوْ أَجِدُ عَلَى ٱلنَّارِ هُدًى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2359,
      "verse_number": 11,
      "verse_key": "20:11",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَلَمَّآ أَتَىٰهَا نُودِىَ يَـٰمُوسَىٰٓ",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2360,
      "verse_number": 12,
      "verse_key": "20:12",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنِّىٓ أَنَا۠ رَبُّكَ فَٱخْلَعْ نَعْلَيْكَ ۖ إِنَّكَ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى",
      "page_number": 312,
      "juz_number": 16
    },
    {
      "id": 2361,
      "verse_number": 13,
      "verse_key": "20:13",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَنَا ٱخْتَرْتُكَ فَٱسْتَمِعْ لِمَا يُوحَىٰٓ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2362,
      "verse_number": 14,
      "verse_key": "20:14",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّنِىٓ أَنَا ٱللَّهُ لَآ إِلَـٰهَ إِلَّآ أَنَا۠ فَٱعْبُدْنِى وَأَقِمِ ٱلصَّلَوٰةَ لِذِكْرِىٓ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2363,
      "verse_number": 15,
      "verse_key": "20:15",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلسَّاعَةَ ءَاتِيَةٌ أَكَادُ أُخْفِيهَا لِتُجْزَىٰ كُلُّ نَفْسٍۭ بِمَا تَسْعَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2364,
      "verse_number": 16,
      "verse_key": "20:16",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَلَا يَصُدَّنَّكَ عَنْهَا مَن لَّا يُؤْمِنُ بِهَا وَٱتَّبَعَ هَوَىٰهُ فَتَرْدَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2365,
      "verse_number": 17,
      "verse_key": "20:17",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَا تِلْكَ بِيَمِينِكَ يَـٰمُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2366,
      "verse_number": 18,
      "verse_key": "20:18",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ هِىَ عَصَاىَ أَتَوَكَّؤُا۟ عَلَيْهَا وَأَهُشُّ بِهَا عَلَىٰ غَنَمِى وَلِىَ فِيهَا مَـَٔارِبُ أُخْرَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2367,
      "verse_number": 19,
      "verse_key": "20:19",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ أَلْقِهَا يَـٰمُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2368,
      "verse_number": 20,
      "verse_key": "20:20",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَلْقَىٰهَا فَإِذَا هِىَ حَيَّةٌ تَسْعَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2369,
      "verse_number": 21,
      "verse_key": "20:21",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ خُذْهَا وَلَا تَخَفْ ۖ سَنُعِيدُهَا سِيرَتَهَا ٱلْأُولَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2370,
      "verse_number": 22,
      "verse_key": "20:22",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱضْمُمْ يَدَكَ إِلَىٰ جَنَاحِكَ تَخْرُجْ بَيْضَآءَ مِنْ غَيْرِ سُوٓءٍ ءَايَةً أُخْرَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2371,
      "verse_number": 23,
      "verse_key": "20:23",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "لِنُرِيَكَ مِنْ ءَايَـٰتِنَا ٱلْكُبْرَى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2372,
      "verse_number": 24,
      "verse_key": "20:24",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 270,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2373,
      "verse_number": 25,
      "verse_key": "20:25",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبِّ ٱشْرَحْ لِى صَدْرِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2374,
      "verse_number": 26,
      "verse_key": "20:26",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَيَسِّرْ لِىٓ أَمْرِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2375,
      "verse_number": 27,
      "verse_key": "20:27",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱحْلُلْ عُقْدَةً مِّن لِّسَانِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2376,
      "verse_number": 28,
      "verse_key": "20:28",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "يَفْقَهُوا۟ قَوْلِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2377,
      "verse_number": 29,
      "verse_key": "20:29",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱجْعَل لِّى وَزِيرًا مِّنْ أَهْلِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2378,
      "verse_number": 30,
      "verse_key": "20:30",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "هَـٰرُونَ أَخِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2379,
      "verse_number": 31,
      "verse_key": "20:31",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱشْدُدْ بِهِۦٓ أَزْرِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2380,
      "verse_number": 32,
      "verse_key": "20:32",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَشْرِكْهُ فِىٓ أَمْرِى",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2381,
      "verse_number": 33,
      "verse_key": "20:33",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "كَىْ نُسَبِّحَكَ كَثِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2382,
      "verse_number": 34,
      "verse_key": "20:34",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَنَذْكُرَكَ كَثِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2383,
      "verse_number": 35,
      "verse_key": "20:35",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّكَ كُنتَ بِنَا بَصِيرًا",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2384,
      "verse_number": 36,
      "verse_key": "20:36",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ قَدْ أُوتِيتَ سُؤْلَكَ يَـٰمُوسَىٰ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2385,
      "verse_number": 37,
      "verse_key": "20:37",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ مَنَنَّا عَلَيْكَ مَرَّةً أُخْرَىٰٓ",
      "page_number": 313,
      "juz_number": 16
    },
    {
      "id": 2386,
      "verse_number": 38,
      "verse_key": "20:38",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِذْ أَوْحَيْنَآ إِلَىٰٓ أُمِّكَ مَا يُوحَىٰٓ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2387,
      "verse_number": 39,
      "verse_key": "20:39",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَنِ ٱقْذِفِيهِ فِى ٱلتَّابُوتِ فَٱقْذِفِيهِ فِى ٱلْيَمِّ فَلْيُلْقِهِ ٱلْيَمُّ بِٱلسَّاحِلِ يَأْخُذْهُ عَدُوٌّ لِّى وَعَدُوٌّ لَّهُۥ ۚ وَأَلْقَيْتُ عَلَيْكَ مَحَبَّةً مِّنِّى وَلِتُصْنَعَ عَلَىٰ عَيْنِىٓ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2388,
      "verse_number": 40,
      "verse_key": "20:40",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِذْ تَمْشِىٓ أُخْتُكَ فَتَقُولُ هَلْ أَدُلُّكُمْ عَلَىٰ مَن يَكْفُلُهُۥ ۖ فَرَجَعْنَـٰكَ إِلَىٰٓ أُمِّكَ كَىْ تَقَرَّ عَيْنُهَا وَلَا تَحْزَنَ ۚ وَقَتَلْتَ نَفْسًا فَنَجَّيْنَـٰكَ مِنَ ٱلْغَمِّ وَفَتَنَّـٰكَ فُتُونًا ۚ فَلَبِثْتَ سِنِينَ فِىٓ أَهْلِ مَدْيَنَ ثُمَّ جِئْتَ عَلَىٰ قَدَرٍ يَـٰمُوسَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2389,
      "verse_number": 41,
      "verse_key": "20:41",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَٱصْطَنَعْتُكَ لِنَفْسِى",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2390,
      "verse_number": 42,
      "verse_key": "20:42",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱذْهَبْ أَنتَ وَأَخُوكَ بِـَٔايَـٰتِى وَلَا تَنِيَا فِى ذِكْرِى",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2391,
      "verse_number": 43,
      "verse_key": "20:43",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱذْهَبَآ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2392,
      "verse_number": 44,
      "verse_key": "20:44",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَقُولَا لَهُۥ قَوْلًا لَّيِّنًا لَّعَلَّهُۥ يَتَذَكَّرُ أَوْ يَخْشَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2393,
      "verse_number": 45,
      "verse_key": "20:45",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَا رَبَّنَآ إِنَّنَا نَخَافُ أَن يَفْرُطَ عَلَيْنَآ أَوْ أَن يَطْغَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2394,
      "verse_number": 46,
      "verse_key": "20:46",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ لَا تَخَافَآ ۖ إِنَّنِى مَعَكُمَآ أَسْمَعُ وَأَرَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2395,
      "verse_number": 47,
      "verse_key": "20:47",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأْتِيَاهُ فَقُولَآ إِنَّا رَسُولَا رَبِّكَ فَأَرْسِلْ مَعَنَا بَنِىٓ إِسْرَٰٓءِيلَ وَلَا تُعَذِّبْهُمْ ۖ قَدْ جِئْنَـٰكَ بِـَٔايَةٍ مِّن رَّبِّكَ ۖ وَٱلسَّلَـٰمُ عَلَىٰ مَنِ ٱتَّبَعَ ٱلْهُدَىٰٓ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2396,
      "verse_number": 48,
      "verse_key": "20:48",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا قَدْ أُوحِىَ إِلَيْنَآ أَنَّ ٱلْعَذَابَ عَلَىٰ مَن كَذَّبَ وَتَوَلَّىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2397,
      "verse_number": 49,
      "verse_key": "20:49",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَمَن رَّبُّكُمَا يَـٰمُوسَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2398,
      "verse_number": 50,
      "verse_key": "20:50",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ رَبُّنَا ٱلَّذِىٓ أَعْطَىٰ كُلَّ شَىْءٍ خَلْقَهُۥ ثُمَّ هَدَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2399,
      "verse_number": 51,
      "verse_key": "20:51",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَمَا بَالُ ٱلْقُرُونِ ٱلْأُولَىٰ",
      "page_number": 314,
      "juz_number": 16
    },
    {
      "id": 2400,
      "verse_number": 52,
      "verse_key": "20:52",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ عِلْمُهَا عِندَ رَبِّى فِى كِتَـٰبٍ ۖ لَّا يَضِلُّ رَبِّى وَلَا يَنسَى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2401,
      "verse_number": 53,
      "verse_key": "20:53",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِى جَعَلَ لَكُمُ ٱلْأَرْضَ مَهْدًا وَسَلَكَ لَكُمْ فِيهَا سُبُلًا وَأَنزَلَ مِنَ ٱلسَّمَآءِ مَآءً فَأَخْرَجْنَا بِهِۦٓ أَزْوَٰجًا مِّن نَّبَاتٍ شَتَّىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2402,
      "verse_number": 54,
      "verse_key": "20:54",
      "hizb_number": 32,
      "rub_el_hizb_number": 125,
      "ruku_number": 271,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ وَٱرْعَوْا۟ أَنْعَـٰمَكُمْ ۗ إِنَّ فِى ذَٰلِكَ لَـَٔايَـٰتٍ لِّأُو۟لِى ٱلنُّهَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2403,
      "verse_number": 55,
      "verse_key": "20:55",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ مِنْهَا خَلَقْنَـٰكُمْ وَفِيهَا نُعِيدُكُمْ وَمِنْهَا نُخْرِجُكُمْ تَارَةً أُخْرَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2404,
      "verse_number": 56,
      "verse_key": "20:56",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَرَيْنَـٰهُ ءَايَـٰتِنَا كُلَّهَا فَكَذَّبَ وَأَبَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2405,
      "verse_number": 57,
      "verse_key": "20:57",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ أَجِئْتَنَا لِتُخْرِجَنَا مِنْ أَرْضِنَا بِسِحْرِكَ يَـٰمُوسَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2406,
      "verse_number": 58,
      "verse_key": "20:58",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَلَنَأْتِيَنَّكَ بِسِحْرٍ مِّثْلِهِۦ فَٱجْعَلْ بَيْنَنَا وَبَيْنَكَ مَوْعِدًا لَّا نُخْلِفُهُۥ نَحْنُ وَلَآ أَنتَ مَكَانًا سُوًى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2407,
      "verse_number": 59,
      "verse_key": "20:59",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ مَوْعِدُكُمْ يَوْمُ ٱلزِّينَةِ وَأَن يُحْشَرَ ٱلنَّاسُ ضُحًى",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2408,
      "verse_number": 60,
      "verse_key": "20:60",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَتَوَلَّىٰ فِرْعَوْنُ فَجَمَعَ كَيْدَهُۥ ثُمَّ أَتَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2409,
      "verse_number": 61,
      "verse_key": "20:61",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ لَهُم مُّوسَىٰ وَيْلَكُمْ لَا تَفْتَرُوا۟ عَلَى ٱللَّهِ كَذِبًا فَيُسْحِتَكُم بِعَذَابٍ ۖ وَقَدْ خَابَ مَنِ ٱفْتَرَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2410,
      "verse_number": 62,
      "verse_key": "20:62",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَتَنَـٰزَعُوٓا۟ أَمْرَهُم بَيْنَهُمْ وَأَسَرُّوا۟ ٱلنَّجْوَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2411,
      "verse_number": 63,
      "verse_key": "20:63",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوٓا۟ إِنْ هَـٰذَٰنِ لَسَـٰحِرَٰنِ يُرِيدَانِ أَن يُخْرِجَاكُم مِّنْ أَرْضِكُم بِسِحْرِهِمَا وَيَذْهَبَا بِطَرِيقَتِكُمُ ٱلْمُثْلَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2412,
      "verse_number": 64,
      "verse_key": "20:64",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَجْمِعُوا۟ كَيْدَكُمْ ثُمَّ ٱئْتُوا۟ صَفًّا ۚ وَقَدْ أَفْلَحَ ٱلْيَوْمَ مَنِ ٱسْتَعْلَىٰ",
      "page_number": 315,
      "juz_number": 16
    },
    {
      "id": 2413,
      "verse_number": 65,
      "verse_key": "20:65",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ يَـٰمُوسَىٰٓ إِمَّآ أَن تُلْقِىَ وَإِمَّآ أَن نَّكُونَ أَوَّلَ مَنْ أَلْقَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2414,
      "verse_number": 66,
      "verse_key": "20:66",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ بَلْ أَلْقُوا۟ ۖ فَإِذَا حِبَالُهُمْ وَعِصِيُّهُمْ يُخَيَّلُ إِلَيْهِ مِن سِحْرِهِمْ أَنَّهَا تَسْعَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2415,
      "verse_number": 67,
      "verse_key": "20:67",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَوْجَسَ فِى نَفْسِهِۦ خِيفَةً مُّوسَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2416,
      "verse_number": 68,
      "verse_key": "20:68",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قُلْنَا لَا تَخَفْ إِنَّكَ أَنتَ ٱلْأَعْلَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2417,
      "verse_number": 69,
      "verse_key": "20:69",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَلْقِ مَا فِى يَمِينِكَ تَلْقَفْ مَا صَنَعُوٓا۟ ۖ إِنَّمَا صَنَعُوا۟ كَيْدُ سَـٰحِرٍ ۖ وَلَا يُفْلِحُ ٱلسَّاحِرُ حَيْثُ أَتَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2418,
      "verse_number": 70,
      "verse_key": "20:70",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأُلْقِىَ ٱلسَّحَرَةُ سُجَّدًا قَالُوٓا۟ ءَامَنَّا بِرَبِّ هَـٰرُونَ وَمُوسَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2419,
      "verse_number": 71,
      "verse_key": "20:71",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ ءَامَنتُمْ لَهُۥ قَبْلَ أَنْ ءَاذَنَ لَكُمْ ۖ إِنَّهُۥ لَكَبِيرُكُمُ ٱلَّذِى عَلَّمَكُمُ ٱلسِّحْرَ ۖ فَلَأُقَطِّعَنَّ أَيْدِيَكُمْ وَأَرْجُلَكُم مِّنْ خِلَـٰفٍ وَلَأُصَلِّبَنَّكُمْ فِى جُذُوعِ ٱلنَّخْلِ وَلَتَعْلَمُنَّ أَيُّنَآ أَشَدُّ عَذَابًا وَأَبْقَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2420,
      "verse_number": 72,
      "verse_key": "20:72",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ لَن نُّؤْثِرَكَ عَلَىٰ مَا جَآءَنَا مِنَ ٱلْبَيِّنَـٰتِ وَٱلَّذِى فَطَرَنَا ۖ فَٱقْضِ مَآ أَنتَ قَاضٍ ۖ إِنَّمَا تَقْضِى هَـٰذِهِ ٱلْحَيَوٰةَ ٱلدُّنْيَآ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2421,
      "verse_number": 73,
      "verse_key": "20:73",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّآ ءَامَنَّا بِرَبِّنَا لِيَغْفِرَ لَنَا خَطَـٰيَـٰنَا وَمَآ أَكْرَهْتَنَا عَلَيْهِ مِنَ ٱلسِّحْرِ ۗ وَٱللَّهُ خَيْرٌ وَأَبْقَىٰٓ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2422,
      "verse_number": 74,
      "verse_key": "20:74",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ مَن يَأْتِ رَبَّهُۥ مُجْرِمًا فَإِنَّ لَهُۥ جَهَنَّمَ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2423,
      "verse_number": 75,
      "verse_key": "20:75",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَمَن يَأْتِهِۦ مُؤْمِنًا قَدْ عَمِلَ ٱلصَّـٰلِحَـٰتِ فَأُو۟لَـٰٓئِكَ لَهُمُ ٱلدَّرَجَـٰتُ ٱلْعُلَىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2424,
      "verse_number": 76,
      "verse_key": "20:76",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 272,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "جَنَّـٰتُ عَدْنٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَـٰرُ خَـٰلِدِينَ فِيهَا ۚ وَذَٰلِكَ جَزَآءُ مَن تَزَكَّىٰ",
      "page_number": 316,
      "juz_number": 16
    },
    {
      "id": 2425,
      "verse_number": 77,
      "verse_key": "20:77",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ أَوْحَيْنَآ إِلَىٰ مُوسَىٰٓ أَنْ أَسْرِ بِعِبَادِى فَٱضْرِبْ لَهُمْ طَرِيقًا فِى ٱلْبَحْرِ يَبَسًا لَّا تَخَـٰفُ دَرَكًا وَلَا تَخْشَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2426,
      "verse_number": 78,
      "verse_key": "20:78",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَتْبَعَهُمْ فِرْعَوْنُ بِجُنُودِهِۦ فَغَشِيَهُم مِّنَ ٱلْيَمِّ مَا غَشِيَهُمْ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2427,
      "verse_number": 79,
      "verse_key": "20:79",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَأَضَلَّ فِرْعَوْنُ قَوْمَهُۥ وَمَا هَدَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2428,
      "verse_number": 80,
      "verse_key": "20:80",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "يَـٰبَنِىٓ إِسْرَٰٓءِيلَ قَدْ أَنجَيْنَـٰكُم مِّنْ عَدُوِّكُمْ وَوَٰعَدْنَـٰكُمْ جَانِبَ ٱلطُّورِ ٱلْأَيْمَنَ وَنَزَّلْنَا عَلَيْكُمُ ٱلْمَنَّ وَٱلسَّلْوَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2429,
      "verse_number": 81,
      "verse_key": "20:81",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ مِن طَيِّبَـٰتِ مَا رَزَقْنَـٰكُمْ وَلَا تَطْغَوْا۟ فِيهِ فَيَحِلَّ عَلَيْكُمْ غَضَبِى ۖ وَمَن يَحْلِلْ عَلَيْهِ غَضَبِى فَقَدْ هَوَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2430,
      "verse_number": 82,
      "verse_key": "20:82",
      "hizb_number": 32,
      "rub_el_hizb_number": 126,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَإِنِّى لَغَفَّارٌ لِّمَن تَابَ وَءَامَنَ وَعَمِلَ صَـٰلِحًا ثُمَّ ٱهْتَدَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2431,
      "verse_number": 83,
      "verse_key": "20:83",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "۞ وَمَآ أَعْجَلَكَ عَن قَوْمِكَ يَـٰمُوسَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2432,
      "verse_number": 84,
      "verse_key": "20:84",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ هُمْ أُو۟لَآءِ عَلَىٰٓ أَثَرِى وَعَجِلْتُ إِلَيْكَ رَبِّ لِتَرْضَىٰ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2433,
      "verse_number": 85,
      "verse_key": "20:85",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَإِنَّا قَدْ فَتَنَّا قَوْمَكَ مِنۢ بَعْدِكَ وَأَضَلَّهُمُ ٱلسَّامِرِىُّ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2434,
      "verse_number": 86,
      "verse_key": "20:86",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَرَجَعَ مُوسَىٰٓ إِلَىٰ قَوْمِهِۦ غَضْبَـٰنَ أَسِفًا ۚ قَالَ يَـٰقَوْمِ أَلَمْ يَعِدْكُمْ رَبُّكُمْ وَعْدًا حَسَنًا ۚ أَفَطَالَ عَلَيْكُمُ ٱلْعَهْدُ أَمْ أَرَدتُّمْ أَن يَحِلَّ عَلَيْكُمْ غَضَبٌ مِّن رَّبِّكُمْ فَأَخْلَفْتُم مَّوْعِدِى",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2435,
      "verse_number": 87,
      "verse_key": "20:87",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ مَآ أَخْلَفْنَا مَوْعِدَكَ بِمَلْكِنَا وَلَـٰكِنَّا حُمِّلْنَآ أَوْزَارًا مِّن زِينَةِ ٱلْقَوْمِ فَقَذَفْنَـٰهَا فَكَذَٰلِكَ أَلْقَى ٱلسَّامِرِىُّ",
      "page_number": 317,
      "juz_number": 16
    },
    {
      "id": 2436,
      "verse_number": 88,
      "verse_key": "20:88",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "فَأَخْرَجَ لَهُمْ عِجْلًا جَسَدًا لَّهُۥ خُوَارٌ فَقَالُوا۟ هَـٰذَآ إِلَـٰهُكُمْ وَإِلَـٰهُ مُوسَىٰ فَنَسِىَ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2437,
      "verse_number": 89,
      "verse_key": "20:89",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 273,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَفَلَا يَرَوْنَ أَلَّا يَرْجِعُ إِلَيْهِمْ قَوْلًا وَلَا يَمْلِكُ لَهُمْ ضَرًّا وَلَا نَفْعًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2438,
      "verse_number": 90,
      "verse_key": "20:90",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ قَالَ لَهُمْ هَـٰرُونُ مِن قَبْلُ يَـٰقَوْمِ إِنَّمَا فُتِنتُم بِهِۦ ۖ وَإِنَّ رَبَّكُمُ ٱلرَّحْمَـٰنُ فَٱتَّبِعُونِى وَأَطِيعُوٓا۟ أَمْرِى",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2439,
      "verse_number": 91,
      "verse_key": "20:91",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالُوا۟ لَن نَّبْرَحَ عَلَيْهِ عَـٰكِفِينَ حَتَّىٰ يَرْجِعَ إِلَيْنَا مُوسَىٰ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2440,
      "verse_number": 92,
      "verse_key": "20:92",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ يَـٰهَـٰرُونُ مَا مَنَعَكَ إِذْ رَأَيْتَهُمْ ضَلُّوٓا۟",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2441,
      "verse_number": 93,
      "verse_key": "20:93",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "أَلَّا تَتَّبِعَنِ ۖ أَفَعَصَيْتَ أَمْرِى",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2442,
      "verse_number": 94,
      "verse_key": "20:94",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ يَبْنَؤُمَّ لَا تَأْخُذْ بِلِحْيَتِى وَلَا بِرَأْسِىٓ ۖ إِنِّى خَشِيتُ أَن تَقُولَ فَرَّقْتَ بَيْنَ بَنِىٓ إِسْرَٰٓءِيلَ وَلَمْ تَرْقُبْ قَوْلِى",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2443,
      "verse_number": 95,
      "verse_key": "20:95",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَمَا خَطْبُكَ يَـٰسَـٰمِرِىُّ",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2444,
      "verse_number": 96,
      "verse_key": "20:96",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ بَصُرْتُ بِمَا لَمْ يَبْصُرُوا۟ بِهِۦ فَقَبَضْتُ قَبْضَةً مِّنْ أَثَرِ ٱلرَّسُولِ فَنَبَذْتُهَا وَكَذَٰلِكَ سَوَّلَتْ لِى نَفْسِى",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2445,
      "verse_number": 97,
      "verse_key": "20:97",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "قَالَ فَٱذْهَبْ فَإِنَّ لَكَ فِى ٱلْحَيَوٰةِ أَن تَقُولَ لَا مِسَاسَ ۖ وَإِنَّ لَكَ مَوْعِدًا لَّن تُخْلَفَهُۥ ۖ وَٱنظُرْ إِلَىٰٓ إِلَـٰهِكَ ٱلَّذِى ظَلْتَ عَلَيْهِ عَاكِفًا ۖ لَّنُحَرِّقَنَّهُۥ ثُمَّ لَنَنسِفَنَّهُۥ فِى ٱلْيَمِّ نَسْفًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2446,
      "verse_number": 98,
      "verse_key": "20:98",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "إِنَّمَآ إِلَـٰهُكُمُ ٱللَّهُ ٱلَّذِى لَآ إِلَـٰهَ إِلَّا هُوَ ۚ وَسِعَ كُلَّ شَىْءٍ عِلْمًا",
      "page_number": 318,
      "juz_number": 16
    },
    {
      "id": 2447,
      "verse_number": 99,
      "verse_key": "20:99",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "كَذَٰلِكَ نَقُصُّ عَلَيْكَ مِنْ أَنۢبَآءِ مَا قَدْ سَبَقَ ۚ وَقَدْ ءَاتَيْنَـٰكَ مِن لَّدُنَّا ذِكْرًا",
      "page_number": 319,
      "juz_number": 16
    },
    {
      "id": 2448,
      "verse_number": 100,
      "verse_key": "20:100",
      "hizb_number": 32,
      "rub_el_hizb_number": 127,
      "ruku_number": 274,
      "manzil_number": 4,
      "sajdah_number": null,
      "text_uthmani": "مَّنْ أَعْرَضَ عَنْهُ فَإِنَّهُۥ يَحْمِلُ يَوْمَ ٱلْقِيَـٰمَةِ وِزْرًا",
      "page_number": 319,
      "juz_number": 16
    }
  ]
}