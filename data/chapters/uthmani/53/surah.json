{
  "id": 53,
  "name": "The Star",
  "verses": [
    {
      "id": 4785,
      "verse_number": 1,
      "verse_key": "53:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " وَٱلنَّجْمِ إِذَا هَوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4786,
      "verse_number": 2,
      "verse_key": "53:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا ضَلَّ صَاحِبُكُمْ وَمَا غَوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4787,
      "verse_number": 3,
      "verse_key": "53:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا يَنطِقُ عَنِ ٱلْهَوَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4788,
      "verse_number": 4,
      "verse_key": "53:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا وَحْىٌ يُوحَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4789,
      "verse_number": 5,
      "verse_key": "53:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلَّمَهُۥ شَدِيدُ ٱلْقُوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4790,
      "verse_number": 6,
      "verse_key": "53:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذُو مِرَّةٍ فَٱسْتَوَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4791,
      "verse_number": 7,
      "verse_key": "53:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَهُوَ بِٱلْأُفُقِ ٱلْأَعْلَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4792,
      "verse_number": 8,
      "verse_key": "53:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ دَنَا فَتَدَلَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4793,
      "verse_number": 9,
      "verse_key": "53:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَكَانَ قَابَ قَوْسَيْنِ أَوْ أَدْنَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4794,
      "verse_number": 10,
      "verse_key": "53:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَوْحَىٰٓ إِلَىٰ عَبْدِهِۦ مَآ أَوْحَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4795,
      "verse_number": 11,
      "verse_key": "53:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا كَذَبَ ٱلْفُؤَادُ مَا رَأَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4796,
      "verse_number": 12,
      "verse_key": "53:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَفَتُمَـٰرُونَهُۥ عَلَىٰ مَا يَرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4797,
      "verse_number": 13,
      "verse_key": "53:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ رَءَاهُ نَزْلَةً أُخْرَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4798,
      "verse_number": 14,
      "verse_key": "53:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عِندَ سِدْرَةِ ٱلْمُنتَهَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4799,
      "verse_number": 15,
      "verse_key": "53:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عِندَهَا جَنَّةُ ٱلْمَأْوَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4800,
      "verse_number": 16,
      "verse_key": "53:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِذْ يَغْشَى ٱلسِّدْرَةَ مَا يَغْشَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4801,
      "verse_number": 17,
      "verse_key": "53:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا زَاغَ ٱلْبَصَرُ وَمَا طَغَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4802,
      "verse_number": 18,
      "verse_key": "53:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَقَدْ رَأَىٰ مِنْ ءَايَـٰتِ رَبِّهِ ٱلْكُبْرَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4803,
      "verse_number": 19,
      "verse_key": "53:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَفَرَءَيْتُمُ ٱللَّـٰتَ وَٱلْعُزَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4804,
      "verse_number": 20,
      "verse_key": "53:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَنَوٰةَ ٱلثَّالِثَةَ ٱلْأُخْرَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4805,
      "verse_number": 21,
      "verse_key": "53:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَكُمُ ٱلذَّكَرُ وَلَهُ ٱلْأُنثَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4806,
      "verse_number": 22,
      "verse_key": "53:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "تِلْكَ إِذًا قِسْمَةٌ ضِيزَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4807,
      "verse_number": 23,
      "verse_key": "53:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هِىَ إِلَّآ أَسْمَآءٌ سَمَّيْتُمُوهَآ أَنتُمْ وَءَابَآؤُكُم مَّآ أَنزَلَ ٱللَّهُ بِهَا مِن سُلْطَـٰنٍ ۚ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ وَمَا تَهْوَى ٱلْأَنفُسُ ۖ وَلَقَدْ جَآءَهُم مِّن رَّبِّهِمُ ٱلْهُدَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4808,
      "verse_number": 24,
      "verse_key": "53:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمْ لِلْإِنسَـٰنِ مَا تَمَنَّىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4809,
      "verse_number": 25,
      "verse_key": "53:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلِلَّهِ ٱلْـَٔاخِرَةُ وَٱلْأُولَىٰ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4810,
      "verse_number": 26,
      "verse_key": "53:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "۞ وَكَم مِّن مَّلَكٍ فِى ٱلسَّمَـٰوَٰتِ لَا تُغْنِى شَفَـٰعَتُهُمْ شَيْـًٔا إِلَّا مِنۢ بَعْدِ أَن يَأْذَنَ ٱللَّهُ لِمَن يَشَآءُ وَيَرْضَىٰٓ",
      "page_number": 526,
      "juz_number": 27
    },
    {
      "id": 4811,
      "verse_number": 27,
      "verse_key": "53:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّ ٱلَّذِينَ لَا يُؤْمِنُونَ بِٱلْـَٔاخِرَةِ لَيُسَمُّونَ ٱلْمَلَـٰٓئِكَةَ تَسْمِيَةَ ٱلْأُنثَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4812,
      "verse_number": 28,
      "verse_key": "53:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا لَهُم بِهِۦ مِنْ عِلْمٍ ۖ إِن يَتَّبِعُونَ إِلَّا ٱلظَّنَّ ۖ وَإِنَّ ٱلظَّنَّ لَا يُغْنِى مِنَ ٱلْحَقِّ شَيْـًٔا",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4813,
      "verse_number": 29,
      "verse_key": "53:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَعْرِضْ عَن مَّن تَوَلَّىٰ عَن ذِكْرِنَا وَلَمْ يُرِدْ إِلَّا ٱلْحَيَوٰةَ ٱلدُّنْيَا",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4814,
      "verse_number": 30,
      "verse_key": "53:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذَٰلِكَ مَبْلَغُهُم مِّنَ ٱلْعِلْمِ ۚ إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِمَنِ ٱهْتَدَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4815,
      "verse_number": 31,
      "verse_key": "53:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلِلَّهِ مَا فِى ٱلسَّمَـٰوَٰتِ وَمَا فِى ٱلْأَرْضِ لِيَجْزِىَ ٱلَّذِينَ أَسَـٰٓـُٔوا۟ بِمَا عَمِلُوا۟ وَيَجْزِىَ ٱلَّذِينَ أَحْسَنُوا۟ بِٱلْحُسْنَى",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4816,
      "verse_number": 32,
      "verse_key": "53:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلَّذِينَ يَجْتَنِبُونَ كَبَـٰٓئِرَ ٱلْإِثْمِ وَٱلْفَوَٰحِشَ إِلَّا ٱللَّمَمَ ۚ إِنَّ رَبَّكَ وَٰسِعُ ٱلْمَغْفِرَةِ ۚ هُوَ أَعْلَمُ بِكُمْ إِذْ أَنشَأَكُم مِّنَ ٱلْأَرْضِ وَإِذْ أَنتُمْ أَجِنَّةٌ فِى بُطُونِ أُمَّهَـٰتِكُمْ ۖ فَلَا تُزَكُّوٓا۟ أَنفُسَكُمْ ۖ هُوَ أَعْلَمُ بِمَنِ ٱتَّقَىٰٓ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4817,
      "verse_number": 33,
      "verse_key": "53:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَفَرَءَيْتَ ٱلَّذِى تَوَلَّىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4818,
      "verse_number": 34,
      "verse_key": "53:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَعْطَىٰ قَلِيلًا وَأَكْدَىٰٓ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4819,
      "verse_number": 35,
      "verse_key": "53:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَعِندَهُۥ عِلْمُ ٱلْغَيْبِ فَهُوَ يَرَىٰٓ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4820,
      "verse_number": 36,
      "verse_key": "53:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَمْ لَمْ يُنَبَّأْ بِمَا فِى صُحُفِ مُوسَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4821,
      "verse_number": 37,
      "verse_key": "53:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِبْرَٰهِيمَ ٱلَّذِى وَفَّىٰٓ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4822,
      "verse_number": 38,
      "verse_key": "53:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "أَلَّا تَزِرُ وَازِرَةٌ وِزْرَ أُخْرَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4823,
      "verse_number": 39,
      "verse_key": "53:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَن لَّيْسَ لِلْإِنسَـٰنِ إِلَّا مَا سَعَىٰ",
      "page_number": 527,
      "juz_number": 27
    },
    {
      "id": 4824,
      "verse_number": 40,
      "verse_key": "53:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
