{
  "id": 53,
  "name": "The Star",
  "verses": [
    {
      "id": 4785,
      "verse_number": 1,
      "verse_key": "53:1",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927450,
          "resource_id": 135,
          "text": "سوگند به ستاره، آنگاه كه افول كند"
        }
      ]
    },
    {
      "id": 4786,
      "verse_number": 2,
      "verse_key": "53:2",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927451,
          "resource_id": 135,
          "text": "که یار شما [= محمد ﷺ] هرگز گمراه نشده و به راه تباهى نیفتاده است؛"
        }
      ]
    },
    {
      "id": 4787,
      "verse_number": 3,
      "verse_key": "53:3",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927452,
          "resource_id": 135,
          "text": "و از روی هوای نفس سخن نمی‌گوید."
        }
      ]
    },
    {
      "id": 4788,
      "verse_number": 4,
      "verse_key": "53:4",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927453,
          "resource_id": 135,
          "text": "[آنچه می‌گوید] نیست جز وحی [الهی] که به [او] وحی می‌شود."
        }
      ]
    },
    {
      "id": 4789,
      "verse_number": 5,
      "verse_key": "53:5",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927454,
          "resource_id": 135,
          "text": "[جبرئیل که فرشته‌اى است] پُرتوان، به او آموزش داده است."
        }
      ]
    },
    {
      "id": 4790,
      "verse_number": 6,
      "verse_key": "53:6",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927455,
          "resource_id": 135,
          "text": "[همان فرشتۀ] خوش‌منظری كه [با چهرۀ حقیقی‌اش در برابر پیامبر] ایستاد."
        }
      ]
    },
    {
      "id": 4791,
      "verse_number": 7,
      "verse_key": "53:7",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927456,
          "resource_id": 135,
          "text": "در حالی که او در افق بالا [در آسمان] قرار داشت."
        }
      ]
    },
    {
      "id": 4792,
      "verse_number": 8,
      "verse_key": "53:8",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927457,
          "resource_id": 135,
          "text": "سپس [جبرئیل] نزدیک شد؛ و نزدیک‌تر؛"
        }
      ]
    },
    {
      "id": 4793,
      "verse_number": 9,
      "verse_key": "53:9",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927458,
          "resource_id": 135,
          "text": "تا آنکه [فاصلۀ او با پیامبر] به اندازۀ طول دو کمان یا کمتر بود."
        }
      ]
    },
    {
      "id": 4794,
      "verse_number": 10,
      "verse_key": "53:10",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927459,
          "resource_id": 135,
          "text": "آنگاه [آن فرشتۀ بزرگوار] آنچه را که باید به بندۀ الله وحی می‌کرد، وحی نمود."
        }
      ]
    },
    {
      "id": 4795,
      "verse_number": 11,
      "verse_key": "53:11",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927460,
          "resource_id": 135,
          "text": "آنچه را که [پیامبر] دید، قلبش دروغ نپنداشت."
        }
      ]
    },
    {
      "id": 4796,
      "verse_number": 12,
      "verse_key": "53:12",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927461,
          "resource_id": 135,
          "text": "آیا با او دربارۀ آنچه می‌بیند مجادله می‌کنید؟"
        }
      ]
    },
    {
      "id": 4797,
      "verse_number": 13,
      "verse_key": "53:13",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927462,
          "resource_id": 135,
          "text": "در حقیقت، پیامبر یک بار دیگر نیز آن فرشته را [به صورت كامل] مشاهده كرده بود."
        }
      ]
    },
    {
      "id": 4798,
      "verse_number": 14,
      "verse_key": "53:14",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927463,
          "resource_id": 135,
          "text": "در كنارِ [درخت بزرگ] «سِدرةُ المُنتَهی» [در آسمان هفتم]"
        }
      ]
    },
    {
      "id": 4799,
      "verse_number": 15,
      "verse_key": "53:15",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927464,
          "resource_id": 135,
          "text": "که آرامشگاه بهشت، همان‌جاست."
        }
      ]
    },
    {
      "id": 4800,
      "verse_number": 16,
      "verse_key": "53:16",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927465,
          "resource_id": 135,
          "text": "آنگاه كه هاله‌اى آن درخت را پوشانده بود."
        }
      ]
    },
    {
      "id": 4801,
      "verse_number": 17,
      "verse_key": "53:17",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927466,
          "resource_id": 135,
          "text": "چشم [پیامبر، به چپ و راست] منحرف نشد و [از حد] تجاوز نكرد."
        }
      ]
    },
    {
      "id": 4802,
      "verse_number": 18,
      "verse_key": "53:18",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927467,
          "resource_id": 135,
          "text": "به راستی که او برخی از بزرگ‌ترین نشانه‌هاى پروردگارش را [در آنجا] مشاهده كرد."
        }
      ]
    },
    {
      "id": 4803,
      "verse_number": 19,
      "verse_key": "53:19",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927468,
          "resource_id": 135,
          "text": "[ای مشرکان،] آیا «لات» و «عُزّی» را دیده‌اید؟"
        }
      ]
    },
    {
      "id": 4804,
      "verse_number": 20,
      "verse_key": "53:20",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927469,
          "resource_id": 135,
          "text": "و «مَنات» آن سومین [نمادِ بی‌ارزش] را؟ [آیا هیچ سود و زیانی به شما می‌رسانند؟]"
        }
      ]
    },
    {
      "id": 4805,
      "verse_number": 21,
      "verse_key": "53:21",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927470,
          "resource_id": 135,
          "text": "آیا [ادعا می‌کنید كه] فرزند پسر برای شماست و دختر برای الله؟"
        }
      ]
    },
    {
      "id": 4806,
      "verse_number": 22,
      "verse_key": "53:22",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927471,
          "resource_id": 135,
          "text": "در این صورت، این تقسیمی ناعادلانه است."
        }
      ]
    },
    {
      "id": 4807,
      "verse_number": 23,
      "verse_key": "53:23",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927472,
          "resource_id": 135,
          "text": "این[نماد]ها فقط اسم‌هایی [بى‌معنا] هستند كه خود و پدرانتان با آنها [معبودهاى دروغین خود را] نام نهاده‌اید و الله هیچ دلیلی [بر حقانیت این کار] نازل نکرده است. [مشرکان، در اعتقاداتشان] فقط پیرو گمان و خواسته‌های دلِ خویشند؛ حال آنكه [موجبات] هدایت، از جانب پروردگارشان به آنان رسیده است."
        }
      ]
    },
    {
      "id": 4808,
      "verse_number": 24,
      "verse_key": "53:24",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927473,
          "resource_id": 135,
          "text": "آیا انسان هر چه [از شفاعت این معبودان باطل] آرزو کند برایش میسر است؟"
        }
      ]
    },
    {
      "id": 4809,
      "verse_number": 25,
      "verse_key": "53:25",
      "hizb_number": 53,
      "rub_el_hizb_number": 210,
      "ruku_number": 462,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927474,
          "resource_id": 135,
          "text": "دنیا و آخرت از آنِ الله است [و به هر کس، هر چه را صلاح بداند می‌بخشد]."
        }
      ]
    },
    {
      "id": 4810,
      "verse_number": 26,
      "verse_key": "53:26",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 526,
      "juz_number": 27,
      "translations": [
        {
          "id": 927475,
          "resource_id": 135,
          "text": "چه بسیار فرشتگانی در آسمان‌ها هستند كه شفاعتشان به هیچ وجه سودمند نخواهد بود؛ مگر پس از آنكه الله، براى هر کس که بخواهد و راضی باشد، اجازه دهد."
        }
      ]
    },
    {
      "id": 4811,
      "verse_number": 27,
      "verse_key": "53:27",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927476,
          "resource_id": 135,
          "text": "كسانى كه به آخرت باور ندارند، بر فرشتگان الهی نام دختر می‌گذارند [و آنان را دختران الله مى‌دانند]."
        }
      ]
    },
    {
      "id": 4812,
      "verse_number": 28,
      "verse_key": "53:28",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927477,
          "resource_id": 135,
          "text": "آنان در موردِ این امر، [کاملاً] بى‌اطلاعند [و] فقط از حدس و گمان پیروی می‌کنند؛ در حالى كه براى [شناخت] حقیقت، گمان كافى نیست."
        }
      ]
    },
    {
      "id": 4813,
      "verse_number": 29,
      "verse_key": "53:29",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927478,
          "resource_id": 135,
          "text": "پس [ای پیامبر،] تو نیز از كسى كه از یاد ما روى گردانده و فقط [لذت‌های] زندگى دنیا را مى‌خواهد، روى برتاب."
        }
      ]
    },
    {
      "id": 4814,
      "verse_number": 30,
      "verse_key": "53:30",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927479,
          "resource_id": 135,
          "text": "نهایت درک و فهمشان همین است. بی‌تردید، پروردگارت به حال كسانى كه از راه او منحرف شده‌اند آگاه‌تر است؛ و نیز به حال راه‌یافتگان."
        }
      ]
    },
    {
      "id": 4815,
      "verse_number": 31,
      "verse_key": "53:31",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927480,
          "resource_id": 135,
          "text": "هر چه در آسمان‌ها و زمین است، از آنِ الله است؛ [راه خیر و شر را به انسان ارائه كرد] تا بدكاران را در برابر رفتارشان كیفر دهد و نیكوكاران را [با نعمت‌های بهشت] پاداش نیكو عطا نماید."
        }
      ]
    },
    {
      "id": 4816,
      "verse_number": 32,
      "verse_key": "53:32",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 463,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927481,
          "resource_id": 135,
          "text": "و [نیز براى] کسانی كه از گناهان بزرگ و بی‌حیایی‌ها اجتناب می‌ورزند؛ مگر لغزش‌های كم‌اهمیت [و گناهان صغیره که با توبه و عبادت بخشوده می‌شوند] كه آمرزشِ پروردگارت گسترده است؛ از همان دَم كه شما را از زمین پدید آورد و آنگاه كه به صورت جنین در شكم مادرانتان بودید، به [طبیعتِ] شما آگاه‌تر [از خودتان] است؛ پس خودستایى نكنید، كه او تعالی به حال پرهیزگاران داناتر است."
        }
      ]
    },
    {
      "id": 4817,
      "verse_number": 33,
      "verse_key": "53:33",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927482,
          "resource_id": 135,
          "text": "آیا آن کسی را که [از حق] روی برتافت دیده‌ای؟"
        }
      ]
    },
    {
      "id": 4818,
      "verse_number": 34,
      "verse_key": "53:34",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927483,
          "resource_id": 135,
          "text": "و‌ اندكى انفاق كرد و [سپس] خوددارى نمود."
        }
      ]
    },
    {
      "id": 4819,
      "verse_number": 35,
      "verse_key": "53:35",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927484,
          "resource_id": 135,
          "text": "آیا علم غیب دارد و [آینده را] مى‌بیند؟"
        }
      ]
    },
    {
      "id": 4820,
      "verse_number": 36,
      "verse_key": "53:36",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927485,
          "resource_id": 135,
          "text": "یا از مطالب كتاب آسمانى موسى باخبر نشده است؟"
        }
      ]
    },
    {
      "id": 4821,
      "verse_number": 37,
      "verse_key": "53:37",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927486,
          "resource_id": 135,
          "text": "و [نیز از صحیفه‌های] ابراهیم، همان‌كه حق [رسالت و بندگى ما] را بطور کامل ادا كرد،"
        }
      ]
    },
    {
      "id": 4822,
      "verse_number": 38,
      "verse_key": "53:38",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927487,
          "resource_id": 135,
          "text": "که هیچ ‌گناهکاری بارِ گناه دیگری را بر دوش نخواهد کشید؛"
        }
      ]
    },
    {
      "id": 4823,
      "verse_number": 39,
      "verse_key": "53:39",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927488,
          "resource_id": 135,
          "text": "و اینكه انسان فقط ثمرۀ تلاش [و نیت] خود را خواهد داشت؛"
        }
      ]
    },
    {
      "id": 4824,
      "verse_number": 40,
      "verse_key": "53:40",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927489,
          "resource_id": 135,
          "text": "و اینكه بى‌گمان، تلاش او [در محاسبۀ اعمالش در روز قیامت،] منظور خواهد شد؛"
        }
      ]
    },
    {
      "id": 4825,
      "verse_number": 41,
      "verse_key": "53:41",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927490,
          "resource_id": 135,
          "text": "آنگاه پاداشش را به تمامى خواهند داد،"
        }
      ]
    },
    {
      "id": 4826,
      "verse_number": 42,
      "verse_key": "53:42",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927491,
          "resource_id": 135,
          "text": "و اینکه بازگشت [همۀ امور] به سوی پروردگار توست؛"
        }
      ]
    },
    {
      "id": 4827,
      "verse_number": 43,
      "verse_key": "53:43",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927492,
          "resource_id": 135,
          "text": "و اوست که می‌خندانَد و می‌گریانَد؛"
        }
      ]
    },
    {
      "id": 4828,
      "verse_number": 44,
      "verse_key": "53:44",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 527,
      "juz_number": 27,
      "translations": [
        {
          "id": 927493,
          "resource_id": 135,
          "text": "و اوست که می‌میرانَد و [در آخرت] زنده می‌کند؛"
        }
      ]
    },
    {
      "id": 4829,
      "verse_number": 45,
      "verse_key": "53:45",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927494,
          "resource_id": 135,
          "text": "و هموست كه زوج نر و ماده را می‌آفریند؛"
        }
      ]
    },
    {
      "id": 4830,
      "verse_number": 46,
      "verse_key": "53:46",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927495,
          "resource_id": 135,
          "text": "از نطفه‌اى كه [در رحِم] ریخته می‌شود؛"
        }
      ]
    },
    {
      "id": 4831,
      "verse_number": 47,
      "verse_key": "53:47",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927496,
          "resource_id": 135,
          "text": "و پدیدآوردن دوبارۀ آنها [در قیامت نیز بر عهدۀ] الله است."
        }
      ]
    },
    {
      "id": 4832,
      "verse_number": 48,
      "verse_key": "53:48",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927497,
          "resource_id": 135,
          "text": "و اوست که [شما را] بى‌نیاز می‌کند و سرمایه می‌بخشد؛"
        }
      ]
    },
    {
      "id": 4833,
      "verse_number": 49,
      "verse_key": "53:49",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927498,
          "resource_id": 135,
          "text": "و اوست که پروردگار [ستارۀ] «شِعْرىٰ» است؛"
        }
      ]
    },
    {
      "id": 4834,
      "verse_number": 50,
      "verse_key": "53:50",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927499,
          "resource_id": 135,
          "text": "و اوست که قوم عاد را در گذشته هلاک کرد؛"
        }
      ]
    },
    {
      "id": 4835,
      "verse_number": 51,
      "verse_key": "53:51",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927500,
          "resource_id": 135,
          "text": "و قوم ثمود را [نیز نابود کرد] و [هیچ یک از آنان را] باقی نگذاشت."
        }
      ]
    },
    {
      "id": 4836,
      "verse_number": 52,
      "verse_key": "53:52",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927501,
          "resource_id": 135,
          "text": "و قوم نوح را كه ستمكارتر و سركش‌تر بودند نیز پیش از آن [هلاک نمود]؛"
        }
      ]
    },
    {
      "id": 4837,
      "verse_number": 53,
      "verse_key": "53:53",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927502,
          "resource_id": 135,
          "text": "و شهرهاى زیر و روشده [قوم لوط] را [بالا برد و] درهم كوبید؛"
        }
      ]
    },
    {
      "id": 4838,
      "verse_number": 54,
      "verse_key": "53:54",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927503,
          "resource_id": 135,
          "text": "و آنان را [با باران سنگ] ـ چنان كه باید ـ فروپوشانید؛"
        }
      ]
    },
    {
      "id": 4839,
      "verse_number": 55,
      "verse_key": "53:55",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927504,
          "resource_id": 135,
          "text": "حال [اى انسان،] در مورد كدام یک از نعمت‌های پروردگارت تردید روا مى‌دارى [و مجادله می‌کنی]؟"
        }
      ]
    },
    {
      "id": 4840,
      "verse_number": 56,
      "verse_key": "53:56",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927505,
          "resource_id": 135,
          "text": "این [پیامبر نیز] هشداردهنده‌اى است از [گروه] هشداردهندگان پیشین."
        }
      ]
    },
    {
      "id": 4841,
      "verse_number": 57,
      "verse_key": "53:57",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927506,
          "resource_id": 135,
          "text": "قیامت نزدیک شد؛"
        }
      ]
    },
    {
      "id": 4842,
      "verse_number": 58,
      "verse_key": "53:58",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927507,
          "resource_id": 135,
          "text": "و هیچ کس ـ جز الله ـ نمی‌تواند آن را آشکار ‌کند [و سختی‌هایش را برطرف سازد]."
        }
      ]
    },
    {
      "id": 4843,
      "verse_number": 59,
      "verse_key": "53:59",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927508,
          "resource_id": 135,
          "text": "[ای مشرکان،] آیا از این سخن در شگفتید؟"
        }
      ]
    },
    {
      "id": 4844,
      "verse_number": 60,
      "verse_key": "53:60",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927509,
          "resource_id": 135,
          "text": "و [به آیات قرآن] مى‌خندید و [از شنیدنِ هشدارهایش نمی‌ترسید و] اشک نمى‌ریزید؟"
        }
      ]
    },
    {
      "id": 4845,
      "verse_number": 61,
      "verse_key": "53:61",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927510,
          "resource_id": 135,
          "text": "و در حال غفلت و غرور هستید؟"
        }
      ]
    },
    {
      "id": 4846,
      "verse_number": 62,
      "verse_key": "53:62",
      "hizb_number": 53,
      "rub_el_hizb_number": 211,
      "ruku_number": 464,
      "manzil_number": 7,
      "sajdah_number": 12,
      "page_number": 528,
      "juz_number": 27,
      "translations": [
        {
          "id": 927511,
          "resource_id": 135,
          "text": "پس [همگی] برای الله سجده کنید و [تنها] او را عبادت کنید."
        }
      ]
    }
  ]
}