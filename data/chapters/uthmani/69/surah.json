{
  "id": 69,
  "name": "The Reality",
  "verses": [
    {
      "id": 5324,
      "verse_number": 1,
      "verse_key": "69:1",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " ٱلْحَآقَّةُ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5325,
      "verse_number": 2,
      "verse_key": "69:2",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَا ٱلْحَآقَّةُ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5326,
      "verse_number": 3,
      "verse_key": "69:3",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5327,
      "verse_number": 4,
      "verse_key": "69:4",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5328,
      "verse_number": 5,
      "verse_key": "69:5",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5329,
      "verse_number": 6,
      "verse_key": "69:6",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5330,
      "verse_number": 7,
      "verse_key": "69:7",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَـٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5331,
      "verse_number": 8,
      "verse_key": "69:8",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ",
      "page_number": 566,
      "juz_number": 29
    },
    {
      "id": 5332,
      "verse_number": 9,
      "verse_key": "69:9",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَـٰتُ بِٱلْخَاطِئَةِ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5333,
      "verse_number": 10,
      "verse_key": "69:10",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5334,
      "verse_number": 11,
      "verse_key": "69:11",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَـٰكُمْ فِى ٱلْجَارِيَةِ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5335,
      "verse_number": 12,
      "verse_key": "69:12",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5336,
      "verse_number": 13,
      "verse_key": "69:13",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5337,
      "verse_number": 14,
      "verse_key": "69:14",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5338,
      "verse_number": 15,
      "verse_key": "69:15",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5339,
      "verse_number": 16,
      "verse_key": "69:16",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5340,
      "verse_number": 17,
      "verse_key": "69:17",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَـٰنِيَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5341,
      "verse_number": 18,
      "verse_key": "69:18",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5342,
      "verse_number": 19,
      "verse_key": "69:19",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَـٰبِيَهْ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5343,
      "verse_number": 20,
      "verse_key": "69:20",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنِّى ظَنَنتُ أَنِّى مُلَـٰقٍ حِسَابِيَهْ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5344,
      "verse_number": 21,
      "verse_key": "69:21",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5345,
      "verse_number": 22,
      "verse_key": "69:22",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فِى جَنَّةٍ عَالِيَةٍ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5346,
      "verse_number": 23,
      "verse_key": "69:23",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "قُطُوفُهَا دَانِيَةٌ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5347,
      "verse_number": 24,
      "verse_key": "69:24",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5348,
      "verse_number": 25,
      "verse_key": "69:25",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَأَمَّا مَنْ أُوتِىَ كِتَـٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَـٰلَيْتَنِى لَمْ أُوتَ كِتَـٰبِيَهْ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5349,
      "verse_number": 26,
      "verse_key": "69:26",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَمْ أَدْرِ مَا حِسَابِيَهْ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5350,
      "verse_number": 27,
      "verse_key": "69:27",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "يَـٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5351,
      "verse_number": 28,
      "verse_key": "69:28",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5352,
      "verse_number": 29,
      "verse_key": "69:29",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "هَلَكَ عَنِّى سُلْطَـٰنِيَهْ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5353,
      "verse_number": 30,
      "verse_key": "69:30",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "خُذُوهُ فَغُلُّوهُ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5354,
      "verse_number": 31,
      "verse_key": "69:31",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5355,
      "verse_number": 32,
      "verse_key": "69:32",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5356,
      "verse_number": 33,
      "verse_key": "69:33",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5357,
      "verse_number": 34,
      "verse_key": "69:34",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ",
      "page_number": 567,
      "juz_number": 29
    },
    {
      "id": 5358,
      "verse_number": 35,
      "verse_key": "69:35",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلَيْسَ لَهُ ٱلْيَوْمَ هَـٰهُنَا حَمِيمٌ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5359,
      "verse_number": 36,
      "verse_key": "69:36",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5360,
      "verse_number": 37,
      "verse_key": "69:37",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 502,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَـٰطِـُٔونَ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5361,
      "verse_number": 38,
      "verse_key": "69:38",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5362,
      "verse_number": 39,
      "verse_key": "69:39",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا لَا تُبْصِرُونَ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5363,
      "verse_number": 40,
      "verse_key": "69:40",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5364,
      "verse_number": 41,
      "verse_key": "69:41",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5365,
      "verse_number": 42,
      "verse_key": "69:42",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ",
      "page_number": 568,
      "juz_number": 29
    },
    {
      "id": 5366,
      "verse_number": 43,
      "verse_key": "69:43",
      "hizb_number": 57,
      "rub_el_hizb_number": 227,
      "ruku_number": 503,
      "manzil_number": 7,
      "sajdah_number": null,
