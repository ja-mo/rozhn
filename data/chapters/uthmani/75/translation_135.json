{
  "id": 75,
  "name": "The Resurrection",
  "verses": [
    {
      "id": 5552,
      "verse_number": 1,
      "verse_key": "75:1",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928217,
          "resource_id": 135,
          "text": "به روز قیامت سوگند می‌خورم."
        }
      ]
    },
    {
      "id": 5553,
      "verse_number": 2,
      "verse_key": "75:2",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928218,
          "resource_id": 135,
          "text": "و به نفسِ ملامت‌گر سوگند می‌خورم."
        }
      ]
    },
    {
      "id": 5554,
      "verse_number": 3,
      "verse_key": "75:3",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928219,
          "resource_id": 135,
          "text": "آیا انسان می‌پندارد که [پس از مرگ] هرگز استخوان‌هایش را جمع نخواهیم کرد؟"
        }
      ]
    },
    {
      "id": 5555,
      "verse_number": 4,
      "verse_key": "75:4",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928220,
          "resource_id": 135,
          "text": "آری، ما قادریم که [حتی خطوط سر] انگشتانش را یکسان و مرتب کنیم."
        }
      ]
    },
    {
      "id": 5556,
      "verse_number": 5,
      "verse_key": "75:5",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928221,
          "resource_id": 135,
          "text": "بلکه انسان می‌خواهد که [آزادانه و بدون ترس از آخرت،] در گناه و بدکاری مداومت کند."
        }
      ]
    },
    {
      "id": 5557,
      "verse_number": 6,
      "verse_key": "75:6",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928222,
          "resource_id": 135,
          "text": "[از این رو] می‌پرسد: «روز قیامت کی خواهد بود؟»"
        }
      ]
    },
    {
      "id": 5558,
      "verse_number": 7,
      "verse_key": "75:7",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928223,
          "resource_id": 135,
          "text": "هنگامی ‌که چشم [وحشت‌زده و] خیره شود"
        }
      ]
    },
    {
      "id": 5559,
      "verse_number": 8,
      "verse_key": "75:8",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928224,
          "resource_id": 135,
          "text": "و ماه تیره [و بی‌نور] گردد"
        }
      ]
    },
    {
      "id": 5560,
      "verse_number": 9,
      "verse_key": "75:9",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928225,
          "resource_id": 135,
          "text": "و خورشید و ماه، یکجا جمع شوند،"
        }
      ]
    },
    {
      "id": 5561,
      "verse_number": 10,
      "verse_key": "75:10",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928226,
          "resource_id": 135,
          "text": "آن روز، انسان می‌گوید: «راهِ گریز کجاست؟»"
        }
      ]
    },
    {
      "id": 5562,
      "verse_number": 11,
      "verse_key": "75:11",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928227,
          "resource_id": 135,
          "text": "هرگز! هیچ گریزگاهی نیست."
        }
      ]
    },
    {
      "id": 5563,
      "verse_number": 12,
      "verse_key": "75:12",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928228,
          "resource_id": 135,
          "text": "آن روز، قرارگاه [نهایی،] به سوی پروردگار توست."
        }
      ]
    },
    {
      "id": 5564,
      "verse_number": 13,
      "verse_key": "75:13",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928229,
          "resource_id": 135,
          "text": "در آن روز، انسان را از تمام كارهایى كه پیشاپیش فرستاده و کارهایی که به تاخیر انداخته [و انجام نداده است]، آگاه مى‌كنند."
        }
      ]
    },
    {
      "id": 5565,
      "verse_number": 14,
      "verse_key": "75:14",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928230,
          "resource_id": 135,
          "text": "بلکه انسان به خوبی بر خویشتن آگاه است."
        }
      ]
    },
    {
      "id": 5566,
      "verse_number": 15,
      "verse_key": "75:15",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928231,
          "resource_id": 135,
          "text": "اگر چه [در دفاع از خود،] عذرهایش را در میان آورد."
        }
      ]
    },
    {
      "id": 5567,
      "verse_number": 16,
      "verse_key": "75:16",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928232,
          "resource_id": 135,
          "text": "[ای پیامبر، هنگام نزول قرآن،] زبانت را برای [تکرار و خواندنِ] آن، شتابزده حرکت مده."
        }
      ]
    },
    {
      "id": 5568,
      "verse_number": 17,
      "verse_key": "75:17",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928233,
          "resource_id": 135,
          "text": "مسلماً جمع‌آوری و خواندنِ آن، بر [عهدۀ] ماست."
        }
      ]
    },
    {
      "id": 5569,
      "verse_number": 18,
      "verse_key": "75:18",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928234,
          "resource_id": 135,
          "text": "پس هر گاه [توسط جبرئیل] آن را [بر تو] خواندیم، از خواندنِ او پیروی کن."
        }
      ]
    },
    {
      "id": 5570,
      "verse_number": 19,
      "verse_key": "75:19",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 577,
      "juz_number": 29,
      "translations": [
        {
          "id": 928235,
          "resource_id": 135,
          "text": "سپس بیان‌کردنش بر [عهدۀ] ماست."
        }
      ]
    },
    {
      "id": 5571,
      "verse_number": 20,
      "verse_key": "75:20",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928236,
          "resource_id": 135,
          "text": "هرگز چنین نیست [که شما مشرکان می‌پندارید]! بلکه شما دنیا[ی زودگذر] را دوست دارید"
        }
      ]
    },
    {
      "id": 5572,
      "verse_number": 21,
      "verse_key": "75:21",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928237,
          "resource_id": 135,
          "text": "و آخرت را رها می‌کنید."
        }
      ]
    },
    {
      "id": 5573,
      "verse_number": 22,
      "verse_key": "75:22",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928238,
          "resource_id": 135,
          "text": "آن روز، [اهل ایمان دارای] چهره‌هایی تازه [و شاداب] هستند."
        }
      ]
    },
    {
      "id": 5574,
      "verse_number": 23,
      "verse_key": "75:23",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928239,
          "resource_id": 135,
          "text": "به سوی پروردگارشان می‌نگرند."
        }
      ]
    },
    {
      "id": 5575,
      "verse_number": 24,
      "verse_key": "75:24",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928240,
          "resource_id": 135,
          "text": "و در آن روز، [کافران و بدبختان دارای] چهره‌هایی عبوس [و درهم‌کشیده] هستند."
        }
      ]
    },
    {
      "id": 5576,
      "verse_number": 25,
      "verse_key": "75:25",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928241,
          "resource_id": 135,
          "text": "زیرا یقین دارند که آسیبی کمر‌‌شکن به آنها می‌رسد."
        }
      ]
    },
    {
      "id": 5577,
      "verse_number": 26,
      "verse_key": "75:26",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928242,
          "resource_id": 135,
          "text": "آری، چون [جان] به گلوگاهش برسد."
        }
      ]
    },
    {
      "id": 5578,
      "verse_number": 27,
      "verse_key": "75:27",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928243,
          "resource_id": 135,
          "text": "و گفته شود: چه کسی است که رُقیه بخواند؟ [شاید شفا یابد]."
        }
      ]
    },
    {
      "id": 5579,
      "verse_number": 28,
      "verse_key": "75:28",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928244,
          "resource_id": 135,
          "text": "و یقین کند که زمان جدایی [از دنیا فرا رسیده] است."
        }
      ]
    },
    {
      "id": 5580,
      "verse_number": 29,
      "verse_key": "75:29",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928245,
          "resource_id": 135,
          "text": "و [کار بالا گیرد و] ساق‌ها[ی پایش از سختیِ جان دادن] به هم بپیچد."
        }
      ]
    },
    {
      "id": 5581,
      "verse_number": 30,
      "verse_key": "75:30",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 514,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928246,
          "resource_id": 135,
          "text": "در آن روز، مسیرِ همه به سوی پروردگارت خواهد بود."
        }
      ]
    },
    {
      "id": 5582,
      "verse_number": 31,
      "verse_key": "75:31",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928247,
          "resource_id": 135,
          "text": "[منکرِ معاد،] نه تصدیق کرده و نه نماز گزارده است؛"
        }
      ]
    },
    {
      "id": 5583,
      "verse_number": 32,
      "verse_key": "75:32",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928248,
          "resource_id": 135,
          "text": "بلکه [قرآن را] تکذیب کرد و [از ایمان] روی گرداند."
        }
      ]
    },
    {
      "id": 5584,
      "verse_number": 33,
      "verse_key": "75:33",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928249,
          "resource_id": 135,
          "text": "سپس خرامان به سوی خانواده‌اش بازگشت."
        }
      ]
    },
    {
      "id": 5585,
      "verse_number": 34,
      "verse_key": "75:34",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928250,
          "resource_id": 135,
          "text": "وای بر تو! پس وای بر تو!"
        }
      ]
    },
    {
      "id": 5586,
      "verse_number": 35,
      "verse_key": "75:35",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928251,
          "resource_id": 135,
          "text": "باز هم وای بر تو! پس وای بر تو!"
        }
      ]
    },
    {
      "id": 5587,
      "verse_number": 36,
      "verse_key": "75:36",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928252,
          "resource_id": 135,
          "text": "آیا انسان گمان می‌کند که [بی‌هدف و بدون حساب و جزا] به خود رها می‌شود؟"
        }
      ]
    },
    {
      "id": 5588,
      "verse_number": 37,
      "verse_key": "75:37",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928253,
          "resource_id": 135,
          "text": "آیا [او] نطفه‌ای از منی نبود که [در رحم] ریخته می‌شود؟"
        }
      ]
    },
    {
      "id": 5589,
      "verse_number": 38,
      "verse_key": "75:38",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 578,
      "juz_number": 29,
      "translations": [
        {
          "id": 928254,
          "resource_id": 135,
          "text": "سپس به صورت خونِ بسته درآمد و [الله او را] آفرید و درست و استوار ساخت،"
        }
      ]
    },
    {
      "id": 5590,
      "verse_number": 39,
      "verse_key": "75:39",
      "hizb_number": 58,
      "rub_el_hizb_number": 231,
      "ruku_number": 515,
      "manzil_number": 7,
