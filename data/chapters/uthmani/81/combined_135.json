{
  "id": 81,
  "name": "The Overthrowing",
  "verses": [
    {
      "id": 5801,
      "verse_number": 1,
      "verse_key": "81:1",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": " إِذَا ٱلشَّمْسُ كُوِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928466,
          "resource_id": 135,
          "text": "آنگاه ‌که خورشید درهم پیچیده [و تاریک] گردد،"
        }
      ]
    },
    {
      "id": 5802,
      "verse_number": 2,
      "verse_key": "81:2",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928467,
          "resource_id": 135,
          "text": "و آنگاه ‌که ستارگان بی‌فروغ شوند،"
        }
      ]
    },
    {
      "id": 5803,
      "verse_number": 3,
      "verse_key": "81:3",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928468,
          "resource_id": 135,
          "text": "و آنگاه ‌که کوه‌ها به حرکت درآید،"
        }
      ]
    },
    {
      "id": 5804,
      "verse_number": 4,
      "verse_key": "81:4",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928469,
          "resource_id": 135,
          "text": "و آنگاه ‌که ماده‌شترانِ باردار رها شوند،"
        }
      ]
    },
    {
      "id": 5805,
      "verse_number": 5,
      "verse_key": "81:5",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928470,
          "resource_id": 135,
          "text": "و آنگاه ‌که حیوانات وحشی [در کنار انسان] گرد آورده شوند،"
        }
      ]
    },
    {
      "id": 5806,
      "verse_number": 6,
      "verse_key": "81:6",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928471,
          "resource_id": 135,
          "text": "و آنگاه ‌که دریا‌ها [جوشان و] برافروخته شود،"
        }
      ]
    },
    {
      "id": 5807,
      "verse_number": 7,
      "verse_key": "81:7",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928472,
          "resource_id": 135,
          "text": "و آنگاه ‌که هر کس با همسانِ خود قرین گردد،"
        }
      ]
    },
    {
      "id": 5808,
      "verse_number": 8,
      "verse_key": "81:8",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928473,
          "resource_id": 135,
          "text": "و آنگاه ‌که از دختر زنده‌به‌گورشده پرسیده شود"
        }
      ]
    },
    {
      "id": 5809,
      "verse_number": 9,
      "verse_key": "81:9",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "بِأَىِّ ذَنۢبٍ قُتِلَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928474,
          "resource_id": 135,
          "text": "[که] به کدامین گناه کشته شده است،"
        }
      ]
    },
    {
      "id": 5810,
      "verse_number": 10,
      "verse_key": "81:10",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلصُّحُفُ نُشِرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928475,
          "resource_id": 135,
          "text": "و آنگاه ‌که نامه‌های [اعمال] گشوده شود،"
        }
      ]
    },
    {
      "id": 5811,
      "verse_number": 11,
      "verse_key": "81:11",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928476,
          "resource_id": 135,
          "text": "و آنگاه ‌که پرده از روی آسمان برگرفته شود،"
        }
      ]
    },
    {
      "id": 5812,
      "verse_number": 12,
      "verse_key": "81:12",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928477,
          "resource_id": 135,
          "text": "و آنگاه ‌که دوزخ، افروخته [و شعله‌ور] گردد،"
        }
      ]
    },
    {
      "id": 5813,
      "verse_number": 13,
      "verse_key": "81:13",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928478,
          "resource_id": 135,
          "text": "و آنگاه ‌که بهشت [برای پرواپیشگان] نزدیک آورده شود."
        }
      ]
    },
    {
      "id": 5814,
      "verse_number": 14,
      "verse_key": "81:14",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928479,
          "resource_id": 135,
          "text": "[در آن هنگام،] هر کس خواهد دانست که چه چیزی [برای آخرت خود] آماده کرده است."
        }
      ]
    },
    {
      "id": 5815,
      "verse_number": 15,
      "verse_key": "81:15",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928480,
          "resource_id": 135,
          "text": "سوگند به ستارگانی که [به جایگاه خود] بازمی‌گردند،"
        }
      ]
    },
    {
      "id": 5816,
      "verse_number": 16,
      "verse_key": "81:16",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ٱلْجَوَارِ ٱلْكُنَّسِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928481,
          "resource_id": 135,
          "text": "[شتابان] حرکت می‌کنند [و از نظر] پنهان می‌شوند؛"
        }
      ]
    },
    {
      "id": 5817,
      "verse_number": 17,
      "verse_key": "81:17",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلَّيْلِ إِذَا عَسْعَسَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928482,
          "resource_id": 135,
          "text": "و سوگند به شب، هنگامی ‌که [از تاریکی‌اش کاسته شود و] پشت کند؛"
        }
      ]
    },
    {
      "id": 5818,
      "verse_number": 18,
      "verse_key": "81:18",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَٱلصُّبْحِ إِذَا تَنَفَّسَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928483,
          "resource_id": 135,
          "text": "و سوگند به صبح، هنگامی که بدمد؛"
        }
      ]
    },
    {
      "id": 5819,
      "verse_number": 19,
      "verse_key": "81:19",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928484,
          "resource_id": 135,
          "text": "بی‌تردید، این [قرآن، سخن الله است که ابلاغش به پیامبر، بر عهدۀ] فرستاده‌ای بزرگوار [=جبرئیل] است؛"
        }
      ]
    },
    {
      "id": 5820,
      "verse_number": 20,
      "verse_key": "81:20",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928485,
          "resource_id": 135,
          "text": "[همان فرشته‌ای] که [مقتدر و] نیرومند است و نزد [اللهِ] صاحب‌عرش، مقام والایی دارد؛"
        }
      ]
    },
    {
      "id": 5821,
      "verse_number": 21,
      "verse_key": "81:21",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "مُّطَاعٍ ثَمَّ أَمِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928486,
          "resource_id": 135,
          "text": "[در ملکوت اعلی و عالم فرشتگان] از او فرمان می‌برند و امین [وحی] است."
        }
      ]
    },
    {
      "id": 5822,
      "verse_number": 22,
      "verse_key": "81:22",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا صَاحِبُكُم بِمَجْنُونٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928487,
          "resource_id": 135,
          "text": "[ای مردم،] همنشین شما [= محمد] دیوانه نیست؛"
        }
      ]
    },
    {
      "id": 5823,
      "verse_number": 23,
      "verse_key": "81:23",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928488,
          "resource_id": 135,
          "text": "و بی‌تردید، [محمد]، او [= جبرئیل] را در افق روشن دیده است."
        }
      ]
    },
    {
      "id": 5824,
      "verse_number": 24,
      "verse_key": "81:24",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928489,
          "resource_id": 135,
          "text": "و او بر [ابلاغِ آنچه از طریق] غیب [به وی وحی می‌گردد] بخیل نیست [و همانند کاهنان نیز مزد نمی‌گیرد]."
        }
      ]
    },
    {
      "id": 5825,
      "verse_number": 25,
      "verse_key": "81:25",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا هُوَ بِقَوْلِ شَيْطَـٰنٍ رَّجِيمٍ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928490,
          "resource_id": 135,
          "text": "و این [قرآن] گفتۀ شیطان رانده‌شده نیست."
        }
      ]
    },
    {
      "id": 5826,
      "verse_number": 26,
      "verse_key": "81:26",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "فَأَيْنَ تَذْهَبُونَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928491,
          "resource_id": 135,
          "text": "پس به کجا می‌روید؟"
        }
      ]
    },
    {
      "id": 5827,
      "verse_number": 27,
      "verse_key": "81:27",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَـٰلَمِينَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928492,
          "resource_id": 135,
          "text": "این [قرآن] چیزی جز پند و اندرز برای جهانیان نیست."
        }
      ]
    },
    {
      "id": 5828,
      "verse_number": 28,
      "verse_key": "81:28",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928493,
          "resource_id": 135,
          "text": "برای هر یک از شما که بخواهد راه راست [حق را] در پیش گیرد."
        }
      ]
    },
    {
      "id": 5829,
      "verse_number": 29,
      "verse_key": "81:29",
      "hizb_number": 59,
      "rub_el_hizb_number": 234,
      "ruku_number": 525,
      "manzil_number": 7,
      "sajdah_number": null,
      "text_uthmani": "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَـٰلَمِينَ",
      "page_number": 586,
      "juz_number": 30,
      "translations": [
        {
          "id": 928494,
          "resource_id": 135,
          "text": "و شما [چیزی را] نمی‌خواهید، مگر آنکه پروردگار جهانیان [اراده کند و] بخواهد."
        }
      ]
    }
  ]
}