{
  "id": 108,
  "name": "The Abundance",
  "verses": [
    {
      "id": 6205,
      "verse_number": 1,
      "verse_key": "108:1",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 552,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928870,
          "resource_id": 135,
          "text": "[ای پیامبر،] به راستی که ما خیرِ بی‌نهایت [از جمله رود کوثر در بهشت را] به تو عطا کردیم؛"
        }
      ]
    },
    {
      "id": 6206,
      "verse_number": 2,
      "verse_key": "108:2",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 552,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928871,
          "resource_id": 135,
          "text": "پس برای پروردگارت نماز بخوان و قربانی کن."
        }
      ]
    },
    {
      "id": 6207,
      "verse_number": 3,
      "verse_key": "108:3",
      "hizb_number": 60,
      "rub_el_hizb_number": 240,
      "ruku_number": 552,
      "manzil_number": 7,
      "sajdah_number": null,
      "page_number": 602,
      "juz_number": 30,
      "translations": [
        {
          "id": 928872,
          "resource_id": 135,
          "text": "بی‌تردید، دشمنت [از همۀ خیرات و برکات و نام و نشان، بی‌بهره و] بُریده است."
        }
      ]
    }
  ]
}